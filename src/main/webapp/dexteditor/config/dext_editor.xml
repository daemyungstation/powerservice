    <server_domain></server_domain>
    <to_save_path_url>/dexteditor/img</to_save_path_url>
    <save_foldername_rule>YYYY/MM/</save_foldername_rule>
    <!-- "/", "YYYY/", "YYYY/MM/", "YYYY/MM/DD/" -->
    <save_filename_rule></save_filename_rule>
    <!-- Reserved Field -->
    <image_convert_format></image_convert_format>
    <!-- jpg or empty -->
    <image_convert_width>0</image_convert_width>
    <image_convert_height>0</image_convert_height>
    <image_auto_fit>1</image_auto_fit>
