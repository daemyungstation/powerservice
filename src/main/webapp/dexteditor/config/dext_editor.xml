  <setting>
    <runtimes>ieplugin</runtimes>
    <grid_color>#8b0000</grid_color>
    <grid_color_nm>Dark Red</grid_color_nm>
    <grid_spans>50</grid_spans>
    <grid_form>dot</grid_form>
    <encoding>utf-8</encoding>
    <doctype>XHTML 1.0 Transitional</doctype>
    <ie_compatible></ie_compatible>
    <!-- IE 랜더링 모드을 설정합니다. 예) EmulateIE8 또는 8 등 버전만 입력 합니다.  -->
    <xmlnsname></xmlnsname>
    <doc_lang>ko</doc_lang>
    <lang>ko</lang>
    <width>800px</width>
    <height>400px</height>
    <skinname></skinname>
    <font_family>돋움</font_family>
    <font_size>10pt</font_size>
    <line_height>120%</line_height>
    <show_font_real></show_font_real>
    <default_imemode>1</default_imemode>
    <!-- 0 기본값 사용, 1 : 실제 폰트명, 크기 사용-->
    <accessibility>0</accessibility>
    <topmenu>0</topmenu>
    <!-- 0 보이기, 1숨기기-->
    <toolbar>0</toolbar>
    <!-- 0 모두보이기, 1 첫번째 툴바 숨기기, 2 두번째 툴바 숨기기, 3 툴바 모두 숨기기-->
    <statusbar>0</statusbar>
    <!-- 0 보이기, 1숨기기-->
    <showdialog_pos>1</showdialog_pos>
    <!-- 1이면 작성화면에 맞춤, 그렇지 않으면 (메인)프레임창 중앙-->
    <source_viewtype>1</source_viewtype>
    <user_css_url></user_css_url>
    <!-- http:// 를 포함하는 전체 url-->
    <xhtml_value>0</xhtml_value>
    <!-- 0: 사용하지 않음 / 1 : xhtml 형식-->
    <view_mode_auto_height>0</view_mode_auto_height>
    <!-- 0: 사용하지 않음 / 1: 내용에 따라 height가 늘어남 -->
    <system_title></system_title>
    <!-- 기본값: DEXT5 Editor / 0: 사용하지 않음 / 사용자 입력값 사용 -->
    <table_auto_adjust>1</table_auto_adjust>
    <!-- 0: 사용하지 않음 / 1: 테이블관련 내용 수정시 사이즈 자동 보정 -->
    <auto_body_fit>1</auto_body_fit>
    <!-- 0: 사용하지 않음 / 1: 에더터의 내용을 에디터 사이즈안에 고정 -->
    <use_ruler ruler_init_position="680" view_pointer="1" view_guide_line="1">0</use_ruler>
    <icon_name></icon_name>
    <pageurl>
      <script>
        <symbol></symbol>
      </script>
      <pages>
      </pages>
    </pageurl>
    <image_base_url></image_base_url>
    <!-- 문서의 모든 상대 URL에 대한 기본 URL / 대상을 지정합니다 ,  http://localhost/dext5/  -->
    <cross_domain></cross_domain>
    <context_menu_disable>0</context_menu_disable>
    <enter_tag></enter_tag>
    <img_default_size width="" height=""></img_default_size>
    <!-- 이미지 삽입창의 Width, Height 기본 값을 지정합니다. -->
    <table_property width="" height="" class="" inoutdent="" init_inoutdent="" td_height="">
      <!-- 테이블 생성시 width height className 기본값 설정 inoutdent : 표 들여쓰기 내어쓰기 기본값 설정, init_inoutdent : 표생성시 들여쓰기되어 생성될값, td_height: 생성시 td 기본 height -->
      <class_list></class_list>
      <!-- 테이블에 지정가능한 ClassName  -->
      <line_style>solid,dotted,dashed,double,groove,ridge,inset,outset,none</line_style>
      <!-- 테이블 선모양 리스트 -->
    </table_property>
    <set_default_style body_id="dext_body" desc="0:기본값, 1:에디터 body영역만 스타일 적용, 3:getAPI 사용시 태그에 inline으로 스타일 적용">2</set_default_style>
    <custom_event>
      <image>
        <ondbclick></ondbclick>
        <!-- <img> 테그의 class명 또는 alt가 설정된 값과 같으면 설정된 값을 command로 하여 dext_editor_custom_action을 호출합니다. -->
      </image>
    </custom_event>
  </setting>
  <uploader_setting>
    <develop_langage>JSP</develop_langage>
    <!-- NET, JSP, PHP -->
    <handler_url></handler_url>
