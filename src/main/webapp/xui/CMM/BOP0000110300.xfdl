/***************************************************************************
 * 함  수  명		: fn_initCndt()
 * 기      능		: 조회조건 구성 
 ***************************************************************************/
 function fn_initCndt()
{
	//조회영역 clear
	fn_searchAreaClear();
	
	comp_bizStatCd.setInit();
	
	//조회조건 세팅 
	if(!gfn_isNull(pvInqDs)) {
		ds_bzopInfoInqCndt.loadXML(pvInqDs, true);	
			
		var strInqDiv = ds_bzopInfoInqCndt.getColumn(0, "inqDiv");
		if(strInqDiv == "") strInqDiv = "bzopNm";
		cbo_inqDiv.value = strInqDiv;
		
		//cbo_bizStatCd.value = ds_bzopInfoInqCndt.getColumn(0, "bizStatCd");
		comp_bizStatCd.setValue(ds_bzopInfoInqCndt.getColumn(0, "bizStatCd"));
		
		if(strInqDiv == "orgzCd") {
			//조직 코드
			edt_orgzCd.value = ds_bzopInfoInqCndt.getColumn(0, "orgzCd");
			//조직 명
			edt_orgzNm.value = ds_bzopInfoInqCndt.getColumn(0, "orgzNm");
			//직책
			cbo_dtrsCd.value = ds_bzopInfoInqCndt.getColumn(0, "dtrsCd");
			//직위
			cbo_jbttCd.value = ds_bzopInfoInqCndt.getColumn(0, "jbttCd");
		}
		else if(strInqDiv == "rrno") {
			Div01.msk_rrno.value = ds_bzopInfoInqCndt.getColumn(0, "rrno"); 
		}
		else if(strInqDiv == "tlphNo") {
			Div00.cbo_tlphRgn.value = ds_bzopInfoInqCndt.getColumn(0, "tlphRgnNo"); 
			Div00.edt_00.value      = ds_bzopInfoInqCndt.getColumn(0, "tlphTenmNo"); 
			Div00.edt_01.value      = ds_bzopInfoInqCndt.getColumn(0, "tlphSrno"); 
		}
		else if(strInqDiv == "clphNo") {
			Div03.cbo_tlphRgn.value = ds_bzopInfoInqCndt.getColumn(0, "clphRgnNo"); 
			Div03.edt_00.value      = ds_bzopInfoInqCndt.getColumn(0, "clphTenmNo"); 
			Div03.edt_01.value      = ds_bzopInfoInqCndt.getColumn(0, "clphSrno"); 			
		}
		else {
			edt_inqVl.value = ds_bzopInfoInqCndt.getColumn(0, "inqVl"); 		
		}
		
		btnSrh.setFocus();
	}
	else{
		cbo_inqDiv.value="bzopNm";
	}
	
	comp_bizStatCd.setValue('01');//cbo_bizStatCd.value = '01';
}

/***************************************************************************
 * 함  수  명		: fn_initAfter()
 * 기      능		: 초기화 후처리  
 ***************************************************************************/
 function fn_initAfter()
{
	//부모창의 선택조직 구성 
	if(!gfn_isNull(pvBzopNo)&&!gfn_isNull(pvBzopNm)) {

		var arrCd = pvBzopNo.split(",");	
		var arrNm = pvBzopNm.split(",");	
		
		if(pvSelType == "S") {
			ds_bzopInfoList.rowposition = ds_bzopInfoList.findRow("bzopNo="+arrCd[0]);
		}else {
			ds_rtnBzopInfo.clearData();
			for(var nRow=0; nRow<arrCd.length; nRow++){
				
				var tempDs  = gfn_getBopBzopNm(pvScrnDiv, arrCd[nRow]);
				var tempRow = ds_rtnBzopInfo.addRow();
				
				ds_rtnBzopInfo.copyRow(tempRow,tempDs,0 );	
			
// 				ds_rtnBzopInfo.addRow();
// 				ds_rtnBzopInfo.setColumn(nRow,'bzopNo',arrCd[nRow]);
// 				ds_rtnBzopInfo.setColumn(nRow,'bzopNm',arrNm[nRow]);
			}		
		}
	}		
	
	//조회조건 enable/disable
	cbo_inqDiv_onitemchanged(cbo_inqDiv);	
	
	if(gfn_getLength(edt_inqVl.value) > 0 || gfn_getLength(edt_orgzCd.value) > 0
			|| gfn_getLength(Div01.msk_rrno.value) > 0
			|| gfn_getLength(Div00.cbo_tlphRgn.value+Div00.edt_00.value+Div00.edt_01.value)>3
			|| gfn_getLength(Div03.cbo_tlphRgn.value+Div03.edt_00.value+Div03.edt_01.value)>3) {	
		//조회 시작 
		fn_commSearch(gfn_getCommTypeInit());	
	}			
}

