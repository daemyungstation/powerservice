/***************************************************************************
 * 함  수  명		: fn_selItem()
 * 기      능		: 선택조직 그리드에 data 세팅
 ***************************************************************************/
function fn_selItem(){
    
	var tmprRtnDs = ds_rtnBzopInfo;
	var tmprListDs = ds_bzopInfoList;
	
	if( tmprRtnDs.findRow("bzopNo", tmprListDs.getColumn(tmprListDs.rowposition, "bzopNo")) >= 0 ) {
		var tmprSelOrgz = "("+tmprListDs.getColumn(tmprListDs.rowposition, "bzopNm");
		tmprSelOrgz += "["+tmprListDs.getColumn(tmprListDs.rowposition, "bzopNo")+"])";
		alert("이미 선택되었습니다."+tmprSelOrgz);		
		return false;
	}	
	
    var iRowCnt = tmprRtnDs.rowcount;     
	
	if (iRowCnt >= 5 ){
		alert("5개 이상은 선택할 수 없습니다.","warning");
		return;
	}
	
	tmprRtnDs.addRow();
	tmprRtnDs.copyRow(tmprRtnDs.rowposition, tmprListDs, tmprListDs.rowposition);	
}

