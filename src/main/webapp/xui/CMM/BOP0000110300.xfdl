function fn_init()
{
	//1.param Data check! 
	if(gfn_isNull(pvSelType)) pvSelType = "S";
		
	//2.화면구성 
	fn_initScreen();
	
	//3.조회조건 구성
	fn_initCndt();
	
	//4.after
	fn_initAfter();
}

