/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능		: (공통버튼)조회 프로세스
 ***************************************************************************/
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 페이지 처리 기본설정
	if(!gfn_pageSearch(gfn_getTranTypeMCI(), sType, ds_bzopInfoList, sta_TotalCnt)) return;
	
	if(sType == gfn_getCommTypeInit()) {		
		ds_bzopInfoList.clearData();	
	}
	ds_bzopInfoListTmpr.clearData();
	
	var strInqDiv = cbo_inqDiv.value;
		
	ds_bzopInfoInqCndt.setColumn(0,"scrnDiv"    ,pvScrnDiv);
	ds_bzopInfoInqCndt.setColumn(0,"bizHdqrCd"  ,fv_bizHdqrCd);
	ds_bzopInfoInqCndt.setColumn(0,"inqDiv"     ,strInqDiv);
	//ds_bzopInfoInqCndt.setColumn(0,"bizStatCd"	,cbo_bizStatCd.value);	
	ds_bzopInfoInqCndt.setColumn(0,"bizStatCd"	,comp_bizStatCd.getValue());	
	
	if(strInqDiv =="orgzCd") {
		ds_bzopInfoInqCndt.setColumn(0,"orgzCd"    	,edt_orgzCd.value);
		ds_bzopInfoInqCndt.setColumn(0,"orgzNm"    	,edt_orgzNm.value);
		ds_bzopInfoInqCndt.setColumn(0,"dtrsCd" 	,cbo_dtrsCd.value);
		if(pvScrnDiv == "CMM01") 
			ds_bzopInfoInqCndt.setColumn(0,"jbttCd" ,cbo_jbttCd.value);			
	}else if(strInqDiv =="rrno") {
		ds_bzopInfoInqCndt.setColumn(0,"rrno" 		,Div01.msk_rrno.value);
	}else if(strInqDiv =="tlphNo") {
		ds_bzopInfoInqCndt.setColumn(0,"tlphRgnNo" 	,Div00.cbo_tlphRgn.value);
		ds_bzopInfoInqCndt.setColumn(0,"tlphTenmNo" ,Div00.edt_00.value);
		ds_bzopInfoInqCndt.setColumn(0,"tlphSrno" 	,Div00.edt_01.value);
	}else if(strInqDiv =="clphNo") {
		ds_bzopInfoInqCndt.setColumn(0,"clphRgnNo" 	,Div03.cbo_tlphRgn.value);
		ds_bzopInfoInqCndt.setColumn(0,"clphTenmNo" ,Div03.edt_00.value);
		ds_bzopInfoInqCndt.setColumn(0,"clphSrno" 	,Div03.edt_01.value);
	}else {
		 ds_bzopInfoInqCndt.setColumn(0,"inqVl" ,edt_inqVl.value);
	}	

	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_trscCode("BOP00001R03");				// MCI
	gfn_setKwData_trscType(gfn_getTranTypeSearch());

	gfn_tranMCI(sType 
				, "ds_bzopInfoInqCndt=ds_bzopInfoInqCndt"
				, "ds_bzopInfoListTmpr=ds_bzopInfoList"
				, "fn_commSearch_callBack");
	
}

/***************************************************************************
 * 함  수  명		: fn_commSearch_callBack()
 * 기      능		: [CallBack함수] Transaction Callback함수
 ***************************************************************************/
function fn_commSearch_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	}
	
	// 조회된 결과를 실제보여줄 데이타셋으로 Append처리
	gfn_appendSearchDs(ds_bzopInfoList, ds_bzopInfoListTmpr);
	
	// 페이지건수 조회 처리
	gfn_setTotalRowCount(ds_bzopInfoList, sta_TotalCnt);
	
	// 조회 후 처리
	fn_postSearch(strSvcID, nErrorCode);	
}

/***************************************************************************
 * 함  수  명		: fn_postSearch()
 * 기      능		: 조회 후 처리
 ***************************************************************************/
function fn_postSearch(sType, nErrorCode)
{
	if(nErrorCode != 0) {
		return;
	}
	
	if(ds_bzopInfoList.rowcount < 1){
		alert("조회 결과가 없습니다");
	} 
	
	if(pvSelType == "S") {
		if(!gfn_isNull(pvBzopNo)) {
			var arrCd = pvBzopNo.split(",");
			ds_bzopInfoList.rowposition = ds_bzopInfoList.findRow("bzopNo", arrCd[0]);			
		}
	}	
}

/***************************************************************************
 * 함  수  명		: fn_Conf()
 * 기      능		: 결과 데이터셋 리턴
***************************************************************************/
function fn_Conf(){
	var obj = new Object;
	var code ='';
	var name ='';
	var codein ='';
	
	var bzopdivcd = '';
	
	if(pvSelType == "S") {
		bzopdivcd = ds_bzopInfoList.getColumn(ds_bzopInfoList.rowposition, 'bzopDivCd');	
		code = ds_bzopInfoList.getColumn(ds_bzopInfoList.rowposition, "bzopNo");
		name = ds_bzopInfoList.getColumn(ds_bzopInfoList.rowposition, "bzopNm");	
		codein = "'"+ds_bzopInfoList.getColumn(ds_bzopInfoList.rowposition, "bzopNo")+"'";
		
		ds_rtnBzopInfo.clearData();
		ds_rtnBzopInfo.addRow();
		ds_rtnBzopInfo.copyRow(0, ds_bzopInfoList, ds_bzopInfoList.rowposition);		
	}
	else{
		bzopdivcd = gfn_gridColumeToComma(ds_bzopInfoList, 'bzopDivCd');	
		code = gfn_gridColumeToComma(ds_rtnBzopInfo,'bzopNo');
		name = gfn_gridColumeToComma(ds_rtnBzopInfo,'bzopNm');	
		codein = gfn_gridColumeToCommaQuotation(ds_rtnBzopInfo,'bzopNo');
	}

	obj["bzopdivcd"] = bzopdivcd;
	obj["code"] = code;
	obj["name"] = name;	
	obj["codein"] = codein;	
	if(pvSelType == "S") {
		if(ds_bzopInfoList.rowposition >= 0) {
			obj["inqDs"] = ds_bzopInfoInqCndt.saveXML();
		}
	}
	else {
		if(ds_rtnBzopInfo.rowcount >= 1) {
			obj["inqDs"] = ds_bzopInfoInqCndt.saveXML();
		}	
	}
			
	var ds_rtnDs = new Dataset();
	ds_rtnDs.assign(ds_rtnBzopInfo);	
	
	obj["rtnDs"] = ds_rtnDs;
	
	trace("rtnDs=====>"+ds_rtnDs.saveXML());
	close(obj);	
}

