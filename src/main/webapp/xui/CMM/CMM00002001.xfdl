//**************************************************************************

/***************************************************************************
 * 함  수  명		: btn_reg_onclick()
 * 기      능		: 등록 버튼 이벤트
 ***************************************************************************/
function btn_reg_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_saveCustInfo(gfn_getCommTypeSave(), "reg");
}


/***************************************************************************
 * 함  수  명		: fn_preSave()
 * 기      능		: 저장 전 처리
 ***************************************************************************/
function fn_preSave(sType)
{
	if(!gfn_preSaveDs(ds_custInfo, sType)) return false;
	if(!gfn_preSaveDs(ds_addrDtlt, sType)) return false;
	if(!gfn_preSaveDs(ds_custTlnoInfo, sType)) return false;
	
	return true;
}

/***************************************************************************
 * 함  수  명		: fn_saveCustInfo()
 * 기      능		: 저장
 ***************************************************************************/
function fn_saveCustInfo(sType, status)
{
	tmptTraceReq++
	trace("tmptTraceReq="+tmptTraceReq);
	

	//if (!fn_preSave(sType)) return false;
	
	// 인자값 셋팅
	if( !gfn_isNull(ds_addrDtlt_1.getColumn(ds_addrDtlt_1.rowposition, "addrTypCd")) ){
		ds_addrDtlt.copyRow(ds_addrDtlt.addRow(), ds_addrDtlt_1, ds_addrDtlt_1.rowposition);
	}
	
	if( !gfn_isNull(ds_addrDtlt_2.getColumn(ds_addrDtlt_2.rowposition, "addrTypCd")) ){
		ds_addrDtlt.copyRow(ds_addrDtlt.addRow(), ds_addrDtlt_2, ds_addrDtlt_2.rowposition);
	}

	trace(ds_addrDtlt.saveXML());
	
	if( !gfn_isNull(ds_custTlnoInfo_1.getColumn(ds_custTlnoInfo_1.rowposition, "tlnoTypCd")) ){
		ds_custTlnoInfo.copyRow(ds_custTlnoInfo.addRow(), ds_custTlnoInfo_1, ds_custTlnoInfo_1.rowposition);
	}
	
	
	if( !gfn_isNull(ds_custTlnoInfo_2.getColumn(ds_custTlnoInfo_2.rowposition, "tlnoTypCd")) ){
		ds_custTlnoInfo.copyRow(ds_custTlnoInfo.addRow(), ds_custTlnoInfo_2, ds_custTlnoInfo_2.rowposition);
	}
	
	var tmprCode = "";
	
	if("reg" == status){
		tmprCode = "CUS0000401";
	}else if("modify" == status){
		tmprCode = "CUS0000402";
	}
	
	// KWData 설정
	gfn_kwDataInit();
	
	gfn_setKwData_nameSpace("");
	gfn_setKwData_trscCode(tmprCode);
	gfn_setKwData_trscType("");
	gfn_setKwData_screenId("");
	gfn_setKwData_bizGroupCode("");
	gfn_setKwData_systemCode("KRP_TEST");	
				
	gfn_tranMCI(sType 
				, "ds_custInfo=ds_custInfo ds_addrDtlt=ds_addrDtlt ds_custTlnoInfo=ds_custTlnoInfo"
				, "ds_reslCustInfo=ds_reslCustInfo"
				, "fn_save_callBack");
				
	fn_datasetInit();
}

/***************************************************************************
 * 함  수  명		: fn_save_callBack()
 * 기      능		: [CallBack함수] Transaction Callback함수
 ***************************************************************************/
function fn_save_callBack(strSvcID, nErrorCode, strErrorMsg)
{

	if(nErrorCode != 0) {
		return;
	}
	
	
	trace("end");

	// 저장 후 처리
	fn_postSave(strSvcID);
	
}

/***************************************************************************
 * 함  수  명		: fn_postSave()
 * 기      능		: 저장 후 처리
 ***************************************************************************/
function fn_postSave(sType)
{
	var reslMsg = "["+ ds_reslCustInfo.getColumn(0, "custNm")+"]" + "고객님의 저장이 완료되었습니다.\n고객등록번호:"+ds_reslCustInfo.getColumn(0, "csno");
	alert(reslMsg);
}

