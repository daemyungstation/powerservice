/***************************************************************************
 * 함  수  명		: btn_bzopInfoInq_1_onclick
 * 기      능		: 사업자 찾기 버튼 클릭시
 * 작  성  자      : 정은실(erpgm05)
 ***************************************************************************/
function btn_bzopSearch_onclick(obj:Button,  e:ClickEventInfo)
{ 	
/*############################ !!!필수!!!  include "LIB::lib_BOP.xjs";  !!! #################################*/
	//화면구분
	/*
	구몬 교사 검색(교사검색)					KUM01
	구몬 배달사업자 검색(사업자검색)		KUM02
	EDU 전체 		                            EDU00
	EDU 영업 사업자 검색(EDU 파트너검색)		EDU01
	EDU TM 영업 사업자 검색(TM 파트너검색)	EDU02
	EDU 강사 검색(사업자검색)					EDU03
	EDU 배달사업자 검색(사업자검색)			EDU04
	L&C 영업 사업자 검색(L&C 플래너검색) 	LNC01
	L&C 강사 검색(사업자검색)					LNC02
	정규직/계약직 검색(직원검색) 			CMM01	
	집금사업자(사업자검색)	              	CMM02	
	*/	
	
	var pvScrnDiv 	= "EDU01" ;		 //화면업무구분
	var pvSelType 	= "M" ;			 //S:단건,M:멀티
	var pvBzopNo 	= edt_bzopNo ;	 //사업자번호editbox id :editbox id를 파라미터로 넘기면 검색되어 자동으로 세팅이 됩니다. 멀티선택일 경우 팝업창에서 선택되었던 값을 다시 세팅해줍니다.
	var pvBzopNm 	= edt_bzopNm ;	 //사업자명 editbox id   :editbox id를 파라미터로 넘기면 검색되어 자동으로 세팅이 됩니다.
	var pvInqDs 	= edt_inqDivBzop;//검색조건 editbox id :visable = false 로 한 editbox 가 있어야 팝업창에서 먼저 오픈했을때의 조회조건을 다시 세팅해줍니다. 

	var reVal = gfn_getBopBzopInfo(pvScrnDiv, pvSelType, pvBzopNo, pvBzopNm, pvInqDs); 	
	
	//사업자번호, 사업자명 자동 세팅 외 다른 정보를 사용 할 경우 	
	if(reVal instanceof Object) {
		trace("code-->"+reVal["code"]);	//사업자번호: 자동 세팅이 아닌  따로 사업자번호 사용 할 경우(멸티일 경우 ","로 구분 된 string)
		trace("name-->"+reVal["name"]);	//사업자명: 자동 세팅이 아닌  따로 사업자명 사용 할 경우(멸티일 경우 ","로 구분 된 string)
