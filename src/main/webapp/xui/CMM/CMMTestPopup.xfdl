/***************************************************************************
 * 함  수  명		: btn_prdtInq_onclick
 * 기      능		: 상품 조회
 ***************************************************************************/
function btn_prdtInq_onclick(obj:Button,  e:ClickEventInfo)
{

	var ds_prenInqCndt = new Dataset;
	ds_prenInqCndt.name = "ds_prenInqCndt";
	
	ds_prenInqCndt.addColumn("prdtNm", "string");
	ds_prenInqCndt.addColumn("prdtCd", "string");

	var nRow = ds_prenInqCndt.addRow();

	//TEST Code
	//ds_prenInqCndt.setColumn(nRow, "prdtNm", "빨간펜");
	//ds_prenInqCndt.setColumn(nRow, "prdtCd", "10001");
	
	ds_prenInqCndt.setColumn(nRow, "prdtNm", edt_prdtNm.text);
	
	var sPrmr = {ds_prenInqCndt:ds_prenInqCndt};
	var rtrnVal = gfn_diaLog("PDT02102201", -1, -1, 1010, 710, "EDU_PDT::PDT02102201.xfdl", false, sPrmr, false, false, true);
	
	trace("*** 상품 조회 결과 ***");
	
	if( rtrnVal == null ){
		trace("정보가 null 입니다.");
	}else{
		trace(rtrnVal.saveXML());
		trace("멀티 선택 가능, 첫번째 항목만 셋팅");
		edt_prdtNm.value = rtrnVal.getColumn(0, "prdtNm");
	}
	
}


