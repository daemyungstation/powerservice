// 타이머
function Frame_Top_ontimer(obj:Form, e:TimerEventInfo)
{
	fv_sec++;
	
	var nHour = Math.floor(fv_sec / 3600);
	var nMin =  Math.floor((fv_sec % 3600) / 60);
	var nSec =  Math.floor(((fv_sec % 3600) % 60) % 60);
	
	if (nHour < 10) { nHour = "0" + nHour;}
	if (nMin < 10) { nMin = "0" + nMin;}
	if (nSec < 10) { nSec = "0" + nSec;}
	
	sta_StatusTimer.text = nHour + ":" + nMin + ":" + nSec;
}
