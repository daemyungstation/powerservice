    <Script type="xscript4.0"><![CDATA[/*********************************************
 * 화면명 : 급여산출-개별(전사판매)
 * 작성자 : 정출연
 * 작성일 : 2016.12.14
 * 변경이력 : 
*/

/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var fv_list_tc = 0; // ds_list 페이징 관련 전체데이터 건수

//권한
var rgsnYn; //등록권한
var inqYn; //조회권한
var amntYn; //수정권한
var dltnYn; //삭제권한
var pritYn; //출력권한
var xlsYn; //엑셀권한
var athrPpupYn; //팝업오픈권한
var xlsRrnoYn; //엑셀 개인정보 권한 여부 

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function frm_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	fn_setAthrPgmBtn();
	
	ds_list_tmp.assign(ds_list);

    fn_init();
}
/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {	
		case "search":
			fn_postSearch();
			break;
	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회 전 처리
function fn_preSearch()
{
	var alow_dt = gfn_getCalVal(cal_alow_dt);
	
	if ( gfn_isEmpty(alow_dt) ) {
		alert("작업년월을 입력 해주세요.");
		cal_alow_dt.setFocus();
		return false;
	}
	
	alow_dt = alow_dt.substr(0,6);
	
	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "alow_dt", alow_dt);
	ds_cond.setColumn(0, "dept_code"	, edt_dept_cd.value	);
	ds_cond.setColumn(0, "emple_no"		, edt_emple_no.value);
	ds_cond.setColumn(0, "emple_nm"		, edt_emple_nm.value);
	return true;
}

//조회
function fn_search(obj, e)
{	
	// 유효성 체크
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "search";
    var szController  = "/dlw/alow/selectPerComptGrpList";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_list=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 조회 후 처리
function fn_postSearch() {

	trace(ds_list.saveXML());
	
	gfn_setTotCnt(ds_list, sta_TotalCnt); // 페이지건수 표출
	
	if (ds_list.rowcount < 1) {
		alert("조회된 데이터가 없습니다.");
	}
}


/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//초기화
function fn_init()
{
	cal_alow_dt.value = gfn_getToday().substr(0,6);
	
	edt_dept_cd.value 	= "";
	edt_dept_nm.value 	= "";
	edt_emple_no.value 	= "";
	edt_emple_nm.value 	= "";
}
/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
//조회 버튼 이벤트
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_search(obj, e);
}

// 그리드 헤더 클릭
function grd_onheadclick(obj:Grid, e:GridClickEventInfo)
{ 
	if (e.col == 0) {
		gfn_toggleGridCheck(obj);
	} else {
		gfn_grdHeadClick(obj, e);
	}
}
/********************************************************************************************/
/**   8. 팝업 관련 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 권한에 따른 버튼 설정
function fn_setAthrPgmBtn()
{
	gfn_athrPgm(gfn_gdsCd_admin());
	
	if (rgsnYn == "N") {
		btn_save.visible = false;
	}
	if (inqYn == "N") {
		btn_Search.visible = false;
	}
	if (xlsYn == "N") {
		btn_excl.visible = false;
	}
}

// 급여생성
function btn_alow(obj:Button,  e:ClickEventInfo)
{
	fn_save();
}


//저장 전 처리
function fn_preSave()
{
	var i, nPos, nCnt, alow_dt;
	
	nCnt = ds_list.getCaseCount("'1' == chk");
	alow_dt = cal_alow_dt.value + "";
	
	if (nCnt < 1) {
		alert("선택된 내역이 없습니다.");
		return false;
	}
	
	if ( gfn_isEmpty(alow_dt) ) {
		alert("작업년월을 입력 해주세요.");
		return false;
	}

	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "alow_dt", alow_dt);

	ds_list_tmp.clearData();
	for (i=0; i<ds_list.rowcount; ++i) {
		if ("1" == ds_list.getColumn(i, "chk")) {
			nPos = ds_list_tmp.addRow();
			ds_list_tmp.copyRow(nPos, ds_list, i);
		}
	}

	return true;
}

// 저장
function fn_save(obj, e)
{
	// 유효성 체크
	if ( !fn_preSave() ) {
		return;
	}
	
	var szSvcid       = "save";
    var szController  = "/dlw/alow/totComptAlowGrp";
    var szInDs        = "ds_input=ds_list_tmp:U ds_cond=ds_cond:U";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 저장 후 처리
function fn_postSave() {
	alert("급여생성이 완료 되었습니다.");
}

// 사내조직도 팝업 호출
function fn_openDeptPopup(p_dept_cd) {
	var dept_cd = "";
	if (!gfn_isEmpty(p_dept_cd)) {
		dept_cd = p_dept_cd;
	}
	var sPrmr = {p_dept_cd:dept_cd};
	
	var rst = gfn_diaLog("사내조직도", -1, -1, -1, -1, "COMM::DeptTreePopup.xfdl", false, sPrmr, false, false, true);
	if (!gfn_isEmpty(rst)) {
		edt_dept_cd.value = rst.dept_cd;
		edt_dept_nm.value = rst.dept_nm;
	}
}

