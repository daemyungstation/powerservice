    <Script type="xscript4.0"><![CDATA[/*********************************************
 * 화면명 : 수당지급대장
 * 작성자 : 정출연
 * 작성일 : 2016.12.19
 * 변경이력 : 
*/

/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var fv_list_tc = 0; // ds_list 페이징 관련 전체데이터 건수

//권한
var rgsnYn; //등록권한
var inqYn; //조회권한
var amntYn; //수정권한
var dltnYn; //삭제권한
var pritYn; //출력권한
var xlsYn; //엑셀권한
var athrPpupYn; //팝업오픈권한
var xlsRrnoYn; //엑셀 개인정보 권한 여부 

var fv_alow_close_rslt = "";

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function frm_onload(obj:Form, e:LoadEventInfo)
{
	var emple_no = gfn_getUserInfo("empl_no");
	trace("emple_no : " + emple_no);
	
	gfn_initForm(obj);
		
	gfn_getList("dlwCd","0124","fn_callBack",false);   // 대명코드조회 (01:납입방법,0115:입금취소사유)
		
	fn_setAthrPgmBtn();
	
	ds_list_tmp.assign(ds_list);
    
}
/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {	
		case "selectDlwCode":
			gfn_getCode("0124", "ALL", ds_0124, gds_DmlCd); 
			fn_init();			
			break;			
		case "search":
			fn_postSearch();
			break;			
		case "save":
			fn_postSave();
			break;
	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회 전 처리
function fn_preSearch()
{
	var alow_dt 		= div_monthCal.getDate();
	var dept_cd 		= edt_dept_cd.value;
	var pay_amt_chk 	= ckb_pay_amt.isChecked() ? "Y" : "N"; // 실지급액 0보다 큰건
	var alow_dtl_f7_chk = ckb_alow_dtl_f7.isChecked() ? "Y" : "N"; // 환수발생자
	
	if ( gfn_isEmpty(dept_cd) ) {
		alert("부서를 입력 해주세요.");
		edt_dept_cd.setFocus();
		return false;
	}
	
	ds_cond.clearData();
	ds_cond.addRow();
	
	ds_cond.setColumn(0, "alow_dt"			, alow_dt				);
	ds_cond.setColumn(0, "dept_code"		, edt_dept_cd.value		);
	ds_cond.setColumn(0, "emple_no"			, edt_emple_no.value	);
	ds_cond.setColumn(0, "emple_nm"			, edt_emple_nm.value	);
	ds_cond.setColumn(0, "dept_kind"		, "02"					);
	ds_cond.setColumn(0, "del_fg"			, cbo_del_fg.value		);
	ds_cond.setColumn(0, "incom_cl"			, cbo_incom_cl.value	);
	ds_cond.setColumn(0, "pay_amt_chk"		, pay_amt_chk			);
	ds_cond.setColumn(0, "alow_dtl_f7_chk"	, alow_dtl_f7_chk		); // 환수발생자
	
	return true;
}

//조회
function fn_search(obj, e)
{	
	// 유효성 체크
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "search";
    var szController  = "/dlw/alow/selectPayRollList";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_list=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 조회 후 처리
function fn_postSearch() {

	trace(ds_list.saveXML());
	
	gfn_setTotCnt(ds_list, sta_TotalCnt); // 페이지건수 표출
	
	if (ds_list.rowcount < 1) {
		alert("조회된 데이터가 없습니다.");
	}
}


