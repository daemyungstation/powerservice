function fn_setGridFormat() {
// 	var i, j, str;
// 	for (i=0; i<grd_list.getCellCount("Head"); ++i) {
// 		if (i<1) continue;
// 	}

	var i;
	var prefix 	= "use_yn_";
	var colId 	= "";		
	var useYnId = "";
	var val 	= "";
	var lstColId = [];
	
	for (i=0; i<fv_arrField.length; ++i) {
		colId = fv_arrField[i];
		// trace("colId : " + colId);
		
		useYnId = prefix + colId;
		// trace("useYnId : " + useYnId);
		
		val = ds_all_alow_cd.getColumn(0, useYnId);
		if ("Y" == val) {
			lstColId.push(colId);
		}
	}
	
	var setGridFormat = function(lstId) {
		var i, j, cellCnt;
		var bUse = false;
		var bindColId;
		var hasSaleType = false;
		
		cellCnt = grd_list.getCellCount("Head");
		trace("fn_setGridFormat> cellCnt : " + cellCnt);
		
		for (j=0; j<lstId.length; ++j) {
			if (lstId[j] == "sale_type") {
				hasSaleType = true;
			}
		}
		
		for (i=cellCnt-1; i>0; i--) { // no 컬럼은 항상 표시되므로 i>0	
			
		
			bindColId = grd_list.getCellProperty("Body", i, "text");
			bindColId = bindColId.replace("bind:", "");
			
			// trace("bindColId : " + bindColId);
		
			bUse = false;
			for (j=0; j<lstId.length; ++j) {
				if (lstId[j] == bindColId) {
					bUse = true;
					break;
				}
			}
			
			if (hasSaleType && "sale_type_nm" == bindColId) {
				bUse = true;
			}
			
			if (!bUse) {
				grd_list.deleteContentsCol(i);
			}
		}
	};
	
	fn_initGridFormat();
	setGridFormat(lstColId);
}

function fn_initGridFormat() {
	grd_list.formats = fv_grd_orig_formats;	
}

// 삭제 전처리
function fn_preDelete() {

	if (Dataset.ROWTYPE_INSERT == ds_list.getRowType(ds_list.rowposition)) {
		ds_list.deleteRow(ds_list.rowposition);
		return false;
	}

	if (ds_list.rowposition < 0) {
		alert("삭제할 데이터가 없습니다.");
		return false;
	}
	
	if (!confirm("정말로 삭제하시겠습니까?")) {
		return false;
	}
	
	var alow_cd 	= ds_list.getColumn(ds_list.rowposition, "alow_cd");
	var seq 		= ds_list.getColumn(ds_list.rowposition, "seq");
	
	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "alow_cd"	, alow_cd	);
	ds_cond.setColumn(0, "seq"		, seq		);
	
	ds_cond.updatecontrol = false;
	ds_cond.setRowType(0, Dataset.ROWTYPE_DELETE);
	ds_cond.updatecontrol = true;
	
	return true;
}

