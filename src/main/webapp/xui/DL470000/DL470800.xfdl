/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function frm_onload(obj:Form, e:LoadEventInfo)
{
	var i;
	
	fv_grd_orig_formats = grd_list.formats;
	
	gfn_initForm(obj);
	fn_setAthrPgmBtn();

	gfn_getList("dlwCd","95,06,10,96","fn_callBack",false);
	
	for (i=0; i<fv_arrField.length; ++i) {
		fv_mapField[fv_arrField[i]] = fv_arrField[i];
	}
	
	fv_field_pos_left 	= div_alow_cd.position.left;
	fv_field_pos_top 	= div_alow_cd.position.top;
	fv_field_pos_height	= div_alow_cd.position.height;
	
	fn_hideField();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {	
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("95", "", ds_95, gds_DmlCd); // dept_kind
			gfn_getCode("06", "", ds_06, gds_DmlCd); // posn_cd
			gfn_getCode("10", "", ds_10, gds_DmlCd); // job_duty
			gfn_getCode("96", "", ds_96, gds_DmlCd); // ocp_cluster
			
			fn_init(); // 검색부 초기화
			fn_selectAlowCdList();
			fn_selectProdCdList();
			fn_selectSaleTypeCdList();
			break;
			
		case "selectAlowCdList": // 수당코드 조회
			fn_postSelectAlowCdList();
			break;
			
		case "selectProdCdList": // 상품코드 조회
			fn_postSelectProdCdList();
			break;
			
		case "search":
			fn_postSearch();
			break;
			
		case "selectSaleTypeByStGrp":
			fn_postSelectSaleTypeCdList();
			break;
			
		case "delete":
			fn_postDelete();
			break;
			
		case "save":
			fn_postSave();
			break;
	}
}

