    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                   */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var xAccntSearchButton;

var nTotalListCnt1;
var nTotalListCnt2;
var nTotalListCnt3;
var nTotalListCnt4;

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	    */
/********************************************************************************************/
function DL471300_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj);
	fn_init();
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		btn_excel.visible = true;
	} else {
		btn_excel.visible = false;
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(sSvcId, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd);
		return false;
	} 
	else 
	{
		switch(sSvcId) 
		{   
		    // 조회
		    case "getFeesSynthesisInfo":
				var sTabPageId = tab_fees.tabpages[tab_fees.tabindex].name;
				if(sTabPageId == "tp_memberSpecFees") 
				{ 
					gfn_appendSearchDs(ds_listMemberSpecFees, ds_listMemberSpecFeesTemp);
					gfn_setTotalRowCount(ds_listMemberSpecFees, tab_fees.tp_memberSpecFees.sta_totalCnt, nTotalListCnt1);
					fn_postSearch(sSvcId);
				} 
				else if(sTabPageId == "tp_emplSpecFees") 
				{
					gfn_appendSearchDs(ds_listEmplSpecFees, ds_listEmplSpecFeesTemp);
					gfn_setTotalRowCount(ds_listEmplSpecFees, tab_fees.tp_emplSpecFees.sta_totalCnt, nTotalListCnt2);
					fn_postSearch(sSvcId);
				} 
				else if(sTabPageId == "tp_carryOverRefund") 
				{
					gfn_appendSearchDs(ds_listCarryOverRefund, ds_listCarryOverRefundTemp);
					gfn_setTotalRowCount(ds_listCarryOverRefund, tab_fees.tp_carryOverRefund.sta_totalCnt, nTotalListCnt3);
					fn_postSearch(sSvcId);
				}
				else if(sTabPageId == "tb_financialStatus") 
				{
					gfn_appendSearchDs(ds_listFinancialStatus, ds_listFinancialStatusTemp);
					gfn_setTotalRowCount(ds_listFinancialStatus, tab_fees.tb_financialStatus.sta_totalCnt, nTotalListCnt4);
					fn_postSearch(sSvcId);
				}
				else
				{
					alert("할당되지 아니한 업무영역입니다.");
					return false;
				}
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType, sSvcId)
{
	if (gfn_isNull(sSvcId) == true) 
	{
	    alert("서비스명이 올바르지 않습니다.");
	} 
	
	switch(sSvcId) 
	{
		case "getFeesSynthesisInfo" :			
			var sTabPageId = tab_fees.tabpages[tab_fees.tabindex].name;
			var sMonthCal = div_monthCal.getDate();			
			if (sMonthCal==""){
				alert("작업일을 등록하십시요!!");
				return false;
			}			
			var sEmpleNo = edt_empleNo.value;
			var sAccntNo = edt_accntNo.value;
			var sDeptCd = edt_deptCd.value;
			var sMonthCalTo = div_monthCalTo.getDate();
			
			ds_inqCndt.clearData();
			var nRow = ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(nRow, "svc_id", sTabPageId);
			ds_inqCndt.setColumn(nRow, "month_cal", sMonthCal);
			ds_inqCndt.setColumn(nRow, "month_cal_to", sMonthCalTo);
			ds_inqCndt.setColumn(nRow, "emple_no", sEmpleNo);
			ds_inqCndt.setColumn(nRow, "accnt_no", sAccntNo);
			ds_inqCndt.setColumn(nRow, "dept_cd", sDeptCd);

			ds_inqCndt.setColumn(nRow, "orderBy", "GDATE");
			ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
		break;
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
function fn_commSearch(sType, sSvcId)
{
	if (fn_preSearch(sType, sSvcId) == false) 
	{
		return false;
	}
	switch(sSvcId) 
	{
        case "getFeesSynthesisInfo": 
			var sTabPageId = tab_fees.tabpages[tab_fees.tabindex].name;
			
	        if(sTabPageId == "tp_memberSpecFees")
	        {
				if(!gfn_pageSearch(sType, ds_listMemberSpecFees, tab_fees.tp_memberSpecFees.sta_totalCnt, 500, nTotalListCnt1))
				{
					return;
				}
	        }
			else if(sTabPageId == "tp_emplSpecFees")
			{
				if(!gfn_pageSearch(sType, ds_listEmplSpecFees, tab_fees.tp_emplSpecFees.sta_totalCnt, 500, nTotalListCnt2))
				{
				    return;
				}
			}
			else if(sTabPageId == "tp_carryOverRefund")
			{
				if(!gfn_pageSearch(sType, ds_listCarryOverRefund, tab_fees.tp_carryOverRefund.sta_totalCnt, 500, nTotalListCnt3))
				{
					return;
				}
			}
			else if(sTabPageId == "tb_financialStatus")
			{
				if(!gfn_pageSearch(sType, ds_listFinancialStatus, tab_fees.tb_financialStatus.sta_totalCnt, 500, nTotalListCnt4))
				{
					return;
				}
			}
			else
			{
				alert("할당되지 아니한 업무영역입니다.");
				return false;
			}
			var szSvcid       = "getFeesSynthesisInfo";
			var szController  = "/dlw/pay/list/getFeesSynthesisInfo";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listMemberSpecFeesTemp=ds_output1 ds_listMemberSpecFeesSummary=ds_output1s " + 
								"ds_listEmplSpecFeesTemp=ds_output2 ds_listEmplSpecFeesSummary=ds_output2s " +
								"ds_listCarryOverRefundTemp=ds_output3 ds_listCarryOverRefundSummary=ds_output3s " +
								"ds_listFinancialStatusTemp=ds_output4 ";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;              
    }
}

function fn_postSearch(sSvcId)
{
	switch(sSvcId) 
	{
	    case "getFeesSynthesisInfo":
			var sTabPageId = tab_fees.tabpages[tab_fees.tabindex].name;
			
	        if(sTabPageId == "tp_memberSpecFees")
	        {
				if(ds_listMemberSpecFees.getRowCount() <= 0)
				{
					tab_fees.tp_memberSpecFees.edt_totT21T32Amt.value = "";
					tab_fees.tp_memberSpecFees.edt_totF07F11Amt.value = "";
					tab_fees.tp_memberSpecFees.edt_totF12Amt.value = "";
					tab_fees.tp_memberSpecFees.edt_totT33Amt.value = "";
					tab_fees.tp_memberSpecFees.edt_totF08F10Amt.value = "";
					
					alert("조회결과가 없습니다.");
					return false;
				}
				else
				{
					tab_fees.tp_memberSpecFees.edt_totT21T32Amt.value = gfn_appendComma(ds_listMemberSpecFeesSummary.getColumn(0, "tot_t21t32_amt"));
					tab_fees.tp_memberSpecFees.edt_totF07F11Amt.value = gfn_appendComma(ds_listMemberSpecFeesSummary.getColumn(0, "tot_f07f11_amt"));
					tab_fees.tp_memberSpecFees.edt_totF12Amt.value = gfn_appendComma(ds_listMemberSpecFeesSummary.getColumn(0, "tot_f12_amt"));
					tab_fees.tp_memberSpecFees.edt_totT33Amt.value = gfn_appendComma(ds_listMemberSpecFeesSummary.getColumn(0, "tot_t33_amt"));
					tab_fees.tp_memberSpecFees.edt_totF08F10Amt.value = gfn_appendComma(ds_listMemberSpecFeesSummary.getColumn(0, "tot_f08f10_amt"));
				}
	        }
			else if(sTabPageId == "tp_emplSpecFees")
			{
				if(ds_listEmplSpecFees.getRowCount() <= 0)
				{
