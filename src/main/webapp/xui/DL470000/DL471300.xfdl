		empleNo = "";
	}
	if (gfn_isEmpty(empleNm)) 
	{
		empleNm = "";
	}
	
	var sPrmr = {p_empl_nm : empleNm, p_empl_no : empleNo, p_pageTyp : "ETC", p_typ : "emp"};
	
	var mRet = gfn_diaLog("담당사원 조회", -1, -1, -1, -1, "PS990000::PS992000.xfdl", false, sPrmr, false, false, true, null, null, true);	
		
	if (gfn_isNull(mRet) == false) 
	{
		edt_empleNm.value = mRet.emple_nm;
		edt_empleNo.value = mRet.emple_no;
	}
	else
	{
		edt_empleNm.value = "";
		edt_empleNo.value = "";
	}
}

