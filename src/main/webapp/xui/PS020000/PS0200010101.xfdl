}

function fn_setAccntNo(){
	//trace(ds_accnt_info.saveXML())
	if(ds_accnt_info.getColumn(0, "accnt_no") != ""){	
		var accnt_no = ds_accnt_info.getColumn(0, "accnt_no");
		var mem_no = ds_accnt_info.getColumn(0, "mem_no");
		var mem_nm = ds_accnt_info.getColumn(0, "mem_nm");
		var cell = ds_accnt_info.getColumn(0, "cell");
		var prod_nm = ds_accnt_info.getColumn(0, "prod_nm");
		var emple_nm = ds_accnt_info.getColumn(0, "emple_nm");
		var idn_no = ds_accnt_info.getColumn(0, "idn_no");
		var join_dt = ds_accnt_info.getColumn(0, "join_dt");
		var kstbit = ds_accnt_info.getColumn(0, "kstbit");
		var yn = ds_accnt_info.getColumn(0, "yn");
		
		edt_accnt_no.value = accnt_no;
		edt_mem_no.value = mem_no;
		edt_mem_nm.value = mem_nm;
		edt_cell.value = cell;
		edt_prod_nm.value = prod_nm;
		edt_emple_nm.value = emple_nm;
		edt_brth_mon_day.value = idn_no;
		edt_join_dt.value = join_dt;
		edt_kstbit.value = kstbit;
		edt_yn.value = yn;
	}
	
	if (yn == "Y"){
		alert("CMS 및 CARD 정보를 해지 후 다시 신청해주시기 바랍니다.");
		btn_save.enable = false;
		return;
	}
	
	btn_save.enable = true;
}

