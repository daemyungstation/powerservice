    <Script type="xscript4.0"><![CDATA[/**************************************************************************
 [ PART 1 ]공통 js 파일 include
***************************************************************************/
include "LIB::lib_All.xjs";

var g_mem_no = "";
var g_mem_nm = "";

/**************************************************************************
 [ PART 2 ] Form에서 사용될 전역변수를 선언
**************************************************************************/
var tc_prod = 0;
var lenBrthMonDay; // (양수자) 생년월일

/***************************************************************************
 * 함  수  명		: prod_onload()
 * 기      능		: 폼 onload이벤트 
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function prod_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	
	var mem_no = p_mem_no;
	var mem_nm = p_mem_nm;
	g_mem_no = mem_no;
	g_mem_nm = mem_nm;
	
	edt_mem_no_ys.value = g_mem_no;
	edt_mem_nm_ys.value = g_mem_nm;
	
	// 양도회원정보 초기화
	ds_mem_dtl.clearData();
	ds_mem_dtl.addRow();
	
	var objDate = new Date();
	strDate = objDate.toFormatString("%Y%m%d");
		
	cal_stt_dt.value = strDate;
	
	//fn_commSearch(gfn_getCommTypeInit()); // 조회
	
	// 양수자 고객조회
	fn_srchYsMemberInfo();
}

/***************************************************************************
 * 함  수  명		: fn_preSearch()
 * 기      능		: 조회 전 처리 조회조건 설정
 * 수  정  일      : 2016.01.22 
 ***************************************************************************/
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_pay_mng_list.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "accnt_no", g_accnt_no);

	}
	return true;
}

/***************************************************************************
 * 함  수  명		: fn_preSave()
 * 기      능		: 조회 전 처리 조회조건 설정
 * 수  정  일      : 2016.01.22 
 ***************************************************************************/
function fn_preSave(){
	
	var yd_mem_no = edt_mem_no.value;
	var ys_mem_no = edt_mem_no_ys.value;
	
	if(yd_mem_no == ys_mem_no){
		alert("동일한 회원과 양도양수를 진행하실 수 없습니다.");
		return false;
	}

	return true;
}

function fn_preDel(){
	return true;
}

/***************************************************************************
 * 함  수  명		: fn_ip_save()
 * 기      능 		: 양도양수 처리
 * 수  정  일      : 2016.11.24 
 ***************************************************************************/
// 저장버튼 클릭 시
function fn_ip_save(obj:Button,  e:ClickEventInfo)
{

	if (!fn_preSave()) return false;
	
	var hanov_day = cal_stt_dt.value;
	if (hanov_day == null || hanov_day.length < 0){
		alert("양도양수 일자를 입력해주세요.");
		return;
	}	
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";	

	var cf = confirm("양도양수 처리하시겠습니까?");
	if (cf == false) return;

	    szSvcid       = "saveYdysMemTrans";
		szController  = "/dlw/cust/insydys-mem";
		szInDs        = "ds_input=ds_mem_dtl";
		szOutDs       = "";
		szParam       = "ys_mem_no=" + g_mem_no;
		szRetCallback = "fn_callBack";
		

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
}


/***************************************************************************
 * 함  수  명		: fn_callBack()
 * 기      능		: 조회 콜백 처리
 ***************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
  
    if (errcd < 0) {
		alert("조회를 실패하였습니다.\n" + errmsg);
    } else {
		switch(svcid){
			case "searchMoveGoodsList":
				fn_postSearch();
			break;
			case "saveYdysMemTrans":
				btn_save.enable = false;
				alert("양도양수 처리가 완료되었습니다.");
				opener.fn_change_member();
			break;
			// 양수자 고객조회
			case "srchYsCustBasi":
				var vBrthMonDay = ds_ys_mem_info.getColumn(0, "brth_mon_day");
				lenBrthMonDay = gfn_GetLength(vBrthMonDay);
				
				trace(123);
				trace(lenBrthMonDay);
				
				// 생년월일이 올바르지 않은 경우
				if(lenBrthMonDay != 8) {
					btn_save.enable = false; // 양도양수 불가
					btn_accnt_no.enable = false; // 팝업내 회원조회 불가
					alert("양수자의 생년월일이 정확하지 않습니다.\n\n먼저 생년월일 정보를 저장한 후, 진행하시기 바랍니다.");
				}
			break;
		}
    }
}

