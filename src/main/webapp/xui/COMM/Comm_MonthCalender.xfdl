            <Col id="Week_5">4</Col>
            <Col id="Week_6">5</Col>
          </Row>
        </Rows>
      </Dataset>
    </Objects>
    <Script type="xscript4.0"><![CDATA[
include "LIB::lib_All.xjs";

var nMoveMonth = 3;	// 개월이동 단위

var fv_Today;

function Comm_MonthCalender_onload(obj:Form, e:LoadEventInfo)
{
	gfn_commInit(obj);
	
	var sCurDate 	= gfn_getToday();
	//var sCurDate 	= "20121227";

	fv_Today = sCurDate;
	
	fn_setMonth(sCurDate);
	fn_setCellPos(sCurDate);
}

function btn_NextMonth_onclick(obj:Button,  e:ClickEventInfo)
{
	var sCurDate 	= gfn_addMonth(grd_Month_1.userdate, nMoveMonth);
	fn_setMonth(sCurDate);
	fn_setCellPos(msk_Date.value);
}

function btn_PreMonth_onclick(obj:Button,  e:ClickEventInfo)
{
	var sCurDate 	= gfn_addMonth(grd_Month_1.userdate, -nMoveMonth);
	fn_setMonth(sCurDate);
	fn_setCellPos(msk_Date.value);
}

function fn_setCellPos(sDate)
{
	for(var i=0;i<=2;i++)
	{
		var objGrid			= eval("grd_Month_"+i);
		var objDs			= eval("ds_Month_"+i);
		
		if(objGrid.userdate == sDate.substr(0,6)) {
			for(var j=0;j<objDs.rowcount;j++)
			{
				for(var k=0;k<objDs.getColCount();k++)
				{
					//trace("aaa::"+sDate.substr(6,2) + "  " + String(objDs.getColumn(j,k)).padLeft(2, "0"));
					if(sDate.substr(6,2) == String(objDs.getColumn(j,k)).padLeft(2, "0")) {
						objGrid.useselcolor = true;
						objDs.rowposition = j;
						objGrid.setCellPos((k/3));
						
						//trace("k::"+k);
						
						msk_Date.value = sDate;
						return;
					}
				}
			}
		} else {
			objGrid.useselcolor = false;
		}
	}
}

function fn_setMonth(sDefaultDate)
{
	var sDefaultMonth = sDefaultDate.substr(0,6);

	var arrMonth = new Array();
	arrMonth[0]	= gfn_addMonth(sDefaultMonth+"01", -1).substr(0,6);
	arrMonth[1]	= sDefaultMonth;
	arrMonth[2]	= gfn_addMonth(sDefaultMonth+"01", 1).substr(0,6);
	
	sta_Year.text = sDefaultMonth.substr(0,4);

	
// 	trace("sPreMonth::"+sPreMonth);
// 	trace("sMonth::"+sMonth);
// 	trace("sNextMonth::"+sNextMonth);

	for(var i=0;i<=2;i++)
	{
		var objGrid			= eval("grd_Month_"+i);
		var objDs			= eval("ds_Month_"+i);
		var objStaMonth		= eval("sta_Month_"+i);
		var objStaMonthNm	= eval("sta_MonthNm_"+i);
		
		fn_setMonthDate(arrMonth[i], objGrid, objDs, objStaMonth, objStaMonthNm);
	}

// 	fn_setMonthDate(sPreMonth, ds_preMonth, sta_PreMonth, sta_PreMonthNm);
// 	fn_setMonthDate(sMonth, ds_Month, sta_Month, sta_MonthNm);
// 	fn_setMonthDate(sNextMonth, ds_nextMonth, sta_NextMonth, sta_NextMonthNm);
	
	fn_setFocus("INIT", grd_Month_1);
	
// 	ds_preMonth.rowposition = 1;
// 	grd_PreMonth.setCellPos(2);
}

function fn_setMonthDate(sYYYYMM, objGrid, objDs, objStaMonth, objStaMonthNm)
{
	//trace(sYYYYMM);
	
	var nDay = gfn_getDay(sYYYYMM+"01");		//요일(0:일,1:월,2:화,3:수,4:목,5:금,6:토)
	var sLastDay = String(gfn_getLastDay(sYYYYMM)).substr(6,2);
	var nIndex = nDay;

	objGrid.userdate = sYYYYMM;
		
	objStaMonth.text 	= sYYYYMM.substr(4,2);
	objStaMonthNm.text 	= gfn_getMonthNm(sYYYYMM.substr(4,2));
	
	//trace("sLastDay::"+sLastDay);
	
	objDs.clearData();
	var nRow = objDs.addRow();

	for(var i=1;i<=sLastDay;i++)
	{
		if(nIndex == 7) {
			nRow = objDs.addRow();
			nIndex = 0;
		}
		
		objDs.setColumn(nRow, "Week_"+nIndex, i);
		//objDs.setColumn(nRow, "Week_Color_"+nIndex, "red");	// 해당 날짜(업체지정휴일 등) 색상 변경 요건발생시에 처리
		
		// 오늘날짜 예외처리
		var sToday = sYYYYMM + String(i).padLeft(2, "0");
		if(sToday == fv_Today) {
			objDs.setColumn(nRow, "Week_Color_"+nIndex, "#ffffff")
			objDs.setColumn(nRow, "Week_Today_"+nIndex, "URL('theme://images/bg_caltoday.png') center middle");
		}
		nIndex += 1;
	}
	
// 	trace("nDay::"+nDay);
// 	trace("sLastDay::"+sLastDay);

//	trace(objDs.saveXML());
}

function fn_setFocus(sDiv, objCurGrid)
{
	for(var i=0;i<=2;i++)
	{
		var objGrid			= eval("grd_Month_"+i);
		objGrid.useselcolor = false;
	}
	
	if(sDiv != "INIT") objCurGrid.useselcolor 		= true;
}

function Grid_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	var sDay = obj.getCellValue(e.row, e.cell);
	
	//trace("sDay::"+sDay);
	
	if(gfn_isNull(sDay)) { 
		fn_setCellPos(msk_Date.value);
		return;
	}
	//trace(sDay);

	fn_setFocus("SELECT", obj);
	
	var sDate = obj.userdate+String(obj.getCellValue(e.row, e.cell)).padLeft(2, "0");
	
	//trace("nSelDay::"+sDate);
	
	msk_Date.value = sDate;
}

// 현재선택된 일자 반환
function fn_getValue()
{
	return msk_Date.value;
}
]]></Script>
  </Form>
</FDL>
