/***************************************************************************
 * 기      능		: 조직검색(팝업)
 ***************************************************************************/  
function fn_getBopOrgzInfo()
{
	//EDU 전체 검색                            EDU00
	//EDU 영업 사업자 검색(EDU 파트너검색)	EDU01
	//EDU TM 영업 사업자 검색(TM 파트너검색)	EDU02
	//EDU 강사 검색(사업자검색)				EDU03
	//EDU 배달사업자 검색(사업자검색)		EDU04
	var pvScrnDiv  = "EDU01";          		//화면업무구분
	var pvSelType  = "S";                      //S:단건,M:멀티
	var pvOrgzCd   = Tab00.tabpage1.edt_orgzCd;     //조직코드 editbox id
	var pvOrgzNm   = Tab00.tabpage1.edt_orgzNm; //div_search.edt_orgzNm     //조직명 editbox id
	var pvInqDs    = Tab00.tabpage1.edt_inqDs;    //검색조건 editbox id

	//EDU 영업
	if( Tab00.tabpage1.cbo_orgzDiv.value == "1" ) pvScrnDiv  = "EDU01"; 
	else if( Tab00.tabpage1.cbo_orgzDiv.value == "2" ) pvScrnDiv  = "EDU02"; //TM 영업 사업자
	
	var reVal = gfn_getBopOrgzInfo(pvScrnDiv, pvSelType, pvOrgzCd, pvOrgzNm, pvInqDs);

	if(reVal instanceof Dataset) {		
		trace("code-->"+reVal["code"]);	//사업자번호: 자동 세팅이 아닌  따로 사업자번호 사용 할 경우(멸티일 경우 ","로 구분 된 string)
		trace("name-->"+reVal["name"]);	//사업자명: 자동 세팅이 아닌  따로 사업자명 사용 할 경우(멸티일 경우 ","로 구분 된 string)
		trace("codein-->"+reVal["codein"]);	//single quotation marks가 붙은 사업자번호 ex) '12345678','13467982'

		//리턴받은 dataset 사용	
		var rtnDs = reVal["rtnDs"];		
		
		ds_inqCndt1.setColumn(0, "chcOrgzLvlCd", rtnDs.getColumn(0, "orgzLvlCd")); 
		trace("fn_getBopOrgzInfo() rtnDs-->"+rtnDs.saveXML());
	}
}

