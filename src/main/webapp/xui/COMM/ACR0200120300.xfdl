function FrmDev_onload(obj:Form, e:LoadEventInfo)
{
	// 시스템 공통함수 호출
	gfn_commInit(obj);

	// 폼 공통함수 호출
	fn_init();

	// 공통코드 조회
	//fn_getCommCode();

	// 공통버튼 상태변경
	gfn_setCommBtn_Status(true,false,false,false,false,false);  // 조회, 초기화, 신규, 행추가, 저장, 삭제
	//gfn_setCommBtn_Status(false,false,false,false,false,false);
	
	// 그리드 공통버튼 초기화설정
	gfn_initCommGridBtn(obj, Tab00.tabpage1.div_gridCommBtn, Tab00.tabpage1.grd_list);
	
	// 그리드 공통버튼 초기화설정
	gfn_initCommGridBtn(obj, Tab00.tabpage2.div_gridCommBtn, Tab00.tabpage2.grd_list);

	// 그리드 공통버튼 상태변경
	gfn_setCommGridBtn_Status(Tab00.tabpage1.div_gridCommBtn,false,false,false,false,false,true,false,true,true);
	
	// 그리드 공통버튼 상태변경
	gfn_setCommGridBtn_Status(Tab00.tabpage2.div_gridCommBtn,false,false,false,false,false,true,false,true,true);	
}



