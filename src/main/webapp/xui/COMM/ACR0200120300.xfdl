/***************************************************************************
 * 함  수  명		: fn_init()
 * 기      능		: 폼 초기화 공통함수
 ***************************************************************************/
function fn_init()
{
	var toYmd = gfn_getToday();
	var toYm = toYmd.substr(0,6);

	//@조직탭 기본값 셋팅 
	Tab00.tabpage1.mae_acrsPerdDyStt.value = toYm+"01";
	Tab00.tabpage1.mae_acrsPerdDyEnd.value = toYmd;
	Tab00.tabpage1.cbo_orgzLvlCd.value = "E030"; 
	Tab00.tabpage1.rdo_acrsTyp.value = "1"; //실적유형 
	Tab00.tabpage1.rdo_acrsBas.value = "1"; //실적기준 
	
	//@주문유형탭 기본값 셋팅 
	Tab00.tabpage2.mae_acrsPerdDyStt.value = toYm+"01";
	Tab00.tabpage2.mae_acrsPerdDyEnd.value = toYmd;
	Tab00.tabpage2.rdo_acrsTyp.value = "1"; //실적유형 
	Tab00.tabpage2.rdo_acrsBas.value = "1"; //실적기준 	
}


/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능		: (공통버튼)조회 프로세스
 ***************************************************************************/
function fn_commSearch(sType)
{
	//조직탭일 경우 
	if( Tab00.tabindex == 0 ) fn_commSearch1(sType);
	//주문유형탭일 경우 
	else if( Tab00.tabindex == 1 ) fn_commSearch2(sType);
}

/***************************************************************************
 * 함  수  명		: fn_preSearch()
 * 기      능		: 조회 전 처리
 ***************************************************************************/
function fn_preSearch1(sType)
{
	var tabObj = Tab00.tabpage1;

	//@실적기간
	if (gfn_getLength(tabObj.mae_acrsPerdDyStt.value) == 0) {
		tabObj.mae_acrsPerdDyStt.setFocus();
		alert("실적기간을 입력해 주십시오.");
		return;
	}
	if (gfn_getLength(tabObj.mae_acrsPerdDyEnd.value) == 0) {
		tabObj.mae_acrsPerdDyEnd.setFocus();
		alert("실적기간을 입력해 주십시오.");
		return;
	}	

	//@실적기간 from<= 실적기간 To
	if(tabObj.mae_acrsPerdDyStt.value > tabObj.mae_acrsPerdDyEnd.value){
		tabObj.mae_acrsPerdDyStt.setFocus();
		alert("실적기간 시작일이 종료일보다 큽니다.\n입력값을 다시 확인하고 입력하여 주십시오.");
		return;	
	}
	//@조회구분
	if (gfn_getLength(tabObj.cbo_orgzLvlCd.value) == 0 ) {
		tabObj.cbo_orgzLvlCd.setFocus();
		alert("조회구분을 선택해 주십시오.");
		return;
	}
	//@실적유형
	if (gfn_getLength(tabObj.rdo_acrsTyp.value) == 0 ) {
		tabObj.rdo_acrsTyp.setFocus();
		alert("실적유형을 선택해 주십시오.");
		return;
	}	
	//@실적기준
	if (gfn_getLength(tabObj.rdo_acrsBas.value) == 0 ) {
		tabObj.rdo_acrsBas.setFocus();
		alert("실적기준을 선택해 주십시오.");
		return;
	}

	//@판매실적유형 과 실적 기준을 기준으로 조합(배열 형태로 서버로 전달)
	var acrsTypArnm = "";
	if(ds_inqCndt1.getColumn(0,"acrsTyp") == "1"){ //총접수
		acrsTypArnm = "1";
	}
	if(ds_inqCndt1.getColumn(0,"acrsTyp") == "2" && ds_inqCndt1.getColumn(0,"acrsBas") == "1"){ //총주문
		acrsTypArnm = "2";
	}
	if(ds_inqCndt1.getColumn(0,"acrsTyp") == "2" && ds_inqCndt1.getColumn(0,"acrsBas") == "2"){ //총주문 + 총주문 변경
		acrsTypArnm = "2, 3";
	}
	ds_inqCndt1.setColumn(0, "acrsTypArnm", acrsTypArnm);	

	trace("ds_inqCndt1:"+ds_inqCndt1.saveXML());
	return true;
}


/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능		: (공통버튼)조회 프로세스
 ***************************************************************************/
function fn_commSearch1(sType)
{
	if (!fn_preSearch1(sType)) return false;

	//@그리드 타이틀 세팅
	if(ds_inqCndt1.getColumn(0, "orgzLvlCd") == "E030"){ //총괄단
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 7, "text", "총괄단"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 8, "text", "조직코드"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 9, "text", "총괄단장"); 
	}else if(ds_inqCndt1.getColumn(0, "orgzLvlCd") == "E050"){ //센터 (수석 센터)
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 7, "text", "총괄단"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 8, "text", "센터"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 9, "text", "센터장"); 		
	}else if(ds_inqCndt1.getColumn(0, "orgzLvlCd") == "E150"){ //지구
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 7, "text", "센터"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 8, "text", "지국"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 9, "text", "지구장"); 				
    }else{//사업처, 사업국, 수석지국, 지국
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 7, "text", "센터"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 8, "text", "조직코드"); 
		Tab00.tabpage1.grd_list.setCellProperty( "Head", 9, "text", "지국장"); 				    
    }

	trace("ds_inqCndt1:"+ds_inqCndt1.saveXML());
	
	// KWData 설정
	gfn_kwDataInit();

	gfn_setKwData_trscCode("ACR02001R03");
	gfn_setKwData_trscType("");
	gfn_setKwData_screenId("");
	gfn_setKwData_bizGroupCode("");
	gfn_setKwData_systemCode("KRP"); 	

	gfn_tranMCI(sType 
				, "ds_inqCndt1=ds_inqCndt1"
				, "ds_inqResl1=ds_inqResl1"
				, "fn_commSearch1_callBack");

}

/***************************************************************************
 * 함  수  명		: fn_commSearch_callBack()
 * 기      능		: [CallBack함수] Transaction Callback함수
 ***************************************************************************/
function fn_commSearch1_callBack(strSvcID, nErrorCode, strErrorMsg)
{    
trace("ds_inqResl1:"+ds_inqResl1.saveXML());
	if(nErrorCode != 0) {
		return;
	}else{
		if(ds_inqResl1.rowcount < 1)	{		
			gfn_alert("IORD0000001"); //조회 결과가 없습니다.
		}	
		//검색결과 페이지수 
		gfn_setRowCountOfNotPaging(ds_inqResl1, Tab00.tabpage1.sta_TotalCnt); 		
	}
}





/***************************************************************************
 * 함  수  명		: fn_preSearch2()
 * 기      능		: 조회 전 처리
 ***************************************************************************/
function fn_preSearch2(sType)
{
//alert(Tab00.tabpage2.chk_ordrTyp102.value);
	var tabObj = Tab00.tabpage2;

	//@실적기간
	if (gfn_getLength(tabObj.mae_acrsPerdDyStt.value) == 0) {
		tabObj.mae_acrsPerdDyStt.setFocus();
		alert("실적기간을 입력해 주십시오.");
		return;
	}
	if (gfn_getLength(tabObj.mae_acrsPerdDyEnd.value) == 0) {
		tabObj.mae_acrsPerdDyEnd.setFocus();
		alert("실적기간을 입력해 주십시오.");
		return;
	}	
	//@실적기간 from<= 실적기간 To
	if(tabObj.mae_acrsPerdDyStt.value > tabObj.mae_acrsPerdDyEnd.value){
		tabObj.mae_acrsPerdDyStt.setFocus();
		alert("실적기간 시작일이 종료일보다 큽니다.\n입력값을 다시 확인하고 입력하여 주십시오.");
		return;	
	}
	
	//@주문유형
	var ordrTypArnm = "";
	if( tabObj.chk_ordrTyp101.value ) ordrTypArnm += ",101";  //EDU 영업
	if( tabObj.chk_ordrTyp103.value ) ordrTypArnm += ",103";  //EDU TM
	if( tabObj.chk_ordrTyp104.value ) ordrTypArnm += ",104";  //직원판매
	if( tabObj.chk_ordrTyp105.value ) ordrTypArnm += ",105";  //회사판매
	if( tabObj.chk_ordrTyp102.value ) ordrTypArnm += ",102";  //구몬영업
	ds_inqCndt2.setColumn(0, "ordrTypArnm", ordrTypArnm.substr(1,20) );	
	
	if (gfn_getLength(ordrTypArnm) == 0) {
		tabObj.chk_ordrTyp101.setFocus();
		alert("주문유형를 선택해 주십시오.");
		return;
	}


	//@실적유형
	if (gfn_getLength(tabObj.rdo_acrsTyp.value) == 0 ) {
		tabObj.rdo_acrsTyp.setFocus();
		alert("실적유형을 선택해 주십시오.");
		return;
	}	
	//@실적기준
	if (gfn_getLength(tabObj.rdo_acrsBas.value) == 0 ) {
		tabObj.rdo_acrsBas.setFocus();
		alert("실적기준을 선택해 주십시오.");
		return;
	}

	//@판매실적유형 과 실적 기준을 기준으로 조합(배열 형태로 서버로 전달)
	var acrsTypArnm = "";
	if(ds_inqCndt2.getColumn(0,"acrsTyp") == "1"){ //총접수
		acrsTypArnm = "1";
	}
	if(ds_inqCndt2.getColumn(0,"acrsTyp") == "2" && ds_inqCndt2.getColumn(0,"acrsBas") == "1"){ //총주문
		acrsTypArnm = "2";
	}
	if(ds_inqCndt2.getColumn(0,"acrsTyp") == "2" && ds_inqCndt2.getColumn(0,"acrsBas") == "2"){ //총주문 + 총주문 변경
		acrsTypArnm = "2, 3";
	}
	ds_inqCndt2.setColumn(0, "acrsTypArnm", acrsTypArnm);	

	trace("ds_inqCndt2:"+ds_inqCndt2.saveXML());
	return true;
}


