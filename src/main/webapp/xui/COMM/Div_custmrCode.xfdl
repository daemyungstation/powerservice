include "LIB::lib_All.xjs";
var fv_sCallBack = ""; // 조회 후 콜백 함수명

function frm_onload(obj:Form, e:LoadEventInfo) {
	//
}

function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "search":
				//
			break;
	    }
		return false;
	}
	
	switch (svcid) {
		case "search":
			fn_postSearch();
			break;
	}
}

//조회전 처리 조회조건셋팅
function fn_preSearch()
{
	ds_div_custmr_code_cond.clearData();
	ds_div_custmr_code_cond.addRow();	
	
	var condCnt			= 0;
	ds_div_custmr_code_cond.setColumn(0, "custmr_cd", edt_custmr_cd.value);	
	ds_div_custmr_code_cond.setColumn(0, "custmr_nm", edt_custmr_nm.value);
	
	return true;
}

//조회
function fn_search()
{	
	// 호출 건수 설정 및 조회여부 설정
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "search";
    var szController  = "/common/getCustomerCdList";
    var szInDs        = "ds_input=ds_div_custmr_code_cond";    
    var szOutDs       = "ds_div_custmr_code_list=ds_output";
    
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//조회 결과 
function fn_postSearch()
{
 	if ( ds_div_custmr_code_list.rowcount == 1 ) {
		edt_custmr_nm.value = ds_div_custmr_code_list.getColumn(0, "custmr_nm");
		edt_custmr_cd.value = ds_div_custmr_code_list.getColumn(0, "custmr_cd");
		edt_staff_nm.value = ds_div_custmr_code_list.getColumn(0, "staff_nm");
		
 		if(fv_sCallBack != "") {
			eval(fv_sCallBack + "()");
		}
 	} else {
		btn_custmr_onclick();
 	}
}

function edt_custmr_nm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13 && !gfn_isEmpty(obj.value)) {	
		fn_search();
	}
}

function btn_custmr_onclick(obj:Button,  e:ClickEventInfo)
{
	var ret = fn_openEventGoodsPopup("", edt_custmr_nm.value, false);
	if ( ret && ret.rowcount > 0 ) {
		edt_custmr_cd.value = ret.getColumn(0, "custmr_cd");
		edt_custmr_nm.value = ret.getColumn(0, "custmr_nm");
		edt_staff_nm.value 	= ret.getColumn(0, "staff_nm");
		
		if(fv_sCallBack != "") {
			eval(fv_sCallBack + "()");
		}
	}
}

function edt_custmr_nm_ontextchanged(obj:Edit,  e:TextChangedEventInfo)
{
	if (ds_div_custmr_code_list.rowcount > 0 && !gfn_isEmpty(edt_custmr_cd.value) ) {
		if ( edt_custmr_nm.value != ds_div_custmr_code_list.getColumn(0, "custmr_nm") ) {
			edt_custmr_cd.value = "";
			edt_custmr_nm.value = "";
		}
	}
	else if ( gfn_isEmpty(obj.value) ) {
		edt_custmr_cd.value = "";
	}
}

// 거래처 조회 팝업 (관련테이블 : CUSTMR_MNG)
function fn_openEventGoodsPopup(custmr_cd, custmr_nm) {
    var sPrmr = {p_custmr_cd:custmr_cd, p_custmr_nm:custmr_nm};
    var reVal = gfn_diaLog("거래처 조회", -1, -1, -1, -1, "COMM::CustomerPopup.xfdl", false, sPrmr, false, false, true, null, null, true);	
    return reVal;
}

/***********************************/
// 외부 제공 인터페이스
/***********************************/
function setCd(cd) {
	edt_custmr_cd.value = cd;
}

function setNm(nm) {
	edt_custmr_nm.value = nm;
}

function getCd() {
	return edt_custmr_cd.value;
}

function getNm() {
	return edt_custmr_nm.value;
}

function setReadonly(isReadonly) {
	edt_custmr_cd.readonly 	= isReadonly;
	edt_custmr_nm.readonly 	= isReadonly;
	btn_custmr.enable 		= !isReadonly;
}

function showButton(isVisible) {
	btn_custmr.visible = isVisible;
}

function bindCd(oDataset, sColumnId) {
	bfn_datasetBinding(oDataset, sColumnId, "edt_custmr_cd", "value");
}

function bindNm(oDataset, sColumnId) {
	bfn_datasetBinding(oDataset, sColumnId, "edt_custmr_nm", "value");
}

function bfn_datasetBinding(oDataset, sColumnName, sComponentId, sComponentPorp) {
    var bindItemId = this.name+sColumnName+sComponentId+sComponentPorp;
    if (this.all[bindItemId]) {
        var oldBindItem = this.removeChild(bindItemId);
        oldBindItem.destroy();
    }
    var bindItem = new BindItem();
    bindItem.init(bindItemId, sComponentId, sComponentPorp, oDataset.name, sColumnName);
    this.addChild(bindItemId, bindItem);
    bindItem.bind();
}

function setFocusCd() {
	edt_custmr_cd.setFocus();
}

function setFocusNm() {
	edt_custmr_nm.setFocus();
}

function setCallBack(sFn) {
	fv_sCallBack = sFn;
}

function clear() {
	ds_div_custmr_code_cond.clearData();
	ds_div_custmr_code_list.clearData();
	edt_custmr_cd.value = "";
	edt_custmr_nm.value = "";
	edt_staff_nm.value = "";
}
