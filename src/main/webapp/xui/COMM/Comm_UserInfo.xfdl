    <Script type="xscript4.0"><![CDATA[
include "LIB::lib_All.xjs";

function Comm_UserInfo_onload(obj:Form, e:LoadEventInfo)
{
	fn_setUserInfo();
}

function fn_setUserInfo()
{
	var objExcomm 	= new ExtCommon();
	var rtn 		= gfn_getUserInfo();

	for(var i in rtn)
	{
		//trace(i + "::" + rtn[i]);
		var nRow = this.ds_list_Temp.addRow();
		ds_list_Temp.setColumn(nRow, "ColId", i);
	}
	
	//trace(ds_list_Temp.saveXML());

	if(rtn instanceof Object) {
		for(var i in rtn)
		{
			var nRow = this.ds_list.findRow("ColId", i);
			if(nRow != -1) {
				ds_list.setColumn(nRow, "Value", rtn[i]);
			}
		}
	}	
}

