    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

//var nTotalListCnt = 0; // 문자발송이력 전체건수
var nTotalListCnt = 0;

var xPayMthd    = "";
var xReqBit     = "";
var xResultKind = "";
var xDelFg      = "";
var xAuth_yn    = "";
var xReqCnclBit = "";

var gvMenuId;  // 프로그램 로그 추가 2017.12.14
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL410407_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gvMenuId = GV_LOGMENUID;
	fn_init();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		return false;
	}
	
	switch (svcid) 
	{
		case "getWdrwReqList" : // 문자발송이력 조회		    			
		    gfn_appendSearchDs(ds_listWdrwReq, ds_listWdrwReqTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_listWdrwReq, sta_totalCnt, nTotalListCnt); // 페이지건수 표출
			fn_postSearch(svcid);
		break;
		
		case "getWdrwReqListExcel" : 
			//gfn_exportGridToExcel(grd_listExcel, "고객청구산출현황_"+gfn_getTodayFull()+"");
			alert("엑셀 다운로드가 완료되었습니다.");
		break;
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType, sSvcId)
{
	switch (sSvcId) 
	{
		case "getWdrwReqList" :
		    if (sType == gfn_getCommTypeInit())
			{				
			    var cReqDayStt = cal_reqDayStt.value;
				var sReqDayStt = new String(cReqDayStt);
				var cReqDayEnd = cal_reqDayEnd.value;
				var sReqDayEnd = new String(cReqDayEnd);
		
				if(sReqDayStt > sReqDayEnd)
				{
					alert("조회시작일이 조회 종료일보다 클수 없습니다.");
					return false;
				}
				
				if(gfn_getDayDiff(sReqDayStt, sReqDayEnd) >= 30)
				{
					alert("조회기간은 30일을 넘길수 없습니다.");
					return false;
				}
	
				if((gfn_IsNull(sReqDayStt) == false && sReqDayStt.length == 8) && 
				   (gfn_IsNull(sReqDayEnd) == false && sReqDayEnd.length == 8))
				{
					ds_inqCndt.clearData();
					ds_listWdrwReq.keystring = ""; // 정렬컬럼 초기화
					
					var nRow = ds_inqCndt.addRow();
	
					ds_inqCndt.setColumn(nRow, "req_day_stt"   , sReqDayStt);
					ds_inqCndt.setColumn(nRow, "req_day_end"   , sReqDayEnd);
					ds_inqCndt.setColumn(nRow, "pay_mthd"      , cbo_payMthd.value);
					ds_inqCndt.setColumn(nRow, "req_bit"       , cbo_reqBit.value);
					ds_inqCndt.setColumn(nRow, "result_kind"   , cbo_resultKind.value);
					ds_inqCndt.setColumn(nRow, "del_fg"        , cbo_delFg.value);
					ds_inqCndt.setColumn(nRow, "accnt_no"      , edt_accntNo.value);
					ds_inqCndt.setColumn(nRow, "orderBy"       , "REQ_DAY");
					ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
					if(cbo_payMthd.value == '06') {
						ds_inqCndt.setColumn(nRow, "auth_yn"      , cbo_payAuth.value);
					} else {
						ds_inqCndt.setColumn(nRow, "auth_yn"      , "");
					}
				}
				else
				{
					alert("조회날짜를 정확하게 입력하여주십시오.");
					return false;
				}
			}
			else if (sType == "GridClickSearch")
			{				
			    var cReqDayStt = cal_reqDayStt.value;
				var sReqDayStt = new String(cReqDayStt);
				var cReqDayEnd = cal_reqDayEnd.value;
				var sReqDayEnd = new String(cReqDayEnd);
		
				if(sReqDayStt > sReqDayEnd)
				{
					alert("조회시작일이 조회 종료일보다 클수 없습니다.");
					return false;
				}
				
				if(gfn_getDayDiff(sReqDayStt, sReqDayEnd) >= 30)
				{
					alert("조회기간은 30일을 넘길수 없습니다.");
					return false;
				}
	
				if((gfn_IsNull(sReqDayStt) == false && sReqDayStt.length == 8) && 
				   (gfn_IsNull(sReqDayEnd) == false && sReqDayEnd.length == 8))
				{
					ds_inqCndt.clearData();
					ds_listWdrwReq.keystring = ""; // 정렬컬럼 초기화
		
					ds_inqCndt.clearData();
					var nRow = ds_inqCndt.addRow();
				    
					ds_inqCndt.setColumn(nRow, "req_day_stt"   , sReqDayStt);
					ds_inqCndt.setColumn(nRow, "req_day_end"   , sReqDayEnd);
					ds_inqCndt.setColumn(nRow, "pay_mthd"      , xPayMthd);
					ds_inqCndt.setColumn(nRow, "req_bit"       , xReqBit);
					ds_inqCndt.setColumn(nRow, "result_kind"   , xResultKind);
					ds_inqCndt.setColumn(nRow, "del_fg"        , xDelFg);
					ds_inqCndt.setColumn(nRow, "accnt_no"      , "");
					ds_inqCndt.setColumn(nRow, "orderBy"       , "REQ_DAY");
					ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
					
					ds_inqCndt.setColumn(nRow, "auth_yn", xAuth_yn);
					ds_inqCndt.setColumn(nRow, "req_cncl_bit", xReqCnclBit);
				}
				else
				{
					alert("조회날짜를 정확하게 입력하여주십시오.");
					return false;
				}
			}
			return true;
		break;
		
		default :
			alert("서비스명이 잘못되었습니다.");
			return false;
		break;
	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 문자발송이력 조회
function fn_commSearch(sType, sSvcId)
{

	if (!fn_preSearch(sType, sSvcId)) return false;

	// 호출 건수 설정 및 조회여부 설정
	if (!gfn_pageSearch(sType, ds_listWdrwReq, sta_totalCnt, 40, nTotalListCnt)) return;

	var szSvcid       = "getWdrwReqList";
    var szController  = "/dlw/pay/getWdrwReqList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_listWdrwReqTemp=ds_output ds_calcAndChargeCard=ds_outputCard ds_calcAndChargeCMS=ds_outputCMS ds_calcAndChargeNauthCard=ds_outputCardNauth";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	var sToday = gfn_getToday();
	var sStartDate = gfn_addDate(sToday, -3);

	cal_reqDayStt.value  = sStartDate;
	cal_reqDayEnd.value  = sToday;
	cbo_payMthd.index    = 0;
	cbo_reqBit.index     = 0;
	cbo_resultKind.index = 0;
	cbo_delFg.index      = 0;
	edt_accntNo.index    = "";
	
	ds_inqCndt.clearData();
	ds_listWdrwReq.clearData();
	ds_listWdrwReqTemp.clearData();
	ds_listWdrwReqExcel.clearData();
	ds_calcAndChargeCard.clearData();
	ds_calcAndChargeCMS.clearData();
}

// 조회결과
function fn_postSearch(sSvcId)
{
	switch (sSvcId) 
	{
		case "getWdrwReqList" :
			var nListCnt = ds_listWdrwReq.getRowCount();
			if(nListCnt <= 0)
			{
				alert("조회된 데이터가 없습니다.");
				return false;
			}
		break;
	}
}

