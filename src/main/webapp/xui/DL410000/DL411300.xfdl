    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_list = 0;	// 전체건수
var g_reason_msg;   // 리턴메시지

var xlsYn; 		// 엑셀권한
var athrPpupYn; // 팝업오픈권한
var g_mode; // 작업구분(신규 또는 수정 등등)
var g_acntSrchGb; // S:조회조건의 회원조회, I:상세정보입력시 회원조회)

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL411300_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin()); // 권한
	gfn_getList("dlwCd","0114","fn_callBack",false);
	fn_init(); // 초기화
	
	//fn_commSearch(gfn_getCommTypeInit(), "srchSpecialAcntList"); // 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode" :
				gfn_getCode("0114", "SELECT", ds_kstbit, gds_DmlCd); // 코드조회결과 필터
			break;
			
			case "srchSpecialAcntList": // 특수회원 조회
				gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_list); // 페이지건수 표출
				trace("tc_list : " + tc_list);
			
				// 조회결과 존재시 하단 정보창 DS COPY
				if(ds_list.rowcount > 0){
					ds_dtl_list.clearData();
					ds_dtl_list.addRow();
					ds_dtl_list.copyRow(0, ds_list, ds_list.rowposition);
					
					// 수정, 삭제버튼 enable
					btn_update.enable = true;
					btn_delete.enable = true;
				} else {
					alert("조회 내역이 없습니다.");
					ds_dtl_list.clearData();
					
					// 수정, 삭제버튼 disable
					btn_update.enable = false;
					btn_delete.enable = false;
				}
				
				g_mode = "";
				// 컴포넌트 컨트롤
				fn_controlComponents();
			break;
			
			case "updateExtSpecial": // 저장처리
				if(gfn_isNull(g_reason_msg)) {
					alert("저장 완료");
					fn_commSearch(gfn_getCommTypeInit(), "srchSpecialAcntList"); // 조회
				} else {
					alert(g_reason_msg); // 실패 메시지 리턴
					g_reason_msg = "";
				}
			break;
			
			case "deleteExtSpecial": // 삭제처리
				alert("삭제 완료");
				fn_commSearch(gfn_getCommTypeInit(), "srchSpecialAcntList"); // 조회
			break;
			
			case "srchSpecialAcntListExcel": // 엑셀
				//gfn_exportGridToExcel(grd_list, "특수고객청구관리_"+gfn_getTodayFull()+"");
				
				//var colDS = gfn_excelPrepareExcelDS("grd_list");
				//gfn_excelDown("특수고객청구관리_"+gfn_getTodayFull()+"", "XLS_DLCC", "DlwPayMap.getSpecialAcntList", "DlwPayMap.getCntSpecialAcntList", ds_inqCndt, true, colDS, true);
				
				gfn_exportGridToExcel(grd_list_excel, "특수고객청구관리_"+gfn_getTodayFull()+"");
			break;
				
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
		ds_inqCndt.setColumn(0, "special_bit", cbo_srch_special_bit.value); // 01-임의지정, 02-격월납, 03-산출취소
		ds_inqCndt.setColumn(0, "del_fg", cbo_delFg.value);
		ds_inqCndt.setColumn(0, "orderBy", "ACCNT_NO");
		ds_inqCndt.setColumn(0, "orderDirection", "ASC");
		ds_inqCndt.setColumn(0, "sController", "");
		ds_inqCndt.setColumn(0, "dsName", "");
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회
function fn_commSearch(sType, searchInfo)
{
    switch(searchInfo) {
    
		// 조회
        case "srchSpecialAcntList":
			if (!fn_preSearch(sType)) return false;
			
			// 호출 건수 설정 및 조회여부 설정
			if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 2000, tc_list)) return;
		
			var szSvcid       = "srchSpecialAcntList";
			var szController  = "/dlw/pay/srchSpecialAcntList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
		
		// 엑셀 조회
        case "srchSpecialAcntListExcel":
			var szSvcid       = "srchSpecialAcntListExcel";
			var szController  = "/dlw/pay/srchSpecialAcntList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_excel=ds_output";
			var szParam       = "excel_yn=Y";
			var szRetCallback = "fn_callBack";
		break;
	}

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{
	edt_srch_accnt_no.value = "";
	cbo_srch_special_bit.index = 0; // 사유 : 전체
	cbo_delFg.index = 0; // 삭제여부 : 아니오
	
	if(GV_SYSTEMDIV == "DLMNG") 
	{
		btn_insert.visible = true;
		btn_update.visible = true;
		btn_delete.visible = true;
	}
	else
	{
		btn_insert.visible = false;
		btn_update.visible = false;
		btn_delete.visible = false;
	}
}

function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit(), "srchSpecialAcntList"); // 조회
}

// 회원조회 팝업
function fn_srchAccntNoPop()
{
	if(g_acntSrchGb == "S")
		var sPrmr = {p_srch_typ:"accnt_no", p_srch_vl:edt_srch_accnt_no.value, p_gubun_mode:"", p_option:""};
	else 
		var sPrmr = {p_srch_typ:"accnt_no", p_srch_vl:edt_accnt_no.value, p_gubun_mode:"", p_option:""};
	
	var reVal = gfn_diaLog("회원조회", -1, -1, -1, -1, "PS990000::PS992400.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 회원조회 버튼
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_srch_accntNo") {		// 회원번호조회 버튼 (조회조건)
		g_acntSrchGb = "S";
		fn_srchAccntNoPop();
	}
	else if (obj.name == "btn_accntNo") {		// 회원번호조회 버튼 (상세정보)
		g_acntSrchGb = "I";
		fn_srchAccntNoPop();
	}
}

// onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if (obj.name == "edt_srch_accnt_no") {
			g_acntSrchGb = "S";
		} else if (obj.name == "edt_accnt_no") {
			g_acntSrchGb = "I";
		}
		
		fn_srchAccntNoPop();
	}
}

// 회원조회 팝업에서 회원선택시
function fn_setAccntNo() {
	//trace(ds_accnt_info.saveXML());
	if(g_acntSrchGb == "S")
		edt_srch_accnt_no.value = ds_accnt_info.getColumn(0, "accnt_no");
	else
		edt_accnt_no.value = ds_accnt_info.getColumn(0, "accnt_no");
	
	g_acntSrchGb = "";
}

/* 삭제처리 */
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	if(!confirm("삭제하시겠습니까?")) return false;
		
	var szSvcid       = "deleteExtSpecial";
    var szController  = "/dlw/pay/extSpecial/delete";
    var szInDs        = "ds_input=ds_dtl_list";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}



/* 신규버튼 */
function btn_insert_onclick(obj:Button,  e:ClickEventInfo)
{
	// 상세정보 초기화
	ds_dtl_list.clearData();
	ds_dtl_list.addRow();
	
	g_mode = "insert";
	fn_controlComponents();
	
	// 회원번호 focus
	edt_accnt_no.setFocus();
}


/* 수정버튼 */
function btn_update_onclick(obj:Button,  e:ClickEventInfo)
{
	g_mode = "update";
	fn_controlComponents();
}

/* 취소버튼 */
function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	g_mode = "";
	fn_controlComponents();
}

/* 컴포넌트 컨트롤 */
function fn_controlComponents() {
	if(GV_SYSTEMDIV == "DLMNG") 
	{
		if(g_mode == "insert" || g_mode == "update")
		{
			// 신규 입력시
			if(g_mode == "insert") {
				edt_accnt_no.enable = true;
				btn_accntNo.enable = true;
				cbo_special_bit.enable = true;
				edt_req_pay_gunsu.enable = true;
				edt_special_etc.enable = true;
			}
			// 수정시
			else if(g_mode == "update") {
				cbo_special_bit.enable = true; // 사유
				edt_req_pay_gunsu.enable = true; // 청구요청건수
				edt_special_etc.enable = true;
				cbo_special_bit.setFocus();
			}
		
			btn_save.position2 = "absolute r:82 w:72 b:92 h:20";
			btn_cancel.position2 = "absolute r:6 w:72 b:92 h:20";
			btn_save.visible = true;
			btn_cancel.visible = true;
			btn_insert.visible = false;
			btn_update.visible = false;
			btn_delete.visible = false;
		}
	
		// 취소시
		//else if(g_mode == "cancel")
		// 취소 또는 그리드 선택시
		else
		{
			edt_accnt_no.enable = false;
			btn_accntNo.enable = false;
			cbo_special_bit.enable = false;
			edt_req_pay_gunsu.enable = false;
			edt_special_etc.enable = false;
			
			btn_save.visible = false;
			btn_cancel.visible = false;
		
			if(cbo_delFg.value == "N") {
				btn_insert.visible = true;
				btn_update.visible = true;
				btn_delete.visible = true;
				btn_insert.position2 = "absolute r:158 w:72 b:92 h:20";
				btn_update.position2 = "absolute r:82 w:72 b:92 h:20";
				btn_delete.position2 = "absolute r:6 w:72 b:92 h:20";
			}
			else if(cbo_delFg.value == "Y") {
				btn_update.visible = false;
				btn_delete.visible = false;
				btn_insert.visible = true;
				btn_insert.position2 = "absolute r:6 w:72 b:92 h:20";
			}
		
			// 데이터셋 데이터 유무에 따라 상세정보 설정
			if(ds_list.rowposition == -1) {
				ds_dtl_list.clearData();
			} else {
				ds_dtl_list.copyRow(0, ds_list, ds_list.rowposition);
			}
		}
	}
}

/* 저장처리 */
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if(!confirm("저장하시겠습니까?")) return false;
	
	// 유효성체크
	if(!fn_preSave()) return false;
		
	var szSvcid       = "updateExtSpecial";
    var szController  = "/dlw/pay/extSpecial/"+g_mode;
    var szInDs        = "ds_input=ds_dtl_list";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 저장전
function fn_preSave()
{

	if (gfn_isNull(edt_accnt_no.value)) {
		alert("회원번호가 없습니다.");
		edt_accnt_no.setFocus();
		return false;
	} else if (gfn_isNull(cbo_special_bit.value)) {
		alert("사유를 입력하세요.");
		cbo_special_bit.setFocus();
		return false;
	} else if ( gfn_isNull(edt_req_pay_gunsu.value) && cbo_special_bit.value == "01" ) {
		alert("청구건수를 입력하세요.");
		edt_req_pay_gunsu.setFocus();
		return false;
	}
	
	return true;
}

function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend(), "srchSpecialAcntList");
}

// 엑셀
function btn_excl_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_list.getRowCount() > 0) {
		var colDS = gfn_excelPrepareExcelDS("grd_list");
		gfn_excelDown("특수고객청구관리", "XLS_DLCC", "DlwPayMap.getSpecialAcntList", "DlwPayMap.getCntSpecialAcntList", ds_inqCndt, true, colDS, true);
	} else {
		alert("엑셀다운로드 할 대상이 없습니다.");
	}
}

// 로우 변경시
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	ds_dtl_list.clearData();
	ds_dtl_list.addRow();
	ds_dtl_list.copyRow(0, ds_list, ds_list.rowposition);
	
	g_mode = ""; // CRUD 구분변수 초기화
	
	// 로우 변경시 컴포넌트 초기화
	if(e.row != e.oldrow) {
		fn_controlComponents();
	}
}

// 삭제여부 변경시 초기화
function cbo_delFg_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
