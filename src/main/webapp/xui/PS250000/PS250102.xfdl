// 그리드 헤더 클릭
function Grid_Main_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (!fn_preSearch()) return false;
	
	ds_inqCndt.setColumn(0, "sController", "/edct-trpr/edct-trpr/user-list");
	ds_inqCndt.setColumn(0, "dsName", "ds_output");
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_user, sta_TotalCnt, 25, tc_user)) return;
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_user, sta_TotalCnt, tc_user); // 페이지건수 표출
}

function Grid_Sub_onheadclick(obj:Grid, e:GridClickEventInfo)
