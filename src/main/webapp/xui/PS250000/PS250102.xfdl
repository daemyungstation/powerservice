    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";
var tc_user = 0; // 전체 건수
var today = gfn_getToday(); //오늘날짜를 반환
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS250102_onload(obj:Form, e:LoadEventInfo)
{
	ds_list_copy.clearData();
	ds_list_copy.addRow();
	ds_list_copy.copyRow(0, opener.ds_edctCrs, opener.ds_edctCrs.rowposition);
	
	cal_edct_atnc_dt.value = today;
	
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_createTeamUserInqDs(obj); // 팀에 따른 사용자 조회를 위한 데이터셋 설정
	gfn_getList("cd", "USR030");  // 공통코드조회
	
	gfn_getList("teamCd","", null, null, null, "ds_allTeamCd"); // 팀코드 조회
		
	fn_commSearch(gfn_getCommTypeInit()); // 조회

}
/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
		case "selectTeamCode":
			gfn_getTeamCodeLoad("", "ALL", ds_hgrnTeamCd, ds_allTeamCd, "1");
			fn_init();			
			break;
		
		case "selectUserCode":
			gfn_getUserCdLoad("", "" ,"ALL", ds_userId);
				
			cbo_srch_user_id.index = 0;
			
			if (ds_userId.rowcount > 1) {
				cbo_srch_user_id.enable = true;
			} else {
				cbo_srch_user_id.enable = false;
			}
			break;
		
		case "searchUserList":
			//trace(ds_list_tmp2.saveXML());
			gfn_appendSearchDs(ds_list_user, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_user, sta_TotalCnt, tc_user); // 페이지건수 표출
			fn_srchEdctTrpr(gfn_getCommTypeInit());
			break;
		
		case "srchEdctTrpr":
			gfn_appendSearchDs(ds_list_edctTrpr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리				
			break;
					
		case "insertEdctTrpr":
			for (var i=0; i<ds_list_user.rowcount; i++) {
				ds_list_user.setColumn(i, "rowCheck", "0");
			}
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "deleteEdctTrpr":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "updateEdctTrpr":
			fn_srchEdctTrpr(gfn_getCommTypeInit());
			opener.fn_commSearch(gfn_getCommTypeInit());
			break;
		}
	}
}	

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "edct_crs_id", ds_list_copy.getColumn(0, "edct_crs_id"));
		ds_inqCndt.setColumn(0, "team1_cd", cbo_srch_hgrn_team_cd.value);
		ds_inqCndt.setColumn(0, "team2_cd", cbo_srch_team_cd.value);
		ds_inqCndt.setColumn(0, "user_id", cbo_srch_user_id.value);
		ds_inqCndt.setColumn(0, "bzpt_div2", gds_UserInfo.getColumn(0, "bzpt_div"));
	}
	
	return true;
}

// 조회2 체크
function fn_preSearch_sub(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt_sub.clearData();
		ds_inqCndt_sub.addRow();
		var sEdctCrsId = ds_list_copy.getColumn(0, "edct_crs_id");
		ds_inqCndt_sub.setColumn(0, "edct_crs_id", sEdctCrsId);
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType:String)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
 	if(!gfn_pageSearch(sType, ds_list_user, sta_TotalCnt, 25, tc_user)) return;

 	var szSvcid       = "searchUserList";
    var szController  = "/edct-trpr/edct-trpr/user-list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//교육대상자목록조회
function fn_srchEdctTrpr(sType:String)
{
	ds_list_edctTrpr.clearData();
	if (!fn_preSearch_sub(sType)) return false;
		
	var szSvcid       = "srchEdctTrpr";
	var szController  = "/edct-trpr/edct-trpr/list";
	var szInDs        = "ds_input=ds_inqCndt_sub";
	var szOutDs       = "ds_list_tmp=ds_output";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//교육대상자 저장
function fn_save()
{
	var nDeleteCnt = ds_list_user.getCaseCount("rowCheck=='1'");            
	if (nDeleteCnt < 1) {
		alert("추가할 교육대상자를 선택하세요.");
		return;
	}	
	
	if(!confirm("교육대상자를 등록하시겠습니까?")) {
		return false;
	}
	
	var sEdctCrsId = ds_list_copy.getColumn(0, "edct_crs_id");
	var sEdctSttDt = ds_list_copy.getColumn(0, "edct_stt_dt"); 
	//trace(sEdctSttDt); 
	if(gfn_isNull(sEdctCrsId)){
		alert("교육아이디가 없습니다.");
		return false;
	}
	
	for (var i=0; i<ds_list_user.rowcount; i++) {
		if(ds_list_user.getColumn(i, "rowCheck") == "1"){
			ds_list_user.setColumn(i, "edct_crs_id", sEdctCrsId);
			ds_list_user.setColumn(i, "edct_atnc_dt", sEdctSttDt);
			ds_list_user.setColumn(i, "edct_atnc_yn", "N");
			ds_list_user.setColumn(i, "edct_trpr_id", ds_list_user.getColumn(i, "user_id"));
		}
	}
	//trace(ds_list_user.saveXML());	
	
	var szSvcid       = "insertEdctTrpr";
	var szController  = "/edct-trpr/edct-trpr/insert";
	var szInDs        = "ds_input=ds_list_user::U";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//교육대상자 삭제
function fn_delete()
{
	var nDeleteCnt = ds_list_edctTrpr.getCaseCount("rowCheck=='1'");            
	if (nDeleteCnt < 1) {
		alert("제외할 교육대상자를 선택하세요.");
		return;
	}	
	
	if(!confirm("교육대상자를 제외하시겠습니까?")) {
		return false;
	}
	
	var sEdctCrsId = ds_list_copy.getColumn(0, "edct_crs_id");
	var sEdctPrgrStatCd = ds_list_copy.getColumn(0, "edct_prgr_stat_cd");
	var sEdctTrprId = "";
	
	if(gfn_isNull(sEdctCrsId)){
		alert("교육아이디가 없습니다.");
		return false;
	}
	
	for (var i=0; i<ds_list_edctTrpr.rowcount; i++) {
		if(ds_list_edctTrpr.getColumn(i, "rowCheck") == "1"){
			sEdctTrprId += ds_list_edctTrpr.getColumn(i, "edct_trpr_id")+",";
		}
	}
	ds_inqCndt_sub.clearData();
	ds_inqCndt_sub.addRow();
	ds_inqCndt_sub.setColumn(0, "edct_crs_id", sEdctCrsId);
	ds_inqCndt_sub.setColumn(0, "selectcheck", sEdctTrprId);
	ds_inqCndt_sub.setColumn(0, "edct_prgr_stat_cd", sEdctPrgrStatCd);

	var szSvcid       = "deleteEdctTrpr";
	var szController  = "/edct-trpr/edct-trpr/delete";
	var szInDs        = "ds_input=ds_inqCndt_sub";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function fn_update()
{
	var nDeleteCnt = ds_list_edctTrpr.getCaseCount("rowCheck=='1'");            
	if (nDeleteCnt < 1) {
		alert("교육시간을 수정할 대상자를 선택하세요.");
		return;
	}	
	
	if(!confirm("교육시간을 저장하시겠습니까?")){
		return false;
	}
	
	var sEdctEndDt = ds_list_copy.getColumn(0, "edct_end_dt");//교육종료일
	var sEdctAtncDt = cal_edct_atnc_dt.value;
	
	if(sEdctAtncDt > sEdctEndDt){
		alert("교육일이 해당 교육의 종료일보다 클 수 없습니다.");
		return false;
	}
		
	if(gfn_isNull(cbo_edct_atnc_stt_hr_cd.value) || gfn_isNull(cbo_edct_atnc_end_hr_cd.value)) {
		alert("시간을 입력하세요.");
		return false;
	}
	
	var sEdctCrsId = ds_list_copy.getColumn(0, "edct_crs_id");
	var sEdctPrgrStatCd = ds_list_copy.getColumn(0, "edct_prgr_stat_cd");
	var sEdctTrprId = "";
	for (var i=0; i<ds_list_edctTrpr.rowcount; i++) {
		if(ds_list_edctTrpr.getColumn(i, "rowCheck") == "1"){
			sEdctTrprId += ds_list_edctTrpr.getColumn(i, "edct_trpr_id")+",";
		}
	}
	
	ds_inqCndt_sub.clearData();
	ds_inqCndt_sub.addRow();
	ds_inqCndt_sub.setColumn(0, "edct_crs_id", sEdctCrsId);
	ds_inqCndt_sub.setColumn(0, "selectcheck", sEdctTrprId);
	ds_inqCndt_sub.setColumn(0, "edct_prgr_stat_cd", sEdctPrgrStatCd);
	ds_inqCndt_sub.setColumn(0, "edct_atnc_dt", sEdctAtncDt);
	ds_inqCndt_sub.setColumn(0, "edct_atnc_stt_hr_cd", cbo_edct_atnc_stt_hr_cd.value);
	ds_inqCndt_sub.setColumn(0, "edct_atnc_end_hr_cd", cbo_edct_atnc_end_hr_cd.value);
	
	
	var szSvcid       = "updateEdctTrpr";
	var szController  = "/edct-trpr/edct-trpr/update";
	var szInDs        = "ds_input=ds_inqCndt_sub";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	var sBzptDiv = gds_UserInfo.getColumn(0, "bzpt_div");
	if ("999999" != sBzptDiv){
		cbo_srch_hgrn_team_cd.value = gds_UserInfo.getColumn(0, "hgrn_team_cd");
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "1", ds_allTeamCd, ds_teamCd, "ALL");
		cbo_srch_hgrn_team_cd.enable = false;
		
		cbo_srch_team_cd.value = gds_UserInfo.getColumn(0, "team_cd");
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_teamCd, "ALL");
		cbo_srch_team_cd.enable = false;
	}else{
		cbo_srch_hgrn_team_cd.value = "T20000";
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "1", ds_allTeamCd, ds_teamCd, "ALL");
		cbo_srch_hgrn_team_cd.enable = false;
	}
	cbo_srch_user_id.index = 0;
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 조회
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit());
}

// 콤보 값 변경
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if (obj.name == "cbo_srch_hgrn_team_cd") {
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "1", ds_allTeamCd, ds_teamCd, "ALL");
	} else if (obj.name == "cbo_srch_team_cd") {
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_teamCd, "ALL");
	} 
}

