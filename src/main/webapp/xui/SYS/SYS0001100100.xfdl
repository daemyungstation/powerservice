/***************************************************************************
 * 함  수  명		: fn_commDelete()
 * 기      능		: (공통버튼)삭제 프로세스
 ***************************************************************************/
function fn_commDelete(sType)
{
	if (!fn_preDelete(sType)) return false;

	if(gfn_delDs(ds_list)) {		
		// 삭제 프로세스 처리
		ds_delInput.clearData();
		var del_count = ds_list.rowcount;
		for(var i = 0; i<del_count; i++){
			if(ds_list.getColumn(i,"_chk") == 1){
				var del_row = ds_delInput.addRow();
				ds_delInput.setColumn(del_row, "uuid", ds_list.getColumn(i,"uuid"));
				ds_delInput.setColumn(del_row, "userId", ds_list.getColumn(i,"userId"));
			}
		}
	
		gfn_kwDataInit();

		gfn_setKwData_trscCode("SYS00011R03");
		gfn_setKwData_trscType(gfn_getCommTypeDelete());
		gfn_setKwData_systemCode("KRP_TEST");

		gfn_tranNMCI(gfn_getCommTypeAppend()
					, "ds_delInput=ds_delInput"
					, ""
					, "fn_commDelete_callBack");		
	}
}

