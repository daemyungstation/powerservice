/***************************************************************************
 * 기      능		: (공통) 선택된 사용여부  정보의 데이타셋을 카피한다.
 ***************************************************************************/

function Div00_btnMakeRow_onclick(obj:Button,  e:ClickEventInfo)
{

	if (div_body.tab00.tabindex == 0) {
		var rowCount = ds_input.getColumn(0, "txtCount");
		if (rowCount == "undefined" || rowCount <= 0 ) {
			alert("0 보다 커야합니다.");
			return false;
		}
		
		if (ds_input.getColumn(0, "scheduleType") == "1") {
			if (ds_input.getColumn(0, "sendDate") == undefined) {
				alert("예약 전송인 경우 발송일시를 입력해야 합니다.");
				return false;
			} else if (ds_input.getColumn(0, "sendDate") == "") {
				alert("예약 전송인 경우 발송일시를 입력해야 합니다.");
				return false;
			} else if (ds_input.getColumn(0, "sendDate").length != 14 ) {
				alert("예약 전송인 경우 발송일시 14자리를 입력해야 합니다.");
				return false;
			}
		}
		
		var nRow = 0;
		ds_list.clearData();
		for (var i = 0; i<rowCount; i++ ){
			nRow = gfn_addDs(ds_list);
			ds_list.setColumn(nRow, "smsSrno", i+1);
			ds_list.setColumn(nRow, "lrqntMsgYn", ds_input.getColumn(0, "lrqntMsgYn"));
			ds_list.setColumn(nRow, "smsYn", ds_input.getColumn(0, "smsYn"));
			ds_list.setColumn(nRow, "scheduleType", ds_input.getColumn(0, "scheduleType"));
			ds_list.setColumn(nRow, "destInfo", ds_input.getColumn(0, "destInfo"));
			ds_list.setColumn(nRow, "subject", ds_input.getColumn(0, "subject"));
			ds_list.setColumn(nRow, "smsMsg", ds_input.getColumn(0, "smsMsg"));
			if (ds_input.getColumn(0, "smsYn") != "Y") {
				// 메시지 유형 (text/html)은 mms일 경우에만 반영한다.
				ds_list.setColumn(nRow, "msgType", ds_input.getColumn(0, "msgType"));
			}
			ds_list.setColumn(nRow, "cdrId", ds_input.getColumn(0, "cdrId"));
			ds_list.setColumn(nRow, "callback", ds_input.getColumn(0, "callback"));
			ds_list.setColumn(nRow, "userId", ds_input.getColumn(0, "userId"));
			
			if (ds_input.getColumn(0, "scheduleType") == "1") {
				//send date는 예약전송일때만 반영한다.
				ds_list.setColumn(nRow, "sendDate", ds_input.getColumn(0, "sendDate"));
			}
			
			ds_list.setColumn(nRow, "bizHdqrCd", ds_input.getColumn(0, "bizHdqrCd"));
			ds_list.setColumn(nRow, "smsBswrCd", ds_input.getColumn(0, "smsBswrCd"));
			ds_list.setColumn(nRow, "bswrIdntVl", ds_input.getColumn(0, "bswrIdntVl"));
		}
	}

}

