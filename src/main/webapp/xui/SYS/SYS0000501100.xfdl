/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능		: (공통버튼)조회 프로세스

 ***************************************************************************/
function fn_commSearch(sType)
{
	// 2번째 텝에서만 조회
	if (div_body.tab00.tabindex == 0) { 

		if (!fn_preSearch(sType)) return false;


		// KWData 설정
		gfn_kwDataInit();
		gfn_setKwData_trscCode("SYS00005R08");
		gfn_setKwData_trscType(gfn_getTranTypeSearch());

		gfn_tranNMCI(sType
					, "ds_input=ds_input"
					, "ds_list_temp=ds_list_temp"
					, "fn_commSearch_callBack");
	}
	if (div_body.tab00.tabindex == 1) {

		if (!fn_preSearch(sType)) return false;

		// 페이지 처리 기본설정
		if(!gfn_pageSearch(gfn_getTranTypeDIS(), sType, ds_listRslt, div_body.tab00.tabpage2.sta_RsltTotalCnt)) return;

	 	// KWData 설정
		gfn_kwDataInit();
		gfn_setKwData_trscCode("SYS00001R16");
		gfn_setKwData_screenId("");
		gfn_setKwData_bizGroupCode("");
		gfn_setKwData_systemCode("");
		gfn_setKwData_nameSpace("com.kyowon.krp.sys.dc.smssendingdc.DCSmsSndgSqlMap");

		if(sType != gfn_getCommTypeAppend()) {	// 총건수 조회
			ds_listRslt.clearData();
			// sms 여부가 Y인 경우 (메시지 유형을 볼 필요가 없다.)
			
			if (ds_input.getColumn(0, "smsYn") == "Y") {
				div_body.tab00.tabpage2.grd_list_rslt.setRealColSize(10, 0);
			} else {
				div_body.tab00.tabpage2.grd_list_rslt.setRealColSize(10, 80);
			}
		
			gfn_setKwData_sqlId("countSmsMsgBasiDis");
			gfn_tranDIS(sType
						, "ds_input=ds_input"
						, ""
						, "fn_commSearch_callBack");
		} else {								// 리스트 조회
			gfn_setKwData_sqlId("getListSmsMsgBasiDis");
			gfn_tranDIS(sType
						, "ds_input=ds_input"
						, "ds_list_temp=datas"
						, "fn_commSearch_callBack");
		}
	}
}

