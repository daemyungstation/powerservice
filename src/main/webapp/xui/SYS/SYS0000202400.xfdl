/***************************************************************************
 * 기      능		: (공통) 그리드 oncellclick이벤트 발생시에 로우상태 초기화 처리
 ***************************************************************************/
function div_body_grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.col == 0) {
		gfn_rollbackRowDs(this[obj.binddataset], obj, e);
	} else {
		ds_admn_rltn_list.clearData();
		ds_admn_rltn_list.addRow();
		ds_admn_rltn_list.setColumn(0, "orgzCd", ds_input.getColumn(0, "orgzCd"));
		ds_admn_rltn_list.setColumn(0, "blngOrgzCd", ds_list.getColumn(e.row, "blngOrgzCd"));
		ds_admn_rltn_list.setColumn(0, "userId", ds_list.getColumn(e.row, "userId"));
		ds_admn_rltn_list.setColumn(0, "userNm", ds_list.getColumn(e.row, "userNm"));
		ds_admn_rltn_list.setColumn(0, "jbttCd", ds_list.getColumn(e.row, "jbttCd"));
		ds_admn_rltn_list.setColumn(0, "dtrsCd",  ds_list.getColumn(e.row, "dtrsCd"));
		ds_admn_rltn_list.setColumn(0, "edtYn",   ds_list.getColumn(e.row, "edtYn"));
		ds_admn_rltn_list.setColumn(0, "vrtlUserYn",  ds_list.getColumn(e.row, "vrtlUserYn"));
		ds_admn_rltn_list.setColumn(0, "selRow", e.row);
		//trace(ds_admn_rltn_list.saveXML());
	}
}

function div_body_grd_rltn_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{

	ds_admn_rltn_list.clearData();
	ds_admn_rltn_list.addRow();
	ds_admn_rltn_list.setColumn(0, "orgzCd", ds_input.getColumn(0, "orgzCd"));
	ds_admn_rltn_list.setColumn(0, "blngOrgzCd", ds_userlist_bonsa.getColumn(e.row, "blngOrgzCd"));
	ds_admn_rltn_list.setColumn(0, "userId", ds_userlist_bonsa.getColumn(e.row, "userId"));
	ds_admn_rltn_list.setColumn(0, "userNm", ds_userlist_bonsa.getColumn(e.row, "userNm"));
	ds_admn_rltn_list.setColumn(0, "jbttCd", ds_userlist_bonsa.getColumn(e.row, "jbttCd"));
	ds_admn_rltn_list.setColumn(0, "dtrsCd", ds_userlist_bonsa.getColumn(e.row, "dtrsCd"));
	ds_admn_rltn_list.setColumn(0, "blngorgzNm", ds_userlist_bonsa.getColumn(e.row, "blngorgzNm"));
	//해당조직 사용자가 아닌경우 가상사용자로 등록
	if(ds_input.getColumn(0, "orgzCd") !=  ds_userlist_bonsa.getColumn(e.row, "blngOrgzCd") ){
		ds_admn_rltn_list.setColumn(0, "vrtlUserYn", 'Y');
	}else{
		ds_admn_rltn_list.setColumn(0, "vrtlUserYn", 'N');
	}
	ds_admn_rltn_list.setColumn(0, "edtYn", "");
	ds_admn_rltn_list.setColumn(0, "selRow", "");
	trace(ds_admn_rltn_list.saveXML());
}



