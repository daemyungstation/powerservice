/***************************************************************************
 * 기      능		: 역할 화면 관계를 등록상태로 변경한다.
 ***************************************************************************/
function div_body_Button01_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_list.enableevent = false;
//ds_admn_rltn_list
//ds_role_orgz_rltn_list
	if (gfn_isNull(ds_admn_rltn_list.getColumn(0, "orgzCd")) ) {
		alert("먼저 등록할 조직을 선택하세요.");
		ds_admn_rltn_list.clearData();
		return;
	}
	/*
	if (gfn_isNull(ds_admn_rltn_list.getColumn(0, "menuCd")) ) {
		alert("선택된 메뉴엔 프로그램이 없습니다.\n다시 확인하시기 바랍니다.");
		ds_role_orgz_rltn_list.clearData();
		return;
	}
	*/
	var orgzCd = ds_admn_rltn_list.getColumn(0, "orgzCd");
	var userId = ds_admn_rltn_list.getColumn(0, "userId");
	var fRow = ds_list.findRowExpr("userId=='"+userId+"' && orgzCd=='"+orgzCd+"'");
	if (fRow == -1) {
		gfn_addCommCol(ds_list);
		var nRow = ds_list.addRow();
		
		ds_list.setColumn(nRow, gfn_getColNmStatus(), gfn_getRowTypeInsert());
		
		ds_list.setColumn(nRow, "orgzCd", orgzCd);
		ds_list.setColumn(nRow, "userId", userId);
		ds_list.setColumn(nRow, "blngOrgzCd", ds_admn_rltn_list.getColumn(0, "blngOrgzCd"));		
		ds_list.setColumn(nRow, "userNm", ds_admn_rltn_list.getColumn(0, "userNm"));
		ds_list.setColumn(nRow, "jbttCd", ds_admn_rltn_list.getColumn(0, "jbttCd"));
		ds_list.setColumn(nRow, "dtrsCd", ds_admn_rltn_list.getColumn(0, "dtrsCd"));
		ds_list.setColumn(nRow, "blngorgzNm", ds_admn_rltn_list.getColumn(0, "blngorgzNm"));
		ds_list.setColumn(nRow, "vrtlUserYn", ds_admn_rltn_list.getColumn(0, "vrtlUserYn"));
		ds_list.setColumn(nRow, "edtYn", "Y");
	} else {
		alert("이미 등록된 데이타가 있습니다.\n다시 확인하시기 바랍니다.");
	}
	//ds_role_orgz_rltn_list.clearData();
		
    ds_list.enableevent = true;	
}

