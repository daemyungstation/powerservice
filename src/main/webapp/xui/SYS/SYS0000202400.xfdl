function fn_searchBonsaCall()
{
	
	var recpTrgtNm = this.div_body.edt_recpTrgtNm.value;
	var blngOrgzNm = this.div_body.edt_blngOrgzNm.value;
	var blngTrgtNo = this.div_body.edt_recpTrgtNo.value;
	
	if(gfn_getLength(recpTrgtNm) <= 0 && gfn_getLength(blngOrgzNm) <= 0 && gfn_getLength(blngTrgtNo) <= 0) {
		alert("성명, 사번  또는 부서를 입력하세요!");
		return false;
	}
	
	fn_searchBonsa("S");
}

/***************************************************************************
 * 함  수  명		: fn_searchBonsa()
 * Param			: 1. sDiv : 조회구분(조회버튼클릭:S, 트리노드클릭:T) 
 * 기      능		: 본사조직 받는사람 조회
 ***************************************************************************/
function fn_searchBonsa(sDiv, sOrgzCd)
{
	trace("fn_searchBonsa");
	// 초기화
	//gfn_clearSortMark(div_body.grd_list00);
	ds_userlist_bonsa.clearData();
	//ds_input_bonsa.addRow();
	
	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_nameSpace("com.kyowon.krp.sys.dc.authorityadmnistrationdc.DCAthrAdmnSqlMap");
	gfn_setKwData_trscCode("SYS00002R53");
	gfn_setKwData_sqlId("getOrgzUserList");

	if(sDiv == "S") {
		// 초기화
		ds_input_bonsa.setColumn(0, "blngOrgzCd", "");
	} else {
		// 초기화
		ds_input_bonsa.setColumn(0, "userNm", "");
		ds_input_bonsa.setColumn(0, "orgzNm", "");
		
		// 트리클릭한 조직정보 설정
		ds_input_bonsa.setColumn(0, "blngOrgzCd", sOrgzCd);
		trace("sOrgzCd::"+sOrgzCd);
	}

	gfn_tranDIS(gfn_getCommTypeAppend()
				, "ds_input=ds_input_bonsa"
				, "ds_userlist_bonsa=datas"
				, "fn_searchBonsa_callBack");
}

