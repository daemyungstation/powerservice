// 받는사람 리스트 반환
function fn_getReceiveUserList(objDs)
{
	var sUserList = "";

	for(var i=0;i<objDs.rowcount;i++)
	{
		if(i == 0) {
			sUserList += objDs.getColumn(i, "recpNm");
		} else {
			sUserList += "," + objDs.getColumn(i, "recpNm");
		}
	}

	return sUserList;
}

