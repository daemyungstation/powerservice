include "LIB::lib_All.xjs";

function SYS0000400100_onload(obj:Form, e:LoadEventInfo)
{
	// 시스템 공통함수 호출
	gfn_commInit(obj, true);

	// 공통버튼 상태변경
	gfn_setCommBtn_Status(false,false,false,false,false,false);

	// 폼 공통함수 호출
	fn_init();
}

/***************************************************************************
 * 함  수  명		: fn_init()
 * 기      능		: 폼 초기화 공통함수
 ***************************************************************************/
function fn_init()
{
	//TO-DO: 폼 초기화 관련 처리

	// 입력값 설정
	ds_Input.setColumn(0, "recpUserId", gds_UserInfo.getColumn(0, "userId"));
	ds_Input.setColumn(0, "dpmsUserId", gds_UserInfo.getColumn(0, "userId"));

	//ds_receiveMsg.enableevent 	= false;
	//ds_sendMsg.enableevent 		= false;

	fn_searchReceiveMsgList();			// 받은메세지 리스트조회
	fn_searchSendMsgList();				// 보낸메세지 리스트조회

}

// 받은메세지//////////////////////////////////////////////////////////////////////////////////////////////

// 받은메세지 리스트조회
function fn_searchReceiveMsgList()
{
	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_nameSpace("com.kyowon.krp.sys.dc.generaladmnistrationdc.DCGnrlAdmnSqlMap");
	gfn_setKwData_trscCode("SYS00004R12");
	gfn_setKwData_sqlId("getRecpMsgList");

	gfn_tranDIS(gfn_getCommTypeAppend()
				, "ds_Input=ds_Input"
				, "ds_receiveMsg=datas"
				, "fn_searchReceiveMsgList_callBack");
}

function fn_searchReceiveMsgList_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	}

	if(ds_receiveMsg.rowcount > 0) {
		fn_searchReceiveMsgInfo(0);
	}

	// 받은메세지확인 건수 처리
	var msgCntInfo = "미확인   <fs v='9'><fc v='#f0850c'><b v='true'>"+ds_receiveMsg.getCaseCount("recpCnftYn!='Y'")+"</b></fc></fs> <fc v='#a1a1a1'>통/전체 "+ds_receiveMsg.rowcount+"통</fc>";
	tab_msgList.tpg_receiveMsg.stc_msgCntInfo.text = msgCntInfo;
}

// 받은메세지리스트 중 단건 받은메세지 선택 시 처리
function ds_receiveMsg_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	//trace(e.oldrow+"receivedMsg onrowchange "+e.newrow);
	//fn_searchReceiveMsgInfo(e.newrow);
	//fn_searchReceiveMsgInfo(e.newrow);
}

// 받은메세지 정보 조회
function fn_searchReceiveMsgInfo(nRow)
{
	// 입력값 설정
	ds_Input.setColumn(0, "dpmsNo", ds_receiveMsg.getColumn(nRow, "dpmsNo"));

	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_trscCode("SYS00002R16");

	gfn_tranNMCI(gfn_getCommTypeInit()
				, "ds_Input=ds_Input"
				, "ds_receiveMsgInfo=ds_Output ds_receiveUserList=ds_recpUserList"
				, "fn_searchReceiveMsgInfo_callBack");
}

function fn_searchReceiveAncmInfo(nRow)
{
	// 입력값 설정
	ds_Input.setColumn(0, "ntmtId", ds_receiveMsg.getColumn(nRow, "dpmsNo"));

	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_nameSpace("com.kyowon.krp.sys.dc.generaladmnistrationdc.DCGnrlAdmnSqlMap");
	gfn_setKwData_trscCode("SYS00004R10");
	gfn_setKwData_sqlId("getAncmMsg");

	gfn_tranDIS(gfn_getCommTypeAppend()
				, "ds_Input=ds_Input"
				, "ds_receiveMsg=datas"
				, "fn_ssearchReceiveAncmInfo_callBack");

}

function fn_ssearchReceiveAncmInfo_callBack(strSvcID, nErrorCode, strErrorMsg)
{
}

function fn_searchReceiveMsgInfo_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	}

	// 받는사람 리스트
	var sUserList = fn_getReceiveUserList(ds_receiveUserList);
	tab_msgList.tpg_receiveMsg.txa_recpUserList.value = sUserList;

	// 수신자수
	var sTotCnt = "수신자수 : <b v='true'>"+ds_receiveUserList.getColumn(0, "totCnt")+"</b>명";
	tab_msgList.tpg_receiveMsg.stc_recpCnt.text = sTotCnt;
}

function Tab00_tabpage1_grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	//fn_searchReceiveMsgInfo(e.row);
}

function tab_msgList_tpg_receiveMsg_grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_receiveMsg.getColumn(e.row,"msgKind") == "PM"){
		fn_searchReceiveMsgInfo(e.row);
	}else if(ds_receiveMsg.getColumn(e.row,"msgKind") == "NT"){
		fn_searchReceiveAncmInfo(e.row);
	}
