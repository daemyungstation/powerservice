function fn_getDayColor(hldYn)
{
	var rtnColor;
	
	if(hldYn == 'Y')	// 일요일
		rtnColor = "#ff0000";
	else
		rtnColor = "dafault";

	return rtnColor;
}

function fn_setDay(objDiv, nDay, sDayColor)
{
	objDiv.sta_day.color 	= sDayColor;
	objDiv.sta_day.text 	= nDay;
}

function fn_setDayFont(obj)
{
	for(var i=0;i<41;i++)
	{
		var objDiv = eval("div_Day_"+i);
		if(obj == objDiv) {
			objDiv.sta_day.font = "Dotum,9,bold";
		} else {
			if(objDiv.userdate == fv_Today) {
				objDiv.sta_day.font = "Dotum,9,bold";
			} else {
				objDiv.sta_day.font = "Dotum,9";
			}	
		}
	}
}
function Div00_search_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_Month_0_List.findRow("xRowStatusType","U") != -1){
		if(!confirm("현재 수정중인 데이타가 있습니다. 조회를 하시면 현재 수정 중인 데이타는 전체 초기화됩니다. \n\n조회 하시겠습니까?")) {
			return false;
		}
	}
	var clndDt = Div00.year.value + Div00.mnth.value;
			
	ds_input.setColumn(0,"clndDt",clndDt);
	Div_dataReset();
	fn_commSearch();
	
}


function div_body_Button00_onclick(obj:Button,  e:ClickEventInfo)
{	
	var obj = eval("div_body."+div_body.edt_03.value);
	
	if(div_body.Radio00.value == null || obj == null){
		alert("변경할 데이터를 선택 하십시오.");
		return;
	}
	
	//alert(obj.sta_day.text);
	ds_Month_0_List.setColumn(obj.sta_day.text-1,"hldNm",div_body.edt_05.value);
	ds_Month_0_List.setColumn(obj.sta_day.text-1,"hldExpi",div_body.edt_00.value);
	ds_Month_0_List.setColumn(obj.sta_day.text-1,"hldYn",div_body.Radio00.value);
	//ds_Month_0_List.setColumn(obj.sta_day.text-1,"hldDivCd",div_body.Radio01.value);
	ds_Month_0_List.setColumn(obj.sta_day.text-1,"hldDivCd",div_body.hldDiv.value);
	
	if(div_body.Radio00.value == "Y"){
		/*if(div_body.Radio01.value == null){
			alert("[휴무 구분]을 선택 해 주십시오.");
			return;
		}*/
		if(div_body.hldDiv.value == null){
			alert("[휴무 구분]을 선택 해 주십시오.");
			return;
		}
		
	}
	
	if(ds_Month_0_List.findRow("xRowStatusType","U") == -1){
		alert("변경된 내용이 없습니다.");
		return;
	}else{
		obj.sta_ModifyYN.visible = true;
	}
}

function ds_Month_0_List_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.columnid != gfn_getColNmChk() && e.columnid != gfn_getColNmStatus()) {
		gfn_modify(obj, e);
	}
}

function Div00_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_Month_0_List.findRow("xRowStatusType","U") == -1){
		alert("변경된 내용이 없습니다.");
		return;
	}else{
		fn_commSave();
	}
}


function Div00_mnth_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	//alert(old_mnth);
	if(ds_Month_0_List.findRow("xRowStatusType","U") != -1){
		if(!confirm("현재 수정중인 데이타가 있습니다. 조회를 하시면 현재 수정 중인 데이타는 전체 초기화됩니다. \n\n조회 하시겠습니까?")) {
			Div00.mnth.value = old_mnth;
			return false;
		}
	}
	
	var clndDt = Div00.year.value + Div00.mnth.value;
	old_mnth = Div00.mnth.value;	
	ds_input.setColumn(0,"clndDt",clndDt);
	Div_dataReset();
	fn_commSearch();
	
}

function Div_dataReset(sType){
	div_body.edt_05.value = "";
	div_body.edt_00.value = "";
	div_body.Radio00.index = -1;
	//div_body.Radio01.index = -1;
	div_body.hldDiv.index = -1;
}

function div_body_Radio00_onitemchanged(obj:Radio, e:ItemChangeEventInfo)
{
	if(obj.value == "N"){
		//div_body.Radio01.index = -1;
		div_body.hldDiv.index = -1;
	}
}

function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}
