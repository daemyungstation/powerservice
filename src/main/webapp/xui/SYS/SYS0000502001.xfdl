function Div00_btn_addItem_onclick(obj:Button,  e:ClickEventInfo)
{
	var testData = "";
	for(i = 0; i < ds_list00.getRowCount(); i++){
		var len = ds_list00.getColumn(i,"spczLngt");
		var typ = ds_list00.getColumn(i,"spczAtrbNm");
		var data = ds_list00.getColumn(i,"testData");
		var dftData = ds_list00.getColumn(i,"spczVrblBasiVl");
		if(data == null){
			data = " ";
		}
		if(typ == "??"){
			data = data.padLeft(len, "0");
		}else{
			data = data.padRight(len, " ");
		}
		
		testData = testData + data;
		
		if(dftData == null || dftData == "SPACE"){
			dftData = " ";
			dftData = dftData.padRight(len, " ");
		}
		ds_list00.setColumn(i,"testData",dftData);	

	}
	var nRow = gfn_addDs(ds_temp);
	
	//var nRow = ds_temp.addRow();
	var n = div_body.grd_list.getDatasetRow(div_body.grd_list.currentrow); //선택된 행찾기
	ds_temp.copyRow(nRow ,ds_list,n);
	ds_temp.setColumn(nRow,"testData",testData);
	ds_temp.setColumn(nRow,"xRowStatusType","I");
	
	
}

