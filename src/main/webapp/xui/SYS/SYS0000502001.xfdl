	ds_info.setColumn(0,"spczId",ds_info.getColumn(0,"dmndSpczId"));
	
	if( ds_info.getColumn(0,"rplySpczCntn") == null )
	{
		Div00.TextArea01.readonly = true;
	}else{
		var len = ds_info.getColumn(0,"rplySpczCntn").length;
