/***************************************************************************
 * 함  수  명		: fn_commSave()
 * 기      능		: (공통버튼)저장 프로세스
 ***************************************************************************/
function fn_commSave(sType)
{
	// 필수 입력 체크
	if (!gfn_dsValidCheck(div_body.grd_list, ds_listValid)) {
		return false;
	}

	if (!fn_preSave(sType)) return false;

	for (var i = 0; i <= ds_list.rowcount; i++) {
		var xRowStatus = gfn_trim(ds_list.getColumn(i, "xRowStatusType"));
		if (xRowStatus == "") { continue; }
		var apltCd = gfn_trim(ds_list.getColumn(i, "apltCd"));
	    var bswrCd = gfn_trim(ds_list.getColumn(i, "trscId")).substr(0,3);
	    var bswrNm = gfn_trim(ds_list.getColumn(i, "trscId")).substr(3,8);
	    var trscTyp = gfn_trim(ds_list.getColumn(i, "trscId")).substr(8,1);
	    var srvcClssNm = gfn_trim(ds_list.getColumn(i, "srvcClssNm"));
		if (ds_list.getColumn(i, gfn_getColNmStatus()) == gfn_getRowTypeInsert() || ds_list.getColumn(i, gfn_getColNmStatus()) == gfn_getRowTypeUpdate() ) {
			if ( apltCd != bswrCd) {
				alert("트랜젝션코드의 앞에 3자리가 어플리케이션코드와 다릅니다.\n다시 확인하시기 바랍니다.");
				return;
			}
			if ( gfn_getLength(bswrNm) != 8) {
				alert("트랜젝션코드의 뒤에 8자리로 입력하셔야 합니다.\n다시 확인하시기 바랍니다.");
				return;
			}
			
			if (trscTyp != "C" && trscTyp != "R" && trscTyp != "U" && trscTyp != "D" && trscTyp != "M" && trscTyp != "B") {
				alert("트랜잭션 코드 유형이 잘못되었습니다.\n다시 확인하시기 바랍니다.");
				return;
			}
			
			// SC/DIS의 경우 검사
			if (trscTyp != "B") {
				if (srvcClssNm.length < 15) {
					alert("배치 이외에는 com.kyowon.krp.로 시작하는 full package + . + Class명으로 작성되어야 합니다.");
					return;
				} else if (srvcClssNm.substring(0,15) != "com.kyowon.krp." ) {
					alert("배치 이외에는 com.kyowon.krp.로 시작하는 full package + . + Class명으로 작성되어야 합니다.");
					return;
				}
			}
			
			// DIS의 경우 처리
			if (srvcClssNm.substr(srvcClssNm.length-6, 6) == "SqlMap") {
				if (trscTyp != "R") {
					alert("DIS(SqlMap)은 'R'형 트랜잭션만 가능합니다.");
					return;
				} else {
					var trscNm = gfn_trim(ds_list.getColumn(i, "trscNm"));
					var trscNmArr = trscNm.split(",");
					for (var m = 0; m < trscNmArr.length; m ++ ) {
						if (trscNmArr[m].trim() == "") {
							alert("복수의 트랜잭션 중 비어있는 트랜잭션 명이 있습니다.");
							return;
						}
					}
					if (trscNmArr.length > 1) {
						for (var m = 0; m < trscNmArr.length - 1; m ++ ) {
							for (var n = m + 1; n < trscNmArr.length; n ++ ) {
								if (trscNmArr[m] == trscNmArr[n]) {
									alert("중복된 트랜잭션 명이 있습니다.");
									return;
								}
							}
						}
					}

					var dsNm = gfn_trim(ds_list.getColumn(i, "srvcReslDataNm"));
					var dsNmArr = dsNm.split(",");
					for (var m = 0; m < dsNmArr.length; m ++ ){
						if (dsNmArr[m].trim() == "") {
							alert("복수의 데이터셋 중 비어있는 데이터셋 명이 있습니다.");
							return;
						}
					}
					if (dsNmArr.length > 1) {
						for (var m = 0; m < dsNmArr.length - 1; m ++ ) {
							for (var n = m + 1; n < dsNmArr.length; n ++ ) {
								if (dsNmArr[m] == dsNmArr[n]) {
									alert("중복된 데이터셋 명이 있습니다.");
									return;
								}
							}
						}
					}
					
					if (dsNm != "" && (trscNmArr.length !=  dsNmArr.length)) {
						alert("트랜잭션 명 개수와 데이터셋 명 개수에 차이가 있습니다.");
						return;
					}

				}
			}
		}
	}

	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_nameSpace("");
	gfn_setKwData_trscCode(ds_input.getColumn(0, "trscCode"));
	gfn_setKwData_trscType("");
	gfn_setKwData_screenId("");
	gfn_setKwData_bizGroupCode("");
	gfn_setKwData_systemCode("KRP_TEST");

	gfn_tranNMCI(sType
				, "ds_TrscSystBasi=ds_list:U"
				, ""
				, "fn_commSave_callBack");
}

