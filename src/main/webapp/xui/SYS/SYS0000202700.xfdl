var Error;
var Log;
function Div00_Button02_onclick(obj:Button,  e:ClickEventInfo)
{
	var count = ds_systOprn.getCaseCount("_chk3 == '1'");
	var rowCount = ds_systOprn.getRowCount();
	Error = "";
	Log = "";
	if(count == 0){
		alert("재등록 할 인스턴스를 체크해 주십시오.");
		return;
	}else{
		var objDoc 		= new DomDocument;
		for(var i =0; i<rowCount; i++){
			if(ds_systOprn.getColumn(i,"_chk3") == "1"){	
				var rtrnUrl = ds_systOprn.getColumn(i, "prtclNm")+"://"+ds_systOprn.getColumn(i, "cctnIp")+"/"+ds_systOprn.getColumn(i, "dataPgmRntnUrl");
				//objDoc.url = "http://localhost:7001/BizWork/KWBizIbatisReload.do";	
				objDoc.url = rtrnUrl;
				
			gfn_setWaitCursor(true);		//처리중 로딩 시작
			
				var rtn = objDoc.load(false);
			
				if(rtn) {
					//trace("objDoc.source::"+objDoc.source);
					var str = objDoc.source;
					var stt = "<Dataset" ;
					var end = "</Dataset>" ;
					var sttXml = str.indexOf(stt);
					var endXml = str.indexOf(end) + end.length;
					var newStr = str.substring(sttXml,endXml);
					//newStr = newStr.replace("<init>","init");

					Log = Log + objDoc.source;
					ds_dataPgmRntn.clearData();
					gfn_addCommCol(ds_dataPgmRntn);
					
					var nRow = ds_dataPgmRntn.addRow();
					//alert(gfn_getTodayFull());
					ds_dataPgmRntn.setColumn(nRow, gfn_getColNmStatus(), gfn_getRowTypeUpdate());
					ds_dataPgmRntn.setColumn(nRow, "systIdntId",ds_input.getColumn(0,"systIdntId"));
					ds_dataPgmRntn.setColumn(nRow, "systAdmnDivCd",ds_input.getColumn(0,"systAdmnDivCd"));
					ds_dataPgmRntn.setColumn(nRow, "inscGropId",ds_input.getColumn(0,"inscGropId"));
					ds_dataPgmRntn.setColumn(nRow, "trscSystIdntNo",ds_systOprn.getColumn(i,"trscSystIdntNo"));
					ds_systOprn.setColumn(i,"xmlResult", newStr);
					
					//ds_systOprn.setColumn(i,"xmlResult", objDoc.source);
					
					fn_comnDataPgmRntnSave(gfn_getCommTypeSave());
					ds_systOprn.setColumn(i,"dataPgmRntnDttm", gfn_getTodayFull());
					//trace(ds_dataPgmRntn.saveXML());
				
				} else {
					trace("Error!!");
					Error = Error + "인스턴스ID : "+ds_systOprn.getColumn(i,"inscGropId")+" / 인스턴스 No : "+ds_systOprn.getColumn(i,"inscNo")+"\n";
				}
				gfn_setWaitCursor(false);		//처리중 로딩 종료
			}
		}
		fn_comnDataPgmRntnSave(gfn_getCommTypeSave());
		//fn_commSearch(gfn_getCommTypeInit());
		fn_dataPgmRntnResult();
	}
}

