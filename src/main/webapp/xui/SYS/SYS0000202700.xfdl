		var check = ds_systOprn.getColumn(e.row, "inscGropNo");
		var check2 = ds_systOprn.getColumn(e.row, "_chk");
		for(var i=0; i< ds_systOprn.rowcount; i++){
			if(ds_systOprn.getColumn(i, "inscGropNo") == check){
				ds_systOprn.setColumn(i, "_chk2", check2);
				ds_systOprn.setColumn(i, "_chk3", check2);
			}
		}	
	}else if(e.col == 7){
		var check = ds_systOprn.getColumn(e.row, "inscGropNo");
		
		var	nRowCnt = ds_systOprn.getCaseCount("inscGropNo == "+check);
		var	nRowCnt2 = ds_systOprn.getCaseCount("inscGropNo == "+check + "&& _chk2 == '1'");
		var sNum = ds_systOprn.findRow("inscGropNo", check);
	
		if(nRowCnt == nRowCnt2){
			for(var i = sNum; i<(sNum+nRowCnt); i++){
				ds_systOprn.setColumn(i, "_chk", "1");
			}
		}else{
			for(var i = sNum; i<(sNum+nRowCnt); i++){
				ds_systOprn.setColumn(i, "_chk", "0");
			}
		}	
	}else if (e.col == 11){
	
	}else{
		datas.loadXML(ds_systOprn.getColumn(e.row, "xmlResult"));
		/*var reslDtptCntn = ds_systOprn.getColumn(e.row, "xmlResult");
