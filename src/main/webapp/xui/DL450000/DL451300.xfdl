    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt = 0;    //전체 건수

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL451300_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_init();
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		btn_excel.visible = true;
	} else {
		btn_excel.visible = false;
	}
}
/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) 
	{
		switch (svcid) 
		{			
			case "getMemberMangiExtDataList" :
			    
			break;
		}
	} 
	else 
	{
		switch (svcid) 
		{
			case "getMemberMangiExtDataList" :
			    ds_listMemberMangiExt.filter("");
				gfn_appendSearchDs(ds_listMemberMangiExt, ds_listMemberMangiExtTemp);
				gfn_setTotalRowCount(ds_listMemberMangiExt, sta_TotalCnt, nTotalListCnt, null, true, null); 
				fn_postSearch("getMemberMangiExtDataList");
			break;
		}
	}
}


/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		if (!gfn_isNull(cal_stt_dt.value) || !gfn_isNull(cal_end_dt.value)) {
			if (cal_stt_dt.value > cal_end_dt.value) {
				alert("검색 종료일보다 시작일이 큽니다.");
				return false;
			}
		}
		
		ds_inqCndt.clearData();
		
		var sAccntNo = edt_accntNo.value;
		var sCell    = edt_cell.value;
		var sInsertGbn = cbo_insertGbn.value;
		
		var nRow = ds_inqCndt.addRow();
		ds_inqCndt.setColumn(nRow, "accnt_no", sAccntNo);
		ds_inqCndt.setColumn(nRow, "cell", sCell);
		ds_inqCndt.setColumn(nRow, "insert_gbn", sInsertGbn);
		ds_inqCndt.setColumn(nRow, "dt_typ", cbo_dt_typ.value);
		ds_inqCndt.setColumn(nRow, "stt_dt", cal_stt_dt.value);
		ds_inqCndt.setColumn(nRow, "end_dt", cal_end_dt.value);
		
		ds_inqCndt.setColumn(nRow, "orderBy"       , "ACCNT_NO");
		ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType) 
{
	if(!fn_preSearch(sType)){
		return false;
	}
	
	if(!gfn_pageSearch(sType, ds_listMemberMangiExt, sta_TotalCnt, 500, nTotalListCnt)){
		return;
	}
	
	var szSvcid       = "getMemberMangiExtDataList";
	var szController  = "/dlw/pay/getMemberMangiExtDataList";
	var szInDs        = "ds_input=ds_inqCndt";
	var szOutDs       = "ds_listMemberMangiExtTemp=ds_output";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//초기화
function fn_init()
{
	edt_accntNo.value = "";
	ds_listMemberMangiExtTemp.clearData();
	ds_listMemberMangiExt.clearData();
	
	
	var sStt_dt = gfn_getToday();
	cal_stt_dt.value = sStt_dt.substr(0,6) + "01";
	cal_end_dt.value = gfn_getToday();
}

//조회 결과
function fn_postSearch(svcid)
{
	switch (svcid) 
	{
		case "getMemberMangiExtDataList" :
	        if(ds_listMemberMangiExt.getRowCount() <= 0)
	        {
		        alert("조회된 데이터가 없습니다.");
	        }
		break;
	}
}

function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) 
	{
		fn_commSearch(gfn_getCommTypeInit());
	}
}
/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 그리드 헤더 사이즈 초기화
