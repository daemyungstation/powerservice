	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_listMemberMangiExt, sta_TotalCnt, 500, nTotalListCnt)) return;
	gfn_grdHeadClick(obj, e, ds_inqCndt, null, ds_listMemberMangiExtTemp);
