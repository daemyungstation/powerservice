// 상품 변환 전
function fn_preTransMemProdAccnt()
{
	if (gfn_isNull(gv_bf_prod_cd)) {
		alert("기존 구좌를 입력하세요.");
		edt_srch_accnt_no.setFocus();
		return false;
	}
	if (gfn_isNull(gv_af_prod_cd)) {
		alert("변경할 상품을 입력하세요.");
		edt_srch_prod_cd.setFocus();
		return false;
	}
	if (gfn_isNull(edt_srch_accnt_no.value)) {
		alert("회원번호가 조회되어야합니다.");
		edt_srch_accnt_no.setFocus();
		return false;
	}
	if (gfn_isNull(edt_srch_prod_cd.value)) {
		alert("상품코드가 조회되어야합니다.");
		edt_srch_prod_cd.setFocus();
		return false;
	}
	
	/* 2018.06.19 상품변경 전 유효성체크 추가 */
	//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	// 1. 부가서비스(CMS/CARD) 신청구분 체크
	fn_check_BugaserviceInfo();
	
	// 1-1. 부가서비스 신청구분이 신청상태(1)인 경우, 진행불가
	if(gv_bugaAppCl == 1) return false;
	
	// 2. 동일한 상품코드로 변경 불가
	var vProdCd = edt_srch_prod_cd.value;
	if (vProdCd != "" && gv_bf_prod_cd != "" && vProdCd == gv_bf_prod_cd) {
	//if (gv_af_prod_cd != "" && gv_bf_pr1od_cd != "" && gv_af_prod_cd == gv_bf_prod_cd) {
			alert("동일한 상품입니다.\n동일상품의 경우 상담업무화면의 우측상단에서 상품수정 바랍니다.");
		return false;
	}
	//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	
	// 청구 진행여부 체크 - 2018.04.05
	if(!fn_check_wdrw()) return false;
	
	if (gfn_isNull(cbo_model_mst.value)) {
		alert("상품상세분류를 선택해야합니다.");
		cbo_model_mst.setFocus();
		return false;
	}
	if (ds_list_modlDtl.rowcount > 1 && gfn_isNull(cbo_model_dtl.value)) {
		alert("모델명을 선택해야합니다.");
		cbo_model_dtl.setFocus();
		return false;
	}
	
	ds_inqCndt_trans.clearData();
	ds_inqCndt_trans.addRow();
	ds_inqCndt_trans.setColumn(0, "ded_no", edt_ded_no.value);
	ds_inqCndt_trans.setColumn(0, "mem_nm", edt_mem_nm.value);
	ds_inqCndt_trans.setColumn(0, "bf_tr_accnt_no", gv_accnt_no);
	ds_inqCndt_trans.setColumn(0, "accnt_no", gv_accnt_no);
	ds_inqCndt_trans.setColumn(0, "bf_tr_prod_cd", gv_bf_prod_cd);
	
	ds_inqCndt_trans.setColumn(0, "af_tr_prod_cd", gv_af_prod_cd);
	ds_inqCndt_trans.setColumn(0, "bf_tr_pay_no", edt_bf_tr_pay_no.value);
	ds_inqCndt_trans.setColumn(0, "af_tr_pay_no", edt_af_tr_pay_no.value);
	ds_inqCndt_trans.setColumn(0, "bf_tr_pay_amt", edt_bf_tr_pay_amt.value);
	ds_inqCndt_trans.setColumn(0, "af_tr_pay_amt", edt_af_tr_pay_no.value);
	
	ds_inqCndt_trans.setColumn(0, "bf_tr_dc_no", edt_bf_tr_dc_no.value);
	ds_inqCndt_trans.setColumn(0, "af_tr_dc_no", edt_af_tr_dc_no.value);
	ds_inqCndt_trans.setColumn(0, "bf_tr_allt_pay_no", edt_bf_tr_allt_pay_no.value);
	ds_inqCndt_trans.setColumn(0, "bf_tr_allt_pay_amt", edt_bf_tr_allt_pay_amt.value);
	ds_inqCndt_trans.setColumn(0, "af_tr_allt_pay_no", edt_af_tr_allt_pay_no.value);
	
	ds_inqCndt_trans.setColumn(0, "af_tr_allt_pay_amt", edt_af_tr_allt_pay_amt.value);
	ds_inqCndt_trans.setColumn(0, "bf_tr_add_pay_no", edt_bf_tr_add_pay_no.value);
	ds_inqCndt_trans.setColumn(0, "bf_tr_add_pay_amt", edt_bf_tr_add_pay_amt.value);
	ds_inqCndt_trans.setColumn(0, "af_tr_add_pay_no", edt_af_tr_add_pay_no.value);
	ds_inqCndt_trans.setColumn(0, "af_tr_add_pay_amt", edt_af_tr_add_pay_amt.value);
	
	ds_inqCndt_trans.setColumn(0, "prod_model_kind", cbo_model_mst.value);
	ds_inqCndt_trans.setColumn(0, "prod_model_cd", cbo_model_dtl.value);
	ds_inqCndt_trans.setColumn(0, "prod_kind_nm", cbo_knid_nm.value);
	
	return true;
}

