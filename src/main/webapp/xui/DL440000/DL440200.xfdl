/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{

	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "chkProdTransDedAppYn":
			if (gv_app_yn == "YN") {	//(NYT:미신고 양도양수, PNE:납입이있으면서 납입금액이 같지 않은 경우)
				fn_init(3);
				alert("미신고 양도양수건입니다.\n신고 후 진행하세요.");
				return;
			}
			if (gv_app_yn == "PNE") {	//(NYT:미신고 양도양수, PNE:납입이있으면서 납입금액이 같지 않은 경우)
				fn_init(3);
				alert("납입금액이 맞지 않습니다.");
				return;
			}
			
			gv_bf_prod_cd = ds_accnt_info.getColumn(0, "prod_cd");
			
			edt_bf_mon_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "mon_pay_amt"));
			edt_bf_expr_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "prod_amt"));
			edt_bf_tr_pay_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "pay_amt"));
			edt_bf_tr_allt_pay_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "pay_allt_amt"));
			edt_bf_tr_add_pay_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "pay_add_amt"));
			edt_af_tr_pay_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "pay_amt"));
			edt_af_tr_allt_pay_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "pay_allt_amt"));
			edt_af_tr_add_pay_amt.value = gfn_setComma(ds_accnt_info.getColumn(0, "pay_add_amt"));
			
// 			if (!gfn_isNull(gv_af_prod_cd)) {
// 				// 특별할인은 어떻게 변경할 지 확인 필요
// 				// 엔컴에도 미구현되어 있음
// 				//fn_calcTransPayInfo();
// 			}
			break;
			
			case "selectModelMstInfo":		// 상품상세분류
			fn_comboSelectAdd(ds_list_modlMst);
			cbo_model_mst.index = 0;
			break;
			
			case "selectModelDtlInfo":		// 모델명
			fn_comboSelectAdd(ds_list_modlDtl);
			cbo_model_dtl.index = 0;
			break;
			
			case "selectProdKindList":		// 상품종류
			fn_comboSelectAdd(ds_list_prodKind);
			cbo_knid_nm.index = 0;
			break;
			
			case "transMemProdAccnt":		// 상품 변환
			if (gv_rslMsg == "success") {
				alert("상품변경 완료되었습니다.");
				fn_init(3);
			} else {
				alert("오류가 발생하였습니다.\n관리자에게 문의하세요.");
			}
			break;
			
			case "chkBugaserviceInfo":      // 부가서비스정보 체크 (** CMS 또는 CARD 가 신청중인지 체크)
			if (gv_bugaAppCl == "1") {
				alert("CMS/CARD 부가서비스를 해지한 후, 상품변경 바랍니다.");
			}
			break;
		}
	}
}

