		sPrmr = {p_prod_cd:edt_srch_prod_cd.value, p_prod_nm:edt_srch_prod_nm.value, p_mode:"set"};
	} else if (sType == "cd") {
		sPrmr = {p_prod_cd:edt_srch_prod_cd.value, p_prod_nm:"", p_mode:"set"};
	} else {
		sPrmr = {p_prod_cd:"", p_prod_nm:edt_srch_prod_nm.value, p_mode:"set"};
	}
	var reVal = gfn_diaLog("상품조회", -1, -1, -1, -1, "PS990000::PS992200.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 상품 선택시
function fn_prodSetInfo()
{
	var sProdCd = ds_prodList.getColumn(0, "prod_cd");
	if (gv_bf_prod_cd == sProdCd) {
		alert("동일한 상품입니다.\n다른 상품을 선택하세요.");
		return;
	}
	edt_srch_prod_cd.value = sProdCd;
	edt_srch_prod_nm.value = ds_prodList.getColumn(0, "prod_nm");
	
	fn_selectModelMstInfo();
	
	// 변경 상품 셋팅
	gv_af_prod_cd = sProdCd;
	edt_af_prod_nm.value = ds_prodList.getColumn(0, "prod_nm");
	edt_af_expr_no.value = ds_prodList.getColumn(0, "expr_no");
	edt_af_expr_amt.value = gfn_setComma(ds_prodList.getColumn(0, "prod_amt"));
	edt_af_mon_amt.value = gfn_setComma(ds_prodList.getColumn(0, "mon_pay_amt"));
	
	if (!gfn_isNull(edt_srch_accnt_no.value) && ds_accnt_info.rowcount > 0) {
		fn_chkProdTransDedAppYn();	// 양도양수나 상품변경 공제 미신고건 확인
	} else {
		edt_srch_accnt_no.setFocus();
	}
}

// 콤보 선택 추가
function fn_comboSelectAdd(pDs)
{
	if (pDs.rowcount > 0) {
		pDs.insertRow(0);
		pDs.setColumn(0, "cd", "");
		pDs.setColumn(0, "cd_nm", "선택");
	} else {
		pDs.addColumn("cd", "string", 256);
		pDs.addColumn("cd_nm", "string", 256);
		pDs.addRow();
		pDs.setColumn(0, "cd", "");
		pDs.setColumn(0, "cd_nm", "선택");
	}
}

