// onkeydown
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if (obj.name == "edt_srch_prod_cd") {
			fn_openProdPopup("cd");
		} else if (obj.name == "edt_srch_prod_nm") {
			fn_openProdPopup("nm");
		} else if (obj.name == "edt_srch_accnt_no") {
			fn_srchAccntNoPop();
		}
	}
}

// button click
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_close" || obj.name == "btn_closePop") {	// 닫기
		close();
	} else if (obj.name == "btn_init") {							// 초기화
		fn_init(3);
	} else if (obj.name == "btn_srchAccntNo") {						// 회원조회팝업
		fn_srchAccntNoPop();
	} else if (obj.name == "btn_srchProd") {						// 상품조회팝업
		fn_openProdPopup("btn");
	} else if (obj.name == "btn_transProd") {						// 변환
		fn_transMemProdAccnt();
	}
}

// combo onitemchanged
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	fn_selectModelDtlInfo();
	fn_selectProdKindList();
}

