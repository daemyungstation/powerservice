			if (chk_a.visible && chk_a.value == "Y" && chk_b.visible && chk_b.value == "Y" && chk_c.visible && chk_c.value == "Y"
			    && chk_d.visible && chk_d.value == "Y" && chk_e.visible && chk_e.value == "Y" && chk_g.visible && chk_g.value == "Y" 
