// 해약 수정
function fn_resnUpdatePop(sRow)
{
	var sAccntNo = ds_list_resn.getColumn(sRow, "accnt_no");
	var sProdCd = ds_list_resn.getColumn(sRow, "prod_cd");
		
	var sPrmr = {p_accnt_no:sAccntNo, p_prod_cd:sProdCd, p_gubun:"update", p_mainYn:"N"};
