/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_resn = 0;	// 전체건수
var tc_dataAthrQury;

var gvMenuId;   // 프로그램 로그 추가 2018.03.20
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL440101_onload(obj:Form, e:LoadEventInfo)
{
	//////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2018.03.20 정승철
	gvMenuId = GV_LOGMENUID;
	//////////////////////////////////////////////////////////////////////////////////////////
	
	fn_setAthrPgmBtn(); // 권한에 따른 버튼 설정
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_getList("dlwCd", "08,64,95"); // 공통코드조회
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		btn_excel.visible = true;
	} else {
		btn_excel.visible = false;
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("08", "ALL", ds_acpgMthdCd, gds_DmlCd); 	// 접수방법
			gfn_getCode("64", "ALL", ds_payMthdCd, gds_DmlCd); 		// 해약방법
			//gfn_getCode("95", "ALL", ds_divCd, gds_DmlCd); 			// 구분
			
			fn_init(); // 검색부 초기화
			//fn_commSearch(gfn_getCommTypeInit()); // 고객 조회
			
			break;
			
			
			case "srchResnList": // 해약 조회
			gfn_appendSearchDs(ds_list_resn, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_resn, sta_TotalCnt, tc_resn); // 페이지건수 표출
			if(ds_list_resn.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_list_resn.clearData();
			}
			break;
			
			case "srchResnExcelList": // 엑셀다운
			//	Static00.text=tc_dataAthrQury;
			//	ds_inqCndt.setColumn(0, "cntr_cd", ds_list_excel.getColumn(0, "cntr_cd"));
			//	ds_inqCndt.setColumn(0, "path_typ", ds_list_excel.getColumn(0, "path_typ"));
				ds_inqCndt.setColumn(0, "dataAthrQury", tc_dataAthrQury);
				grd_list.formatid = "excel";
				var colDS = gfn_excelPrepareExcelDS("grd_list");
				
				grd_list.formatid = "default";
				gfn_excelDown("해약목록", "DL440101", "DlwResnMap.getResnList", "DlwResnMap.getResnCount", ds_inqCndt, true, colDS, true);
				trace("-=-=-=-=-=-=-=");
				trace(colDS);
				break;
		}		
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{	
	if (sType == gfn_getCommTypeInit())
	{
		if (!gfn_isNull(cal_srch_stt_dt.value) || !gfn_isNull(cal_srch_end_dt.value)) {
			if (gfn_isNull(cal_srch_stt_dt.value)) {
				alert("검색 시작일을 입력하세요.");
				cal_srch_sttdt.setFocus();
				return false;
			}
			if (gfn_isNull(cal_srch_end_dt.value)) {
				alert("검색 종료일을 입력하세요.");
				cal_srch_enddt.setFocus();
				return false;
			}
			if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
				alert("검색 종료일보다 시작일이 큽니다.");
				return false;
			}
			if(gfn_getDayDiff(cal_srch_stt_dt.value, cal_srch_end_dt.value) > 30) {
				alert("검색 기간은 한달 이내로 설정 하시기 바랍니다.");
				return false;
			}
		}
	
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_resn.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "dateTyp", cbo_srch_dateTyp.value);
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
		ds_inqCndt.setColumn(0, "pay_mthd", cbo_srch_pay_mthd_cd.value);
		//ds_inqCndt.setColumn(0, "div_cd", cbo_srch_div_cd.value);
		ds_inqCndt.setColumn(0, "resn_yn", cbo_srch_div_cd.value);
		ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
		ds_inqCndt.setColumn(0, "acpt_mthd", cbo_srch_acpg_mthd_cd.value);
		
		//alert(cbo_srch_pay_mthd_cd.value + '---------' + cbo_srch_div_cd.value);
		
		ds_inqCndt.setColumn(0, "orderBy", "");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
		

		
	}
	
	return true;
}

// 조회 전
function fn_preSrchExcelDownList()
{	
	if (!gfn_isNull(cal_srch_stt_dt.value) || !gfn_isNull(cal_srch_end_dt.value)) {
		if (gfn_isNull(cal_srch_stt_dt.value)) {
			alert("검색 시작일을 입력하세요.");
			cal_srch_sttdt.setFocus();
			return false;
		}
		if (gfn_isNull(cal_srch_end_dt.value)) {
			alert("검색 종료일을 입력하세요.");
			cal_srch_enddt.setFocus();
			return false;
		}
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색 종료일보다 시작일이 큽니다.");
			return false;
		}
		if(gfn_getDayDiff(cal_srch_stt_dt.value, cal_srch_end_dt.value) > 30) {
			alert("검색 기간은 한달 이내로 설정 하시기 바랍니다.");
			return false;
		}
	}
	
	// 조회조건 및 default 정렬
	ds_inqCndt_excel.clearData();

	ds_inqCndt_excel.addRow();
	ds_inqCndt_excel.setColumn(0, "dateTyp", cbo_srch_dateTyp.value);
	ds_inqCndt_excel.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
	ds_inqCndt_excel.setColumn(0, "end_dt", cal_srch_end_dt.value);
	ds_inqCndt_excel.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
	ds_inqCndt_excel.setColumn(0, "pay_mthd", cbo_srch_pay_mthd_cd.value);
	//ds_inqCndt.setColumn(0, "div_cd", cbo_srch_div_cd.value);
	ds_inqCndt_excel.setColumn(0, "resn_yn", cbo_srch_div_cd.value);
	ds_inqCndt_excel.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
	ds_inqCndt_excel.setColumn(0, "acpt_mthd", cbo_srch_acpg_mthd_cd.value);
	
	ds_inqCndt_excel.setColumn(0, "excel_yn", "Y");
		
	ds_inqCndt_excel.setColumn(0, "orderBy", "");
	ds_inqCndt_excel.setColumn(0, "orderDirection", "desc");
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 해약현황 조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_resn, sta_TotalCnt, 50, tc_resn)) return;
	
	var szSvcid       = "srchResnList";
    var szController  = "/dlw/resn/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

// 엑셀다운로드
function fn_excel()
{
	if (ds_list_resn.rowcount <= 0) {
		alert("엑셀다운로드 데이터가 없습니다.");
		return;
	}
	
	if (!fn_preSrchExcelDownList()) return false;
	
	var szSvcid       = "srchResnExcelList";
    var szController  = "/dlw/resn/list";
    var szInDs        = "ds_input=ds_inqCndt_excel";
    var szOutDs       = "ds_list_excel=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{
	//  검색기간 설정
	cbo_srch_dateTyp.index = 0;
	var sToday = gfn_getToday();
	cal_srch_stt_dt.value = gfn_addDate(sToday, -30);
	cal_srch_end_dt.value = sToday;
	
	// 검색부 콤보 설정
	cbo_srch_pay_mthd_cd.index = 0;
	cbo_srch_div_cd.index = 0;
	cbo_srch_acpg_mthd_cd.index = 0;
	
	edt_srch_accnt_no.value = "";
	edt_srch_prod_cd.value = "";
	edt_srch_prod_nm.value = "";
	
	ds_list_resn.deleteAll();
}

// 회원조회 팝업
function fn_srchAccntNoPop()
{
	var sSrchTyp = "mem_nm";
