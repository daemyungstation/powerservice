function fn_init()
{
	// 환경설정정보 처리
	tab_Menu.tabindex = gds_userEnvrStup.getColumn(0, "basiMenuStupCd");
	
	// CubigCC(CCC)가 아닌경우  Tab 삭제처리
	if(GV_BZDPDIVCD != "CCE" && GV_BZDPDIVCD != "CCL" && GV_BZDPDIVCD != "CCM") {
		this.tab_Menu.removeTabpage(2);
	} else {
		this.tab_Menu.tabpage1.btn_LeftMenuHide.class = "btn_LF_show";
		this.tab_Menu.tabpage2.btn_LeftMenuHide.class = "btn_LF_show";
		this.tab_Menu.tabpage3.btn_LeftMenuHide.class = "btn_LF_show";
		
		btn_LeftMenuShow.class = "btn_LF_hide";
	}
}

function fn_getTopParent(menuCd)
{
	//while((arr = objRegExp.exec(strMessage)) != null)
	
	var parentCd;
	var nRow = gds_Menu.findRow("menuCd", menuCd);
	
	//trace("nRow::"+nRow);
	
	while(nRow != -1)
	{
		//trace("parentCd::"+parentCd);
	
		parentCd 	= gds_Menu.getColumn(nRow, "hgrnMenuCd");
		if(gds_Menu.getColumn(nRow, "menuLvlVl") == 1) break;
		nRow 		= gds_Menu.findRow("menuCd", parentCd);
	}
	
	//trace("parentCd2::"+parentCd);
	return parentCd;
}

function fn_setMenuClass(sMenuCd)
{
	var objBtn = ap_TopFrame.form.div_Menu.components;
	
	for(var i=0;i<objBtn.length;i++)
	{
		var arrBtnName = String(objBtn[i].name).split("_");
		
		if(sMenuCd == arrBtnName[2]) {
			objBtn[i].class = "btn_TF_Menu_S";
		} else {
			objBtn[i].class = "btn_TF_Menu";
		}
	}
}	

function fn_setMenuFilter(sMenuNo, bInit)
{
	if(bInit == null) bInit = false;

	//trace(ds_Menu.saveXML());
	//trace("sMenuNo::"+sMenuNo);
	//ds_Menu.filter("hgrnMenuCd=='"+sMenuNo+"'");
	
	//gfn_debug(gds_Menu);
	
	// 메뉴 타이틀 처리
	//var sHgrnMenuCd = gds_Menu.lookup("menuCd", sMenuNo, "hgrnMenuCd");
	var sHgrnMenuCd = fn_getTopParent(sMenuNo);
	//trace("sHgrnMenuCd::"+sHgrnMenuCd);
	var sMenuTitle = "메뉴" + " <fc v='#b3a36c'><fs v='9'>[ " + gds_Menu.lookup("menuCd", sHgrnMenuCd, "menuNm") + " ]</fs></fc>";
	//var sMenuTitle = "메뉴" + " <fc v='#b3a36c'><fs v='9'>[ " + gds_Menu.lookup("hgrnMenuCd", sHgrnMenuCd, "menuNm") + " ]</fs></fc>";
	tab_Menu.tabpage1.sta_MenuTitle.text = sMenuTitle;
	
	// 현재 선택된 최상위메뉴 Selected 이미지 적용
	fn_setMenuClass(sHgrnMenuCd);
	
	var sFilterStr = fn_getFilterStr(sMenuNo);
	ds_Menu.filter(sFilterStr);
	
	// KRP 예외처리
	if(gfn_getConnectDiv() != "LOCL") {
		if(GV_wsoRvYn != "Y") {
			this.tab_Menu.tabpage1.grd_Menu.treeusecheckbox = false;
			return;
		}
	}

	// 파일존재여부 처리
	var objDs 	= ds_Menu;
	
	objDs.enableevent = false;
	
	for(var i=0;i<objDs.rowcount;i++)
	{
		if(!gfn_isNull(objDs.getColumn(i, "scrnUrlAddr"))) {
			if(gfn_getConnectDiv() == "LOCL") {
				var arrUrl = objDs.getColumn(i, "scrnUrlAddr").split("::");
				// 개발소스위치가 정해져있어 하드코딩 처리
				var sPath 	= "C:\\krpdev\\workspace\\KRP_XUI\\WebContent\\xui\\" + arrUrl[0] + "\\";
				var sFileNm = arrUrl[1];

				sPath = sPath + sFileNm;

				var bExistFile 		= VirtualFile.isExist(sPath);	
				//trace(i + " bExistFile::"+bExistFile + " " + sPath);		
				objDs.setColumn(i, "isExists", bExistFile);
			} else {
				if(GV_wsoRvYn == "Y") {
					var objDoc 	= new DomDocument;
					objDoc.url = objDs.getColumn(i, "scrnUrlAddr");
					var rtn = objDoc.load(false);
					
					//trace("rtn::"+rtn+" Url::"+objDs.getColumn(i, "scrnUrlAddr"));
					
					if(rtn) {
						objDs.setColumn(i, "isExists", true);	
					} else {
						objDs.setColumn(i, "isExists", false);	
					}
				} 
			}
		}	
	}
	objDs.enableevent = true;		

	if(bInit) {
		// 환경설정정보 처리(메뉴 트리 열림(Y)/담힘(N))
		if(gds_userEnvrStup.getColumn(0, "hrchMenuExpeYn") == "Y") {
			tab_Menu.tabpage1.btn_collapse.click();
		} else {
			tab_Menu.tabpage1.btn_expand.click();
		}	
	}	
}

function fn_getFilterStr(sMenuNo)
{
	var sFilterStr 	= "menuCd=='"+sMenuNo+"'";
	var nRow 		= gds_Menu.findRow("menuCd", sMenuNo);
	var nSLevel 	= gds_Menu.getColumn(nRow, "menuLvlVl");
	
	if(nSLevel == 2) {
		sMenuNo 	= gds_Menu.getColumn(nRow, "hgrnMenuCd");
		sFilterStr 	= "menuCd=='"+sMenuNo+"'";
		nRow 		= gds_Menu.findRow("menuCd", sMenuNo);
		nSLevel 	= gds_Menu.getColumn(nRow, "menuLvlVl");
	}
	
	for(var i=nRow+1;i<gds_Menu.rowcount;i++)
	{
		var nLevel = gds_Menu.getColumn(i, "menuLvlVl");
		
		if(nLevel > nSLevel) {
			sFilterStr += "||menuCd=='"+gds_Menu.getColumn(i, "menuCd")+"'";
		} else {
			break;
		}
	}
	
	return sFilterStr;
}


// 트리 열림/닫힘 상태 처리
function fn_setTreeState(objDs, bState)
{
	if(objDs.name == "ds_Menu") {
		for(var i=0;i<objDs.rowcount;i++)
		{
			if(gfn_isNull(objDs.getColumn(i, "scrnUrlAddr"))) objDs.setColumn(i, "treeStat", bState);
		}
	} else {
		for(var i=0;i<objDs.rowcount;i++)
		{
			if(objDs.getColumn(i, "menuLvl") == 0) {
				var sGrpCd = objDs.getColumn(i, "persMenuSrno");
				var nPCnt = objDs.getCaseCount("persMenuSrno=='"+sGrpCd+"'");
				
				if(nPCnt > 1) objDs.setColumn(i, "treeStat", bState);
			}	
		}
	
	}
	//trace(ds_Menu.saveXML());
}

function fn_getScrnUrlAddr(sMenuCd)
{
	var sAddr = gds_Menu.lookup("menuCd", sMenuCd, "scrnUrlAddr");
	//trace("sAddr::"+sAddr);
	return sAddr;
}

// 메뉴 클릭
function Tab00_tabpage1_grd_Menu_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	var objDs = this[obj.binddataset];
	var nRow = e.row;
	var sUrl = objDs.getColumn(nRow, "scrnUrlAddr");	

	if (gfn_getLength(sUrl) > 0){

		//trace("scrnTypCd::"+objDs.getColumn(nRow, "scrnTypCd"));

		// 메뉴 클릭시에 팝업화면으로 오픈되어야 하는 경우 예외처리(CubigCC에서 사용 - Modaless)
		if(objDs.getColumn(nRow, "scrnTypCd") == "10") {
			var nWidth 	= gfn_transNullToNumber(objDs.getColumn(nRow, "wdthLngt"), 0);
			var nHeight = gfn_transNullToNumber(objDs.getColumn(nRow, "vrtcLngt"), 0);
			
			//trace("nWidth::"+nWidth+" nHeight::"+nHeight);
			
			//gfn_diaLog(dialog_id, nLeft, nTop, nWidth, nHeight, stFormURL, bShowTitle, strArgument, isModeless, isLayered, bAutoSize, bBorder)
			gfn_diaLog(objDs.getColumn(nRow, "scrnId"), -1, -1, nWidth, nHeight, sUrl, true, "", true, false, true, true); 
		} else {	
			if(objDs.getColumn(nRow, "ppupDspsYn") == "Y") {
				gfn_openWorkArea(objDs.getColumn(nRow, "scrnId"));			// 팝업업무화면 호출시 사용
			} else {
				gfn_chkOpenMenu(objDs.getColumn(nRow, "menuCd"), objDs);	// 업무영역 오픈	
			}	
		}
	} else {
		_gfn_setTreeRowState(objDs, nRow, "CLICK");
	}
}

// 마이메뉴 클릭
function tab_Menu_tabpage2_grd_MyMenu_oncellclick(obj:Grid, e:GridClickEventInfo)
{
 	var objDs = gds_UserMyMenu;
 	var nRow = e.row;
	
	//trace(gds_Menu.saveXML());

	var nFRow = gds_Menu.findRow("scrnNo", objDs.getColumn(nRow, "scrnNo"));
	if(nFRow != -1) {
		//var sUrl = gds_Menu.lookup("scrnNo", objDs.getColumn(nRow, "scrnNo"), "scrnUrlAddr");
		var sUrl = gds_Menu.getColumn(nFRow, "scrnUrlAddr");
	}
	
	//trace("sUrl::"+sUrl+"::"+objDs.getColumn(nRow, "scrnNo"));

	//trace("gfn_getLength(sUrl)::"+gfn_getLength(sUrl));

	if (gfn_getLength(sUrl) > 0){
		gfn_chkOpenMenu(gds_Menu.getColumn(nFRow, "menuCd"), gds_Menu);
		//gfn_openWorkArea("SYS0000201300");
		//gfn_openWorkArea(objDs.getColumn(nRow, "scrnId"));		// 팝업업무화면 호출시 사용
	} else {
		if(obj.getTreeChildCount(nRow) == 0) return;
		_gfn_setTreeRowState(objDs, nRow, "CLICK");
	}
}

