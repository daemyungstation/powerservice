var dataAthrQury = "";   /// 권한담기
var gv_b2b_comp_cd = ""; //B2B 회사코드

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS030110_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); 		// 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin());		// 권한
	
	bXls = "Y" == ds_athrPgmFilter.getColumn(0, 'xls_yn');
	
	btn_excel.visible = bXls;
	
	var lgnId = gfn_getUserInfo("lgn_id")
	fn_init("Y"); // 검색부 초기화
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			
			break;
			
			case "srchAnalResultList": // 고객 조회
			fn_postSearch();
			break;

		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (gfn_isNull(cbo_srch_yymm.value)) {
		alert("검색년도를 입력하세요.");
		return false;
	}	

	if (gfn_isNull(edt_b2b_comp_nm.value)) {
		alert("B2B회사명을 입력하세요.");
		return false;
	}	

	// 조회조건 및 default 정렬
	ds_inqCndt.clearData();
	ds_mem_list.keystring = ""; // 정렬컬럼 초기화
	
	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "yymm", cbo_srch_yymm.value);
	ds_inqCndt.setColumn(0, "b2b_com_cd", gv_b2b_comp_cd);
	
	return true;
}

// 개인정보 동의여부 조회전
function fn_preSrchPers()
{
	return true;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 실적 통계 조회
function fn_commSearch(sType, searchInfo)
{
	if (!fn_preSearch(sType)) return false;

	var szSvcid       = "srchAnalResultList";
	var szController  = "/dlw/cust/AnalResultlist";
	var szInDs        = "ds_input=ds_inqCndt";
	var szOutDs       = "ds_mem_list=ds_output";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
			
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init(sType)
{
	cbo_srch_yymm.index = 0;

	edt_b2b_comp_nm.value = "";
	gv_b2b_comp_cd = ""; //B2B 회사코드
}

// 조회 결과
function fn_postSearch()
{
	if (ds_mem_list.rowcount < 1) {
		ds_member.clearData();
		
		ds_pers_info.clear();
	} else {
	}
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/

// 검색 버튼 클릭
function btn_srch_onclick(obj:Button,  e:ClickEventInfo)
{
