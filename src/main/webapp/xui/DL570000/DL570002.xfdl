function btn_accnt_onclick(obj:Button,  e:ClickEventInfo)
{
	var sAccntBtnName = obj.name;
	var sPrmr;
	
	if(sAccntBtnName == "btn_srchAccntNo")
	{
	    xSelectAccntInputType = "srchAccnt";
	    sPrmr = {pSrchStandard : "accnt_no", pSrchVal : edt_accntNo.value};
	}
	else if(sAccntBtnName ==  "btn_accntNo")
	{
	    xSelectAccntInputType = "inputAccnt";
	    sPrmr = {pSrchStandard : "accnt_no", pSrchVal : tab_event.tap_eventBasicInfo.btn_accntNo.value};
	}
    
	var reVal = gfn_diaLog("회원조회", -1, -1, -1, -1, "DL570000::DL570002P09.xfdl", false, sPrmr, true, false, true, null, null, true);
}

function fn_setCustAccntNo()
{
    if(xSelectAccntInputType == "srchAccnt")
    {
	    edt_accntNo.value = ds_custAccnt.getColumn(0, "accnt_no");
	}
	else if(xSelectAccntInputType == "inputAccnt")
	{
		tab_event.tap_eventBasicInfo.edt_accntNo.value = ds_custAccnt.getColumn(0, "accnt_no");
