            returnBool = true;
        break;
        
        case "getHistoryTabList" :
            var nRow = ds_listEventMain.rowposition;
            var sMemNo = ds_listEventMain.getColumn(nRow, "mem_no");
            var sAccntNo = ds_listEventMain.getColumn(nRow, "accnt_no");
