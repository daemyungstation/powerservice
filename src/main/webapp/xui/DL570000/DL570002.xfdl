/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL570002_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj);
	fn_init();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcId, errCd, errMsg) 
{
	if (errCd != 0) 
	{
	    alert("조회를 실패하였습니다.\n" + errMsg);
	    return false;    
	}
	else
	{
	    switch (svcId) 
	    {
	        case "selectConsTyp":
				gfn_getConsTypLoad("", "SELECT", ds_listConsTypeCd1);
				fn_consTypeInit2();
			break;
			
			case "getConsTypCd2" :
			    gfn_getConsTypLoad("", "SELECT", ds_listConsTypeCd2);
			break;
			
			case "getConsTypCd3" :
				gfn_getConsTypLoad("", "SELECT", ds_listConsTypeCd3);
			break;
			
			case "selectDlwCode": // 대명코드 조회
			    gfn_getCode("93", "SELECT", ds_codeDieReson, gds_DmlCd);
			    gfn_getCode("07", "SELECT", ds_codeRel, gds_DmlCd);
			    gfn_getCode("0142", "SELECT", ds_codeBrplot, gds_DmlCd);
			    gfn_getCode("0145", "SELECT", ds_codePayOpt, gds_DmlCd);
			    gfn_getCode("0120", "SELECT", ds_codeCardCd, gds_DmlCd);
			    gfn_getCode("0147", "SELECT", ds_codeMthdCd, gds_DmlCd);
			    gfn_getCode("02", "SELECT", ds_codeSort, gds_DmlCd);
			    fn_commSearch(gfn_getCommTypeInit(), "getBranchList");
			break;
			
		    case "getListEventMain" :
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "getEventDetailList" :
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "getEventTabList" :
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "saveEventTabList" :
		        alert("저장되었습니다.");
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "getHistoryTabList" : 
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "savetHistoryTabList" :
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "getExtInfoTabList" : 
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "saveExtInfoTabList":
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
		    
		    case "getBranchList" :
		        fn_postSearch(gfn_getCommTypeInit(), svcId);
		    break;
	    }
	}
}

function fn_postSearch(sType, sSvcId)
{    
    switch (sSvcId) 
	{
	    case "getBranchList":
			cbo_sort.index = 0;
			cbo_branchCd.index = 0;
	    break;
	    
		case "getListEventMain" :
            if(ds_listEventMain.getRowCount() < 1) 
	        {
	            fn_datasetInit();
	            gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, 0);
		        alert("조회 결과가 없습니다.");
		        return false;
	        }
	        else
	        {
	            gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, nTotalListCnt);
	            
	            if(gfn_isNull(xSaveAccntNo) == false)
				{
					var nRow = ds_listEventMain.findRow("accnt_no", xSaveAccntNo);
					ds_listEventMain.rowposition = nRow;
					xSaveAccntNo = "";
				}
				
	            fn_commSearch(gfn_getCommTypeInit(), "getEventDetailList");
	        }
		break;
		
		case "getEventDetailList" :
            if(ds_listEventMainSingle.getRowCount() < 1) 
	        {
		        return false;
	        }
	        else
	        {
	            fn_setEventCRUDComponentType("VIEW");
	        }
	        
	        var sTabPageId = tab_event.tabpages[tab_event.tabindex].name;
				
			if(sTabPageId == "tap_paymentInfo")
			{
			    var nPaymentAmtSum = 0;
	
		        for (var idx =0; idx < ds_listPaymentInfo.getRowCount(); idx++)
		        {
			        nPaymentAmtSum += toNumber(ds_listPaymentInfo.getColumn(idx, "amt")); 
				}
				 
			    tab_event.tap_paymentInfo.edt_totPaymentAmt.value = nPaymentAmtSum;
			}
		break;
		
		case "getEventTabList" :
		    var sTabPageId = tab_event.tabpages[tab_event.tabindex].name;
				
			if(sTabPageId == "tap_paymentInfo")
			{
			    var nPaymentAmtSum = 0;
	
		        for (var idx =0; idx < ds_listPaymentInfo.getRowCount(); idx++)
		        {
			        nPaymentAmtSum += toNumber(ds_listPaymentInfo.getColumn(idx, "amt")); 
				}
				 
			    tab_event.tap_paymentInfo.edt_totPaymentAmt.value = nPaymentAmtSum;
			}
		break;
		
		case "saveEventTabList" :
		    fn_commSearch(gfn_getCommTypeInit(), "getListEventMain");
		break;
		
		case "getHistoryTabList" :
		    
		break;
		
		case "savetHistoryTabList" :
		    var sTabPageId = tab_history.tabpages[tab_history.tabindex].name;
				
			if(sTabPageId == "tap_consHist")
			{
			    tab_history.tap_consHist.edt_consMemoCntn.value = "";
			}
			else if(sTabPageId == "tap_protocolMngrHist")
			{
			    
			}
			
		    fn_commSearch(gfn_getCommTypeInit(), "getHistoryTabList");
		break;
		
		case "getExtInfoTabList" :
		    
		break;
		
		case "saveExtInfoTabList":
		    fn_commSearch(gfn_getCommTypeInit(), "getExtInfoTabList");
		break;
	}
}

