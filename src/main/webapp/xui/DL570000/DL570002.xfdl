function fn_preSaveEvent(sType, svcId)
{
    switch(svcId) 
	{		
		case "saveEventTabList" :
			if (sType == gfn_getCommTypeInit())	
			{
				var sTabPageId = tab_event.tabpages[tab_event.tabindex].name;
				
			    if(sTabPageId == "tap_eventBasicInfo")
			    {
			        xSaveAccntNo = tab_event.tap_eventBasicInfo.edt_accntNo.value;
			        
			        var sAccntNo = tab_event.tap_eventBasicInfo.edt_accntNo.value;
			        var sEventRegDay = tab_event.tap_eventBasicInfo.cal_eventRegDay.value;
			        var nVal8H = tab_event.tap_eventBasicInfo.edt_val8H.value;
			        var nVal8M = tab_event.tap_eventBasicInfo.edt_val8M.value;
			        var nVal9H = tab_event.tap_eventBasicInfo.edt_val9H.value;
			        var nVal9M = tab_event.tap_eventBasicInfo.edt_val9M.value;
			        var nArrH = tab_event.tap_eventBasicInfo.edt_arrH.value;
			        var nArrM = tab_event.tap_eventBasicInfo.edt_arrM.value;
			        var nCompH = tab_event.tap_eventBasicInfo.edt_compH.value;
			        var nCompM = tab_event.tap_eventBasicInfo.edt_compM.value;
			        var nVal7H = tab_event.tap_eventBasicInfo.edt_val7H.value;
			        var nVal7M = tab_event.tap_eventBasicInfo.edt_val7M.value;
			        var nCfnH = tab_event.tap_eventBasicInfo.edt_cfnH.value;
			        var nCfnM = tab_event.tap_eventBasicInfo.edt_cfnM.value;
			        
			        if(gfn_isNull(sAccntNo))
			        {
			            alert("회원번호를 입력해주세요.");
			            tab_event.tap_eventBasicInfo.edt_accntNo.setFocus();
			            return false;
		            }
		            
		            if(gfn_isNull(sEventRegDay))
			        {
			            alert("행사등록일을 입력해주세요.");
			            tab_event.tap_eventBasicInfo.edt_accntNo.setFocus();
			            return false;
		            }
		            
		            if(!gfn_isNull(nVal8H)) 
		            {
			            if(gfn_transNullToNumber(nVal8H) > 24) 
			            {
				            alert("접수일시(시)는 24보다 클 수 없습니다");
				            tab_event.tap_eventBasicInfo.edt_val8H.setFocus();
				            return false;
			            }
		            }
		            
		            if(!gfn_isNull(nVal8M)) 
		            {
			            if(gfn_transNullToNumber(nVal8M) > 60) 
			            {
				            alert("접수일시(분)은 60보다 클 수 없습니다.");
				            tab_event.tap_eventBasicInfo.edt_val8M.setFocus();
				            return false;
			            }
		            }
		            
		            if(!gfn_isNull(nVal9H)) 
		            {
			            if(gfn_transNullToNumber(nVal9H) > 24) 
