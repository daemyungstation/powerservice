function ds_listEventMngr_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.col != 15)
	{
		var nRow = ds_listEventMngr.rowposition;
		var sStartTime = ds_listEventMngr.getColumn(nRow, "start_time");
		var sEndTime = ds_listEventMngr.getColumn(nRow, "end_time");
		var sJobDuty = ds_listEventMngr.getColumn(nRow, "job_duty");
		var sPayAmt = ds_listEventMngr.getColumn(nRow, "pay_amt");
		var sPayAddAmt = ds_listEventMngr.getColumn(nRow, "pay_add_amt");

		if(sJobDuty == "0003")
		{
			if(!gfn_IsNull(sStartTime) && !gfn_IsNull(sEndTime))
			{	
				var nPos1 = ds_listMngrPay.findRow("evt_mngr_cl", sJobDuty);
				
				if (nPos1 >= 0) 
				{
					var sHmhAday = ds_listMngrPay.getColumn(nPos1, "hmh_aday");
					//var sHmmAday = ds_listMngrPay.getColumn(nPos1, "hmm_aday");	
					var sHmmAddTime = ds_listMngrPay.getColumn(nPos1, "hmm_add_time");
				}
					
				var sCtime = parseInt(sEndTime) - parseInt(sStartTime) - parseInt(sHmhAday);
				
				if(parseInt(sCtime) < 0 )
				{
					sCtime = 0;
				}
				
				var nCamt = parseInt(sCtime) * parseInt(sHmmAddTime);
				var nSamt = gfn_transNullToNumber(parseInt(nCamt)) + gfn_transNullToNumber(parseInt(sPayAmt));
				
				var nStax = gfn_transNullToNumber(parseInt(nSamt)) * 0.03 ;
				var nJtax = gfn_transNullToNumber(parseInt(nStax)) * 0.1 ;
				
				nSamt = gfn_transNullToNumber(parseInt(nSamt)) - gfn_transNullToNumber(parseInt(nStax)) - gfn_transNullToNumber(parseInt(nJtax));
				
				ds_listEventMngr.setColumn(nRow, "over_time", sCtime);
				ds_listEventMngr.setColumn(nRow, "pay_add_amt", nCamt);
				ds_listEventMngr.setColumn(nRow, "number_of_day", 1);
				ds_listEventMngr.setColumn(nRow, "samt", nSamt);
			}
		} 
		else if(sJobDuty == "0002")
		{
			if(!gfn_IsNull(sStartTime) && !gfn_IsNull(sEndTime))
			{	
				var nSamt = gfn_transNullToNumber(parseInt(sPayAmt));
				
				var nStax = gfn_transNullToNumber(parseInt(nSamt)) * 0.03 ;
				var nJtax = gfn_transNullToNumber(parseInt(nStax)) * 0.1 ;
				
				nSamt = gfn_transNullToNumber(parseInt(nSamt)) - gfn_transNullToNumber(parseInt(nStax)) - gfn_transNullToNumber(parseInt(nJtax));

				ds_listEventMngr.setColumn(nRow, "samt", nSamt);
			}		
		} 
		else if(sJobDuty == "0001") 
		{
			if(!gfn_IsNull(sStartTime) && !gfn_IsNull(sEndTime))
			{
				var nPayAmt = gfn_transNullToNumber(parseInt(sPayAmt)) * 0.967;
		        var nPayAddAmt = gfn_transNullToNumber(parseInt(sPayAddAmt)) * 0.967;
		        
		        var nSamt = gfn_transNullToNumber(parseInt(nPayAmt)) + gfn_transNullToNumber(parseInt(nPayAddAmt));
				
				ds_listEventMngr.setColumn(nRow, "number_of_day", 1);
				ds_listEventMngr.setColumn(nRow, "samt", nSamt);
			}
		}
		else
		{
		
		}
	}
}

