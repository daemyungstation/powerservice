    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var nTotalListCnt = 0;
var athrPpupYn; // 팝업오픈권한
var fv_gubun
var g_prt = "Y";
var sOzServerIp = "";
var sOzServerPort = "";
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL570009_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_getList("basVl", "oz_server_ip,oz_server_port,open_oz_server_ip,open_oz_server_port,domain_oz_server_ip,domain_oz_server_port", null, null, "BasVl", "ds_basVlInfo");  // 공통코드조회
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_init();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		return false;
	}
	
	switch (svcid) 
	{
	    case "selectDlwCode": // 대명코드 조회
			gfn_getCode("108", "SELECT", ds_codeCnlReasonCd, gds_DmlCd);
			gfn_getCode("02", "SELECT", ds_codeSort, gds_DmlCd);
			fn_commSearch(gfn_getCommTypeInit(), "getBranchList");
		break;
		
		case "BasVl":
                var sServiceUrl = application.services["SVC"].url;
                
                 //2017.12.15 김찬영 SSL개발로 인한 수정
                //if (sServiceUrl.indexOf("61.39.175.220") >= 0) { // 외부
                if (sServiceUrl.indexOf("183.111.69.174") >= 0) 
                { // 외부
                    sOzServerIp = ds_basVlInfo.getColumn(ds_basVlInfo.findRow("bas_vl_nm", "open_oz_server_ip"), "bas_vl");
                    sOzServerPort = ds_basVlInfo.getColumn(ds_basVlInfo.findRow("bas_vl_nm", "open_oz_server_port"), "bas_vl");
                    trace("[OZ Report] 외부 : [" + sServiceUrl + "] ==> OzUrl [" + sOzServerIp + ":" + sOzServerPort + "]");
                } 
                 else if (sServiceUrl.indexOf("devdlcc.com") >= 0 || sServiceUrl.indexOf("dmdlcc.com") >= 0) 
				{ 
					// 2017.12.28 김찬영 SSL 추가
					sOzServerIp = ds_basVlInfo.getColumn(ds_basVlInfo.findRow("bas_vl_nm", "domain_oz_server_ip"), "bas_vl");
					sOzServerPort = ds_basVlInfo.getColumn(ds_basVlInfo.findRow("bas_vl_nm", "domain_oz_server_port"), "bas_vl");
					trace("[OZ Report] SSL : [" + sServiceUrl + "] ==> OzUrl [" + sOzServerIp + ":" + sOzServerPort + "]");
				}
                else 
                { // 내부
                    sOzServerIp = ds_basVlInfo.getColumn(ds_basVlInfo.findRow("bas_vl_nm", "oz_server_ip"), "bas_vl");
                    sOzServerPort = ds_basVlInfo.getColumn(ds_basVlInfo.findRow("bas_vl_nm", "oz_server_port"), "bas_vl");
                    trace("[OZ Report] 내부 : [" + sServiceUrl + "] ==> OzUrl [" + sOzServerIp + ":" + sOzServerPort + "]");
                }

				//alert("임시 아이피 주소 isLocal 변수 false로 바꾸세요."); // 디버그용 코드, 테스트 후 이행을 삭제
                var isLocal = false; 
                
                if (isLocal) {
					/********* 로컬호스트 테스트 용 *********/
					sOzServerIp = "http://183.111.69.174"; 
					sOzServerPort = "8080"; // 디버그용 코드, 테스트 후 이행을 삭제
					/********* 로컬호스트 테스트 용 *********/
                }
                    
                trace("[OZ Report] ServiceUrl [" + sServiceUrl + "] ==> OzUrl [" + sOzServerIp + ":" + sOzServerPort + "]");

            break;
		
		case "getProtocolSynthesisList" : // 문자발송이력 조회		    			
		    gfn_appendSearchDs(ds_listProtocolSynthesis, ds_listProtocolSynthesisTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_listProtocolSynthesis, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출
			fn_postSearch(svcid);
			fv_gubun = cbo_dir_out_gubun.value;
			if(gfn_isNull(fv_gubun)) {
				btn_print.enable = false;
			} else {
				btn_print.enable = true;
			}
		break;
		
		case "getBranchList" :
		    fn_postSearch(svcid);
		break;
	}
}

function fn_postSearch(sSvcId)
{
	switch (sSvcId) 
	{
	    case "getBranchList":
			cbo_srchSort.index = 0;
			cbo_branchCd.index = 0;
	    break;
	    
		case "getProtocolSynthesisList" :
			var nListCnt = ds_listProtocolSynthesis.getRowCount();
			if(nListCnt <= 0)
			{
				alert("조회된 데이터가 없습니다.");
				return false;
			}
		break;
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType, sSvcId)
{
	switch (sSvcId) 
	{
	    case "getBranchList" :
            ds_inqCndt.clearData();
		    ds_inqCndt.addRow();
		    
		    return true;
        break;
        
		case "getProtocolSynthesisList" :
		    if (sType == gfn_getCommTypeInit())
			{				
			    var cSrchSttDt = cal_srchSttDt.value;
				var sSrchSttDt = new String(cSrchSttDt);
				var cSrchEndDt = cal_srchEndDt.value;
				var sSrchEndDt = new String(cSrchEndDt);
				var sSrchDateDiff = gfn_getDayDiff(sSrchSttDt, sSrchEndDt);
				var sSrchDateType = cbo_srchDateType.value;
				var sAccntNo = edt_accntNo.value;
				var sProdCd = edt_prodCd.value;
				var sDelFg = cbo_srchDelFg.value;
				var sSort = cbo_srchSort.value;
				var sBranchCd = cbo_branchCd.value;
	            var sEvtMngrNm = edt_srchEvtMngrNm.value;
	            var sDirOutGubun = cbo_dir_out_gubun.value;
				
				if(gfn_IsNull(sSrchSttDt) == true || sSrchSttDt.length != 8 || 
				   gfn_IsNull(sSrchEndDt) == true || sSrchEndDt.length != 8)
				{
					alert("조회기간의 시작일과 종료일을 정확하게 입력하여 주십시오.");
					return false;
				}
				
				if (sSrchDateDiff > 31)
				{
					alert("조회기간은 최대 한달까지 검색 가능합니다.");
					return false;
				}
				
				ds_inqCndt.clearData();
				var nRow = ds_inqCndt.addRow();
	
				ds_inqCndt.setColumn(nRow, "srch_date_type", sSrchDateType);
				ds_inqCndt.setColumn(nRow, "srch_stt_dt", sSrchSttDt);
				ds_inqCndt.setColumn(nRow, "srch_end_dt", sSrchEndDt);
				ds_inqCndt.setColumn(nRow, "accnt_no", sAccntNo);
				ds_inqCndt.setColumn(nRow, "prod_cd", sProdCd);
				ds_inqCndt.setColumn(nRow, "del_fg", sDelFg);
				ds_inqCndt.setColumn(nRow, "sort", sSort);
				ds_inqCndt.setColumn(nRow, "branch_cd", sBranchCd);
		        ds_inqCndt.setColumn(nRow, "evt_mngr_nm", sEvtMngrNm);
		        ds_inqCndt.setColumn(nRow, "dir_out_gubun", sDirOutGubun);
		        
				ds_inqCndt.setColumn(nRow, "orderBy"       , "TBL_MAIN.ACCNT_NO");
				ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
			}
			return true;
		break;
		
		default :
			alert("서비스명이 잘못되었습니다.");
			return false;
		break;
	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
function fn_commSearch(sType, sSvcId)
{   
    switch (sSvcId)
    {
        case "getBranchList" :
            if (!fn_preSearch(sType, sSvcId)) 
	        {
	            return false;
	        }
	        
	        var szSvcid       = "getBranchList";
	        var szController  = "/dlw/evnt/receipt/getBranchList";
            var szInDs        = "ds_input=ds_inqCndt";
            var szOutDs       = "ds_codeBranchCd=ds_output";
            var szParam       = "";
            var szRetCallback = "fn_callBack";
            
            gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;
        
        case "getProtocolSynthesisList" :
			if (!fn_preSearch(sType, sSvcId)) return false;

	        // 호출 건수 설정 및 조회여부 설정
	        if (!gfn_pageSearch(sType, ds_listProtocolSynthesis, sta_TotalCnt, 2000, nTotalListCnt)) return;

	        var szSvcid       = "getProtocolSynthesisList";
            var szController  = "/dlw/evnt/receipt/getProtocolSynthesisList";
            var szInDs        = "ds_input=ds_inqCndt";
            var szOutDs       = "ds_listProtocolSynthesisTemp=ds_output";
            var szParam       = "";
            var szRetCallback = "fn_callBack";
            
            gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;
    }
}

