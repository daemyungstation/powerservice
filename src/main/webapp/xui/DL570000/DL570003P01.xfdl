// 저장  전
function fn_preOcbCardSave(){
     
	if (gfn_isNull(branch_nm.value)) {
		
			alert("장례식장명을 입력하세요.");
			return false;
		
	/*	}else if(gfn_isNull(branch_tel.value)){
		
