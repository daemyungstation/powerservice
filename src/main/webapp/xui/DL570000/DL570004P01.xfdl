/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
   		switch (svcid) {
		//  공통코드
		case "selectDlwCode": // 대명코드 조회
			gfn_getCode("0138", "SELECT", ds_evt_jik, gds_DmlCd); // 직책
		break;   		
   		// 조회
		case "getEventManList":
		  	ds_evt_mana_mng.clear();
   			gfn_appendSearchDs(ds_evt_mana_mng, ds_list_tmp);
   			fn_postSearch();
   		break;
		
		//입력
		case "insrtEventMan":
			alert("저장하였습니다.");
			//gfn_appendSearchDs(ds_evt_mana_mng, ds_list_tmp);
			
			//trace('********입력='+ds_evt_mana_mng.getColumn(0,"evt_mngr_cd"));	
			fn_postSearch();
			fn_EvtMng("getEventManList") ;
		break;
		
		
		case "updateEventMan":
			alert("수정하였습니다.");
			fn_postSearch();
			fn_EvtMng("getEventManList") ;
		break;
		

		case "deleteEventMan":
		    alert("삭제완료했습니다..");
			
			fn_EvtMng("getEventManList") ;
			close();
		break;

	}
   
   
}

	
