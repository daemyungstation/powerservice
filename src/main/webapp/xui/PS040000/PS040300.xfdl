// 일괄배정 버튼
function btn_bndeAsgn_onclick(obj:Button,  e:ClickEventInfo)
{
	// 일괄배정할 IVR 콜백 설정
	var sCalbId = "";
	for(var i=ds_list_calb.rowcount-1; i>=0; i--) {
		if(ds_list_calb.getColumn(i, "_chk") == 1 && ds_list_calb.getColumn(i, "calb_resl_cd") != "30") {
			if (sCalbId == "") sCalbId = ds_list_calb.getColumn(i, "calb_id");
			else sCalbId += "," + ds_list_calb.getColumn(i, "calb_id");
		}
	}
	
	if (gfn_isNull(sCalbId)) {
		alert("배정할 IVR콜백 건을 선택하세요.");
		return false;
	}
	
	var sPrmr = {p_selectcheck:sCalbId};
	var reVal = gfn_diaLog("일괄배정", -1, -1, -1, -1, "PS040000::PS040301.xfdl", false, sPrmr, false, false, true, null, null, true);
}

