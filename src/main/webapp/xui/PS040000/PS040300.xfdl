    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var tc_calb = 0; // 전체 건수

var initCnt = 0;

// 권한 설정
var amntYn; 	// 수정권한
var rcvrYn; 	// 회수
var closeYn; 	// 임의종결

var gvMenuId;   // 프로그램 로그 추가 2017.12.11 정승철
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS040300_onload(obj:Form, e:LoadEventInfo)
{
	// 프로그램 로그 추가 2017.12.11 정승철
	gvMenuId = GV_LOGMENUID;
	
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_createTeamUserInqDs(obj); // 팀에 따른 사용자 조회를 위한 데이터셋 설정
	fn_setAthrPgmBtn();				// 권한에 따른 버튼 설정
	gfn_getList("cd", "CNS040,CNS060,CNS200");  // 공통코드조회
	gfn_getList("teamCd","", null, null, null, "ds_allTeamCd"); // 팀코드 조회
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
 			gfn_getCode("CNS040", "ALL", ds_calbReslCd); // 처리상태
 			gfn_getCode("CNS060", "ALL", ds_ArsSrvcAddrCd); // IVR서비스
 			gfn_getCode("CNS200", "ALL", ds_calbDivCd); // 콜백예약유형
 			 			
 			++initCnt;
			fn_init(initCnt);
			break;
			
			case "selectTeamCode":
			gfn_getTeamCodeLoad("", "ALL", ds_srch_hgrnTeamCd, ds_allTeamCd, "1");
			gfn_getTeamCodeLoad("", "SELECT", ds_hgrnTeamCd, ds_allTeamCd, "1");
			
			cbo_srch_hgrn_team_cd.index = 0;
			
			++initCnt;
			fn_init(initCnt);			
			break;
			
			case "selectUserCode":
			gfn_getUserCdLoad("", "" ,"ALL", ds_srch_chprId);
			
			cbo_srch_chpr_id.index = 0;
			if (ds_srch_chprId.rowcount > 1) {
				cbo_srch_chpr_id.enable = true;
			} else {
				cbo_srch_chpr_id.enable = false;
			}
			break;
			
			case "selectChprId":
			gfn_getUserCdLoad("", "" ,"SELECT", ds_chprId);
			
			cbo_chpr_id.index = 0;
			if (ds_chprId.rowcount > 1) {
				cbo_chpr_id.enable = true;
			} else {
				cbo_chpr_id.enable = false;
			}
			break;
			
			case "srchCalbDtl":
			gfn_appendSearchDs(ds_list_calb, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_calb, sta_TotalCnt, tc_calb); // 페이지건수 표출
			fn_postSearch();
			break;
			
			case "updateCalbDtl":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "closeCalbDtl":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "withdrawCalbDtl":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_calb.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "calb_resl_cd", cbo_srch_calb_resl_cd.value);
		ds_inqCndt.setColumn(0, "calb_div_cd", cbo_srch_calb_div_cd.value);
	
	// 	***********************************************
	// 	수정자 :정훈연구원
	// 	요청자 :정성안팀장
	// 	수정이유 : 업체간 조회조건 변경
	// 	문의사항이 있으면 요청자에게 문의하세요
	
// 		ds_inqCndt.setColumn(0, "team1_cd", cbo_srch_hgrn_team_cd.value);
// 		ds_inqCndt.setColumn(0, "team2_cd", cbo_srch_team_cd.value);
	//************************************************************	
		ds_inqCndt.setColumn(0, "chpr_id", cbo_srch_chpr_id.value);
		ds_inqCndt.setColumn(0, "ars_srvc_addr", cbo_srch_ars_srvc_addr_cd.value);
		
		ds_inqCndt.setColumn(0, "orderBy", "calb_resr_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

// 저장 전
function fn_preSave()
{
	if (gfn_isNull(cbo_hgrn_team_cd.value)) {
		alert("상담그룹1을 선택하세요.");
		cbo_hgrn_team_cd.setFocus();
		return false;
	} else if (gfn_isNull(cbo_team_cd.value)) {
		alert("상담그룹2을 선택하세요.");
		cbo_team_cd.setFocus();
		return false;
	} else if (gfn_isNull(cbo_chpr_id.value)) {
		alert("담당자를 선택하세요.");
		cbo_chpr_id.setFocus();
		return false;
	}
	
	ds_calb.setColumn(0, "chpr_id", cbo_chpr_id.value);
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 예약콜 조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_calb, sta_TotalCnt, 25, tc_calb)) return;
	
	var szSvcid       = "srchCalbDtl";
    var szController  = "/cons/calb-dtl/list/admin";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

// 저장 버튼
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if (!fn_preSave()) return false;
	
	var szSvcid       = "updateCalbDtl";
    var szController  = "/cons/calb-dtl/save/update";
    var szInDs        = "ds_input=ds_calb";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

// 임의종결 버튼
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	if (confirm("임의종결하시겠습니까?")) {
		var szSvcid       = "closeCalbDtl";
		var szController  = "/cons/calb-dtl/save/close";
		var szInDs        = "ds_input=ds_calb";
		var szOutDs       = "";
		var szParam       = "";
		var szRetCallback = "fn_callBack";

		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
	}
}

// 회수 버튼
function btn_rcvr_onclick(obj:Button,  e:ClickEventInfo)
{
	if (confirm("회수하시겠습니까?")) {
		var szSvcid       = "withdrawCalbDtl";
		var szController  = "/cons/calb-dtl/save/withdraw";
		var szInDs        = "ds_input=ds_calb";
		var szOutDs       = "";
		var szParam       = "";
		var szRetCallback = "fn_callBack";

		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
	}
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init(sType)
{
	if (sType != 2) return false;
	initCnt = 0;
	
	//gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_chpr_id);
	cbo_srch_calb_resl_cd.index = 1; // 처리상태 - 접수
	cbo_srch_calb_div_cd.index = 0;
	cbo_srch_dateTyp.index = 3;
	
	// 	***********************************************
	// 	수정자 :정훈연구원
	// 	요청자 :정성안팀장
	// 	수정이유 : 업체간 조회조건 변경
	// 	문의사항이 있으면 요청자에게 문의하세요
	
	
	var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
	var sTeamCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
	if(sUserAthrCd.substr(0,2)=='TM'){ //TM
		cbo_srch_hgrn_team_cd.value = 'T20000';
		cbo_srch_hgrn_team_cd.enable = false;
		gfn_getTeamCodeLoad(cbo_srch_hgrn_team_cd.value, "ALL", ds_srch_teamCd, ds_allTeamCd);
		cbo_srch_team_cd.value = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
		cbo_srch_team_cd.enable = false;
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_chpr_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL");
		cbo_srch_chpr_id.index = 0;
		if(sTeamCd == "T20100") {
			cbo_srch_ars_srvc_addr_cd.value = '30000';
		} else {
			cbo_srch_ars_srvc_addr_cd.value = '20000';
		}
		cbo_srch_ars_srvc_addr_cd.enable =false;
	}else{//컨택
// 		cbo_srch_hgrn_team_cd.value = 'T10000';
// 		cbo_srch_hgrn_team_cd.enable = false;
// 		gfn_getTeamCodeLoad(cbo_srch_hgrn_team_cd.value, "ALL", ds_srch_teamCd, ds_allTeamCd);
// 		cbo_srch_team_cd.value = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
// 		cbo_srch_team_cd.enable = false;
// 		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_chpr_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL");
// 		cbo_srch_chpr_id.index = 0;
		cbo_srch_ars_srvc_addr_cd.value = '10000';
		cbo_srch_ars_srvc_addr_cd.enable = false;
		
	
		cbo_srch_hgrn_team_cd.index = 0;
		cbo_srch_hgrn_team_cd.enable = true;
		cbo_srch_team_cd.index =0;
		cbo_srch_team_cd.enable = false;
		cbo_srch_chpr_id.index =0;
		cbo_srch_chpr_id.enable = false;
	}
	//*************************************************
	fn_changeDateType();
}

// 날짜 선택
function fn_changeDateType()
{
	gfn_dateSrchDefaultChangeEvnt(cbo_srch_dateTyp, cal_srch_stt_dt, cal_srch_end_dt);
}

// 검색 결과
function fn_postSearch()
{
	if (ds_list_calb.rowcount < 1) {
		ds_calb.clearData();
		fn_setCnpnCorn("NULL");
	} else {
		fn_setCalbDtl();
	}
}

// 상세 설정
function fn_setCalbDtl()
{
	ds_calb.clearData();
	ds_calb.addRow();
	ds_calb.copyRow(0, ds_list_calb, ds_list_calb.rowposition);
	
	fn_setCnpnCorn("VIEW");
}

// 컴포넌트 제어
function fn_setCnpnCorn(sType)
{
	stc_txt_ivr.text = "IVR콜백 상세정보 보기";
	
	gfn_phoneNumObj(edt_calb_resr_tlno); // 예약번호
	gfn_phoneNumObj(edt_inco_tlno); // 인입번호
	
	if (gfn_isNull(ds_calb.getColumn(0, "team1_cd"))) {
		gfn_teamUserInit(cbo_hgrn_team_cd, cbo_team_cd, cbo_chpr_id);
	} else {
		gfn_teamUserSelect(cbo_hgrn_team_cd, cbo_team_cd, cbo_chpr_id, "1", ds_allTeamCd, ds_teamCd, "SELECT", "selectChprId");
		cbo_team_cd.value = ds_calb.getColumn(0, "team2_cd");
		gfn_teamUserSelect(cbo_hgrn_team_cd, cbo_team_cd, cbo_chpr_id, "2", ds_allTeamCd, ds_teamCd, "SELECT", "selectChprId");
		cbo_chpr_id.value = ds_calb.getColumn(0, "chpr_id");
	}
	
	if (sType == "NULL") {
		cbo_hgrn_team_cd.visible = false;
		cbo_team_cd.visible = false;
		cbo_chpr_id.visible = false;
		
		btn_save.visible = false;
		btn_cancel.visible = false;
		btn_rcvr.visible = false;
		btn_close.visible = false;
		btn_update.visible = false;
		
		mae_calb_resr_dttm.mask = "";
		mae_rgsn_dttm.mask = "";
		mae_amnt_dttm.mask = "";
	} else {
		cbo_hgrn_team_cd.visible = true;
		cbo_team_cd.visible = true;
		cbo_chpr_id.visible = true;
		
		mae_calb_resr_dttm.mask = "####-##-## ##:##";
		mae_rgsn_dttm.mask = "####-##-## ##:##";
		mae_amnt_dttm.mask = "####-##-## ##:##";
		
		if(sType == "VIEW") {
			cbo_hgrn_team_cd.enable = false;
			cbo_team_cd.enable = false;
			cbo_chpr_id.enable = false;
			
			btn_save.visible = false;
			btn_cancel.visible = false;
			
			btn_rcvr.visible = false;
			btn_close.visible = false;
			btn_update.visible = false;
			
			var sCalbReslCd = ds_calb.getColumn(0, "calb_resl_cd");
			if (sCalbReslCd == "20" && rcvrYn == "Y") {								// 미처리
				btn_rcvr.visible = true;
			}
			if ((sCalbReslCd == "10" || sCalbReslCd == "20") && closeYn == "Y") {	// 미처리, 접수
				btn_close.visible = true;
			}
			if ((sCalbReslCd == "10" || sCalbReslCd == "20") && amntYn == "Y") {
				btn_update.visible = true;
			}
		} else if (sType == "UPDATE") {
			cbo_hgrn_team_cd.enable = true;
			cbo_team_cd.enable = true;
			cbo_chpr_id.enable = true;
			//정훈
			btn_save.visible = true;
			btn_cancel.visible = true;
			btn_rcvr.visible = false;
			btn_close.visible = false;
			btn_update.visible = false;
			
			btn_save.position2 = "absolute r:74 w:69 b:90 h:20";
			btn_cancel.position2 = "absolute r:0 w:72 b:90 h:20";
			
			stc_txt_ivr.text = "IVR콜백 상세정보 수정";
			
			// 	***********************************************
			// 	수정자 :정훈연구원
			// 	요청자 :정성안팀장
			// 	수정이유 : 수정시 셋팅조건 변경
			// 	문의사항이 있으면 요청자에게 문의하세요
			var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
			if(sUserAthrCd.substr(0,2)=='TM'){ //TM
				cbo_hgrn_team_cd.value = 'T20000';
				cbo_hgrn_team_cd.enable = false;
				gfn_getTeamCodeLoad(cbo_hgrn_team_cd.value, "ALL", ds_teamCd, ds_allTeamCd);
				cbo_team_cd.value = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
				cbo_team_cd.enable = false;
				gfn_teamUserSelect(cbo_hgrn_team_cd, cbo_team_cd, cbo_chpr_id, "2", ds_allTeamCd, ds_teamCd, "SELECT", "selectChprId");
			}else{
				cbo_hgrn_team_cd.value = 'T10000';
				cbo_hgrn_team_cd.enable = false;
				gfn_getTeamCodeLoad(cbo_hgrn_team_cd.value, "ALL", ds_teamCd, ds_allTeamCd);
			}
			//***********************************************
		}
	}	
}

// 권한에 따른 버튼 설정
function fn_setAthrPgmBtn()
{
	gfn_athrPgm(gfn_gdsCd_admin());
	
	btn_bndeAsgn.visible = false;
	rcvrYn = "N";
	closeYn = "N";
	for (var i=0; i<ds_indvFnctAthrPgmFilter.rowcount; i++) {
		if (ds_indvFnctAthrPgmFilter.getColumn(i, "cnpn_id") == "bndeAsgn") {
			if (ds_indvFnctAthrPgmFilter.getColumn(i, "amnt_yn") == "1") {
				btn_bndeAsgn.visible = true;
			}
		}
		
		if (ds_indvFnctAthrPgmFilter.getColumn(i, "cnpn_id") == "close") {
			if (ds_indvFnctAthrPgmFilter.getColumn(i, "amnt_yn") == "1") {
				closeYn = "Y";
			}
		}
		
		if (ds_indvFnctAthrPgmFilter.getColumn(i, "cnpn_id") == "rcvr") {
			if (ds_indvFnctAthrPgmFilter.getColumn(i, "amnt_yn") == "1") {
				rcvrYn = "Y";
			}
		}
	}
}

