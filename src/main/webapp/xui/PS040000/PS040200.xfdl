// grid onmousemove
function grd_list_onmousemove(obj:Grid, e:GridMouseEventInfo)
{
    obj.tooltiptext = "";
	if (e.row < 0) {
	    return;
	}
	
	var sConsMemoCntn = ds_list_cons.getColumn(e.row, "cons_memo_cntn");
	if (gfn_isNull(sConsMemoCntn)) {
		return;
	}
	
	// 1 라인이 100 바이트 이상인 경우 줄넘김 추가
	var nBytes = 0;
	var sNewConsMemoCntn = "";
	for (var i = 0; i < sConsMemoCntn.length; i++) {
	    // 문자 복사
	    sNewConsMemoCntn += sConsMemoCntn.charAt(i);
	    // 내용에 줄넘김이 있는 경우 바이트 초기화
	    if (i + 1 < sConsMemoCntn.length &&
	        sConsMemoCntn.charAt(i) + sConsMemoCntn.charAt(i + 1) == "\r\n") {
	        i++;
	        sNewConsMemoCntn += sConsMemoCntn.charAt(i);
	        nBytes = 0;
	        continue;
	    } else if (sConsMemoCntn.charAt(i) == "\n") {
	        nBytes = 0;
	        continue;
	    }
	    // 바이트 계산 및 줄넘김 추가
	    nBytes += (sConsMemoCntn.charCodeAt(i) > 127) ? 2 : 1;
		if (nBytes >= 100) {
			sNewConsMemoCntn += "\r\n";
			nBytes = 0;
		}
	}
	obj.tooltiptext = sNewConsMemoCntn;
}

function phone_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	obj.value = gfn_phoneNumStr(obj.value);
}

// 전화번호 설정
function phone_ontextchanged(obj:Edit,  e:TextChangedEventInfo)
{
	obj.value = gfn_phoneNumStr(obj.value);
	obj.setCaretPos(-1);
}

// 상담사 다중선택 팝업창관리
function tab_mem_cons_tp_cons_div_consFilter_btn_close_onclick(obj:Button,  e:ClickEventInfo)
{	
	fn_applyConsFilter(); // 확인
}

function tab_mem_cons_tp_cons_div_consFilter_btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_hideConsFilter(); // 취소
}

function tab_mem_cons_tp_cons_div_consFilter_btn_release_onclick(obj:Button,  e:ClickEventInfo)
{
	for(var i=0;i<ds_srch_cnsrId.rowcount;i++)
	{
		ds_srch_cnsrId.setColumn(i, "chk", "");
	}
	edt_user_id.value = '';
	edt_user_nm.value = '';
	fn_hideConsFilter();
}

function fn_applyConsFilter() {
	// 체크 여부확인
	var user_id = "";
	var user_nm = "";
	var chkfg = false
	
	for(var i=0;i<ds_srch_cnsrId.rowcount;i++)
	{
		if(ds_srch_cnsrId.getColumn(i, "chk") == 1) { // 체크 여부 확인
			chkfg = true;
			if(i != 0){
				user_id += ",\'" + ds_srch_cnsrId.getColumn(i, "user_id") + "\'" ;
				user_nm += "," + ds_srch_cnsrId.getColumn(i, "user_nm") ;
			}
		}		
	}
	
	user_id = user_id.substr(1);
	user_nm = user_nm.substr(1);
	
	if (chkfg == false){
		alert("선택한 상담원이 없습니다.");
		return;
	}	
	
	edt_user_id.value = user_id;
	edt_user_nm.value = user_nm;
	
	div_consFilter.visible = false;
}

// 상담원 선택
function tab_mem_cons_tp_cons_div_consFilter_Grid00_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (ds_srch_cnsrId.rowposition == 0) {
		if ("1" == ds_srch_cnsrId.getColumn(e.row, "chk")) {
			for(var i=0;i<ds_srch_cnsrId.rowcount;i++)
			{
				ds_srch_cnsrId.setColumn(i, "chk", "1");
			}
		} else {
			for(var i=0;i<ds_srch_cnsrId.rowcount;i++)
			{
				ds_srch_cnsrId.setColumn(i, "chk", "");
			}
		}	
	}
}

function fn_hideConsFilter() {
	div_consFilter.visible = false;
}

function btn_div_popup(obj:Button,  e:ClickEventInfo){
	div_consFilter.visible = true;
}
