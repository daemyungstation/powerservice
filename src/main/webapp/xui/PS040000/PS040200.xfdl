}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		alert(errcd);
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
			gfn_getCode("CNS010", "ALL", ds_consChnl); // 접수채널
 			gfn_getCode("CNS020", "ALL", ds_consDspsmddlDtptCd); // 처리방법
 			gfn_getCode("CNS030", "ALL", ds_consStatCd); // 처리상태
 			++initCnt;
			fn_init(initCnt);
			break;
			
			case "selectTeamCode":
			gfn_getTeamCodeLoad("", "ALL", ds_srch_hgrnTeamCd, ds_allTeamCd, "1");
			cbo_srch_hgrn_team_cd.index = 0;
			++initCnt;
			fn_init(initCnt);			
			break;
			
			case "selectUserCode":
			gfn_getUserCdLoad("", "" ,"ALL", ds_srch_cnsrId);
			
			cbo_srch_cnsr_id.index = 0;
			if (ds_srch_cnsrId.rowcount > 1) {
				cbo_srch_cnsr_id.enable = true;
				btn_call_id.visible = true;
				edt_user_nm.visible = true;
			} else {
				cbo_srch_cnsr_id.enable = false;
				btn_call_id.visible = false;	
				edt_user_nm.visible = false;
			}
			break;
			
			case "consTyp1":
				gfn_getConsTypLoad("", "ALL", ds_cons_typ); //상담대분류
				cbo_srch_cons_typ_cd1.index = 0; // 초기화
			break;
			
			case "consTyp2":
				gfn_getConsTypLoad(cbo_srch_cons_typ_cd1.value, "ALL", ds_cons_typ2);
			    cbo_srch_cons_typ_cd2.index = 0; // 초기화
			break;
			
			case "consTyp3":
				gfn_getConsTypLoad(cbo_srch_cons_typ_cd2.value, "ALL", ds_cons_typ3);
			    cbo_srch_cons_typ_cd3.index = 0; // 초기화
			break;
			
			case "srchCons":
			gfn_appendSearchDs(ds_list_cons, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_cons, sta_TotalCnt, tc_cons); // 페이지건수 표출
			break;
			
			case "srchConsExcel":
			ds_inqCndt.setColumn(0, "cntr_cd", ds_list_excel.getColumn(0, "cntr_cd"));
			ds_inqCndt.setColumn(0, "path_typ", ds_list_excel.getColumn(0, "path_typ"));
			ds_inqCndt.setColumn(0, "dataAthrQury", ds_list_excel.getColumn(0, "dataAthrQury"));
			var colDS = gfn_excelPrepareExcelDS("grd_list");
			gfn_excelDown("상담이력관리", "XLS_CONS", "ConsMap.getConsList", "ConsMap.getConsCount", ds_inqCndt, true, colDS);
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 상담이력 조회전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
	
		if (cal_srch_stt_dt.value == null|| cal_srch_end_dt.value == null){
			alert("전체 조회의 경우 시스템 부하로 인하여 제한합니다.(전산팀)");
			return false;
		}

		if (gfn_getDayDiff(cal_srch_stt_dt.value, cal_srch_end_dt.value) > 31) {
			alert("날짜 범위는 31일을 초과 할 수 없습니다.");
			return false;
		}
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
	
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_cons.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		
		ds_inqCndt.setColumn(0, "dt_typ", cbo_srch_dt_typ.value);
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "cons_stat_cd", cbo_srch_cons_stat_cd.value);
		ds_inqCndt.setColumn(0, "cons_dspsmddl_dtpt_cd", cbo_srch_cons_dspsmddl_dtpt_cd.value);
		ds_inqCndt.setColumn(0, "acpg_chnl_cd", cbo_srch_cons_chnl.value);
		
		ds_inqCndt.setColumn(0, "user_typ", cbo_srch_user_typ.value);
		ds_inqCndt.setColumn(0, "team1_cd", cbo_srch_hgrn_team_cd.value);
		ds_inqCndt.setColumn(0, "team2_cd", cbo_srch_team_cd.value);
		//ds_inqCndt.setColumn(0, "user_id", cbo_srch_cnsr_id.value);
		ds_inqCndt.setColumn(0, "user_id", edt_user_id.value);
		ds_inqCndt.setColumn(0, "cell", edt_clph_tlno.value);
		
		ds_inqCndt.setColumn(0, "consno", edt_cons_id.value);
		
		ds_inqCndt.setColumn(0, "cons_typ1_cd", cbo_srch_cons_typ_cd1.value);
		ds_inqCndt.setColumn(0, "cons_typ2_cd", cbo_srch_cons_typ_cd2.value);
		ds_inqCndt.setColumn(0, "cons_typ3_cd", cbo_srch_cons_typ_cd3.value);
		ds_inqCndt.setColumn(0, "cons_memo_cntn", edt_consMemoCntn.value);
		
		
		ds_inqCndt.setColumn(0, "dept_yn", "Y");
		
		ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");		
	}
	
	return true;
}

