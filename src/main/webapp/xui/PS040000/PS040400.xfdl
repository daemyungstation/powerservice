	if (xlsYn == "Y") {
		btn_excel.visible = true;		// 엑셀다운로드
 	}
}

// 날짜 선택
function fn_changeDateType()
{
	gfn_dateSrchDefaultChangeEvnt(cbo_srch_dateTyp, cal_srch_stt_dt, cal_srch_end_dt);
}

// 버튼 삭제권한
function fn_setdelete() {
   var fv_emple_no = gfn_getUserInfo("user_id");
		trace(fv_emple_no);
		
		// 2018.01.03 정승철 - VOC 담당자 변경으로 인해 수정함 (** 이인호M => 나지식M )
		//if (  fv_emple_no == '2016120004'  || fv_emple_no == '2015090021' || fv_emple_no == '2016020003' ) {
		if (  fv_emple_no == '2017110016'  || fv_emple_no == '2018110004'  || fv_emple_no == '2015090021' || fv_emple_no == '2016020003' ) {
