			btn_delete.visible = false;
		}
}		
		

// 검색 결과
function fn_postSearch()
{
	if (ds_list_voc.rowcount < 1) {
		ds_voc.clearData();
		fn_setCnpnCorn("NULL");
	} else {
		fn_setVocDtl();
	}
}

// 상세 설정
function fn_setVocDtl()
{
	ds_voc.clearData();
	ds_voc.addRow();
	ds_voc.copyRow(0, ds_list_voc, ds_list_voc.rowposition);
	
	fn_setCnpnCorn("VIEW");
}

// 컴포넌트 제어
function fn_setCnpnCorn(sType)
{
	stc_txt_voc.text = "VOC 상세정보 보기";
	
	gfn_phoneNumObj(edt_sms_rpl_cnpl_tlno); // SMS
	gfn_phoneNumObj(edt_tlph_rpl_cnpl_tlno); // 전화
	
	// VOC 콜처리자 정보 설정
	if (gfn_isNull(ds_voc.getColumn(0, "voc_actp_team1_cd"))) {
		gfn_teamUserInit(cbo_actp_hgrn_team_cd, cbo_actp_team_cd, cbo_actp_user_id);
	} else {
		gfn_teamUserSelect(cbo_actp_hgrn_team_cd, cbo_actp_team_cd, cbo_actp_user_id, "1", ds_allTeamCd, ds_actp_teamCd, "SELECT", "selectActpUserId");
		cbo_actp_team_cd.value = ds_voc.getColumn(0, "voc_actp_team2_cd");
		gfn_teamUserSelect(cbo_actp_hgrn_team_cd, cbo_actp_team_cd, cbo_actp_user_id, "2", ds_allTeamCd, ds_actp_teamCd, "SELECT", "selectActpUserId");
		cbo_actp_user_id.value = ds_voc.getColumn(0, "voc_actp_id");
	}
	
	cbo_actp_hgrn_team_cd.enable = false;
	
	// 처리상태 설정
	cbo_voc_dsps_stat_cd.visible = false;
	edt_cbo_voc_dsps_stat_nm.visible = true;

	if (sType == "NULL") {
		// 버튼
		btn_update.visible = false;
		btn_delete.visible = false;
		btn_save.visible = false;
		btn_cancel.visible = false;
		
		// 일시
		mae_rgsn_dttm.mask = "";
		mae_amnt_dttm.mask = "";
		
		// 컴포넌트 제어
		fn_setComponent("visible", false);
	} else {
		mae_rgsn_dttm.mask = "####-##-## ##:##";
		mae_amnt_dttm.mask = "####-##-## ##:##";
		
		fn_setdelete();  //권한
		
		
		
		
		fn_setComponent("visible", true);
			
		if (sType == "VIEW") {
			btn_save.visible = false;
			btn_cancel.visible = false;
			
			// 권한 시스템관리자, 센터운영자만 회수 및 수정 가능
			var sAthrCd = gds_UserInfo.getColumn(0, "athr_cd");
			var sUserId = gds_UserInfo.getColumn(0, "user_id");

			if (sAthrCd=="AT01" || sAthrCd=="CC10" || sAthrCd=="CC20" || sAthrCd=="OP10" || sAthrCd=="OP20"
			|| sAthrCd=="TM10" || sAthrCd=="TM20" || sAthrCd=="TM30") {
				if (ds_voc.getColumn(0, "voc_dsps_stat_cd") == "20") { // 처리중
					if (amntYn == "Y") {
						btn_update.visible = true;
						//btn_delete.visible = true;
					}
				} else if (ds_voc.getColumn(0, "voc_dsps_stat_cd") == "10") { // 접수
					if (amntYn == "Y") {
						btn_update.visible = true;
						//btn_delete.visible = true;
					}
				}
			} else {
				// 본인의 것만 수정가능
				if (sUserId == ds_voc.getColumn(0, "voc_dssr_id") && ds_voc.getColumn(0, "voc_dsps_stat_cd") == "20") {
					if (amntYn == "Y") {
						btn_update.visible = true;
						//btn_delete.visible = true;
					}
				} else {
					btn_update.visible = false;
					btn_delete.visible = false;
				}
			}
			
			fn_setComponent("enable", false);
		} else if (sType == "UPDATE") {
			stc_txt_voc.text = "VOC 상세정보 수정";
		
			btn_update.visible = false;
			btn_delete.visible = false;
			btn_save.visible = true;
			btn_cancel.visible = true;
			
			btn_save.position2 = "absolute r:74 w:72 b:215 h:20";
			btn_cancel.position2 = "absolute r:0 w:72 b:215 h:20";
			
			fn_setComponent("enable", true);
			
			cbo_voc_dsps_stat_cd.visible = true;
			cbo_voc_dsps_stat_cd.position2 = "absolute l:595 w:100 b:137 h:21";
			edt_cbo_voc_dsps_stat_nm.visible = false;
			
			if (ds_voc.getColumn(0, "voc_dsps_stat_cd") == "10") { // 접수
				cbo_voc_dsps_stat_cd.index = 1;
			}
		}
	}
}

// 컴포넌트 제어
function fn_setComponent(sType, sFlag)
{
	eval(eval("'cbo_voc_typ_cd.'+sType+' = '+sFlag"));
	eval(eval("'tae_voc_titl.'+sType+' = '+sFlag"));
	eval(eval("'cbo_voc_impc_cd.'+sType+' = '+sFlag"));
	
	//eval(eval("'cbo_actp_hgrn_team_cd.'+sType+' = '+sFlag"));
	eval(eval("'cbo_actp_team_cd.'+sType+' = '+sFlag"));
	eval(eval("'cbo_actp_user_id.'+sType+' = '+sFlag"));
	
	eval(eval("'tae_voc_acpg_cntn.'+sType+' = '+sFlag"));
	eval(eval("'tae_voc_dsps_cntn.'+sType+' = '+sFlag"));
	eval(eval("'chk_sms_rpl_yn.'+sType+' = '+sFlag"));
	eval(eval("'edt_sms_rpl_cnpl_tlno.'+sType+' = '+sFlag"));
	eval(eval("'chk_tlph_rpl_yn.'+sType+' = '+sFlag"));
	eval(eval("'edt_tlph_rpl_cnpl_tlno.'+sType+' = '+sFlag"));
}

