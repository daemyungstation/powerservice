    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var tc_voc = 0; // 전체 건수

var initCnt = 0;

// 권한 설정
var amntYn; 	// 수정권한
var xlsYn; 		// 엑셀권한

var gvMenuId;  // 프로그램 로그 추가 2017.12.14
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS040400_onload(obj:Form, e:LoadEventInfo)
{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.12 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_createPagingDataset(obj); 			// 페이징처리 위한 데이터셋 설정
	gfn_createTeamUserInqDs(obj); 			// 팀에 따른 사용자 조회를 위한 데이터셋 설정
	fn_setAthrPgmBtn();						// 권한에 따른 버튼 설정
	gfn_getList("cd", "CNS080,CNS090"); 	// 공통코드조회
	gfn_getList("teamCd","", null, null, null, "ds_allTeamCd"); // 팀코드 조회
	fn_setdelete(); /// 버튼 삭제권한
	fn_commSearch(gfn_getCommTypeInit()); 	// 조회
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
 			gfn_getCode("CNS080", "SELECT", ds_srchVocTypCd); 	// VOC유형코드
 			gfn_getCode("CNS080", "ALL", ds_vocTypCd); 			// VOC유형코드
 			gfn_getCode("CNS090", "SELECT", ds_vocImpcCd); 		// 중요도
 			
 			++initCnt;
			fn_init(initCnt);
			break;
			
			case "selectTeamCode":
			gfn_getTeamCodeLoad("", "ALL", ds_srch_hgrnTeamCd, ds_allTeamCd, "1");
			gfn_getTeamCodeLoad("", "SELECT", ds_actp_hgrnTeamCd, ds_allTeamCd, "1");

			++initCnt;
			fn_init(initCnt);			
			break;
			
			case "selectUserCode":
			gfn_getUserCdLoad("", "" ,"ALL", ds_srch_userId);
			
			cbo_srch_user_id.index = 0;
			if (ds_srch_userId.rowcount > 1) {
				cbo_srch_user_id.enable = true;
			} else {
				cbo_srch_user_id.enable = false;
			}
			break;
			
			case "selectActpUserId":
			gfn_getUserCdLoad("", "" ,"SELECT", ds_actp_userId);
			
			cbo_actp_user_id.index = 0;
			if (ds_actp_userId.rowcount > 1) {
				cbo_actp_user_id.enable = true;
			} else {
				cbo_actp_user_id.enable = false;
			}
			break;
			
			case "srchVocDtl":
			gfn_appendSearchDs(ds_list_voc, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_voc, sta_TotalCnt, tc_voc); // 페이지건수 표출
			fn_postSearch();
			break;
			
			case "updateVocDtl":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "withdrawVocDtl":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			
			case "deleteVocDtl":
				alert("삭제완료했습니다.");
				fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "srchVocDtlExcel":
			gfn_exportGridToExcel(grd_list_excel,"VOC관리"+gfn_getTodayFull());
			break;
			
			
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_voc.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "voc_dsps_stat_cd_list", cbo_srch_voc_dsps_stat_cd_list.value);
		ds_inqCndt.setColumn(0, "voc_typ_cd", cbo_srch_voc_typ_cd.value);
		ds_inqCndt.setColumn(0, "voc_dssr_team1_cd", cbo_srch_hgrn_team_cd.value);
		ds_inqCndt.setColumn(0, "voc_dssr_team2_cd", cbo_srch_team_cd.value);
		ds_inqCndt.setColumn(0, "voc_dssr_id", cbo_srch_user_id.value);
		
		ds_inqCndt.setColumn(0, "orderBy", "rgsn_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

// 저장 전
function fn_preSave()
{	
	if (gfn_isNull(ds_voc.getColumn(0, "voc_typ_cd"))) {
		alert("VOC유형을 선택하세요.");
		cbo_voc_typ_cd.setFocus();
		return false;
	} else if (gfn_isNull(ds_voc.getColumn(0, "voc_titl"))) {
		alert("제목을 입력하세요.");
		tae_voc_titl.setFocus();
		return false;
	} else if (gfn_isNull(ds_voc.getColumn(0, "voc_acpg_cntn"))) {
		alert("내용을 입력하세요.");
		cbo_voc_acpg_cntn.setFocus();
		return false;
	} else if (gfn_isNull(ds_voc.getColumn(0, "voc_impc_cd"))) {
		alert("중요도를 선택하세요.");
		cbo_voc_impc_cd.setFocus();
		return false;
	} else if (gfn_isNull(cbo_voc_dsps_stat_cd.value)) {
		alert("처리상태를 선택하세요.");
		cbo_voc_dsps_stat_cd.setFocus();
		return false;
	} else if (gfn_isNull(ds_voc.getColumn(0, "voc_actp_team1_cd"))) {
		alert("VOC 콜처리자 그룹1을 선택하세요.");
		cbo_actp_hgrn_team_cd.setFocus();
		return false;
	} else if (gfn_isNull(cbo_actp_team_cd.value)) {
		alert("VOC 콜처리자 그룹2를 선택하세요.");
		cbo_actp_team_cd.setFocus();
		return false;
	} else if (gfn_isNull(cbo_actp_user_id.value)) {
		alert("VOC 콜처리자를 선택하세요.");
		cbo_actp_user_id.setFocus();
		return false;
	}
	
	if (ds_voc.getColumn(0, "voc_dsps_stat_cd") == "30") {
		if (gfn_isNull(ds_voc.getColumn(0, "voc_dsps_cntn"))) {
			alert("처리내용을 입력하세요.");
			tae_voc_dsps_cntn.setFocus();
			return false;
		}
	}
	
	if (ds_voc.getColumn(0, "old_voc_actp_id") != ds_voc.getColumn(0, "voc_actp_id")) {
		if (!confirm("VOC콜처리자 정보를 변경하시겠습니까?")) {
			return false;
		}
	}
	
	ds_voc.setColumn(0, "voc_dsps_stat_cd", cbo_voc_dsps_stat_cd.value)
	ds_voc.setColumn(0, "voc_actp_id", cbo_actp_user_id.value);
	
	return true;
}

