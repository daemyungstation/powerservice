		ConsTypSelect(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd, "1");
	} else if (obj.name == "cbo_srch_cons_typ2_cd") {	// 상담유형 중분류
		ConsTypSelect(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd, "2");
