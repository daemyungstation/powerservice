	gfn_getList("consTyp", "", null, null, "consTyp1");   			// 상담유형대분류코드조회
	gfn_getList("teamCd", null, null, null, null, "ds_allTeamCd"); 	// 팀코드 조회
	gfn_getList("hrchCd", "CAM990", null, null, "srchHrchCdSet");	// 캠페인유형(계층유형코드)
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "consTyp1":		// 상담유형 대분류
			gfn_getConsTypLoad("", "ALL", ds_cons_typ1);
			++initCnt;
			fn_init(initCnt);
			break;
			
			case "consTyp2":		// 상담유형 중분류
			gfn_getConsTypLoad(cbo_srch_cons_typ1_cd.value, "ALL", ds_cons_typ2);
			break;
			
			case "consTyp3":		// 상담유형 소분류
			gfn_getConsTypLoad(cbo_srch_cons_typ2_cd.value, "ALL", ds_cons_typ3);
			break;
		
			case "srchHrchCdSet":	// 캠페인유형 계층코드
			gfn_getHrchCodeLoad("", "ALL", ds_cmpgTyp1Cd, null, 1);
			++initCnt;
			fn_init(initCnt);
		    break;
			
			case "selectTeamCode":	// 상담그룹 설정
			gfn_getTeamCodeLoad("", "ALL", ds_srch_hgrnTeamCd, ds_allTeamCd, "1");
			++initCnt;
			fn_init(initCnt);
			break;
			
			case "selectUserCode":	// 상담사 설정
			gfn_getUserCdLoad("", "" ,"ALL", ds_srch_userId);
			
			cbo_srch_user_id.index = 0;
			if (ds_srch_userId.rowcount > 1) {
				cbo_srch_user_id.enable = true;
				btn_call_id.visible = true;
				edt_user_nm.visible = true;				
			} else {
				cbo_srch_user_id.enable = false;
				btn_call_id.visible = false;	
				edt_user_nm.visible = false;				
			}
			break;
			
			case "srchRecHstr":
			gfn_appendSearchDs(ds_list_recHstr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_recHstr, sta_TotalCnt, tc_ctiHstr); // 페이지건수 표출
			
			if (ds_list_recHstr.rowcount > 0) {
				var sRecConsYn = ds_list_recHstr.getColumn(0, "rec_cons_yn");
				var sRecProdYn = ds_list_recHstr.getColumn(0, "rec_prod_yn");
				var sRecObYn = ds_list_recHstr.getColumn(0, "rec_ob_yn");
				
				if (sRecConsYn == "Y") {
					if (sRecProdYn == "Y") {
						if (sRecObYn == "Y") {
							grd_list.formatid = "all";
						} else {
							grd_list.formatid = "cons_prod";
						}
					} else {
						if (sRecObYn == "Y") {
							grd_list.formatid = "cons_ob";
						} else {
							grd_list.formatid = "cons";
						}
					}
				} else {
					if (sRecProdYn == "Y") {
						if (sRecObYn == "Y") {
							grd_list.formatid = "prod_ob";
						} else {
							grd_list.formatid = "prod";
						}
					} else {
						if (sRecObYn == "Y") {
							grd_list.formatid = "ob";
						} else {
							grd_list.formatid = "rec";
						}
					}
				}
			} else {
				grd_list.formatid = "rec";
			}
			break;
		}
	}
}

