/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (gfn_isNull(cal_srch_stt_dt.value) || gfn_isNull(cal_srch_end_dt.value)) {
			alert("검색기간을 입력하세요.");
			return false;
		}
	
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_recHstr.keystring = ""; // 정렬컬럼 초기화
		ds_inqCndt.addRow();
		
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "call_typ_cd", cbo_srch_call_typ.value);
		ds_inqCndt.setColumn(0, "call_typ_dtl_cd", cbo_srch_call_typ_dtl.value);
		ds_inqCndt.setColumn(0, "anino", edt_srch_anino.value);
		ds_inqCndt.setColumn(0, "team1_cd", cbo_srch_hgrn_team_cd.value);
		ds_inqCndt.setColumn(0, "team2_cd", cbo_srch_team_cd.value);
		//ds_inqCndt.setColumn(0, "user_id", cbo_srch_user_id.value);
		ds_inqCndt.setColumn(0, "user_id", edt_user_id.value);
		
		ds_inqCndt.setColumn(0, "bzpt_div_yn", "Y");
		
		ds_inqCndt.setColumn(0, "rec_cons_yn", "N");
		ds_inqCndt.setColumn(0, "cons_mem_nm", edt_srch_cons_mem_nm.value);
		ds_inqCndt.setColumn(0, "cons_typ1_cd", cbo_srch_cons_typ1_cd.value);
		ds_inqCndt.setColumn(0, "cons_typ2_cd", cbo_srch_cons_typ2_cd.value);
		ds_inqCndt.setColumn(0, "cons_typ3_cd", cbo_srch_cons_typ3_cd.value);
		ds_inqCndt.setColumn(0, "cons_conc_yn", cbo_cons_conc_yn.value);
		if (!gfn_isNull(edt_srch_cons_mem_nm.value) || !gfn_isNull(cbo_srch_cons_typ1_cd.value) || !gfn_isNull(cbo_srch_cons_typ2_cd.value)
		|| !gfn_isNull(cbo_srch_cons_typ3_cd.value) || !gfn_isNull(cbo_cons_conc_yn.value)) {
			ds_inqCndt.setColumn(0, "rec_cons_yn", "Y");
		}
		
		ds_inqCndt.setColumn(0, "rec_prod_yn", "N");
		ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
		ds_inqCndt.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
		if (!gfn_isNull(edt_srch_accnt_no.value) || !gfn_isNull(edt_srch_prod_cd.value)) {
			ds_inqCndt.setColumn(0, "rec_prod_yn", "Y");
		}
		
		ds_inqCndt.setColumn(0, "rec_ob_yn", "N");
		ds_inqCndt.setColumn(0, "cmpg_id", gv_cmpg_id);
		ds_inqCndt.setColumn(0, "cmpg_typ1_cd", cbo_srch_cmpg_typ1_cd.value);
		ds_inqCndt.setColumn(0, "cmpg_typ2_cd", cbo_srch_cmpg_typ2_cd.value);
		ds_inqCndt.setColumn(0, "ob_conc_yn", cbo_ob_conc_yn.value);
		if (!gfn_isNull(gv_cmpg_id) || !gfn_isNull(cbo_srch_cmpg_typ1_cd.value) || !gfn_isNull(cbo_srch_cmpg_typ2_cd.value)
		|| !gfn_isNull(cbo_ob_conc_yn.value)) {
			ds_inqCndt.setColumn(0, "rec_ob_yn", "Y");
		}
		
		ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 녹취 조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_recHstr, sta_TotalCnt, 100, tc_ctiHstr)) return;
	
	var szSvcid       = "srchRecHstr";
    var szController  = "/cti-hstr/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

