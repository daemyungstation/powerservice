		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL", null, edt_user_nm, btn_call_id);
