// button onclick
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_init") {				// 초기화 버튼
		fn_init(3);
	} else if (obj.name == "btn_search") {		// 검색 버튼
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_srchAcntNo") {	// 회원번호조회 버튼
		fn_srchAccntNoPop();
	} else if (obj.name == "btn_srchProdCd") {	// 상품조회 버튼
		fn_openProdPopup("btn");
	} else if (obj.name == "btn_srchCmpgId") {	// 캠페인조회 버튼
		fn_srchCmpgIdPop("btn");
	}
}

// onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if (obj.name == "edt_srch_accnt_no") {			// 회원번호
			fn_srchAccntNoPop();
		} else if (obj.name == "edt_srch_prod_cd") {	// 상품코드
			fn_openProdPopup("cd");
		} else if (obj.name == "edt_srch_prod_nm") {	// 상품명
			fn_openProdPopup("nm");
		} else if (obj.name == "edt_srch_cmpg_nm") {	// 캠페인
			fn_srchCmpgIdPop("nm");
		} else {										// 기타 - 조회
			fn_commSearch(gfn_getCommTypeInit());
		}
	}
}

