		cbo_srch_team_cd.value = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
		cbo_srch_team_cd.enable = false;
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL");
		btn_call_id.visible = true;
		edt_user_nm.visible = true;			
		cbo_srch_user_id.index = 0;
	}else{
		gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id);		// 상담그룹 및 상담사 초기화
		ConsTypInit(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd); 	// 상담유형 초기화	
	}
	
	//gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id);		// 상담그룹 및 상담사 초기화
	//ConsTypInit(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd); 	// 상담유형 초기화	
	cbo_cons_conc_yn.index = 0;			// 상담 계약여부
	edt_srch_accnt_no.value = "";		// 회원번호
	edt_srch_prod_cd.value = "";		// 상품코드
	edt_srch_prod_nm.value = "";
	gv_cmpg_id = "";					//	캠페인ID
	edt_srch_cmpg_nm.value = "";
	cbo_srch_cmpg_typ1_cd.index = 0;	// 캠페인유형1
	cbo_srch_cmpg_typ2_cd.index = 0;	// 캠페인유형2
	cbo_srch_cmpg_typ2_cd.enable = false;
	cbo_ob_conc_yn.index = 0;			// 캠페인 계약여부
	
	if ("Y" == xlsYn) {
		btn_excel.visible = true;
	}
}

