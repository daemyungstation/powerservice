function tab_mem_cons_tp_cons_div_consFilter_btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_hideConsFilter(); // 취소
}

function tab_mem_cons_tp_cons_div_consFilter_btn_release_onclick(obj:Button,  e:ClickEventInfo)
{
	for(var i=0;i<ds_srch_userId.rowcount;i++)
	{
		ds_srch_userId.setColumn(i, "chk", "");
	}
	edt_user_id.value = '';
	edt_user_nm.value = '';
	fn_hideConsFilter();
}

function fn_applyConsFilter() {
	// 체크 여부확인
	var user_id = "";
	var user_nm = "";
	var chkfg = false
	
	for(var i=0;i<ds_srch_userId.rowcount;i++)
	{
		if(ds_srch_userId.getColumn(i, "chk") == 1) { // 체크 여부 확인
			chkfg = true;
			if(i != 0){
				user_id += ",\'" + ds_srch_userId.getColumn(i, "user_id") + "\'" ;
				user_nm += "," + ds_srch_userId.getColumn(i, "user_nm") ;
			}
		}		
	}
	
	user_id = user_id.substr(1);
	user_nm = user_nm.substr(1);
	
	if (chkfg == false){
		alert("선택한 상담원이 없습니다.");
		return;
	}	
	
	edt_user_id.value = user_id;
	edt_user_nm.value = user_nm;
	
	div_consFilter.visible = false;
}

// 상담원 선택
function tab_mem_cons_tp_cons_div_consFilter_Grid00_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (ds_srch_userId.rowposition == 0) {
		if ("1" == ds_srch_userId.getColumn(e.row, "chk")) {
			for(var i=0;i<ds_srch_userId.rowcount;i++)
			{
				ds_srch_userId.setColumn(i, "chk", "1");
			}
		} else {
			for(var i=0;i<ds_srch_userId.rowcount;i++)
			{
				ds_srch_userId.setColumn(i, "chk", "");
			}
		}	
	}
}

function fn_hideConsFilter() {
	div_consFilter.visible = false;
}

function btn_div_popup(obj:Button,  e:ClickEventInfo){
	div_consFilter.visible = true;
}

