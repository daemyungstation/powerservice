// 초기화
function fn_init(sType)
{
	if (sType != 3) return false;
	initCnt = 0;
	
	var sToday = gfn_getToday();
	cal_srch_stt_dt.value = sToday;		// 녹취일자
	cal_srch_end_dt.value = sToday;
	cbo_srch_call_typ.index = 0;		// 콜유형
	cbo_srch_call_typ_dtl.index = 0;
	edt_srch_anino.value = "";			// 인입번호
	
	edt_srch_cons_mem_nm.value = "";	// 회원명
	
	var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
	if(sUserAthrCd.substr(0,2)=='TM'){
		cbo_srch_hgrn_team_cd.value = 'T20000';
		cbo_srch_hgrn_team_cd.enable = false;
