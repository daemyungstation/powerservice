function fn_setDtlComponent(sType)
{
	if(sType == "NULL") 
	{
		cbo_storeCd.enable = false;
	    cbo_typeCd.enable = false;
	    cbo_resind.enable = false;
	
        cbo_procMthd.enable = false;
        txt_procDesc.enable = false;
        
        var sStoreCd = cbo_storeCd.value;
        fn_typeCdSetting(sType, sStoreCd);
	} 
	else if(sType == "VIEW")
	{
		cbo_storeCd.enable = false;
	    cbo_typeCd.enable = false;
	    cbo_resind.enable = false;
	
        cbo_procMthd.enable = false;
        txt_procDesc.enable = false;
        
        var sStoreCd = cbo_storeCd.value;
        fn_typeCdSetting(sType, sStoreCd);
	}
	else if(sType == "INSERT")
	{
	    cbo_storeCd.enable = false;
	    cbo_typeCd.enable = false;
	    cbo_resind.enable = false;
	
        cbo_procMthd.enable = true;
        txt_procDesc.enable = true;
        
        var sStoreCd = cbo_storeCd.value;
        fn_typeCdSetting(sType, sStoreCd);
	}
	else if(sType == "UPDATE")
	{
        cbo_storeCd.enable = true;
	    cbo_typeCd.enable = true;
	    cbo_resind.enable = true;
	
        cbo_procMthd.enable = false;
        txt_procDesc.enable = false;
        
        var sStoreCd = cbo_storeCd.value;
        fn_typeCdSetting(sType, sStoreCd);
	}
}
