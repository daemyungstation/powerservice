    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var nTotalCount = 0;	     
var gvMenuId;        // 프로그램 로그 추가 2017.12.14
var xSaveType = "select";
var xSaveVocNo = "";

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS041100_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); 			// 페이징처리 위한 데이터셋 설정
	gfn_createTeamUserInqDs(obj); 			// 팀에 따른 사용자 조회를 위한 데이터셋 설정
	//gfn_getList("cd", "CNS210,CNS090"); 	// 공통코드조회
	gfn_getList("teamCd","", null, null, null, "ds_allTeamCd"); // 팀코드 조회
	fn_init();
	fn_commSearch(gfn_getCommTypeInit(), "getVocMngNewList"); 	// 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd);
		return false;
	} 
	else 
	{
		switch (svcid) 
		{
			case "selectCode" :
 			    //gfn_getCode("CNS210", "SELECT", ds_srchIndCd); 	// VOC유형코드
			break;
			
			case "selectTeamCode" :
			    gfn_getTeamCodeLoad("", "ALL", ds_srchHgrnTeamCd, ds_allTeamCd, "1");
			break;
			
			case "selectUserCode" :
			    gfn_getUserCdLoad("", "" ,"ALL", ds_srchClearMan);
			
			    cbo_srchClearMan.index = 0;
			    if (ds_srchClearMan.getRowCount() > 1) 
			    {
				    cbo_srchClearMan.enable = true;
			    } 
			    else 
			    {
				    cbo_srchClearMan.enable = false;
			    }
			break;
			
			case "getVocMngNewList":
			    gfn_appendSearchDs(ds_vocMngNewList, ds_vocMngNewListTemp);
			    gfn_setTotalRowCount(ds_vocMngNewList, sta_TotalCnt, nTotalCount);
			    fn_postSearch(gfn_getCommTypeInit(), svcid);
			break;
			
			case "insertVocMngNewList" :
				alert("데이터 가져오기가 완료되었습니다.");
				fn_commSearch(gfn_getCommTypeInit(), "getVocMngNewList");
			break;
			
			case "saveVocMngNewHist" :
				alert("저장이 완료 되었습니다.");
			    fn_commSearch(gfn_getCommTypeInit(), "getVocMngNewList");
			break;
			
			case "deleteVocMngNewList" :
				alert("삭제가 완료 되었습니다.");
				fn_commSearch(gfn_getCommTypeInit(), "getVocMngNewList");
			break;
			
			case "sendVocMngList" : 
				alert("전송이 완료되었습니다.");
				fn_commSearch(gfn_getCommTypeInit(), "getVocMngNewList");
			break;
			
			case "getVocMngNewListExcel" :
				if(ds_vocMngNewListExcel.getRowCount() > 0)
				{
				    gfn_exportGridToExcel(grd_listExcel,"VOC관리"+gfn_getTodayFull());
				}
				else
				{
				    alert("조회된 데이터가 없습니다.");
				    return false;
				}
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회전
function fn_preSearch(sType, svcId)
{
	switch(svcId) 
	{
		case "getVocMngNewList" :
			if (sType == gfn_getCommTypeInit())	
			{	
				if (cal_srchSttDt.value > cal_srchEndDt.value) 
				{
					alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
					return false;
				}
				
				// 조회조건 및 default 정렬
				ds_inqCndt.clearData();
				ds_vocMngNewList.keystring = ""; // 정렬컬럼 초기화
				
				ds_inqCndt.addRow();
				ds_inqCndt.setColumn(0, "stt_dt", cal_srchSttDt.value);
				ds_inqCndt.setColumn(0, "end_dt", cal_srchEndDt.value);
				ds_inqCndt.setColumn(0, "ind", cbo_srchInd.value);
				ds_inqCndt.setColumn(0, "hgrn_team_cd", cbo_srchHgrnTeamCd.value);
				ds_inqCndt.setColumn(0, "team_cd", cbo_srchTeamCd.value);
				ds_inqCndt.setColumn(0, "clear_man", cbo_srchClearMan.value);
				ds_inqCndt.setColumn(0, "send_yn", cbo_srchSendYn.value);
				ds_inqCndt.setColumn(0, "voc_input", cbo_srchVocInput.value);
				
				ds_inqCndt.setColumn(0, "orderBy", "VMN.VOC_NO");
				ds_inqCndt.setColumn(0, "orderDirection", "ASC");
			}
		break;
	}
	
	return true;
}

// 저장 전
function fn_preSave(sType, svcId)
{
	switch(svcId) 
	{
		case "insertVocMngNewList" :
			if(!confirm("데이터 가져오기를 실행 하시겠습니까?"))
			{
			    return false;
			}
			
			ds_inqCndt.clearData();
							
			var nRow = ds_inqCndt.addRow();
			ds_inqCndt.setColumn(nRow, "stt_dt", cal_srchSttDt.value); // 실제 사용되지 않음. 
			ds_inqCndt.setColumn(nRow, "end_dt", cal_srchEndDt.value); // 실제 사용되지 않음.
		break;
		
		case "saveVocMngNewHist" :
			if (sType == gfn_getCommTypeInit())	
			{
				xSaveVocNo = ds_vocMngNewSingle.getColumn(0, "voc_no");
				
				if(xSaveType == "update")
				{
					var sStoreCd = ds_vocMngNewSingle.getColumn(0, "store_cd");
					var sTypeCd = ds_vocMngNewSingle.getColumn(0, "type_cd");
					
					if (gfn_isNull(sStoreCd))
					{
						alert("카테고리1을 입력하여 주십시오.");
						cbo_storeCd.setFocus();
						return false;
					} 
					if (gfn_isNull(sTypeCd))
					{
						alert("카테고리2를 입력하여 주십시오.");
						cbo_typeCd.setFocus();
						return false;
					}
				}
				else if(xSaveType == "insert")
				{
					var sProcMthd = ds_vocMngNewSingle.getColumn(0, "proc_mthd");
					var sProcDesc = ds_vocMngNewSingle.getColumn(0, "proc_desc");
					
					if (gfn_isNull(sProcMthd))
					{
						alert("처리방법을 입력하여 주십시오.");
						cbo_procMthd.setFocus();
						return false;
					} 
					if (gfn_isNull(sProcDesc))
					{
						alert("처리요약을 입력하여 주십시오.");
						txt_procDesc.setFocus();
						return false;
					} 
				}
			}
		break;
		
		case "deleteVocMngNewList":
			var nChkRowCnt = ds_vocMngNewList.getCaseCount("chk == '1'");

			if(nChkRowCnt < 1)
			{
				alert("선택된 회원이 없습니다.");
				return false;
			}
			
			ds_vocMngNewListInDs.clearData();
			var nCopyAddRow = 0;
			
			for(var idx = 0; idx < ds_vocMngNewList.getRowCount(); idx++)
			{
				if(ds_vocMngNewList.getColumn(idx, "chk") == "1")
				{
					nCopyAddRow = ds_vocMngNewListInDs.addRow();
					ds_vocMngNewListInDs.copyRow(nCopyAddRow, ds_vocMngNewList, idx);
					nCopyAddRow++;
				}
			}
				
			if(!confirm("삭제 하시겠습니까?"))
			{
			    return false;
			}
		break;
	}
	
	return true;
}

function fn_preSend(sType, svcId)
{
	switch(svcId) 
	{
		case "sendVocMngList" :
			if (sType == gfn_getCommTypeInit())	
			{	
				var nChkRowCnt = ds_vocMngNewList.getCaseCount("chk == '1'");
				var nChkSendYn = ds_vocMngNewList.getCaseCount("chk == '1' && send_yn == 'Y'");

				if(nChkRowCnt < 1)
				{
					alert("선택된 회원이 없습니다.");
					return false;
				}
				
				if(nChkSendYn > 0)
				{
					alert("이미 전송된 데이터가 존재합니다. 전송이 완료되지 않은 데이터만 선택하여 주십시오.");
					return false;
				}
				
				ds_vocMngNewListInDs.clearData();
				var nCopyAddRow = 0;
				
				for(var idx = 0; idx < ds_vocMngNewList.getRowCount(); idx++)
				{
					if(ds_vocMngNewList.getColumn(idx, "chk") == "1")
					{
						nCopyAddRow = ds_vocMngNewListInDs.addRow();
						ds_vocMngNewListInDs.copyRow(nCopyAddRow, ds_vocMngNewList, idx);
						nCopyAddRow++;
					}
				}
					
				if(!confirm("전송 하시겠습니까?"))
				{
					return false;
				}
			}
		break;
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// VOC 조회
function fn_commSearch(sType, svcId)
{
	if (!fn_preSearch(sType, "getVocMngNewList"))
	{
	    return false;
	}
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_vocMngNewList, sta_TotalCnt, 30, nTotalCount)) return;
	
	var szSvcid       = "getVocMngNewList";
    var szController  = "/cons/vocMngNew/main/list/getVocMngNewList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_vocMngNewListTemp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, "", "", "S", gvMenuId);
}

// 저장 버튼
function fn_commSave(sType, svcId)
{
	switch(svcId)
	{
	    case "insertVocMngNewList":
			if (!fn_preSave(sType, "insertVocMngNewList")) 
			{
				return false;
			}
			
			var szSvcid       = "insertVocMngNewList";
			var szController  = "/cons/vocMngNew/main/list/insertVocMngNewList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";

			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, "", "", "I", gvMenuId);
	    break;
	    
		case "saveVocMngNewHist" :
			if (!fn_preSave(sType, "saveVocMngNewHist")) 
			{
				return false;
			}
			
			var szSvcid       = "saveVocMngNewHist";
			var szController  = "/cons/vocMngNew/main/list/saveVocMngNewHist";
			var szInDs        = "ds_input=ds_vocMngNewSingle";
			var szOutDs       = "";
			var szParam       = "save_type=" + xSaveType;
			var szRetCallback = "fn_callBack";

			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, "", "", "I", gvMenuId);
		break;
		
		case "deleteVocMngNewList" :
		    if (!fn_preSave(sType, "deleteVocMngNewList")) 
			{
				return false;
			}
			
			var szSvcid       = "deleteVocMngNewList";
			var szController  = "/cons/vocMngNew/main/list/deleteVocMngNewList";
			var szInDs        = "ds_input=ds_vocMngNewListInDs";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";

			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, "", "", "I", gvMenuId);
		break;
	}
}

function fn_commSend(sType, svcId)
{
	switch(svcId)
	{
	    case "sendVocMngList":
			if (!fn_preSend(sType, "sendVocMngList")) 
			{
				return false;
			}
			
			var szSvcid       = "sendVocMngList";
			var szController  = "/cons/vocMngNew/main/list/sendVocMngList";
			var szInDs        = "ds_input=ds_vocMngNewListInDs";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";

			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, "", "", "I", gvMenuId);
	    break;
	}
}

function btn_update_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_setCRUDComponentType("UPDATE");
}

function btn_insert_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_setCRUDComponentType("INSERT");
}

function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_setVocMngNewSingle();
	fn_setCRUDComponentType("VIEW");
}

// 검색 결과
function fn_postSearch(sType, svcId)
{
	switch(svcId) 
	{
		case "getVocMngNewList" :
			if (sType == gfn_getCommTypeInit())	
			{
				if (ds_vocMngNewList.getRowCount() < 1) 
				{
					ds_vocMngNewSingle.clearData();
					fn_setCRUDComponentType("NULL");
					alert("조회된 데이터가 없습니다.");
				} 
				else 
				{
					if(gfn_isNull(xSaveVocNo) == false)
					{
						var nRow = ds_vocMngNewList.findRow("voc_no", xSaveVocNo);
						ds_vocMngNewList.rowposition = nRow;
						xSaveVocNo = "";
					}
					fn_setVocMngNewSingle();
					fn_setCRUDComponentType("VIEW");
				}
			}
		break;
	}
	
	return true;
}
