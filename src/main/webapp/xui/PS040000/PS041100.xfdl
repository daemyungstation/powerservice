function fn_typeCdSetting(sType, sStoreCd)
{
	if(sType == "UPDATE")
	{
		if(sStoreCd == "D1")
		{
			ds_codeTypeCd.filter("code == 'D1' || code == 'D2' || code == 'D3'");
		}
		else if(sStoreCd == "D2")
		{
			ds_codeTypeCd.filter("code == 'D4' || code == 'D5' || code == 'D6'");
		}
		else if(sStoreCd == "D3")
		{
			ds_codeTypeCd.filter("code == 'D7' || code == 'D8' || code == 'D9'");
		}
		else if(sStoreCd == "D4")
		{
			ds_codeTypeCd.filter("code == 'D10' || code == 'D11' || code == 'D12'");
		}
		else
		{
			ds_codeTypeCd.filter("");
		}
	}
	else
	{
		ds_codeTypeCd.filter("");
	}
