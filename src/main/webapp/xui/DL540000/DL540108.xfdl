			}else if(ncnt > 1){
				var sPrmr = {p_prod_cd:"", p_prod_nm:Smngr_nm, p_mode:"reg"};
				var reVal = gfn_diaLog("품목 조회", -1, -1, -1, -1, "PS990000::PS992220.xfdl", false, sPrmr, true, false, true, null, null, true);			
			}		
		}else if(obj.getCellPos() == 4){
			if(ncnt < 1){
