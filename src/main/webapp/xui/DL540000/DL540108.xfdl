 [ PART 2 ] Form에서 사용될 전역변수를 선언
**************************************************************************/
var tc_prod = 0;

/***************************************************************************
 * 함  수  명		: prod_onload()
 * 기      능		: 폼 onload이벤트 
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function prod_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	
	var accnt_no = p_accnt_no;
	var seq = p_seq;
	
	g_accnt_no = accnt_no;
	g_seq = seq;
	
	fn_GoodsSearch(); // 품목 전체조회
	
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/***************************************************************************
 * 함  수  명		: fn_preSearch()
 * 기      능		: 조회 전 처리 조회조건 설정
 * 수  정  일      : 2016.01.22 
 ***************************************************************************/
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_rep_goods.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "seq", g_seq);

	}
	return true;
}

/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능 		: 조회
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
 // 대체용품 조회
function fn_commSearch(sType)
{	
    if (!fn_preSearch(sType)) return false;	
	
	var szSvcid       = "searchRepList";
    var szController  = "/dlw/newType/evnt/repList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_rep_goods=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
}

// 품목 데이터셋 담기
function fn_GoodsSearch()
{	

	var szSvcid       = "searchGoodsList";
    var szController  = "/dlw/newType/evnt/goodsList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_goods_all=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
}

// 저장버튼 클릭 시
function fn_rep_save(obj:Button,  e:ClickEventInfo)
{
	var cf = confirm("저장하시겠습니까?");
	if (cf == false) return;

	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";

	szSvcid       = "saveEvntRepGoodsSave";
	szController  = "/dlw/newType/evnt/repSave";
	szInDs        = "ds_input=ds_rep_goods:U";
	szOutDs       = "";
	szParam       = "accnt_no=" + g_accnt_no + " seq=" + g_seq;
	szRetCallback = "fn_callBack";
		

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
}

// 삭제버튼 클릭 시
function fn_wh_del(obj:Button,  e:ClickEventInfo)
{
	var cf = confirm("삭제하시겠습니까?");
	if (cf == false) return;
	
	fn_commDel(gfn_getCommTypeDelete());
		
	alert("저장 작업을 진행해야만 삭제처리 됩니다.")
}

/***************************************************************************
 * 함  수  명		: fn_srchCallBack()
 * 기      능		: 조회 콜백 처리
 ***************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
  
    if (errcd < 0) {
		alert("조회를 실패하였습니다.\n" + errmsg);
    } else {
		switch(svcid){
			case "saveEvntRepGoodsSave":
				alert("저장되었습니다.");
			break;
		}
    }
}

/***************************************************************************
 * 함  수  명		: btn_close_onclick()
 * 기      능		: x 버튼 이벤트
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

/***************************************************************************
