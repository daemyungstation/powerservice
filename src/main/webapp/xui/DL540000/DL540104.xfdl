    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_evt = 0;    //전체 건수
var strDate = "";  //현재 날짜
var g_row = 0;
var g_seq = 0;
var g_rpt_no = "";
var g_accnt_no = "";


/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL540104_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
    //gfn_getList("dlwCd","02","fn_callBack",false);   // 대명코드조회 (02:행사종류)
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	var seq = parent.parent.edt_seq.value; // 행사순번 가져오기
	var rpt_no = parent.parent.edt_rpt_no.value; // 행사보고서 번호 가져오기
	var accnt_no = parent.parent.edt_accnt_no.value; // 회원 번호 가져오기
	g_seq = seq;
	g_rpt_no = rpt_no;
	g_accnt_no = accnt_no;

	fn_commSearch(gfn_getCommTypeInit());
	fn_MngrSearch();
}
/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		return false;
	} else {
		switch (svcid) {
			case "srchManager":
				//trace(ds_list.saveXML())
			break;
  			case "saveManager":				
				alert("저장되었습니다.");
											
				fn_commSearch(gfn_getCommTypeInit());
				parent.parent.fn_commSearch(gfn_getCommTypeInit(), "srchEvtDetail");
			break;
					
			case "delMngrClass": // 
				alert("삭제되었습니다.");
				fn_commSearch(gfn_getCommTypeInit());
			break;
			case "searchMngrList":
				fn_mngrpay(); // 헁사자 수당기초자료
			break;
			
			case "srchManagerPay":
				//trace(ds_mngr_pay.saveXML())
			break;
		}
	}
}


/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType) {

	// 조회조건 및 default 정렬
	ds_inqCndt.keystring = ""; // 정렬컬럼 초기화
	
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	
	ds_inqCndt.setColumn(0, "evt_seq", g_seq);
	
	ds_inqCndt.setColumn(0, "orderBy", "EVENT_REG_DAY");
	ds_inqCndt.setColumn(0, "orderDirection", "DESC");

	return true;
}

function fn_preSave() {
	var cnt = 0;
	for(var i=0;i<ds_list.rowcount;i++)
	{
		if(ds_list.getColumn(i, "work_dm").length != 8) { // 체크 여부 확인
			alert("근무일자가 제대로 입력되었는지 확인바랍니다.");
			return false;
		}	

		if(ds_list.getColumn(i, "give_pay_dm").length != 8) { // 체크 여부 확인
			alert("지급일자가 제대로 입력되었는지 확인바랍니다.");
			return false;
		}
		if(ds_list.getColumn(i, "job_duty") == "0001"){
			cnt = cnt + 1;
		}
	}
	// CP는 한명만 등록하도록 체크
	if(cnt > 1){
		alert("기존에 등록된 CP가 있습니다. 등록 된 CP를 확인바랍니다.");
		return false;
	}
	fn_mobile_post("a");
	return true;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    if (!fn_preSearch()) return false;

	szSvcid       = "srchManager";
	szController  = "/dlw/newType/evnt/srchMngr";
	szInDs        = "ds_input=ds_inqCndt";
	szOutDs       = "ds_list=ds_output";
	szParam       = "";
	szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 수정 & 저장
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    if (!fn_preSave()) return false;
    
    //trace(ds_list.saveXML())

	szSvcid       = "saveManager";
