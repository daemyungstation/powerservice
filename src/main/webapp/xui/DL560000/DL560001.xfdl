    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt1 = 0;
var nTotalListCnt2 = 0;
var nTotalListCnt3 = 0;
var nTotalListCnt4 = 0;
var nTotalListCnt5 = 0;

var xSaveType;
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL560001_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj);
	fn_init();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcId, errCd, errMsg) 
{
	if (errCd != 0) 
	{
	    return false;    
	} 
	else
	{
	    switch (svcId) 
	    {
		    case "getIvrMgmtList" :
		        var sTabPageId = tab_main.tabpages[tab_main.tabindex].name;
				
			    if(sTabPageId == "tap_ivr")
			    {
					gfn_appendSearchDs(ds_listIvrMgmt, ds_listIvrMgmtTemp);
					gfn_setTotalRowCount(ds_listIvrMgmt, tab_main.tap_ivr.sta_totalCnt, nTotalListCnt1);
					fn_postSearch(gfn_getCommTypeInit(), svcId);
				}
				else if(sTabPageId == "tap_work")
				{
					gfn_appendSearchDs(ds_listWorkMgmt, ds_listWorkMgmtTemp);
					gfn_setTotalRowCount(ds_listWorkMgmt, tab_main.tap_work.sta_totalCnt, nTotalListCnt2);
					fn_postSearch(gfn_getCommTypeInit(), svcId);
				}
				else if(sTabPageId == "tap_genenum")
				{
					gfn_appendSearchDs(ds_listGenenumMgmt, ds_listGenenumMgmtTemp);
					gfn_setTotalRowCount(ds_listGenenumMgmt, tab_main.tap_genenum.sta_totalCnt, nTotalListCnt3);
					fn_postSearch(gfn_getCommTypeInit(), svcId);
				}
				else if(sTabPageId == "tap_holyday")
				{
					gfn_appendSearchDs(ds_listHolyday, ds_listHolydayTemp);
					gfn_setTotalRowCount(ds_listHolyday, tab_main.tap_holyday.sta_totalCnt, nTotalListCnt4);
					fn_postSearch(gfn_getCommTypeInit(), svcId);
				}
				else if(sTabPageId == "tap_script")
				{
					gfn_appendSearchDs(ds_listScriptMgmt, ds_listScriptMgmtTemp);
					gfn_setTotalRowCount(ds_listScriptMgmt, tab_main.tap_script.sta_totalCnt, nTotalListCnt5);
					fn_postSearch(gfn_getCommTypeInit(), svcId);
				}
				else
				{
				
				}
		    break;
		    
		    case "saveIvrMgmtList" :
		        alert("저장이 완료 되었습니다.");
			    fn_commSearch(gfn_getCommTypeInit(), "getIvrMgmtList");
		    break;
		    
		    case "deleteIvrMgmtList" :
		        alert("삭제가 완료 되었습니다.");
			    fn_commSearch(gfn_getCommTypeInit(), "getIvrMgmtList");
		    break;
	    }
	}
}

function fn_postSearch(sType, sSvcId)
{    
    switch (sSvcId) 
	{
		case "getIvrMgmtList" :
		    var sTabPageId = tab_main.tabpages[tab_main.tabindex].name;
				
			if(sTabPageId == "tap_ivr")
			{
			    var nRow = ds_listIvrMgmt.getRowCount();
				
				if(nRow <= 0)
				{
				    ds_listIvrMgmtSingle.clearData();
					fn_setCRUDComponentType("NULL");
					alert("조회결과가 없습니다.");
					return false;
				}
				else
				{
				    fn_setMgmtDataSingle();
			        fn_setCRUDComponentType("VIEW");
				}
