function fn_preSearch(sType, sSvcId)
{
    var returnBool = false;
    
	switch (sSvcId)
    {
        case "getIvrMgmtList" :
            var sTabPageId = tab_main.tabpages[tab_main.tabindex].name;
            var sSttRegDm = cal_sttRegDm.value;
            var sEndRegDm = cal_endRegDm.value;
            
            ds_inqCndt.clearData();
            var nRow = ds_inqCndt.addRow();
            
            ds_inqCndt.setColumn(nRow, "tab_gubun", sTabPageId)
            ds_inqCndt.setColumn(nRow, "stt_reg_dm", sSttRegDm);
            ds_inqCndt.setColumn(nRow, "end_reg_dm", sEndRegDm);
            
            ds_inqCndt.setColumn(nRow, "orderBy", "REG_DM");
            ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
            
            returnBool = true;
        break;
    }
    
    return returnBool;
}

