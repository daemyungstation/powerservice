/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
function fn_commSearch(sType, sSvcId)
{
    switch (sSvcId)
    {
        case "getIvrMgmtList" :
	        var sTabPageId = tab_main.tabpages[tab_main.tabindex].name;
			
	        if(sTabPageId == "tap_ivr")
	        {
				if(!gfn_pageSearch(sType, ds_listIvrMgmt, tab_main.tap_ivr.sta_totalCnt, 2000, nTotalListCnt1))
				{
					return;
				}
	        }
			else if(sTabPageId == "tap_work")
			{
				if(!gfn_pageSearch(sType, ds_listWorkMgmt, tab_main.tap_work.sta_totalCnt, 2000, nTotalListCnt2))
				{
				    return;
				}
			}
			else if(sTabPageId == "tap_genenum")
			{
				if(!gfn_pageSearch(sType, ds_listGenenumMgmt, tab_main.tap_genenum.sta_totalCnt, 2000, nTotalListCnt3))
				{
				    return;
				}
			}
			else if(sTabPageId == "tap_holyday")
			{
				if(!gfn_pageSearch(sType, ds_listHolyday, tab_main.tap_holyday.sta_totalCnt, 2000, nTotalListCnt4))
				{
				    return;
				}
			}
			else if(sTabPageId == "tap_script")
			{
				if(!gfn_pageSearch(sType, ds_listScriptMgmt, tab_main.tap_script.sta_totalCnt, 2000, nTotalListCnt5))
				{
				    return;
				}
			}
			else
			{
				alert("할당되지 아니한 업무영역입니다.");
				return false;
			}
			
			if (!fn_preSearch(sType, sSvcId)) 
	        {
	            return false;
	        }

	        var szSvcid       = "getIvrMgmtList";
            var szController  = "/dlw/ivrMgmt/ext/getIvrMgmtList";
            var szInDs        = "ds_input=ds_inqCndt";
            var szOutDs       = "ds_listIvrMgmtTemp=ds_output1 ds_listWorkMgmtTemp=ds_output2 ds_listGenenumMgmtTemp=ds_output3 " + 
                                "ds_listHolydayTemp=ds_output4 ds_listScriptMgmtTemp=ds_output5";
            var szParam       = "";
            var szRetCallback = "fn_callBack";
	        
            gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;
    }
}

