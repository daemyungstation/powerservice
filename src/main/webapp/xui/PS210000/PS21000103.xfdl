//콜백
function fn_callBack(svcid, errcd, errmsg) {

	trace(svcid);
	
    if (errcd != 0) {
		trace("errcd  :   " +errcd +"   \n errmsg   :   "+ errmsg +"    \n svcid   :   "+ svcid)
		return false;
	} else {
		switch(svcid){
		case "srchCdSet": //코드셋						
			gfn_getCode("CAM180", "SELECT", ds_dpmsGbnCdList, ds_cdSet); //업무구분
			gfn_getCode("CAM100", "SELECT", ds_dpmsStatCdList,ds_cdSet); //진행상태
			gfn_getCode("CAM090", "SELECT", ds_dpmsReslCdList,ds_cdSet); //통화결과
			
			tab_dpms_resl.tabpage1.cbo_dpms_gbn_cd.index = 0;
			tab_dpms_resl.tabpage1.cbo_dpms_stat_cd.index = 0;
			tab_dpms_resl.tabpage1.cbo_dpms_resl_cd.index = 0;
			break;
		
		case "saveTrgtCustAlct" : //저장콜백
			//ds_trgtCustAlct.clearData();
			//ds_trgtCustAlct.addRow();
			//ds_trgtCustAlct.copyRow(ds_trgtCustAlct.rowposition,ds_list_tmp,ds_list_tmp.rowposition);
			//parent.div_cmpgInfo.ds_list_trgtCustAlct.copyRow(parent.div_cmpgInfo.ds_list_trgtCustAlct.rowposition,ds_trgtCustAlct,ds_trgtCustAlct.rowposition);
			var setRowIndex = parent.div_cmpgInfo.ds_list_trgtCustAlct.rowposition;
			
			parent.div_cmpgInfo.setRowIndex = setRowIndex;//재조회
			parent.div_cmpgInfo.fn_commSearch(gfn_getCommTypeInit());//재조회
			break;
		
		case "saveAutoTrgtCustAlct" : //저장콜백
			ds_auto_trgtCustAlct.clearData();
			ds_auto_trgtCustAlct.addRow();
			ds_auto_trgtCustAlct.copyRow(ds_auto_trgtCustAlct.rowposition,ds_list_tmp,ds_list_tmp.rowposition);
			
			var sKey = ds_auto_trgtCustAlct.getColumn(ds_auto_trgtCustAlct.rowposition, 'trgt_cust_dtpt_id');
			
			var nRowindx = parent.div_cmpgInfo.ds_list_trgtCustAlct.findRow('trgt_cust_dtpt_id',sKey);
			if(nRowindx != -1){
				parent.div_cmpgInfo.ds_list_trgtCustAlct.copyRow(nRowindx, ds_auto_trgtCustAlct,ds_auto_trgtCustAlct.rowposition);
			}
			break;
		
		case "srchDpmsReslHstr" : //처리결과이력 조회 콜백
			gfn_appendSearchDs(ds_dpmsReslHstr, ds_list_tmp_hstr); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_dpmsReslHstr, tab_dpms_resl.tabpage2.sta_totalCnt, total_count); // 페이지건수 표출
			fn_postSearch();
						
			if(ds_trgtCustAlct.getColumn(ds_trgtCustAlct.rowposition, "dpms_stat_cd") == 60) {
				tab_dpms_resl.tabpage1.mae_reserve_proc_tm.value = ds_trgtCustAlct.getColumn(ds_trgtCustAlct.rowposition, "reserve_proc_tm"); //예약시간 셋팅

				tab_dpms_resl.tabpage1.stc_01.visible = true;
				tab_dpms_resl.tabpage1.stc_02.visible = true;
				tab_dpms_resl.tabpage1.stc_03.visible = true;
				
				tab_dpms_resl.tabpage1.cal_reserve_proc_dt.visible = true;
				tab_dpms_resl.tabpage1.mae_reserve_proc_tm.visible = true;
			}else{
				tab_dpms_resl.tabpage1.stc_01.visible = false;
				tab_dpms_resl.tabpage1.stc_02.visible = false;
				tab_dpms_resl.tabpage1.stc_03.visible = false;
				
				tab_dpms_resl.tabpage1.cal_reserve_proc_dt.visible = false;
				tab_dpms_resl.tabpage1.mae_reserve_proc_tm.visible = false;
			}

			break;
			
		case "srchDlv" :	// 발송 조회
			if (ds_dlvList.rowcount > 0) {
				var sPrmr = {dlv_stat_cd : ""};	
				gfn_diaLog("물류 수정", -1, -1, 673,655, "DL450000::PS450402.xfdl", false, sPrmr, false, false, true, null, null, true);
			} else {
				gfn_setWaitCursor(false);
				alert("조회된 발송정보가 없습니다.");
			}
			break;
		}
	}
}

