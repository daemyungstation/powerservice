/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	//trace("svcid : " + svcid);

	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			
			case "getUpluslist": // 조회
				
				// 조회결과 존재시 하단 정보창 DS COPY
				if(ds_list.rowcount > 0){
					if(p_url == 'DL50000104'){
						if(ds_list.getColumn(ds_list.rowposition, "chk") == 'Y') {
							btn_prod.visible = false;
						} else {
							btn_prod.visible = true;
						}
					}
					if(ds_list.getColumn(ds_list.rowposition, "sex") == "0001") {
						edt_sex.value = "남자";
					} else {
						edt_sex.value = "여자";
					}
					
					if(ds_list.getColumn(ds_list.rowposition, "pay_mthd") == "CMS" || ds_list.getColumn(ds_list.rowposition, "pay_mthd") == "04") {
						edt_card_nm.value = ds_list.getColumn(ds_list.rowposition, "bank_nm");
						edt_card_no.value = ds_list.getColumn(ds_list.rowposition, "bank_accnt_no");
						edt_expire_dt.value = "";
					} else {
						edt_card_nm.value = ds_list.getColumn(ds_list.rowposition, "card_nm");
						edt_card_no.value = ds_list.getColumn(ds_list.rowposition, "card_no");
						edt_expire_dt.value = ds_list.getColumn(ds_list.rowposition, "expire_dt");
					}	
					//edt_ordMstVdrBnfId.value = gfn_phoneNumStr(ds_list.getColumn(ds_list.rowposition, "ord_mst_vdr_bnf_id"));
				} else {
					alert("유플러스 상품 회원이 아닙니다.");
					close();
				}
				
			break;
			
			case "directUpdate": // 저장
				close(ds_list.saveXML());
			break;
		}
	}
}

