//조회
function fn_commSearch(sType, searchInfo) {

	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
		
        // 산출마감체크 조회
		case "getUpluslist":
		if (!fn_preSearch(sType, "getUpluslist")) return false;
	    szSvcid       = "getUpluslist";
	    if(p_url == 'DL50000104') {
			szController  = "/cmpg/trgt-cust-alct/alct/uplusPoplist2";
	    } else {
			szController  = "/cmpg/trgt-cust-alct/alct/uplusPoplist";
	    }
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_list=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
    }
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","");
}


/* 조회전 처리*/
function fn_preSearch(sType, svcid) {
	var return_bool = false;
	ds_inqCndt.clearData();
    ds_inqCndt.addRow();
    if(p_url == 'PS21000102') {
		ds_inqCndt.setColumn(0, "id_no",p_id_no);
	} else if(p_url == 'DL50000104'){
		ds_inqCndt.setColumn(0, "mem_nm",p_mem_nm);
		ds_inqCndt.setColumn(0, "cell",p_cell);
		ds_inqCndt.setColumn(0, "alct_yn",'Y');
		//ALCT_YN
	}
//    ds_inqCndt.setColumn(0, "id_no",p_id_no); // 
//     if(!gfn_isNull(p_ord_mst_idx)) {
// 		ds_inqCndt.setColumn(0, "ord_mst_idx",p_ord_mst_idx); // 
//     }    
    return_bool = true;
    //alert(ds_inqCndt.saveXML());
    
    return return_bool;
}

