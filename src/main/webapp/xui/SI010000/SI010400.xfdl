function btn_resultFileDownload_onclick(obj:Button,  e:ClickEventInfo)
{
	var sSrchDt = cbo_srchDt.value;
	var rtnFolder = FileDialog00.open("공제결과 데이터 다운로드", FileDialog.SELFOLDER, "%MYDOCUMENT%", "");
	var vFile_down = new VirtualFile();
	var sServiceUrl = application.services["SVC"].url;
	
	vFile_down.open(rtnFolder + "\\" + "DMLIFE_" + sSrchDt + ".txt", VirtualFile.openWrite);
	HttpObj.onload.setHandler(HttpObject00_onload);
	
	if(gfn_isNull(sServiceUrl) == true)
	{
		return false;
	}
		
	if (sServiceUrl.indexOf("61.39.175.220") >= 0) { // 외부
		var rtn = HttpObj.download("http://61.39.175.220:8080/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false);
	} else if (sServiceUrl.indexOf("https://devdlcc.com") >= 0) { 
		var rtn = HttpObj.download("https://devdlcc.com:8443/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false);
	} else if (sServiceUrl.indexOf("http://dmdlcc.com") >= 0) {
		var rtn = HttpObj.download("http://dmdlcc.com:8080/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false);
	} else if (sServiceUrl.indexOf("https://dmdlcc.com") >= 0) { 
		var rtn = HttpObj.download("https://dmdlcc.com:8443/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false); // 운영
	} else if (sServiceUrl.indexOf("61.39.175.221") >= 0) {
		var rtn = HttpObj.download("http://61.39.175.221:8080/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false); // 운영
	} else if (sServiceUrl.indexOf("192.168.10.62") >= 0) {
		var rtn = HttpObj.download("http://192.168.10.62:8080/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false); // 개발
	} else if (sServiceUrl.indexOf("127.0.0.1") >= 0) {
		var rtn = HttpObj.download("http://127.0.0.1:8080/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false);
	} else { // 내부
		var rtn = HttpObj.download("http://192.168.10.61:8080/powerservice/dlw/deduc/resultFileDownload?sSrchDt=" + sSrchDt, vFile_down, false);
	}
	
}

