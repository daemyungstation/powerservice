function fn_divMonthCalValueChanged()
{
	var sMonthCal = div_monthCal.getDate();
	if(gfn_isNull(sMonthCal) == true)
	{
		var sToday = gfn_getToday();
		div_monthCal.setDate(sToday);

		fn_commSearch(gfn_getCommTypeInit(), "getGongjeExtDt"); // 공제 산출스케줄 일자조회
		
		if(ds_gongje_ext_dt.getRowCount() > 0)
		{
			fn_commSearch(gfn_getCommTypeInit(), "getGongjeDayExt"); // 조회
		}
		else
		{
			alert("해당월에 산출된 데이터가 없습니다.");
			return false;
		}
	}
	else
	{
		fn_commSearch(gfn_getCommTypeInit(), "getGongjeExtDt"); // 공제 산출스케줄 일자조회
		
		if(ds_gongje_ext_dt.getRowCount() > 0)
		{
			fn_commSearch(gfn_getCommTypeInit(), "getGongjeDayExt"); // 조회
		}
		else
		{
			alert("해당월에 산출된 데이터가 없습니다.");
			return false;
		}
	}
}

