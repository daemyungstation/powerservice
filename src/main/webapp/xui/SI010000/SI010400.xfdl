/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(sSvcId, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd);
		return false;
	} 
	else 
	{
		switch(sSvcId) 
		{   
		    // 조회
		    case "getGongjeDayExt":
				gfn_appendSearchDs(ds_listGongjeDayExt, ds_listGongjeDayExtTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_listGongjeDayExt, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출
				fn_postSearch(sSvcId);
			break;
			
			case "getGongjeExtDt" :
				trace("공제 산출스케줄 조회완료");
				cbo_srchDt.index = 0;
			break;
			
			case "getGongjeTypeLastResultList" :
				//trace("공제구분별 최종결과반영 조회완료");
				fn_controlComponents();								
			break;
			
			case "resultFileUpload" :
				alert("결과파일 업로드가 완료되었습니다.");
				fn_commSearch(gfn_getCommTypeInit(), "getGongjeDayExt");
			break;
			
			case "convertFile" : 
				fn_commSearch(gfn_getCommTypeInit(), "getGongjeDayExt");
			break;
			
			// 공제결과 삭제
			case "deleteGongjeResult" :
				alert("공제결과 삭제완료");
				fn_commSearch(gfn_getCommTypeInit(), "getGongjeDayExt"); // 재조회
			break;
			
			// 공제파일 전송완료시
			case "sendGongjeFile" :
				alert("공제파일 전송완료");
			break;
		}
	}
}


function fn_postSearch(sSvcId)
{
	switch(sSvcId) 
	{
	    // 조회시
	    case "getGongjeDayExt":
			//trace("==공제 처리여부 조회완료");
			
			// 공제처리중인 데이터가 없는 경우 다운로드 버튼 open
			fn_controlComponents();
			
			// 진행중인 공제데이터 조회
			fn_commSearch(gfn_getCommTypeInit(), "getGongjeSendingList");
			// 공제구분별 최종결과반영 조회
			fn_commSearch(gfn_getCommTypeInit(), "getGongjeTypeLastResultList");
		break;
		
	}
}

/* 컴포넌트 valindation 처리 */
function fn_validateionComponents(objname) {			
	//산출 데이터 건수
	var mem_ext_cnt =  parseInt(edt_ext_mem_cnt.value);
	var pay_ext_cnt =  parseInt(edt_ext_pay_cnt.value);
	
	//결과 데이터 건수 (성공 실패)
	var mem_rst_o_cnt =  parseInt(ds_Result_data.getColumn(0,"mem_rst_o_cnt"));
	var mem_rst_x_cnt =  parseInt(ds_Result_data.getColumn(0,"mem_rst_x_cnt"));
	var pay_rst_o_cnt =  parseInt(ds_Result_data.getColumn(0,"pay_rst_o_cnt"));
	var pay_rst_x_cnt =  parseInt(ds_Result_data.getColumn(0,"pay_rst_x_cnt"));		
	var chk_yn = "OK";
	
	trace(objname);
			
	switch(objname){
		case "btn_resultFileUpload" :
			if(cbo_resultFileType.value == "M"){					
				if(stc_file_mem_yn.text == "Y" || edt_ext_mem_cnt == 0){
					chk_yn = "NG";
				}						
			}else{
				if(stc_file_pay_yn.text == "Y" || edt_ext_pay_cnt == 0){
					chk_yn = "NG";
				}
			}			
			if (chk_yn == "NG"){
				alert("업로드할 데이터가 없거나 이미 처리하였습니다.!!");
				return false;
			}								
		break;
		
		case "btn_resultFileReflection" :
			if(cbo_resultFileType.value == "M"){					
				if(stc_file_mem_yn.text == "N" || mem_ext_cnt == (mem_rst_o_cnt + mem_rst_x_cnt)){
					chk_yn = "NG";
				}						
			}else{
				if(stc_file_pay_yn.text == "N" || pay_ext_cnt == (pay_rst_o_cnt + pay_rst_x_cnt)){
					chk_yn = "NG";
				}
			}			
			if (chk_yn == "NG"){
				alert("결과반영할 데이터가 없거나 이미 처리하였습니다.!!");
				return false;
			}			
		break;					
	}
	
	return true;
}

/* 컴포넌트 control 처리 */
function fn_controlComponents() {	 
	//산출 데이터 건수
	var mem_ext_cnt =  parseInt(edt_ext_mem_cnt.value);
	var pay_ext_cnt =  parseInt(edt_ext_pay_cnt.value);
	
	//결과 데이터 건수 (성공 실패)
	var mem_rst_o_cnt =  parseInt(ds_Result_data.getColumn(0,"mem_rst_o_cnt"));
	var mem_rst_x_cnt =  parseInt(ds_Result_data.getColumn(0,"mem_rst_x_cnt"));
	var pay_rst_o_cnt =  parseInt(ds_Result_data.getColumn(0,"pay_rst_o_cnt"));
	var pay_rst_x_cnt =  parseInt(ds_Result_data.getColumn(0,"pay_rst_x_cnt"));			
			
	// 선택한 일자에 데이터가 있으면 엑셀은 자동으로 생성됨
	if(retVal > 0) {
		btn_down_basicExcel.visible = true;
		//btn_down_payExcel.visible = true;
	} else {
		btn_down_basicExcel.visible = false;
		//btn_down_payExcel.visible = false;
	}	
	
	// 파일 업로드 결과 스타일 수정
	if(stc_file_mem_yn.text=="Y"){
		stc_file_mem_yn.style.backgroundColor = "chartreuse";		
	} else {
		stc_file_mem_yn.style.backgroundColor = "darkorange";		
	}
	
	if(stc_file_pay_yn.text=="Y"){
		stc_file_pay_yn.style.backgroundColor = "chartreuse";		
	} else {
		stc_file_pay_yn.style.backgroundColor = "darkorange";		
	}
	
	// 결과 반영 데이터 확인 후 결과 Y,N값 표기	
	if(mem_ext_cnt == (mem_rst_o_cnt + mem_rst_x_cnt)){
		stc_mem_rst_yn.style.backgroundColor = "chartreuse";
		stc_mem_rst_yn.text = "Y" ;
	} else {
		stc_mem_rst_yn.style.backgroundColor = "darkorange ";
		stc_mem_rst_yn.text = "N" ;
	}
	
	if(pay_ext_cnt == (pay_rst_o_cnt + pay_rst_x_cnt)){
		stc_pay_rst_yn.style.backgroundColor = "chartreuse";
		stc_pay_rst_yn.text = "Y" ;
		
	} else {
		stc_pay_rst_yn.style.backgroundColor = "darkorange ";
		stc_pay_rst_yn.text = "N" ;
	}
	
	// 결과데이터 합계 SET
	edt_sum_ext_cnt.value = mem_ext_cnt + pay_ext_cnt;
	edt_rst_o_sum_cnt.value = mem_rst_o_cnt + pay_rst_o_cnt;
	edt_rst_x_sum_cnt.value = mem_rst_x_cnt + pay_rst_x_cnt;
		
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_validationCheck(sType, sSvcId)
{
	if (gfn_isNull(sSvcId) == true) 
	{
		alert("올바르지 않은 서비스 명입니다.");
		return false;
	}
	
	else 
	//if(
	{
		switch (sSvcId) 
		{						
			case "getGongjeDayExt" :
				var vSrchDt = cbo_srchDt.value;
				
				if((gfn_isNull(vSrchDt) == true) || vSrchDt.length != 8)
				{
					alert("조회일자를 입력해주세요.");
					cbo_srchDt.setFocus();
					return false;
				}
				
				return true;
			break;
		}
	}
}
/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
function fn_commSearch(sType, sSearchInfo)
{
	if (fn_validationCheck(sType, sSearchInfo) == false) 
	{
		return false;
	}
	switch(sSearchInfo) 
	{
        case "getGongjeDayExt": 
			if(!gfn_pageSearch(sType, ds_listGongjeDayExt, sta_TotalCnt, 50, nTotalListCnt))
			{
			    return;
			}
			
			ds_inqCndt.clearData();
			
			var nRow = ds_inqCndt.addRow();
			var sTabPageId = tab_deductionMgmt.tabpages[tab_deductionMgmt.tabindex].name;
			
			ds_inqCndt.setColumn(nRow, "srchDt", cbo_srchDt.value);
			ds_inqCndt.setColumn(nRow, "svc_id", sTabPageId); // 페이지탭의 이름에 따른 업무 서비스 구분.
			ds_inqCndt.setColumn(nRow, "orderBy", "GDATE");
			ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");

			var szSvcid       = "getGongjeDayExt";
			var szController  = "/dlw/deduc/list/getGongjeDayExt";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listGongjeDayExtTemp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;
        
        // 공제 산출스케줄 조회
        case "getGongjeExtDt": 
			ds_inqCndt.clearData();
			var nRow = ds_inqCndt.addRow();
			ds_inqCndt.setColumn(nRow, "ext_month", div_monthCal.getDate());
			ds_inqCndt.setColumn(nRow, "ext_chk", "Y");

			var szSvcid       = "getGongjeExtDt";
			var szController  = "/dlw/deduc/getGongjeExtDt";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_gongje_ext_dt=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			//gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
        break;        
        
        // 공제구분별 최종결과반영 조회
        case "getGongjeTypeLastResultList": 
			ds_inqCndt.clearData();
			var nRow = ds_inqCndt.addRow();
			ds_inqCndt.setColumn(nRow, "srchDt", cbo_srchDt.value);

			var szSvcid       = "getGongjeTypeLastResultList";
			var szController  = "/dlw/deduc/getGongjeTypeLastResultList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_Result_data=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
        break;   
                  
		// 공제구분별 최종결과반영 조회
        case "testExt": 
			var szSvcid       = "testExt";
			var szController  = "/dlw/deduc/testExt";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
        break;                    
    }
}

function fn_commSave(saveInfo) {

    var szSvcid = "";
    var szController = "";
    var szInDs = "";
    var szOutDs = "";
    var szParam = "";
    var szRetCallback = "";    
    var payMthd = "";
    
    //조회일자
    var vSrchDt = cbo_srchDt.value;
    
    //결과타입(회원/입금)
    var g_type = cbo_resultFileType.value;
    
    //결과데이터 총합
    var tot_ext_cnt = parseInt(edt_ext_mem_cnt.value) + parseInt(edt_ext_pay_cnt.value);

    switch(saveInfo) {    	
    	// 결과업로드
    	case "resultFileUpload" : 
			szSvcid       = "resultFileUpload";
			szController  = "/dlw/deduc/resultFileUpload";
			szInDs        = "";
			szOutDs       = "";
			szParam       = "g_date=" + vSrchDt + " g_type=" + g_type + " g_tot_ext_cnt=" + tot_ext_cnt;
			szRetCallback = "fn_callBack";
    	break;
    	
		// 파일변환
		case "convertFile":
			szSvcid       = "convertFile";
			szController  = "/dlw/deduc/GconvertFile";
			szInDs        = "";
			szOutDs       = "";
			szParam       = "g_date=" + vSrchDt + " g_type=" + g_type;
			szRetCallback = "fn_callBack";
		break;
		
		// 공제결과 삭제
		case "deleteGongjeResult":
			szSvcid       = "deleteGongjeResult";
			szController  = "/dlw/deduc/deleteGongjeResult";
			szInDs        = "";
			szOutDs       = "";
			szParam       = "";
			szRetCallback = "fn_callBack";
		break;
    }
	
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

