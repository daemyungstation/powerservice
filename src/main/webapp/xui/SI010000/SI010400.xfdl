// 공제 (일반 또는 선수금) 엑셀 다운로드
function fn_fileDownloadExcel(objName) {
	var p_gubun;
	if(objName == "btn_down_basicExcel") {
		if(!confirm("공제 일반엑셀을 다운로드 하시겠습니까?")) return false;
		p_gubun = "basic";
	} else if(objName == "btn_down_payExcel") {
		if(!confirm("공제 선수금엑셀을 다운로드 하시겠습니까?")) return false;
		p_gubun = "pay";
	}
	
	var p_yyyymmdd = cbo_srchDt.value;
	trace("p_yyyymmdd : " + p_yyyymmdd);
	
	if ( gfn_isEmpty(p_yyyymmdd) ) {
		return false;
	}
	
	var sUrl = application.services["SVC"].url;
	trace("sUrl : " + sUrl);
	//sUrl = sUrl + "/dlw/deduc/downloadFile?yyyymmdd=" + p_yyyymmdd;
	sUrl = sUrl + "/dlw/deduc/downloadFile?yyyymmdd=" + p_yyyymmdd + "&gubun=" + p_gubun;
   
	var dir = FileDialog00.open("공제 일반엑셀 다운", FileDialog.SELFOLDER);
	if (gfn_isEmpty(dir)) {
		return;
	}
	
	if(p_gubun == "basic") gblDir = dir + "\\" + "DMLIFE_"+p_yyyymmdd+".xlsx";
	else if(p_gubun == "pay") gblDir = dir + "\\" + "DMLIFE_PAY_"+p_yyyymmdd+"_001.xlsx";
	
	VirtualFile00.open(gblDir, VirtualFile.openWrite );
	var bSucc = HttpObj.download(sUrl, VirtualFile00 ,true);
}

