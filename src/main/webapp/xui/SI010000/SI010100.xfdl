        case "srchHangsa":
        if (!fn_preSearch(sType,"srchHangsa")) return false;
		szSvcid       = "searchHangsa";
		szController  = "/gongje/new/hangsa";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_gongjehangsa=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;          
         
        case "srchPay":
        if (!fn_preSearch(sType,"srchPay")) return false;
        if(!gfn_pageSearch(sType, ds_gongjepay, sta_TotalCnt, 50, tc_prod)) return;
		szSvcid       = "searchPay";
		szController  = "/gongje/new/pay";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_gongjepay_temp=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;      
            
        case "srchPay_Cncl":
        if (!fn_preSearch(sType,"srchPay_Cncl")) return false;
		szSvcid       = "searchPay_Cncl";
		szController  = "/gongje/new/paycncl";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_gongjepay_cncl=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;       
               
        case "srchRess_Cncl":
        if (!fn_preSearch(sType,"srchRess_Cncl")) return false;
		szSvcid       = "searchRess_Cncl";
		szController  = "/gongje/new/resscncl";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_gongjeress_cncl=ds_output";								
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;  
                       
        case "srchHangsa_Cncl":
        if (!fn_preSearch(sType,"srchHangsa_Cncl")) return false;
		szSvcid       = "searchHangsa_Cncl";
		szController  = "/gongje/new/hangsacncl";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_gongjehangsa_cncl=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;                         
    }
    // 트랜젝션 공통함수 호출
    /* 신규/타사 또는 해약 또는 정보변경 또는 양도양수 또는 행사탭에서
       조회의 경우 5번의 트랜잭션 발생으로 인해 srchNew 에서만 공통로그 입력하도록 - 2018.03.20 정승철 */
    if(searchInfo == "srchNew" ||        // 신규/타사
       searchInfo == "srchPay" ||        // 선수금
       searchInfo == "srchPay_Cncl" ||   // 선수금취소
       searchInfo == "srchRess_Cncl" ||  // 해약취소
       searchInfo == "srchHangsa_Cncl")  // 행사취소
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
	else
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


