/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	Edit_rs.value = ""; // 초기화
	new_cnt = 0;
	ress_cnt = 0;
	chng_cnt = 0;
	yds_cnt = 0;
	hangsa_cnt = 0;
	pay_cnt = 0;
	
	switch (svcid) {
		case "excelchk":
		Edit_rs.value = errcd;
		break;	
		
		case "cancelExcel":
		Edit_rs.value = errcd;
		break;
		
		case "searchNew":
		break;
		
		case "searchRess":
		break;
				
		case "searchChng":
		break;
						
		case "searchYds":
		break;
								
		case "searchHangsa":
		break;
		
		case "sendNewup":
		break;
										
		case "searchPay":
		gfn_appendSearchDs(ds_gongjepay, ds_gongjepay_temp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		gfn_setTotalRowCount(ds_gongjepay, sta_TotalCnt, tc_prod); // 페이지건수 표출		
		break;	
											
        case "sendNewup":
			alert("전송처리 되었습니다.");
        break;		
        															
        case "sendPayup":
			alert("전송처리 되었습니다.");
        break;	
                															
        case "sendPaycnclup":
			alert("전송처리 되었습니다.");
        break;	                															
	}
}



/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType,svcid) {

	var stt_dt = cal_stt_dt.value;
	var end_dt = cal_end_dt.value;
	
	if (tab_00.tabindex == 6 || tab_00.tabindex == 7 || tab_00.tabindex == 8) {
		var accnt_no = srch_accnt_no.value;
		var fcnt = srch_fcnt.value;
		var tcnt = srch_tcnt.value;				
	}
	
	// 조회조건 및 default 정렬
	ds_inqCndt.keystring = ""; // 정렬컬럼 초기화
	
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	
	ds_inqCndt.setColumn(0, "stt_dt", stt_dt);
	ds_inqCndt.setColumn(0, "end_dt", end_dt);
	
	if (tab_00.tabindex == 6) {
		ds_inqCndt.setColumn(0, "accnt_no", accnt_no);
		ds_inqCndt.setColumn(0, "fcnt", fcnt);
		ds_inqCndt.setColumn(0, "tcnt", tcnt);		
	} else if (tab_00.tabindex == 7 || tab_00.tabindex == 8) {
		ds_inqCndt.setColumn(0, "accnt_no", accnt_no);
	}
	
	if (tab_00.tabindex == 0) {
	} else {
		ds_accntInqCndt.setColumn(0, "orderBy", "seq");
		ds_accntInqCndt.setColumn(0, "orderDirection", "asc");
	}

	return true;
}

