// 조회전 Temp 데이터 삭제 후 신고데이터 Insert
function fn_TempSerach(sType, searchInfo){
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
	if (!fn_preSearch(sType, searchInfo)) return false;
	
	switch(searchInfo) {   
		case "TempBase":   // 신규/타사, 해약, 정보변경, 양도양수, 행사
		szSvcid       = "searchTemp";
		szController  = "/gongje/new/temp";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "";
		szParam       = "";
		szRetCallback = "fn_callBack";  
		break;
		
		case "TempPay":    // 선수금
		szSvcid       = "searchTempPay";
		szController  = "/gongje/new/temppay";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "";
		szParam       = "";
		szRetCallback = "fn_callBack";  
		break;	
	}
	 
    // 트랜젝션 공통함수 호출(비동기)
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);	
}

