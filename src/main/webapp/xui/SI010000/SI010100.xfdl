/********************************************************************************************/
//조회 버튼 이벤트
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	var stt_dt = cal_stt_dt.value;
	var end_dt = cal_end_dt.value;
	var accnt_no = srch_accnt_no.value;
	var fcnt = srch_fcnt.value;
	var tcnt = srch_tcnt.value;		
	
	if (tab_00.tabindex == 0 || tab_00.tabindex == 1 || tab_00.tabindex == 2 || tab_00.tabindex == 3 || tab_00.tabindex == 4 || tab_00.tabindex == 5) {
		if(stt_dt == '' || stt_dt == null){
			alert('조회일 시작일을 지정하여주세요!');
			return
		}else if(end_dt == '' || end_dt == null){
			alert('조회일 종료일을 지정하여주세요!');
			return
		}
	}else if(tab_00.tabindex == 6 || tab_00.tabindex == 7 || tab_00.tabindex == 8){
		if(accnt_no == '' || accnt_no == null){
			alert('회원번호를 입력하여주세요!');
			return
		}
	}
	
	
	/****************************************
	tabindex 값
			  0 : 신규/타사
			  1 : 해약
		   	  2 : 정보변경
	 		  3 : 양도양수
			  4 : 행사
			  5 : 선수금
			  6 : 선수금취소
			  7 : 해약취소
			  8 : 행사취소
	*****************************************/
	if (tab_00.tabindex == 0 || tab_00.tabindex == 1 || tab_00.tabindex == 2 || tab_00.tabindex == 3 || tab_00.tabindex == 4) {
		// 조회시 임시테이블 데이터 초기화 및 데이터 입력
		fn_TempSerach(gfn_getCommTypeSave(), "TempBase");
		
