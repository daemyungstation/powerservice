		szRetCallback = "fn_callBack";
		break;
		
        case "srchChng":
        if (!fn_preSearch(sType,"srchChng")) return false;
		szSvcid       = "searchChng";
		szController  = "/gongje/new/chng";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_gongjechng=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        
        case "srchYds":
        if (!fn_preSearch(sType,"srchYds")) return false;
		szSvcid       = "searchYds";
