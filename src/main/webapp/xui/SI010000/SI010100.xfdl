// 탭에 따른 조회조건 visible 처리
function tab_00_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{
	if (tab_00.tabindex == 0 || tab_00.tabindex == 1 || tab_00.tabindex == 2 || tab_00.tabindex == 3 || tab_00.tabindex == 4 || tab_00.tabindex == 5){
		srch_accnt_no.visible = false;
		srch_fcnt.visible = false;
		srch_tcnt.visible = false;
		stc_no.visible = false;
		stc_cnt.visible = false;
		stc_aro.visible = false;
		cal_stt_dt.visible = true;
		cal_end_dt.visible = true;
		stc_dt_dash.visible = true;
		stc_date.visible = true;
	}else if (tab_00.tabindex == 6){
		srch_accnt_no.visible = true;
		srch_fcnt.visible = true;
		srch_tcnt.visible = true;	
		stc_no.visible = true;
		stc_cnt.visible = true;
		stc_aro.visible = true;
		cal_stt_dt.visible = false;
		cal_end_dt.visible = false;	
		stc_dt_dash.visible = false;
		stc_date.visible = false;			
	}else if (tab_00.tabindex == 7 || tab_00.tabindex == 8){
		srch_accnt_no.visible = true;
		srch_fcnt.visible = false;
		srch_tcnt.visible = false;	
		stc_no.visible = true;
		stc_cnt.visible = false;
		stc_aro.visible = false;
		cal_stt_dt.visible = false;
		cal_end_dt.visible = false;	
		stc_dt_dash.visible = false;
		stc_date.visible = false;	
	}
	
	if (tab_00.tabindex == 5){
		sta_TotalCnt.visible = true;
	}else{
		sta_TotalCnt.visible = false;
	}
}

// 선수금 엑셀다운은 별도 처리(엑셀 파일 생성 및 다운로드)
function btn_http_onclick(){

	var date = new Date();
	yyyy = date.getFullYear();
	mm = (date.getMonth() + 1);
	mm = Right("0" + mm,2);
	dd = date.getDate();
	dd = Right("0" + dd,2);
	
	ymd = yyyy + mm + dd;

	var stt_dt = cal_stt_dt.value;
	var end_dt = cal_end_dt.value;

	//obj.enable = false;
	var sUrl = application.services["SVC"].url;
    sUrl = sUrl + "/gongje/new/exceldown?stt_dt="+stt_dt+"&end_dt="+end_dt;
    //alert(sUrl);
    tab_00.pay.img_loading.visible = true;
    
	VirtualFile00.open("C:\\TEMP\\DMLIFE_PAY_"+ymd+".xls", VirtualFile.openWrite );	//내문서로 다운로드 받음. 파일명 지정함.
	var bSucc = HttpObject00.download(sUrl, VirtualFile00 ,true);
}
