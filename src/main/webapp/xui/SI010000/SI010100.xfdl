// 선수금 엑셀 버튼클릭 이벤트
function fn_downloadExcel()
{	
	var objDate = new Date();
	var ymd = objDate.toFormatString("%Y%m%d");
	
	if (ds_gongjepay.rowcount < 1) {
		alert("다운로드할 데이터가 없습니다.");
		return;
	}
	
	var stt_dt = cal_stt_dt.value;
	var end_dt = cal_end_dt.value;
		
	ds_inqCndt.setColumn(0, "stt_dt", stt_dt);
	ds_inqCndt.setColumn(0, "end_dt", end_dt);

	//var cntr_cd = gfn_getUserInfo("cntr_cd");

	//ds_inqCndt.setColumn(0, "dataAthrQury", "");
	var colDS = fn_excelPrepareExcelDS(tab_00.pay.Grid_pay);
	// SI010100
	gfn_excelDown("DMLIFE_PAY_"+ymd, "SI010100", "GongjePayMap.getGongjePayList", "GongjePayMap.getGongjePayCount", ds_inqCndt, true, colDS);
}

// 엑셀 준비 함수
function fn_excelPrepareExcelDS(grdNM:Grid) {
	var colDS:Dataset = null;
	if (this["ds_tempExcelDown"]) {
		colDS = this["ds_tempExcelDown"];
	} else {
		colDS = new Dataset;
		colDS.name = "ds_tempExcelDown";
		this.addChild(colDS.name, colDS);
	}
	
	
	
	//trace(tab_00.pay.Grid_pay.getCellCount("Head"))
	colDS.addColumn("xls_colm_id", "string", 256);
	colDS.addColumn("xls_colm_nm", "string", 256);
	colDS.addColumn("xls_colm_type_cd", "string", 256);
	
	for (var i=0; i< grdNM.getCellCount("Head"); i++) {
		var colId = grdNM.getCellProperty("Body", i, "text")
		if (colId==null || colId.search("expr") > -1)
			continue;
			
		colId = colId.replace("bind:", "");
		colDS.addRow();
		colDS.setColumn(colDS.rowposition, "xls_colm_id", colId);
		colDS.setColumn(colDS.rowposition, "xls_colm_nm", grdNM.getCellProperty("Head", i, "text"));
		colDS.setColumn(colDS.rowposition, "xls_colm_type_cd", "10");
	}
	
	return colDS;
}

//Right 함수
function Right(str, n){ 
    if (n <= 0)
       return "";
    else if (n > String(str).length)
       return str;
    else {
       var iLen = String(str).length;
       return String(str).substring(iLen, iLen - n);
    }
}

