//조회
function fn_commSearch(sType, searchInfo, gbn) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
        case "srchDelayPayCondList": 
	    szSvcid       = "srchDelayPayCondList";
		szController  = "/dlw/cond/delay-pay-list";
		szInDs        = "";
		szOutDs       =  "ds_dpProdAll=ds_outDpProdAll ds_dpProd1Y=ds_outDpProd1Y ds_dpProd2Y=ds_outDpProd2Y ds_dpProd3Y=ds_outDpProd3Y"
                      + " ds_dpProd1=ds_outDpProd1 ds_dpProd2=ds_outDpProd2 ds_dpProd3=ds_outDpProd3 ds_dpProd4=ds_outDpProd4 ds_dpProd5=ds_outDpProd5 ds_dpProd6=ds_outDpProd6"
                      + " ds_dpProd7=ds_outDpProd7 ds_dpProd8=ds_outDpProd8 ds_dpProd9=ds_outDpProd9 ds_dpProd10=ds_outDpProd10 ds_dpProd11=ds_outDpProd11 ds_dpProd12=ds_outDpProd12"
                      + " ds_dpBondAll=ds_outDpBondAll ds_dpBond1Y=ds_outDpBond1Y ds_dpBond2Y=ds_outDpBond2Y ds_dpBond3Y=ds_outDpBond3Y" 
                      + " ds_dpBond1=ds_outDpBond1 ds_dpBond2=ds_outDpBond2 ds_dpBond3=ds_outDpBond3 ds_dpBond4=ds_outDpBond4 ds_dpBond5=ds_outDpBond5 ds_dpBond6=ds_outDpBond6"
                      + " ds_dpBond7=ds_outDpBond7 ds_dpBond8=ds_outDpBond8 ds_dpBond9=ds_outDpBond9 ds_dpBond10=ds_outDpBond10 ds_dpBond11=ds_outDpBond11 ds_dpBond12=ds_outDpBond12"
		szParam       = "bas_dt="+cbo_bas_month.value+"/01";
		szRetCallback = "fn_callBack";
        break;
        case "srchBasMonth": 
	    szSvcid       = "srchBasMonth";
		szController  = "/dlw/cond/bas-month";
		szInDs        = "";
		szOutDs       = "ds_bas_month=ds_output"
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
       
    }
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


