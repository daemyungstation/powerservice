{	
	fn_exportGridToExcel("delayPay_"+gfn_getTodayFull()+"");
}

 function fn_exportGridToExcel(strFileName, strSheetName, arrMultiList)
{    
	var objExport = new ExportObject();
	if(gfn_isNull(strFileName)) strFileName = gfn_getFormId(this) + "_" + gfn_getTodayFull();
    if(gfn_isNull(strSheetName)) strSheetName = "Sheet1";
	
	var sTxtFullPath = gfn_fileSavePath("", strFileName, "EXCEL");
	if(sTxtFullPath.length <= 0) return false;
	strFileName = sTxtFullPath ;
	
	objExport.exporttype       = ExportTypes.EXCEL;
	objExport.activepagename   = strSheetName;	
	objExport.exportfilename   = strFileName;
	objExport.exportopenmode   = "overwrite";
	
	fn_createExcelTitle("grd_title", "■상품구분기준 (전체연체)");
	objExport.addExportItem(ExportItemTypes.GRID, grd_title, strSheetName+"!A1", "", "", "", "onlyvalue"); 
	objExport.addExportItem(ExportItemTypes.GRID, grd_totalList, strSheetName+"!A2" );
	objExport.addExportItem(ExportItemTypes.GRID, grd_bottom1, strSheetName+"!A19", "", "", "","","","","","cellline");
	
	var stt_ln = 22;
	
	for (var i=1; i<=15; i++) {
		var grd_tail = "";
		var grd_cnt = "";
		if (i == 13) {
			grd_tail = "1Y";
			grd_cnt = "13~24회";
		} else if (i == 14) {
			grd_tail = "2Y";
			grd_cnt = "25~36회";
		} else if (i == 15) {
			grd_tail = "3Y";
			grd_cnt = "37회 이상";
		} else {
			grd_tail = i+"";
			grd_cnt = i+"회";
		}
		if (i>1) {
			stt_ln += 20; 
		}
		fn_createExcelTitle("grd_prodTitle"+i, "■"+grd_cnt+" 연체");		
		objExport.addExportItem(ExportItemTypes.GRID, eval("grd_prodTitle"+i), strSheetName+"!A"+(stt_ln-1)+"", "", "", "", "onlyvalue"); 
		objExport.addExportItem(ExportItemTypes.GRID, eval("grd_Prod"+grd_tail), strSheetName+"!A"+stt_ln+"" );
		objExport.addExportItem(ExportItemTypes.GRID, grd_bottom1, strSheetName+"!A"+(stt_ln+17), "", "", "","","","","","cellline");
	}
	
	stt_ln += 20;
	
	fn_createExcelTitle("grd_titleBond", "■채권구분기준 (전체연체)");
	objExport.addExportItem(ExportItemTypes.GRID, grd_titleBond, strSheetName+"!A"+(stt_ln-1)+"", "", "", "", "onlyvalue"); 
	objExport.addExportItem(ExportItemTypes.GRID, grd_delayList, strSheetName+"!A"+stt_ln+"" );
	objExport.addExportItem(ExportItemTypes.GRID, grd_bottom1, strSheetName+"!A"+(stt_ln+5)+"", "", "", "","","","","","cellline");

	stt_ln += 8;
	
	for (var i=1; i<=15; i++) {
		var grd_tail = "";
		var grd_cnt = "";
		if (i == 13) {
			grd_tail = "1Y";
			grd_cnt = "13~24회";
		} else if (i == 14) {
			grd_tail = "2Y";
			grd_cnt = "25~36회";
		} else if (i == 15) {
			grd_tail = "3Y";
			grd_cnt = "37회 이상";
		} else {
			grd_tail = i+"";
			grd_cnt = i+"회";
		}
		if (i>1) {
			stt_ln += 8; 
		}

		fn_createExcelTitle("grd_bondTitle"+i, "■"+grd_cnt+" 연체");		
		objExport.addExportItem(ExportItemTypes.GRID, eval("grd_bondTitle"+i), strSheetName+"!A"+(stt_ln-1)+"", "", "", "", "onlyvalue"); 
		objExport.addExportItem(ExportItemTypes.GRID, eval("grd_Bond"+grd_tail), strSheetName+"!A"+stt_ln+"" );
		objExport.addExportItem(ExportItemTypes.GRID, grd_bottom1, strSheetName+"!A"+(stt_ln+5)+"", "", "", "","","","","","cellline");
	}
	
	objExport.exporteventtype = "itemrecord";
	objExport.exportuitype ="exportprogress";
    objExport.exportmessageprocess = "%d 항목을 출력중입니다.%d/%d";
    objExport.exportsavemode = "save";
	
	objExport.export();
}

