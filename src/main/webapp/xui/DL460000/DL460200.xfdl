/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		return false;
	} else {
		switch (svcid) {			
			case "srchDelayPayCondList":
			//그리드 생성, 바인딩, visible??
			for (var i=1; i<=15; i++) {
				var grd_tail = "";
				if (i == 13) {
					grd_tail = "1Y";
				} else if (i == 14) {
					grd_tail = "2Y";
				} else if (i == 15) {
					grd_tail = "3Y";
				} else {
					grd_tail = i+"";
				}
			    // 상품구분 기준
			    var tmpProdGrid = new Grid("grd_Prod"+grd_tail, 10, 10, 10, 10);	
			    this.addChild("grd_Prod"+grd_tail, tmpProdGrid); 
			    eval("grd_Prod"+grd_tail).formats = grd_ProdFrame.formats;
			    eval("grd_Prod"+grd_tail).binddataset = "ds_dpProd"+grd_tail;
			    eval("grd_Prod"+grd_tail).show()

			    //채권구분 기준
			    var tmpBondGrid = new Grid("grd_Bond"+grd_tail, 10, 10, 10, 10);
			    this.addChild("grd_Bond"+grd_tail, tmpBondGrid); 
			    tmpBondGrid.formats = grd_BondFrame.formats;
				tmpBondGrid.binddataset = "ds_dpBond"+grd_tail;
				tmpBondGrid.show()
			}
			break;
			
			case "srchBasMonth":
				cbo_bas_month.index = 0;
				if (ds_bas_month.rowcount < 1) {
					cbo_bas_month.enable = false;
					btn_Search.enable = false;
					btn_excl.enable = false;
				}
			break;
		}
	}
}


