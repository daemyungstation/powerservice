    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var stt_dt;
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL460604_01_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	stt_dt = p_stt_dt;
	div_monthCal.setDate(stt_dt);
	fn_commSearch("samsungMesFileCheckFile",""); // 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) 
	{
		return false;
	}
	else
	{
		switch (svcid) 
		{
			case "samsungMesFileCheckFile":
				ds_file.filter("");
				if (ds_file.rowcount <= 0) 
				{
					ds_file.clearData();
					ds_file_send.clearData();
					ds_file_recv.clearData();
					alert("검색 결과가 없습니다");	
				} 
				else 
				{
					ds_file.filter("type =='1'");
					ds_file_send.copyData(ds_file,true);
					ds_file.filter("type =='2'");
					ds_file_recv.copyData(ds_file,true);
					ds_file.clearData();
				}
			break;
			
			case "samsungMesFileReflection":
				alert("삼성디지털플라자 시책금 회신파일의 적용이 완료 되었습니다.");
				fn_commSearch("samsungMesFileCheckFile",""); // 조회					
			break;
		}
	}
}
/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//  조회
function fn_commSearch(searchInfo, param) 
{
   	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    stt_dt = div_monthCal.getDate(); // 송신월 
    
    switch(searchInfo) 
    {
		case "samsungMesFileCheckFile" :
			ds_inqCndt.clearData();
			var nRow = ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(nRow, "stt_reg_dt", stt_dt);
			
			szSvcid       = "samsungMesFileCheckFile";
			szController  = "/dlw/sdp/mes/samsungMesFileCheckFile";
			szInDs        = "ds_input=ds_inqCndt";
			szOutDs       = "ds_file=ds_output";
			szParam       = "";
			szRetCallback = "fn_callBack";
		break;
		
		case "samsungMesFileReflection" :
				szSvcid       = "samsungMesFileReflection";
				szController  = "/dlw/sdp/mes/samsungMesFileReflection";
				szInDs        = "";
				szOutDs       = "";
				szParam       = "file_name=" + param;
				szRetCallback = "fn_callBack";
		break;
	}
	
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch("samsungMesFileCheckFile",""); // 조회
}

function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close("1");
}

function grd_fileList01_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
    if (e.cell == "4" && ds_file_recv.getColumn(e.row,"returnval") == "N") 
    {
		fn_reflectionFile();
	}
}

function fn_reflectionFile()
{
	fn_commSearch("samsungMesFileReflection", ds_file_recv.getColumn(ds_file_recv.rowposition, "file_name")) ;
}
]]></Script>
