    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS240101_1_onload(obj:Form, e:LoadEventInfo)
{
	ds_list_tmp.clearData();
	ds_list_tmp.addRow();
	ds_list_tmp.copyRow(0, opener.ds_list_emsh, opener.ds_list_emsh.rowposition);
	//trace(ds_list_tmp.saveXML());
	var basCd = ds_list_tmp.getColumn(0, "exam_dtsc_bas_cd");
	var lvl2 = ds_list_tmp.getColumn(0, "lvl2_scr_vl");
	
	if (basCd == "10"){//균등
		cbo_exam_dtsc.enable = true;
		cbo_lvl1_scr_vl.enable = false;
		cbo_lvl2_scr_vl.enable = false;
		cbo_lvl3_scr_vl.enable = false;
		cbo_exam_dtsc.value = lvl2;
		
	}else{//차등
		cbo_exam_dtsc.enable = false;
		cbo_lvl1_scr_vl.enable = true;
		cbo_lvl2_scr_vl.enable = true;
		cbo_lvl3_scr_vl.enable = true;
		cbo_exam_dtsc.value = "";
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "updateEmsh":
 			opener.fn_commSearch(gfn_getCommTypeInit());
 			break;
 		}	
 	}		
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//저장
function fn_update()
{
	var szSvcid       = "updateEmsh";
	var szController  = "/exam/emsh/save";
	var szInDs        = "ds_input=ds_list_tmp";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 콤보 값 변경
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if (obj.name =="cbo_exam_dtsc_bas_cd"){
		if (obj.value == "10"){//균등
			cbo_exam_dtsc.enable = true;
			cbo_lvl1_scr_vl.enable = false;
			cbo_lvl2_scr_vl.enable = false;
			cbo_lvl3_scr_vl.enable = false;
			cbo_exam_dtsc.value = cbo_lvl2_scr_vl.value;
			cbo_lvl1_scr_vl.value = cbo_lvl2_scr_vl.value;
			cbo_lvl3_scr_vl.value = cbo_lvl2_scr_vl.value;
			
		}else{//차등
			cbo_exam_dtsc.enable = false;
			cbo_lvl1_scr_vl.enable = true;
			cbo_lvl2_scr_vl.enable = true;
			cbo_lvl3_scr_vl.enable = true;
			cbo_exam_dtsc.value = "";
		}
	}else{
		cbo_lvl1_scr_vl.value = obj.value;
		cbo_lvl2_scr_vl.value = obj.value;
		cbo_lvl3_scr_vl.value = obj.value;
	}	
}

/***************************************************************************
 * 함  수  명		: btn_close_onclick()
 * 기      능		: x 버튼 이벤트
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if(gfn_isNull(edit_emsh_nm.value)){
		alert("평가지명을 입력하세요");
		edit_emsh_nm.setFocus();
		return false;
	}
	
	if(gfn_isNull(cbo_exam_dtsc_bas_cd.value)){
		alert("배점기준을 선택하세요");
		return false;
	}
	
	if(cbo_exam_dtsc_bas_cd.value == "10"){//균등
		if(gfn_isNull(cbo_exam_dtsc.value)){
			alert("배점기준을 선택하세요");
			return false;
		}
	}
	
	if(gfn_isNull(cbo_lvl1_scr_vl.value) || gfn_isNull(cbo_lvl2_scr_vl.value) || gfn_isNull(cbo_lvl3_scr_vl.value)){
		alert("배점을 선택하세요");
		return false;
	}
	
	if (!confirm("평가지 수정을 하시겠습니까?")) {
	   return false;
	}
	
	fn_update();
}
