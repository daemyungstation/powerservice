function fn_getPreWinKey(winKey)
{
	var sPrewinKey 	= "";
	
	var nRow = gds_OpenMenu.findRow("WINID", winKey);
	if(nRow != -1 && nRow > 0)
		sPrewinKey = gds_OpenMenu.getColumn(nRow-1, "WINID");
	else if(nRow == 0 && gds_OpenMenu.rowcount > 1)
		sPrewinKey = gds_OpenMenu.getColumn(nRow+1, "WINID");
		
	return sPrewinKey;
}

