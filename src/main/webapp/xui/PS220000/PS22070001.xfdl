/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
//탭페이지 변경시
function tab_dpms_resl_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{
	if(e.postindex ==gfn_getTabPageIndexByTabPageNm(obj, "tabpage1")){ //고객별조회
		fn_commSearch_cmpgCust(gfn_getCommTypeInit());
	}else if(e.postindex ==gfn_getTabPageIndexByTabPageNm(obj, "tabpage2")){
		fn_commSearch_cmpgUser(gfn_getCommTypeInit());
	}else if(e.postindex ==gfn_getTabPageIndexByTabPageNm(obj, "tabpage3")){
		fn_commSearch_cmpgSurveyChoice(gfn_getCommTypeInit());
	}else if(e.postindex ==gfn_getTabPageIndexByTabPageNm(obj, "tabpage4")){
		fn_commSearch_cmpgSurveyAnswer(gfn_getCommTypeInit());
	}else if(e.postindex ==gfn_getTabPageIndexByTabPageNm(obj, "tabpage5")){
		fn_commSearch_cmpgSurveyDetail(gfn_getCommTypeInit());
	}
}

//페이징처리
function tab_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	switch(obj){
		case tab1.grd_list_cmpgCust: //고객별
			fn_commSearch_cmpgCust(gfn_getCommTypeAppend());
		break;
		
		case tab2.grd_list_user :	//상담사별
			fn_commSearch_cmpgUser(gfn_getCommTypeAppend());
		break;
		
		case tab3.grd_list_surveyChoice://설문객관식
			fn_commSearch_cmpgSurveyChoice(gfn_getCommTypeAppend());
		break;
		
		case tab4.grd_list_surveyAnswer : //설문주관식
			fn_commSearch_cmpgSurveyAnswer(gfn_getCommTypeAppend());
		break;
		
		case tab5.grd_list_surveyDetail : //설문상세
			fn_commSearch_cmpgSurveyDetail(gfn_getCommTypeAppend());
		break;
	}
}

//정렬
function tab_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj){
		case tab1.grd_list_cmpgCust:
			if (!fn_preSearch_cmpgCust()) return false;
			ds_inqCndt.setColumn(0, "sController", "/cmpg/dpms-resl/customer");
			ds_inqCndt.setColumn(0, "dsName", "ds_output");
			
			if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_cmpgCust, tab1.sta_totalCnt, 20, total_count)) return;
			gfn_grdHeadClick(obj, e, ds_inqCndt);
			gfn_setTotalRowCount(ds_list_cmpgCust, tab1.sta_totalCnt, total_count); // 페이지건수 표출
		break;
	}
}

