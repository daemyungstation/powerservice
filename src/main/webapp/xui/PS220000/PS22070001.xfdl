//콜백
function fn_callBack(svcid, errcd, errmsg) {
    if (errcd != 0) {
		trace("errcd  :   " +errcd +"   \n errmsg   :   "+ errmsg +"    \n svcid   :   "+ svcid)
		trace(errcd);
		return false;
	} else {
		switch (svcid) {
		    case "srchCmpgCust" ://고객별조회콜백
		    gfn_appendSearchDs(ds_list_cmpgCust, ds_list_tmp_cmpgCust); 
			gfn_setTotalRowCount(ds_list_cmpgCust, tab1.sta_totalCnt, total_count); // 페이지건수 표출
		    break;
		    
		    case "srchCmpgCustTotal" ://고객별총건수조회콜백(엑셀)
			gfn_exportGridToExcel(tab1.grd_list_cmpgCust_total,"고객별_"+gfn_getTodayFull());
		    break;
		    
		    case "srchCmpgUser": //상담삽별 조회
		    gfn_appendSearchDs(ds_list_cmpgUser, ds_list_tmp_cmpgUser); 
			gfn_setTotalRowCount(ds_list_cmpgUser, tab2.sta_totalCnt, total_count); // 페이지건수 표출
		    break;
		    
		    case "srchCmpgUserTotal" ://상담사별별총건수조회콜백(엑셀)
			gfn_exportGridToExcel(tab2.grd_list_user_total,"상담사별_"+gfn_getTodayFull());
		    break;
		    
			case "srchCmpgSurveyChoice"://설문(객관식)
			gfn_appendSearchDs(ds_list_cmpgSurveyChoice, ds_list_tmp_cmpgSurveyChoice); 
			gfn_setTotalRowCount(ds_list_cmpgSurveyChoice, tab3.sta_totalCnt, total_count); // 페이지건수 표출
			break;
			
			case "srchCmpgSurveyChoiceTotal" ://설문(객관식)총건수조회콜백(엑셀)
			gfn_exportGridToExcel(tab3.grd_list_surveyChoice_total,"설문(객관식)_"+gfn_getTodayFull());
		    break;
			
			case "srchCmpgSurveyAnswer"://설문(주관식)
			gfn_appendSearchDs(ds_list_cmpgSurveyAnswer, ds_list_tmp_cmpgSurveyAnswer); 
			gfn_setTotalRowCount(ds_list_cmpgSurveyAnswer, tab4.sta_totalCnt, total_count); // 페이지건수 표출
			break;
			
			case "srchCmpgSurveyAnswerTotal" ://설문(주관식)총건수조회콜백(엑셀)
			gfn_exportGridToExcel(tab4.grd_list_surveyAnswer_total,"설문(주관식)_"+gfn_getTodayFull());
		    break;
			
			case "srchCmpgSurveyDetail"://설문(상세)
			gfn_appendSearchDs(ds_list_cmpgSurveyDetail, ds_list_tmp_cmpgSurveyDetail); 
			gfn_setTotalRowCount(ds_list_cmpgSurveyDetail, tab5.sta_totalCnt, total_count); // 페이지건수 표출
			break;
			
			case "srchCmpgSurveyDetailTotal" ://설문(상세)총건수조회콜백(엑셀)
			gfn_exportGridToExcel(tab5.grd_list_surveyDetail_total,"설문(상세)_"+gfn_getTodayFull());
		    break;
		}
	}
}		   


