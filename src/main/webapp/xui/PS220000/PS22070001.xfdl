{
	switch(obj){
		case tab1.btn_excelDown: //엑셀다운 고객별
			gfn_exportGridToExcel(tab1.grd_list_cmpgCust,"고객별_"+gfn_getTodayFull());
		break;
		
		case tab1.btn_excelDown_total : //전체엑셀다운 고객별
			fn_commSearch_cmpgCust_total(gfn_getCommTypeInit());
		break;
		
		case tab2.btn_excelDown://엑셀다운 상담사별
			gfn_exportGridToExcel(tab2.grd_list_user,"상담사별_"+gfn_getTodayFull());
		break;
		
		case tab2.btn_excelDown_total : //전체엑셀다운 고객별
			fn_commSearch_cmpgUser_total(gfn_getCommTypeInit());
		break;
		
		case tab3.btn_excelDown://엑셀다운 설문(객관식)
			gfn_exportGridToExcel(tab3.grd_list_surveyChoice,"설문(객관식)_"+gfn_getTodayFull());
		break;
		
		case tab3.btn_excelDown_total : //전체엑셀다운 설문(객관식)
			fn_commSearch_cmpgSurveyChoice_total(gfn_getCommTypeInit());
		break;
		
		case tab4.btn_excelDown://엑셀다운 설문(주관식)
			gfn_exportGridToExcel(tab4.grd_list_surveyAnswer,"설문(주관식)_"+gfn_getTodayFull());
		break;
		
		case tab4.btn_excelDown_total : //전체엑셀다운 설문(객관식)
			fn_commSearch_cmpgSurveyAnswer_total(gfn_getCommTypeInit());
		break;
		
		case tab5.btn_excelDown://엑셀다운 설문(상세)
			gfn_exportGridToExcel(tab5.grd_list_surveyDetail,"설문(상세)_"+gfn_getTodayFull());
		break;
		
		case tab5.btn_excelDown_total : //전체엑셀다운 설문(상세)
			fn_commSearch_cmpgSurveyDetail_total(gfn_getCommTypeInit());
		break;
	}
}


