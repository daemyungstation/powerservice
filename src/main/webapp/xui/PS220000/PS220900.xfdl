function fn_callBack(svcid, errcd, errmsg) {
    if (errcd != 0) {
		trace(errcd);
		return false;
	} else {
		switch (svcid) {
		    case "srchHrchCdSet": // 캠페인 유형
				gfn_getHrchCodeLoad("","SELECT",ds_cmpgTypCdList,null,1);
				cbo_srch_cmpg_typ_cd1.index = 3;
				gfn_getHrchCodeLoad("hrch_cd=='3300' ||hrch_cd=='3400'","SELECT",ds_cmpgTypCdList2,null,null,'Y');
				cbo_srch_cmpg_typ_cd2.index = 0;
				cbo_srch_cmpg_typ_cd00.index = 0;
			break;
	
			case "srchExtcTrgt" : //조회콜백
				gfn_setTotalRowCount(ds_list_tmp, sta_totalCnt, total_count); // 페이지건수 표출
				fn_postSearch();
			break;
			
			case "sendMsg":
				alert("발송되었습니다.");
			break;
			
		}
	}
}
