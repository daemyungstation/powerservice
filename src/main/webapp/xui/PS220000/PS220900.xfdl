//버튼클릭이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
		case btn_Search: //조회버튼
		if(cbo_srch_cmpg_typ_cd2.index == 0){
			alert("캠페인 유형을 선택하세요");
			return;
		}else{
			if(fn_preSrch()){
				fn_commSearch(gfn_getCommTypeInit()); // 조회
			}
		}
		break;
		
		case btn_Init : //초기화버튼
			fn_init();
		break;
		
		case btn_excelDw : //엑셀다운로드
			if(ds_list_tmp.rowcount >= 1){
				var colDS = gfn_excelPrepareExcelDS("grd_list_extc")
				gfn_excelDown("추출고객 대상리스트(GRID)", "XLS_DLCC", "UnpyMngeMap.getSmsUnpy", "UnpyMngeMap.getSmsUnpyCount", ds_inqCndt, true, colDS);
			}else{
				alert('조회된 대상고객이 없습니다.');
				return;
			}
		break;
		
		case btn_excelDw2 : //SMS 형식 엑셀 다운로드 
			if(ds_list_tmp.rowcount >= 1){
				gfn_exportGridToExcel(grd_list_extc_excel,"추출고객 대상리스트(엑셀)_"+gfn_getTodayFull());
			}else{
				alert('조회된 대상고객이 없습니다.');
				return;
			}
		break;
		
		case btn_Init_sms : // 보내기 초기화 
			TextArea_chat_msg_cntn.value = null;
			cbo_srch_cmpg_typ_cd00.index = 0;
			cbo_typ_rprs.index = 0;
			edt_cntr_rprs_tlno.value = null;
			edt_basi_dt.value = null;
			edt_add_dt.value = null;
			chk_rightfg.value = true;
			msk_resr_dt.value = "";
			cbo_resr_tm.index = 0;
			msk_resr_dt.enable = false;
			cbo_resr_tm.enable = false;
			sta_chat_msg_size.text = "0";
			edt_add_dt.enable = false;
		break;
		
		case btn_fileDown: //양식받
			fdl_btn_fixedFormDown.downloadfilename = "smsall_sample.xlsx";			
			fdl_btn_fixedFormDown.downloadurl = fv_svcPath +"/file/download-template?file_nm=smsall_sample.xlsx";
			var bSucc = fdl_btn_fixedFormDown.download();
			if(bSucc){
				alert('다운로드 완료');
			}
			return false;//기본으로 뜨는 파일 다이얼로그를 띄우지 않도록 처리함.
		break;
		
		case btn_excelImport: //엑셀 업로드
			var varRet = FileDialog00.open("FileOpen", FileDialog.LOAD, "%DOCUMENT%");
			var objExtComm = new ExtCommon(); // ExtCommon 오브젝트 생성
			
			var v_Ret = objExtComm.excelImportByIndex(varRet.fullpath, 0/*시트INDEX*/, "ds_list_tmp2", 0);
			
			ds_list_tmp2.deleteRow(0);//헤더삭제
			
			if(ds_list_tmp2.rowcount > 0){
				ds_list_excel.clearData();
				ds_list_excel_tmp.clearData();
			}
		
			for(i=0; i<ds_list_tmp2.rowcount; i++){
				var nAddRow = ds_list_excel_tmp.addRow();
				var sClphNo = gfn_Trim(ds_list_tmp2.getColumn(i, "Col03"));
				
				if(gfn_isNull(sClphNo)){
					ds_list_excel.clearData();
					alert("전화번호가 없는 행이 있습니다. \n"+ (i+2) +"행");
					return;
				}
				
				sClphNo = sClphNo.replace("-","");
				ds_list_excel_tmp.setColumn(nAddRow , "cust_id", ds_list_tmp2.getColumn(i, "Col01"));
				ds_list_excel_tmp.setColumn(nAddRow , "cust_nm", ds_list_tmp2.getColumn(i, "Col02"));
				ds_list_excel_tmp.setColumn(nAddRow , "clph_no", gfn_Trim(sClphNo));
				ds_list_excel_tmp.setColumn(nAddRow , "rowCheck", '1');
			}
			
			var sExpr = "rowidx==findRowExpr(\"clph_no=='\" + clph_no + \"'\")";	
			ds_list_excel_tmp.filter(sExpr);
			ds_list_excel.copyData(ds_list_excel_tmp, true);
			ds_list_excel_tmp.filter("");
		break;
		
		case btn_Init_sms_list : // 엑셀업로드 초기화 
			if(ds_list_excel.rowcount > 0){
				if(confirm("초기화 하시겠습니까?")){
					ds_list_excel.clearData();
				}
			}
		break;
		
		case btn_Send: //보내기버튼 
			ds_inqSend.clearData();
			ds_inqSend.addRow();
			if (fn_checkBefore() == false) {
				return false;
			}
			if(ds_list_excel.getColCount == 1){
				ds_inqSend.setColumn(0, "broadcast_yn", "N");//동보발송유무
			}else{
				ds_inqSend.setColumn(0, "broadcast_yn", "Y");//동보발송유무
			}
			
			fn_send();
		break;
		
		case btn_Send_new: //보내기버튼(new) 
			
			ds_inqSend.clearData();
			ds_inqSend.addRow();
			if (fn_checkBefore() == false) {
				return false;
			}
			if(ds_list_excel.getColCount == 1){
				ds_inqSend.setColumn(0, "broadcast_yn", "N");//동보발송유무
			}else{
				ds_inqSend.setColumn(0, "broadcast_yn", "Y");//동보발송유무
			}
			
			fn_send_new();
		break;
	}
}



