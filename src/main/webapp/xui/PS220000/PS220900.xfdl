//조회전 처리 조회조건셋팅
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		ds_inqCndt.clearData();
	
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "cmpg_typ_cd", cbo_srch_cmpg_typ_cd1.value);
		ds_inqCndt.setColumn(0, "cmpg_typ_cd2", cbo_srch_cmpg_typ_cd2.value);
		ds_inqCndt.setColumn(0, "pay_mthd", cbo_srch_pay_mthd.value);
		ds_inqCndt.setColumn(0, "ichae_dt", edt_ichae_dt.value);		
		ds_inqCndt.setColumn(0, "stt_month_cnt", edt_stt_month_cnt.value);
		ds_inqCndt.setColumn(0, "end_month_cnt",  edt_end_month_cnt.value);
		ds_inqCndt.setColumn(0, "stt_true_cnt", edt_stt_true_cnt.value);
		ds_inqCndt.setColumn(0, "end_true_cnt",  edt_end_true_cnt.value);
	
		if(cbo_srch_cmpg_typ_cd2.value == '3300'){// 당월미납
			ds_inqCndt.setColumn(0, "extc_yn", 'N');
			//ds_inqCndt.setColumn(0, "data_dt", gfn_addDate(cal_srch_vasidt.value,1));
		}else if(cbo_srch_cmpg_typ_cd2.value == '3400'){// 1회 미납 
			ds_inqCndt.setColumn(0, "extc_yn", 'N');
		}
	}
	return true;
}

function fn_preSrch(){
	if(!gfn_isNull(edt_stt_month_cnt.value)){
		if(gfn_isNull(edt_end_month_cnt.value)){
			alert("당월회차를 입력해 주세요.");
			return false;
		}
	}
	
	if(!gfn_isNull(edt_end_month_cnt.value)){
		if(gfn_isNull(edt_stt_month_cnt.value)){
			alert("당월회차를 입력해 주세요.");
			return false;
		}
	}
	
	if(edt_stt_month_cnt.value > edt_end_month_cnt.value){
		alert("당월회차를 확인해 주세요.");
		return false;
	}
	
	
	if(!gfn_isNull(edt_stt_true_cnt.value)){
		if(gfn_isNull(edt_end_true_cnt.value)){
			alert("실납입회차를 입력해 주세요.");
			return false;
		}
	}
	
	if(!gfn_isNull(edt_end_true_cnt.value)){
		if(gfn_isNull(edt_stt_true_cnt.value)){
			alert("실납입회차를 입력해 주세요.");
			return false;
		}
	}
	
	if(edt_stt_true_cnt.value > edt_end_true_cnt.value){
		alert("실납입회차를 확인해 주세요.");
		return false;
	}
 
    return true;
}


//발송전 체크
function fn_checkBefore()
{
	if(gfn_isNull(TextArea_chat_msg_cntn.value)){
		alert("메시지 내용을 입력하세요");
		TextArea_chat_msg_cntn.setFocus();
		return false;
	}
	
	
	if(gfn_isNull(cbo_typ_rprs.value)){
		alert("발신팀을 선택하세요");
		return false;
	}
	
	if(gfn_isNull(edt_cntr_rprs_tlno.value)){
		alert("발신번호를 입력하세요");
		edt_cntr_rprs_tlno.setFocus();
		return false;
	}
	
	if(cbo_srch_cmpg_typ_cd00.index == 1){ //당월미납
		if(gfn_isNull(edt_add_dt.value)){ //추가 이체이
			alert("추가 이체일을 입력하세요");
			edt_add_dt.setFocus();
			return false;
		}
	}
	
	if(chk_rightfg.value == false){//예약발송일때
		if(gfn_isNull(msk_resr_dt.value)){
			alert("예약일자를 입력하세요");
			msk_resr_dt.setFocus();
			return false;
		}
		if(gfn_isNull(cbo_resr_tm.value)){
			alert("예약시간을 선택하세요");
			cbo_resr_tm.setFocus();
			return false;
		}
	}
	
	
	if(ds_list_excel.rowcount == 0){
		alert("일괄 발송할 목록을 업로드하세요")
		return false;	
	}
	
	
	if (confirm("문자메시지를 발송하시겠습니까?")) {
		return true;
	}else{
		return false;
	}
}


