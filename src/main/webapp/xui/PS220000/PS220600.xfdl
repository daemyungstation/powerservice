//버튼클릭 이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
		case btn_search: //처리목록 조회버튼
			if(cbo_srch_cmpg_id_list.index  == 0 ){
				alert("캠페인을 선택하세요");
				return;
			}else if(gfn_isNull(edt_srch_sub_trgt_list_id.value)){
				if(chk_total_fg.value != 'Y'){ //전체건수가 아닐때 
					alert("서브대상고객을 선택하세요");
					return;
				}
			}
			fn_commSearch_trgtCustAlct(gfn_getCommTypeInit());//처리목록 조회
		break;
		
		case btn_init: //처리목록 초기화버튼 
			fn_init_trgtCustAlct();
		break;
		
		case btn_cmpgDetailView : //캠페인상세정보 버튼
			var sPrmr = {cmpg_id:cbo_srch_cmpg_id_list.value};
			gfn_openCmpgInfoPop(sPrmr);//캠페인상세 오픈
		break;
		
		case btn_refresh : //새로고침 버튼 클릭 
			btn_cmpgDetailView.visible = false;
			fn_commSearch(gfn_getCommTypeInit());
			ds_list_subTrgtList.clearData();
			fn_setAssignNum();//캠페인 모니터링 변경
			
			sta_totalCnt.text ="검색결과";
			ds_list_trgtCustAlct.clearData();
			ds_trgtCustAlct.clearData();
			fn_changeComponent("R");
		break;
		
		case btn_openCampResultHist: //발신이력보기
			var trgt_cust_dtpt_id = ds_list_trgtCustAlct.getColumn(ds_list_trgtCustAlct.rowposition ,'trgt_cust_dtpt_id');
			var sub_trgt_list_id = ds_list_trgtCustAlct.getColumn(ds_list_trgtCustAlct.rowposition,'sub_trgt_list_id');
			
			var sPrmr = {cmpg_id:cbo_srch_cmpg_id_list.value,trgt_cust_dtpt_id:trgt_cust_dtpt_id,sub_trgt_list_id:sub_trgt_list_id};
			gfn_diaLog("발신이력", -1, -1, 719, 388, "PS220000::PS220601.xfdl", false, sPrmr, false, true, true, null ,null ,true);
		break;
		
		case btn_update: //수정버튼
			fn_changeComponent("U");
		break;
		
		case btn_save: //저장버튼
			fn_saveData();
		break;
		
		case btn_cancel: //취소버튼
			fn_copyRow();
			fn_changeComponent("R");
		break;
		
		case btn_subTrgtSrch : //서브대상고객 선택팝업
			var nRowIdx = 0;
			nRowIdx = ds_cmpgInfo.findRow('cmpg_id',cbo_srch_cmpg_id_list.value);
			var sPrmr = {trgt_list_id:ds_cmpgInfo.getColumn(nRowIdx , 'trgt_list_id'),page_url:"PS220600"};
		    gfn_diaLog("서브타겟리스트 선택", -1, -1, 719, 388, "PS220000::PS220101_01.xfdl", false, sPrmr, false, true, true, null ,null ,true);
		break;
		
		case btn_excelDw : //엑셀 다운로드 
			 if(ds_list_trgtCustAlct.rowcount <1){
				alert("조회 건 수가 없습니다.");
				return;
			 }else{
			 var colDS = gfn_excelPrepareExcelDS("grd_list")
			 
				var extc_typ_cd = ds_list_subTrgtList.getColumn(0,'trgt_list_extc_typ_cd');
    
				if(ds_cmpgIdCd.getColumn(ds_cmpgIdCd.findRow("cmpg_id", cbo_srch_cmpg_id_list.value), "extc_typ_yn") == 'Y' || extc_typ_cd == '30') {
					gfn_excelDown("캠페인_", "XLS_DLCC", "CmpgTelHandleMap.getNewTypeTrgtCustTelHandleList", "CmpgTelHandleMap.getNewTypeTrgtCustTelHandleCount", ds_inqCndt_trgtCustAlct, true, colDS);
				} else if (cbo_srch_cmpg_id_list.value == 'CMP202007294809448' || extc_typ_cd == '40'){
					gfn_excelDown("캠페인_", "XLS_DLCC", "CmpgTelHandleMap.getDirectTrgtCustTelHandleList", "CmpgTelHandleMap.getDirectTrgtCustTelHandleCount", ds_inqCndt_trgtCustAlct, true, colDS);
				} else if (cbo_srch_cmpg_id_list.value == 'CMP202108301483542' || extc_typ_cd == '50'){
					gfn_excelDown("캠페인_", "XLS_DLCC", "CmpgTelHandleMap.getUplusTrgtCustTelHandleList", "CmpgTelHandleMap.getUplusTrgtCustTelHandleCount", ds_inqCndt_trgtCustAlct, true, colDS);
				} else {
					gfn_excelDown("캠페인_", "XLS_DLCC", "CmpgTelHandleMap.getTrgtCustTelHandleList", "CmpgTelHandleMap.getTrgtCustTelHandleCount", ds_inqCndt_trgtCustAlct, true, colDS);
				}
				//gfn_exportGridToExcel(grd_list,"캠페인_"+gfn_getTodayFull());
			 }
		break;
	}
}



