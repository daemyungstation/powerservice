//조회
function fn_commSearch(sType)
{	
	ds_cmpgInfo.keystring = "";
	
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "cmpg_prgr_stat_cd", "20");
	//정훈 소스변경 업체간 검색조건 변경
	var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
	if(sUserAthrCd.substr(0,2)=='TM'){
		ds_inqCndt.setColumn(0, "cmpg_prfm_team_cd", gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd'));
	}
	
	var szSvcid       = "srchCmpg";
    var szController  = "/cmpg/tel-handle/total/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_cmpgInfo=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,false);
}

//처리목록 조회
function fn_commSearch_trgtCustAlct(sType)
{	
    if (!fn_preSearch_trgtCustAlct(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_trgtCustAlct, sta_totalCnt, 500, total_count)) return; 
	
	var szSvcid       = "";
    var szController  = "";
    var extc_typ_cd = ds_list_subTrgtList.getColumn(0,'trgt_list_extc_typ_cd');
    trace('---------------'+cbo_srch_cmpg_id_list.value)
    if(ds_cmpgIdCd.getColumn(ds_cmpgIdCd.findRow("cmpg_id", cbo_srch_cmpg_id_list.value), "extc_typ_yn") == 'Y' || extc_typ_cd == '30') {
		szSvcid       = "srchNewTypeTrgtCustAlct";
        szController  = "/cmpg/tel-handle/newtypelist";
	} else if (cbo_srch_cmpg_id_list.value == 'CMP202007294809448' || extc_typ_cd == '40'){
		szSvcid       = "srchDirectTrgtCustAlct";
        szController  = "/cmpg/tel-handle/directlist";
	} else if (cbo_srch_cmpg_id_list.value == 'CMP202108301483542' || extc_typ_cd == '50'){
		szSvcid       = "srchDirectTrgtCustAlct";
        szController  = "/cmpg/tel-handle/upluslist";
	} else {

		szSvcid       = "srchTrgtCustAlct";
        szController  = "/cmpg/tel-handle/list";
	}
	
// 	var szSvcid       = "srchTrgtCustAlct";
//     var szController  = "/cmpg/tel-handle/list";
    var szInDs        = "ds_input=ds_inqCndt_trgtCustAlct";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

//처리목록 수정 저장
function fn_saveData(){
 if(!fn_preSaveData()) return false;
	ds_trgtCustAlct.setColumn(ds_trgtCustAlct.rowposition, 'call_stts', cbo_dpms_resl_cd.text);
	var szSvcid       = "saveTrgtCustAlct";
    var szController  = "/cmpg/tel-handle/update";
    var szInDs        = "ds_input=ds_trgtCustAlct";
    var szOutDs       = "ds_trgtCustAlct=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
