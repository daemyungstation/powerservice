//조회 결과 
function fn_postSearch()
{
	if(ds_cmpgInfo.rowcount < 1) {
		trace("조회 결과가 없습니다.");
 	}
 	
	if (oParameter.cmpg_id != null) {
		fn_commSearch_trgtCustAlct("param");
	}
	return;
}

//처리목록 조회 결과 
function fn_postSearch_trgtCustAlct()
{
	if(ds_list_trgtCustAlct.rowcount < 1) {
		fn_copyRow();
		fn_changeComponent("R");
 	} else {
		fn_copyRow();
		fn_changeComponent("R");
	}
	return;
}

//상태에따른 component 제어
function  fn_changeComponent(sMode){
	switch(sMode){
		case "R" :
			if(ds_list_trgtCustAlct.rowcount < 1){
				btn_openCampResultHist.enable = false;
				btn_update.enable = false;
				btn_alct.enable = false;
				cbo_dpms_stat_cd.visible = false;
				cbo_dpms_resl_cd.visible = false;
			}else{
				if(ds_list_trgtCustAlct.getColumn(ds_list_trgtCustAlct.rowposition,'dpms_stat_nm')==null){
					btn_openCampResultHist.enable = false;
					btn_update.enable = false;
					btn_alct.enable = true;
				}else{
					btn_openCampResultHist.enable = true;
					btn_update.enable = true;
					btn_alct.enable = false;
				}
				cbo_dpms_stat_cd.visible = true;
				cbo_dpms_resl_cd.visible = true;
			}
			btn_openCampResultHist.visible = true;
			btn_update.visible = true;
			
			
			btn_save.visible = false;
			btn_cancel.visible = false;
			
			cbo_dpms_stat_cd.readonly = true;
			cbo_dpms_resl_cd.readonly = true;
			tea_dpms_resl_cntn.readonly = true;
			
			
		break;
		
		case "U" :
			btn_save.visible = true;
			btn_cancel.visible = true;
			
			btn_openCampResultHist.visible = false;
			btn_update.visible = false;
			
			btn_save.move(btn_save.position.left,btn_update.position.top);
			btn_cancel.move(btn_cancel.position.left,btn_update.position.top);
			
			cbo_dpms_stat_cd.visible = true;
			cbo_dpms_resl_cd.visible = true;
			
			cbo_dpms_stat_cd.readonly = false;
			cbo_dpms_resl_cd.readonly = false;
			if(cbo_dpms_stat_cd.value ==null || cbo_dpms_stat_cd.value ==""){
				cbo_dpms_stat_cd.index = 0;
			}
			
			if(cbo_dpms_resl_cd.value ==null || cbo_dpms_resl_cd.value ==""){
				cbo_dpms_resl_cd.index = 0;
			}
			
			tea_dpms_resl_cntn.readonly = false;
			
		break;
	}
}

//리스트 데이터셋 뷰 데이터셋에 카피
function fn_copyRow(){
	if(ds_list_trgtCustAlct.rowcount<1){
		ds_trgtCustAlct.clearData();
		ds_trgtCustAlct.addRow();
	}else{
		ds_trgtCustAlct.clearData();
		ds_trgtCustAlct.addRow();
		ds_trgtCustAlct.copyRow(ds_trgtCustAlct.rowposition,ds_list_trgtCustAlct,ds_list_trgtCustAlct.rowposition);
		
		//최종처리일시 
 		var rgsnDttmStr = edt_dpms_dttm.value;
 		if(rgsnDttmStr != null && rgsnDttmStr != "" ){
			edt_dpms_dttm.value = gfn_strDttm(rgsnDttmStr);
		};
		
		//일반전화
		var homeTelStr =  edt_home_tlno.value;
		if(homeTelStr != null && homeTelStr != "" ){
			edt_home_tlno.value = gfn_phoneNumStr(homeTelStr);
		};
		
		//휴대전화
		var cellStr =  edt_clph_tlno.value;
		if(cellStr != null && cellStr != "" ){
			edt_clph_tlno.value = gfn_phoneNumStr(cellStr);
		};
		
		//회사전화
		var compStr =  edt_comp_tlno.value;
		if(compStr != null && compStr != "" ){
			edt_comp_tlno.value = gfn_phoneNumStr(compStr);
		};
	}
}

//캠페인에따른 정보 (건수) 변경
function fn_setAssignNum(){
	var rowFg =ds_cmpgInfo.findRow('cmpg_id',cbo_srch_cmpg_id_list.value);
	if(rowFg != -1){
		
		if(ds_cmpgInfo.getColumn(rowFg , 'assignnum') != ""){ //할당건수
			edt_assignnum.value = gfn_commaStr(ds_cmpgInfo.getColumn(rowFg,'assignnum'));
		}else{
			edt_assignnum.value = 0;
		}
		
		if(ds_cmpgInfo.getColumn(rowFg , 'st30') != ""){ //처리완료
			edt_st30.value = gfn_commaStr(ds_cmpgInfo.getColumn(rowFg,'st30'));
		}else{
			edt_st30.value = 0;
		}
		
		if(ds_cmpgInfo.getColumn(rowFg , 'st10') != ""){ //미처리
			edt_st10.value = gfn_commaStr(ds_cmpgInfo.getColumn(rowFg,'st10'));
		}else{
			edt_st10.value = 0;
		}
		
		if(ds_cmpgInfo.getColumn(rowFg , 'st20') != ""){ //진행중
			edt_st20.value = gfn_commaStr(ds_cmpgInfo.getColumn(rowFg,'st20'));
		}else{
			edt_st20.value = 0;
		}
		
		if(ds_cmpgInfo.getColumn(rowFg , 'total_rate') != ""){ //처리율
			edt_total_rate.value = ds_cmpgInfo.getColumn(rowFg,'total_rate')+"%";
		}else{
			edt_total_rate.value = 0+"%";
		}
		
		if(ds_cmpgInfo.getColumn(rowFg , 'st50') != ""){ //당일처리
			edt_st50.value = gfn_commaStr(ds_cmpgInfo.getColumn(rowFg,'st50'));
		}else{
			edt_st50.value = 0;
		}
		
		if(ds_cmpgInfo.getColumn(rowFg , 'day_rate') != ""){ //당일처리율
			edt_day_rate.value = ds_cmpgInfo.getColumn(rowFg,'day_rate')+"%";
		}else{
			edt_day_rate.value = 0+"%";
		}
		
		sta_cust_data_extc_cnt.visible = true;
		var custDataExtcCnt = ds_cmpgInfo.getColumn(rowFg,'cust_data_extc_cnt');
		var assignnum = 0;
		sta_cust_data_extc_cnt.text = "<fs v='9'><fc v='#333333ff'><b v='true'>총추출건수 : </fs></fc></b>";
		sta_cust_data_extc_cnt.text +=  "<fs v='9'>"+custDataExtcCnt+"</fs>"; 
		sta_cust_data_extc_cnt.text += "<fs v='9'><fc v='#333333ff'> 건</fs></fc>";
		sta_cust_data_extc_cnt.text += "<fs v='9'><fc v='#333333ff'><b v='true'> 할당건 : </fs></fc></b>";
		
		if(ds_cmpgInfo.getColumn(rowFg , 'assignnum') != ""){ //할당건수
			assignnum =ds_cmpgInfo.getColumn(rowFg,'assignnum');
		}else{
			assignnum = 0;
		}
		sta_cust_data_extc_cnt.text +=  "<fs v='9'>"+assignnum+"</fs>"; 
		sta_cust_data_extc_cnt.text += "<fs v='9'><fc v='#333333ff'> 건</fs></fc>";
		sta_cust_data_extc_cnt.text += "<fs v='9'><fc v='#333333ff'><b v='true'> 미배정 : </fs></fc></b>";
		sta_cust_data_extc_cnt.text +=  "<fs v='9'>"+(parseInt(custDataExtcCnt)-parseInt(assignnum))+"</fs>"; 
		sta_cust_data_extc_cnt.text += "<fs v='9'><fc v='#333333ff'> 건</fs></fc>";
	}else{
		sta_cust_data_extc_cnt.visible = false;
		edt_assignnum.value = "";
		edt_st30.value = "";
		edt_st10.value = "";
		edt_st20.value = "";
		edt_total_rate.value = "";
		edt_st50.value = "";
		edt_day_rate.value = "";
	}
}

function fn_resetSubTrgt(){
	ds_list_subTrgtList.clearData();
	edt_srch_sub_trgt_list_id.value = null;
}
