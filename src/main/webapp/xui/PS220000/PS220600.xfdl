{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.22 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////
	
	chk_alct_yn.value = 'Y';
	gfn_getList("cd","CAM100,CAM090","fn_callBack",null,"srchCdSet","ds_cdSet");   // 코드셋
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정

	oParameter = parent.pv_usrParameter; //프로그램 파라미터 조회
	
	fn_commSearch(gfn_getCommTypeInit());//캠페인조회
	gfn_getList("hrchCd","CAM990","fn_callBack",null,"srchHrchCdSet");   //캠페인유형(계층유형코드)
	fn_setAthrPgmBtn();				// 권한에 따른 버튼 설정
	fn_init_trgtCustAlct();
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		btn_excelDw.visible = true;
	} else {
		btn_excelDw.visible = false;
	}
}


//초기화
function fn_init_trgtCustAlct()
{
	cbo_srch_date_typ.index = 0;
	cbo_srch_dpms_stat_cd.index = 0;
	cbo_srch_dpms_resl_cd.index  = 0;
	cal_srch_startd.value = gfn_addDate(gfn_getToday(),-7);
	cal_srch_endd.value = gfn_getToday();
	edt_srch_cnsr_nm.value = null;
	edt_srch_mem_nm.value = null;
	edt_srch_tlno.value = null;
	chk_alct_yn.value = 'Y';
	
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
//콜백
function fn_callBack(svcid, errcd, errmsg) {
    if (errcd != 0) {
		trace("errcd  :   " +errcd +"   \n errmsg   :   "+ errmsg +"    \n svcid   :   "+ svcid)
		trace(errcd);
		return false;
	} else {
		switch (svcid) {
			case "srchCmpg"://캠페인조회
				ds_cmpgIdCd.copyData(ds_cmpgInfo);
				ds_cmpgIdCd.insertRow(0);
				ds_cmpgIdCd.setColumn(ds_cmpgIdCd.rowposition, "cmpg_nm", "선택");
				if(oldCmpgId == ""){
					cbo_srch_cmpg_id_list.index = 0;
				}else{
					var indexNo = ds_cmpgIdCd.findRow('cmpg_id',oldCmpgId);
					if(indexNo != -1){
						cbo_srch_cmpg_id_list.value = ds_cmpgIdCd.getColumn(indexNo,"cmpg_id");
						fn_setAssignNum();
					}else{
						cbo_srch_cmpg_id_list.value = "";
					}
				}
				fn_postSearch();
			break;
			case "srchHrchCdSet": // 캠페인 유형
                gfn_getHrchCodeLoad("","ALL",ds_srchCmpgTypCdList,null,1);
				cbo_srch_cmpg_type.index = 0;
            break;	
			case "srchCdSet": //코드셋
				gfn_getCode("CAM100", "ALL", ds_dpmsStatCdList_srch,ds_cdSet); //진행상태
				gfn_getCode("CAM090", "ALL", ds_dpmsReslCdList_srch,ds_cdSet); //통화결과
				gfn_getCode("CAM100", "SELECT", ds_dpmsStatCdList,ds_cdSet); //진행상태
				gfn_getCode("CAM090", "SELECT", ds_dpmsReslCdList,ds_cdSet); //통화결과
			   
				cbo_srch_dpms_stat_cd.index = 0;
				cbo_srch_dpms_resl_cd.index = 0;
		    break;
		    
		    case "srchTrgtCustAlct"://처리목록 조회
				gfn_appendSearchDs(ds_list_trgtCustAlct, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_trgtCustAlct, sta_totalCnt, total_count); // 페이지건수 표출
				fn_postSearch_trgtCustAlct();
		    break;
		    
		    case "srchNewTypeTrgtCustAlct"://처리목록 조회
				gfn_appendSearchDs(ds_list_trgtCustAlct, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_trgtCustAlct, sta_totalCnt, total_count); // 페이지건수 표출
				fn_postSearch_trgtCustAlct();
		    break;
		    
		    case "srchDirectTrgtCustAlct"://처리목록 조회
				gfn_appendSearchDs(ds_list_trgtCustAlct, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_trgtCustAlct, sta_totalCnt, total_count); // 페이지건수 표출
				fn_postSearch_trgtCustAlct();
		    break;
		    
		    case "saveTrgtCustAlct"://처리목록 저장
				fn_commSearch_trgtCustAlct(gfn_getCommTypeInit());//처리목록 조회
		    break;
		}		
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch_trgtCustAlct(sType)
{

	switch(sType) {
	case "param":
		cbo_srch_cmpg_id_list.index = ds_cmpgIdCd.findRow("cmpg_id", oParameter.cmpg_id);
		cbo_onitemchanged(cbo_srch_cmpg_id_list);
		cbo_srch_dpms_stat_cd.index = ds_dpmsStatCdList_srch.findRow("cd", oParameter.dpms_stat_cd);
		if (oParameter.cnsr_nm != null) {
			cbo_srchTyp.index = 2;
			edt_srchVal.value = oParameter.cnsr_nm;
		}
		
		if (oParameter.sub_trgt_list_id != null) {
			ds_list_subTrgtList.clearData();
			ds_list_subTrgtList.addRow();
			ds_list_subTrgtList.setColumn(0, "sub_trgt_list_id", oParameter.sub_trgt_list_id);
			ds_list_subTrgtList.setColumn(0, "sub_trgt_list_nm", oParameter.sub_trgt_list_nm);
			edt_srch_sub_trgt_list_id.value = oParameter.sub_trgt_list_nm;
		}
		
	case gfn_getCommTypeInit():
		// 조회조건 및 default 정렬
		ds_list_trgtCustAlct.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt_trgtCustAlct.clearData();
		ds_inqCndt_trgtCustAlct.addRow();
		if(chk_total_fg.value != 'Y'){ //전체건수가 아닐때 
			ds_inqCndt_trgtCustAlct.setColumn(0, "sub_trgt_list_id", ds_list_subTrgtList.getColumn(ds_list_subTrgtList.rowposition,'sub_trgt_list_id'));
		}else{
			ds_inqCndt_trgtCustAlct.setColumn(0, "cmpg_id", cbo_srch_cmpg_id_list.value);
		}
		
		ds_inqCndt_trgtCustAlct.setColumn(0, "alct_yn", chk_alct_yn.value);
		ds_inqCndt_trgtCustAlct.setColumn(0, "dpms_stat_cd", cbo_srch_dpms_stat_cd.value);
		ds_inqCndt_trgtCustAlct.setColumn(0, "dpms_resl_cd", cbo_srch_dpms_resl_cd.value);
		if(cbo_srch_date_typ.index == 0){
			ds_inqCndt_trgtCustAlct.setColumn(0, "startd", cal_srch_startd.value);
			ds_inqCndt_trgtCustAlct.setColumn(0, "endd", cal_srch_endd.value);
		}else{
			ds_inqCndt_trgtCustAlct.setColumn(0, "dpms_startd", cal_srch_startd.value);
			ds_inqCndt_trgtCustAlct.setColumn(0, "dpms_endd", cal_srch_endd.value);
		}
		
		ds_inqCndt_trgtCustAlct.setColumn(0, "cust_nm", edt_srch_mem_nm.value);
		ds_inqCndt_trgtCustAlct.setColumn(0, "total_tlno", edt_srch_tlno.value);
		ds_inqCndt_trgtCustAlct.setColumn(0, "cnsr_nm", edt_srch_cnsr_nm.value);
		ds_inqCndt_trgtCustAlct.setColumn(0, "orderBy", "cust_nm");
		ds_inqCndt_trgtCustAlct.setColumn(0, "orderDirection", "asc");
		break;	
	}
	
	return true;
}

//저장전 처리
function fn_preSaveData(){

	if(gfn_isNull(cbo_dpms_stat_cd.value)){
		alert("진행상태를 선택하세요.");
		return false;
	}
		
	if(gfn_isNull(cbo_dpms_resl_cd.value)){
		alert("통화결과를 선택하세요.");
		return false;
	}
	
	return true;
}
