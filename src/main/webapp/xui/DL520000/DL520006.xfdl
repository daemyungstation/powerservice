		}
		
		//trace(ds_list_cancel.saveXML());
		//return;
		
		
		var day = new Date(); 
		var close_dt =new Date(); 
		var yymmdd = close_dt.toFormatString("%y%m%d");
		//var yymmdd = "20170425";
		var inv_dt = close_dt.toFormatString("%Y%m%d");
		var inv_time = close_dt.toFormatString("%X").replace(":","")+"";
