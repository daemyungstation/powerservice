				gv_chkIUMode = "none";
				
				//fn_initDtlInfoBox();
				//fn_dtlModeBtnCtrl("default");
				//fn_commSearch(gfn_getCommTypeInit(), "getMemberRefundDetail");
			break;
			
			// CMS 입금(취소)반영
			case "updateByCmsRefundProcess":
				alert("입금반영완료");
				
				// 재조회
		        opener.fn_commSearch(gfn_getCommTypeInit(), "getMemberRefundList"); // 조회
		        close();
			break;
			
			// 카드취소
			case "pymnCancelNicePay":
				// 취소 성공시
				if(rslt_cd == "03") {
					alert("취소가 완료되었습니다.");
					
