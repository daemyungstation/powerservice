/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS030600_onload(obj:Form, e:LoadEventInfo)
{
	//////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.04 정승철
	gvMenuId = GV_LOGMENUID;
	//////////////////////////////////////////////////////////////////////////////////////////	
	
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin());
	fn_init("Y");
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		alert(errmsg);
		return false;
	} else {
		switch (svcid) {
			case "srchCmphPsct":
			
			gfn_appendSearchDs(ds_list_cmphPsct, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_cmphPsct, sta_TotalCnt, tc_cmphPsct); // 페이지건수 표출
			
			if (ds_list_cmphPsct.rowcount == 0) {
				alert("조회결과가 없습니다.");
			}
			break;
			
			case "srchCmphPsctExcelList":
			// 최초 한번만 head body binding
			if (grd_list_excel.head.getCellCount() <= 1) {
				grd_list_excel.setCellProperty("head", 0, "text", "NO");
				grd_list_excel.setCellProperty("Body", 0, "text", "expr:currow+1");
				grd_list_excel.setFormatColProperty(0, "size", 40);
				
				var sHeadTxt = ds_list_header_excel.getColumn(0, "dgLbQry").split(",");
				var sBodyId = ds_list_header_excel.getColumn(0, "dfQry").split(",");
				for (var i=0; i<sHeadTxt.length; i++) {
					grd_list_excel.insertContentsCol(i+1);
					grd_list_excel.setCellProperty("head", i+1, "text", sHeadTxt[i]);						// 헤더 설정
					grd_list_excel.setCellProperty("body", i+1, "text", "bind:"+sBodyId[i].toLowerCase());	// bind data
					grd_list_excel.setFormatColProperty(i+1, "size", 100);
				}
			}
			var lvl = ds_list_header_excel.getColumn(0, "lvl");
			var dgLbQry = ds_list_header_excel.getColumn(0, "dgLbQry");
			var dfQry = ds_list_header_excel.getColumn(0, "dfQry");
			var dataAthrQury = ds_list_header_excel.getColumn(0, "dataAthrQury");
			var paramEmpleNo = ds_list_header_excel.getColumn(0, "paramEmpleNo");
			var paramAs = ds_list_header_excel.getColumn(0, "paramAs");
			
			ds_inqCndt.setColumn(0, "lvl" , lvl);
			ds_inqCndt.setColumn(0, "dgLbQry" , dgLbQry);
			ds_inqCndt.setColumn(0, "dfQry" , dfQry);
			ds_inqCndt.setColumn(0, "dataAthrQury" , dataAthrQury);
			ds_inqCndt.setColumn(0, "paramEmpleNo" , paramEmpleNo);
			ds_inqCndt.setColumn(0, "paramAs" , paramAs);
			
			var colDS = gfn_excelPrepareExcelDS("grd_list_excel");
			gfn_excelDown("종합현황", "XLS_DLCC", "DlwConsProdMap.getTotCondListLv"+lvl+"", "DlwConsProdMap.getTotCondLv"+lvl+"Count", ds_inqCndt, true, colDS, true);
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (gfn_isNull(cal_srch_stt_dt.value) || gfn_isNull(cal_srch_end_dt.value)) {
			alert("검색기간을 입력하세요.");
			return false;
		}
	
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		if (gfn_getMonthDiff(cal_srch_stt_dt.value, cal_srch_end_dt.value) > 2) {
			alert("검색기간은 3개월미만입니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "work_mnth", div_monthCal.getDate());
		ds_inqCndt.setColumn(0, "srch_typ", cbo_srch_typ.value);
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "mem_nm", edt_srch_mem_nm.value);
		ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
		ds_inqCndt.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
		ds_inqCndt.setColumn(0, "prod_nm", edt_srch_prod_nm.value);
		
		//ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		//ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

