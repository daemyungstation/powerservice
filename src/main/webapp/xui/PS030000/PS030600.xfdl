// button onclick
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_init") {				// 초기화 버튼
		fn_init("N");
	} else if (obj.name == "btn_accntNo") {		// 회원번호조회 버튼
		fn_srchAccntNoPop();
	} else if (obj.name == "btn_srchProd") {	// 상품조회 버튼
		fn_openProdPopup("btn");
	} else if (obj.name == "btn_search") {		// 검색 버튼
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_excel") {		// 엑셀다운로드 버튼
		fn_srchExcelDownList();
	} else if (obj.name == "btn_regMem") {		// 회원 등록 팝업
		fn_regMemPop();
	}
}

// onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if (obj.name == "edt_srch_accnt_no") {
			fn_srchAccntNoPop();
		} else if (obj.name == "edt_srch_prod_cd") {
			fn_openProdPopup("cd");
		} else if (obj.name == "edt_srch_prod_nm") {
			fn_openProdPopup("nm");
		} else {
			fn_commSearch(gfn_getCommTypeInit());
		}
	}
}

