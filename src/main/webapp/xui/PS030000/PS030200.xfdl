/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS030200_onload(obj:Form, e:LoadEventInfo)
{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.04 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////	
	
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	
	gfn_getList("cd", "CNS080");  // 공통코드조회
	gfn_getList("teamCd","", null, null, null, "ds_teamCd"); //팀코드조회
	//fn_setPropertyAthr(); // 권한그룹별로 속성 설정 
    fn_setAthrPgmBtn(); // 권한에 따른 버튼 설정
    
	fn_init();
	fn_commSearch(gfn_getCommTypeInit());
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
 			gfn_getCode("CNS080", "SELECT", ds_custTypCd); // 고객유형코드
			break;
			
			case "selectTeamCode": // 팀코드 조회
			gfn_getTeamCodeLoad("", "ALL", ds_team1Cd, ds_teamCd, "1");
			break;
			
			case "selectUserCode": // 상담사
			gfn_getUserCdLoad("", "" ,"ALL", ds_actpId);
			
			cbo_srch_actp_id.index = 0;
			if (ds_actpId.rowcount > 1) {
				cbo_srch_actp_id.enable = true;
			} else {
				cbo_srch_actp_id.enable = false;
			}
			break;
			
			case "srchCustMemo":
			gfn_appendSearchDs(ds_list_custUnusMemo, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_custUnusMemo, sta_totalCnt, tc_cust_memo); // 페이지건수 표출
			fn_postSearch();
			break;
			
			case "deleteMemMemo":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "saveMemMemo":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "srchCustMemoExcel": // 엑셀다운
			gfn_exportGridToExcel(grd_list, "특이고객관리_"+gfn_getTodayFull()+"");
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 팀 코드1
function fn_preChngTeam1()
{
	if (gfn_isNull(cbo_srch_team1_cd.value)) {
		cbo_srch_team2_cd.index = "0";
		cbo_srch_team2_cd.enable = false;
		
		cbo_srch_actp_id.index = "0";
		cbo_srch_actp_id.enable = false;
		return false;
	}
	return true;
}

// 팀 코드2
function fn_preChngTeam2()
{
	if (gfn_isNull(cbo_srch_team2_cd.value)) {
		cbo_srch_actp_id.index = "0";
		cbo_srch_actp_id.enable = false;
		return false;
	}
	
	ds_inqCndt_actpId.clearData();
	ds_inqCndt_actpId.addRow();
	ds_inqCndt_actpId.setColumn(0, "team_cd", cbo_srch_team2_cd.value);
	ds_inqCndt_actpId.setColumn(0, "use_yn", "Y");
	ds_inqCndt_actpId.setColumn(0, "user_stat_cd", "10");
	//user 소팅 변경
	//수정자 정훈연구원
	//요청자 정성안팀장
	//문의사항있을시 요청자에게 문의하세요
	ds_inqCndt_actpId.setColumn(0, "orderBy", "user_nm");
	ds_inqCndt_actpId.setColumn(0, "orderDirection", "ASC");
	
	return true;
}

// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_custUnusMemo.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "mem_nm", edt_srch_mem_nm.value);
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "open_yn", cbo_srch_open_yn.value);
		
		// 권한이 TM 일 경우 상담그룹 조회 조건 설정
		var sAthrCd = gds_UserInfo.getColumn(0, "athr_cd");
		if (sAthrCd.indexOf("TM") >= 0) {
			ds_inqCndt.setColumn(0, "team1_cd", "");
			ds_inqCndt.setColumn(0, "team2_cd", "");
			ds_inqCndt.setColumn(0, "bzpt_div_yn", "Y");
		} else {
			ds_inqCndt.setColumn(0, "team1_cd", cbo_srch_team1_cd.value);
			ds_inqCndt.setColumn(0, "team2_cd", cbo_srch_team2_cd.value);
			ds_inqCndt.setColumn(0, "bzpt_div_yn", "N");
		}
		
		ds_inqCndt.setColumn(0, "actp_id", cbo_srch_actp_id.value);
		ds_inqCndt.setColumn(0, "mine_flag", chk_srch_mine_flag.value);
		ds_inqCndt.setColumn(0, "cust_typ_cd", cbo_srch_cust_typ_cd.value);
		
		ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

// 삭제
function fn_preDelete()
{
	if (gfn_isNull(ds_custUnusMemo.getColumn(0, "cust_unus_memo_id"))) {
		alert("삭제하려는 고객특이메모가 없습니다.");
		return false;
	}
	return true;
}

// 저장
function fn_preSave()
{
	if (gfn_isNull(cbo_cust_typ.value)) {
		alert("고객유형을 선택하세요");
		cbo_cust_typ.setFocus();
		return false;
	}
	
	if (gfn_isNull(txt_cust_unus_memo_cntn.value)) {
		alert("고객특이메모를 입력하세요");
		txt_cust_unus_memo_cntn.setFocus();
		return false;
	}
	
	return true;
}

function fn_preSrchMem()
{
	if (gfn_isNull(ds_custUnusMemo.getColumn(0, "mem_no"))) {
		alert("회원정보가 없습니다.");
		return false;
	}
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원메모조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_custUnusMemo, sta_totalCnt, 25, tc_cust_memo)) return;
	
	var szSvcid       = "srchCustMemo";
    var szController  = "/cons/cust-unus-memo/list/admin";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

// 삭제
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	if (!fn_preDelete()) return false;

	if (confirm("해당 특이고객정보를 삭제하시겠습니까?")) {
		var szSvcid       = "deleteMemMemo";
		var szController  = "/cons/cust-unus-memo/delete";
		var szInDs        = "";
		var szOutDs       = "";
		var szParam       = "cust_unus_memo_id=" + ds_custUnusMemo.getColumn(0, "cust_unus_memo_id");
		var szRetCallback = "fn_callBack";
		
		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
	}
}

// 저장
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if (!fn_preSave()) return false;
	
	var szSvcid       = "saveMemMemo";
    var szController  = "/cons/cust-unus-memo/save";
    var szInDs        = "ds_input=ds_custUnusMemo";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

