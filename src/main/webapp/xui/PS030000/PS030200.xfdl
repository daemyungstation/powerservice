function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_resetColsize(grd_list);
}

// 검색부 콤보 변경
function combo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if (obj == cbo_srch_team1_cd) {
		fn_changeTeam1Cd();
	} else if (obj == cbo_srch_team2_cd) {
		fn_changeTeam2Cd();
	} else if (obj == cbo_srch_dateTyp) {
		fn_changeDateType();
	}
}

// 취소
function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_list_custUnusMemo.rowcount < 1) {
		ds_custUnusMemo.clearData();
		fn_setBtn("E");
	} else {
		fn_setCustMemo();
		fn_setBtn("UD");
		fn_setEnable(false);
	}
}

// 수정
function btn_update_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_setBtn("SC");
	fn_setEnable(true);
	dtpt_txt.text = "특이고객 수정";
}

// 조회
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit());
}

// 초기화
function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

// 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/cons/cust-unus-memo/list/admin"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_custUnusMemo, sta_totalCnt, 25, tc_cust_memo)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_custUnusMemo, sta_totalCnt, tc_cust_memo); // 페이지건수 표출
	
	fn_setCustMemo();
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

// onkeydown
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		fn_commSearch(gfn_getCommTypeInit());
	}
}

// 상세보기
function btn_memo_dtpt_onclick(obj:Button,  e:ClickEventInfo)
{
	if (!fn_preSrchMem()) return false;
	
	var sPrmr = {p_mem_no:ds_custUnusMemo.getColumn(0, "mem_no")};
	var reVal = gfn_diaLog("회원상세", -1, -1, -1, -1, "PS030000::PS030201.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 그리드 셀 클릭시
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	fn_setCustMemo();
}

