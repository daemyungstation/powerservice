/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	cbo_srch_dateTyp.index = 4;
	cal_srch_stt_dt.value = "";
	cal_srch_end_dt.value = "";
	cbo_srch_open_yn.index = 0;
	cbo_srch_team1_cd.index = 0;
	
	cbo_srch_cust_typ_cd.index = 0;
	cbo_cust_typ.index = 0;
	cbo_cust_typ.enable = false;
	
	edt_srch_mem_nm.value = "";
	cbo_srch_team2_cd.index = 0;
	cbo_srch_team2_cd.enable = false;
	cbo_srch_actp_id.index = 0;
	cbo_srch_actp_id.enable = false;
	chk_srch_mine_flag.value = false;
	
	fn_changeDateType();
	
	// 권한이 TM 일 경우 상담그룹 조회 조건 설정
	var sAthrCd = gds_UserInfo.getColumn(0, "athr_cd");
	if (sAthrCd.indexOf("TM") >= 0) {
		var sHgrnTeamCd = gds_UserInfo.getColumn(0, "hgrn_team_cd");
		var sTeamCd = gds_UserInfo.getColumn(0, "team_cd");
			
		cbo_srch_team1_cd.value = sHgrnTeamCd;
		fn_changeTeam1Cd();
		cbo_srch_team2_cd.value = sTeamCd;
		fn_changeTeam2Cd();
		
		cbo_srch_team1_cd.enable = false;
		cbo_srch_team2_cd.enable = false;
	}
}

// 날짜 선택
function fn_changeDateType()
{
	gfn_dateSrchDefaultChangeEvnt(cbo_srch_dateTyp, cal_srch_stt_dt, cal_srch_end_dt);
}

// 조회결과
function fn_postSearch()
{
	if (ds_list_custUnusMemo.rowcount < 1) {
		ds_custUnusMemo.clearData();
		fn_setBtn("E");
		mae_rgsn_dttm.mask = "";
		mae_amnt_dttm.mask = "";
	} else {
		fn_setCustMemo();
		//fn_setBtn("UD");
		fn_setEnable(false);
		
		mae_rgsn_dttm.mask = "####-##-## ##:##";
		mae_amnt_dttm.mask = "####-##-## ##:##";
	}
}

// 버튼 제어
function fn_setBtn(sTyp)
{
	if (sTyp == "UD") {
		btn_save.visible = false;
		btn_cancel.visible = false;
		
		if (amntYn == "Y" && dltnYn == "Y") {
			btn_update.position2 = "absolute r:74 w:72 b:89 h:20";
			btn_delete.position2 = "absolute r:0 w:72 b:89 h:20";
			btn_update.visible = true;
			btn_delete.visible = true;
		} else if (amntYn == "Y") {		// 수정 권한
			btn_update.position2 = "absolute r:0 w:72 b:89 h:20";
			btn_update.visible = true;
			btn_delete.visible = false;
		} else if (dltnYn == "Y") {		// 삭제 권한
			btn_delete.position2 = "absolute r:0 w:72 b:89 h:20";
			btn_update.visible = false;
			btn_delete.visible = true;
		}
		btn_memo_dtpt.visible = true;
	} else if (sTyp == "SC") {
		btn_update.visible = false;
		btn_delete.visible = false;
		
		btn_save.position2 = "absolute r:74 w:72 b:89 h:20";
		btn_cancel.position2 = "absolute r:0 w:72 b:89 h:20";
		btn_save.visible = true;
		btn_cancel.visible = true;
		btn_memo_dtpt.visible = true;
	} else if (sTyp == "E") {
		btn_update.visible = false;
		btn_delete.visible = false;
		btn_save.visible = false;
		btn_cancel.visible = false;
		btn_memo_dtpt.visible = false;
	}
}

// enable 제어
function fn_setEnable(sType)
{
	if (sType) {
		rdo_open_yn.enable = true;
		txt_cust_unus_memo_cntn.readonly = false;
		cbo_cust_typ.enable = true;
	} else {
		rdo_open_yn.enable = false;
		txt_cust_unus_memo_cntn.readonly = true;
		cbo_cust_typ.enable = false;
	}
}

// 회원메모설정
function fn_setCustMemo()
{
	ds_custUnusMemo.clearData();
	ds_custUnusMemo.addRow();
	ds_custUnusMemo.copyRow(0, ds_list_custUnusMemo, ds_list_custUnusMemo.rowposition);
	
	fn_setEnable(false);
	fn_setBtn("UD");
	dtpt_txt.text = "특이고객 보기";
}

// 팀 코드1 변경
function fn_changeTeam1Cd()
{
	if (!fn_preChngTeam1()) return false;
	
	gfn_getTeamCodeLoad(cbo_srch_team1_cd.value, "ALL", ds_team2Cd, ds_teamCd);
	cbo_srch_team2_cd.index = "0";
	cbo_srch_team2_cd.enable = true;
	
	cbo_srch_actp_id.index = "0";
	cbo_srch_actp_id.enable = false;
}

// 팀 코드2 변경
function fn_changeTeam2Cd()
{
	if (!fn_preChngTeam2()) return false;
	
	gfn_getList("userCd", "", null, null, null, null, "ds_inqCndt_actpId"); //상담사조회
}

