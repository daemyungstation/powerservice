    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";
var nTotalListCnt = 0; // 전체건수

var gvMenuId;  // 프로그램 로그 추가 2017.12.14
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/

function PS031500_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); 		// 페이징처리 위한 데이터셋 설정
	gvMenuId = GV_LOGMENUID;
	fn_init();
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd)
		return false;
	} 
	else 
	{
		switch (svcid) 
		{						
			case "getAccntNoDbl":
				gfn_appendSearchDs(ds_list, ds_listTmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출
				
// 				if(ds_list.getRowCount() <= 0)
// 				{
// 					gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, 0);
// 				}
// 				else
// 				{
// 					gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, ds_list.getRowCount());
// 				}
				fn_postSearch();
			break;
			
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원 조회
function fn_commSearch(sType, searchInfo)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 1000, nTotalListCnt)) return;
	
	switch(searchInfo) 
	{
        case "getAccntNoDbl": 
			
			var szSvcid       = "getAccntNoDbl";
			var szController  = "/dlw/double/getAccntNoDbl";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listTmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
        break;
    }

	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	srch_accnt_no.value = "";
	srch_mem_no.value   = "";
	
	ds_listTmp.clearData();
	ds_list.clearData();
	//sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
}
// 조회 validation check.
function fn_preSearch(sType)
{
	ds_inqCndt.clearData();
	var nRow = ds_inqCndt.addRow();
	ds_inqCndt.setColumn(nRow, "accnt_no", srch_accnt_no.value);
	ds_inqCndt.setColumn(nRow, "mem_no", srch_mem_no.value);
	
	//ds_list.clearData();

	return true;
}

// 조회 결과
function fn_postSearch()
{
	if (ds_list.getRowCount() < 1) 
	{
		ds_list.clearData();
		alert("조회된 결과가 없습니다.");
	} 
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/

function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

// 검색 버튼 클릭
function btn_srch_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit(), "getAccntNoDbl");
}

// 그리드 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/dlw/double/getAccntNoDbl"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명

	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list, sta_TotalCnt, 1000, nTotalListCnt)) return;
	gfn_grdHeadClick(obj, e, ds_inqCndt, null, ds_listTmp);
	gfn_setTotalRowCount(ds_list, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend(), "getAccntNoDbl");
}

// onkeydown
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) 
	{
		if (obj.name == "srch_accnt_no") 
		{
			fn_commSearch(gfn_getCommTypeInit(), "getAccntNoDbl");
		} 
		else if (obj.name == "srch_mem_no")
		{
			fn_commSearch(gfn_getCommTypeInit(), "getAccntNoDbl");
		}
	}
}

function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	//gfn_exportGridToExcel(grd_list, "두구좌회원관리" + gfn_getTodayFull() + "");
	if(ds_list.rowcount > 0) 
	{		
		var colDS = gfn_excelPrepareExcelDS("grd_list");
		gfn_excelDown("두구좌회원관리", "XLS_DLCC", "DlwDoubleMap.getAccntNoDbl", "DlwDoubleMap.getAccntNoDblCount", ds_inqCndt, true, colDS, true);
	} 
	else 
	{
		alert("엑셀다운로드 할 대상이 없습니다.");
	}
}
