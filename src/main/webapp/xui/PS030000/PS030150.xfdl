{
	if (gfn_isNull(mem_no.value)) {
		return false;
	}
	return true;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원 조회
function fn_commSearch(sType, searchInfo)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_mem_list, sta_TotalCnt, 100, tc_mem)) return;
	
	switch(searchInfo) {
        case "srchCustBasi": 
			var szSvcid       = "srchCustBasi";
			var szController  = "/cons/cust-basi/resncredit";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
        break;
                
        case "srchCustBasiTot":
			var szSvcid       = "srchCustBasiTot";
			var szController  = "/cons/cust-basi/resncredit";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "excel_fg=Y";
			var szRetCallback = "fn_callBack";
        break;   
        
        case "delCustBasiTot":     	
			var szSvcid       = "delCustBasiTot";
			var szController  = "/cons/cust-basi/delresncredit";
			var szInDs        = "ds_input=ds_inqDel";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
	            
    }
    trace(szSvcid + "  " + szController);	
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


