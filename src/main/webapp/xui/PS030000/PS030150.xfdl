function fn_prePrint() {

	var cnt = ds_mem_list.getCaseCount("_chk == '1'");
	if (cnt < 1) {
        alert("출력할 항목을 선택해주세요.");
        return false;
    }
    
    ds_list_print.clearData();
    ds_mem_list.filter("_chk == '1'");
    var nRow = ds_list_print.copyData(ds_mem_list, true);
    ds_mem_list.filter("");
    
    ds_list_print.enableevent = false;
    ds_list_print.updatecontrol = false;
    for (var i=0; i<ds_list_print.rowcount; ++i) {
		ds_list_print.setRowType(i, Dataset.ROWTYPE_INSERT);
    }
    ds_list_print.updatecontrol = true;
    ds_list_print.enableevent = true; 
    return true;
}

function btn_Delete_onclick(obj:Button,  e:ClickEventInfo)
{
	var cf = confirm("정말 삭제하시겠습니까?");	
	if (cf == false) return;
	
	// 체크 여부확인
	var chkfg = false;
	var nRow  = 0;
	ds_inqDel.clearData();
	
	for(var i=0;i<ds_mem_list.rowcount;i++)
	{
		if(ds_mem_list.getColumn(i, "_chk") == 1) { // 체크 여부 확인
			nRow = ds_inqDel.insertRow(0);
			trace(ds_mem_list.getColumn(i,"target_ym"));
			trace(ds_mem_list.getColumn(i,"accnt_no"));
			ds_inqDel.setColumn(nRow,"accnt_no",ds_mem_list.getColumn(i,"accnt_no"));
			ds_inqDel.setColumn(nRow,"target_ym",ds_mem_list.getColumn(i,"target_ym"));	
			chkfg = true;
		}
	}
	
	if (chkfg == false){
		alert("삭제할 대상을 선택하세요.");
		return;
	}
	
	fn_commSearch(gfn_getCommTypeInit(), "delCustBasiTot");
	
}
