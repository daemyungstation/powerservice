    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCount1 = 0;
var nTotalListCount2 = 0;
var fv_popChk = "";
var gv_bef_emple_cd = "";
var gv_athrCd = gfn_getUserInfo("athr_cd");

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS031400_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_init();
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		btn_excel.visible = true;
	} else {
		btn_excel.visible = false;
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcId, errcd, errmsg) 
{
	if (errcd != 0) 
	{
		switch (svcId) 
		{
			case "getSignListMainList" :
			    
			break;
		}
	} 
	else 
	{
		switch (svcId) 
		{
			case "PS031400DLWCD":
				gfn_getCode("0225", "ALL", ds_codeSendStat, ds_codeDlwCds); // 전자서명전송상태
				gfn_getCode("0225", "NULL", ds_codeComboSendStat, ds_codeDlwCds); // 콤보전자서명전송상태
			break;
			
			case "getCodeSignGbnFull" :
				ds_codeSignGbn1.assign(ds_codeComboSignGbn1);				
				ds_codeSignGbn1.filter("cd == '01' || cd == '03'");
				/*
				var nRow = ds_codeSignGbn1.insertRow(0);
				ds_codeSignGbn1.setColumn(nRow, "cd", "");
				ds_codeSignGbn1.setColumn(nRow, "cd_nm", "전체");
				*/
				
				cmb_signGbn1.index = 0;
				fn_commSearch(gfn_getCommTypeInit(), "getCodeSignGbn2");
			break;
			
			case "getCodeSignGbn2" :
				if(cmb_signGbn1.value=='03'){
					ds_codeSignGbn2.filter("cd == '00019' || cd == '00040'");
				} else {
					ds_codeSignGbn2.filter("");
				}
				var nRow = ds_codeSignGbn2.insertRow(0);
				ds_codeSignGbn2.setColumn(nRow, "cd", "");
				ds_codeSignGbn2.setColumn(nRow, "cd_nm", "전체");
				cmb_signGbn2.index = 0;
			break;
			
			case "getSignListMainList" :
				var sTabPageId = tab_sign.tabpages[tab_sign.tabindex].name;
				
				if(sTabPageId == "tp_signList")
				{
					gfn_appendSearchDs(ds_listSignListMain, ds_listSignListMainTemp);
				    gfn_setTotalRowCount(ds_listSignListMain, tab_sign.tp_signList.sta_TotalCnt, nTotalListCount1, null, true, null); 
				    fn_postSearch(gfn_getCommTypeInit(), svcId);
				}
				else if(sTabPageId == "tp_signHist")
				{
					gfn_appendSearchDs(ds_listSignHistMain, ds_listSignHistMainTemp);
					gfn_setTotalRowCount(ds_listSignHistMain, tab_sign.tp_signHist.sta_TotalCnt, nTotalListCount2, null, true, null);
					fn_postSearch(gfn_getCommTypeInit(), svcId);
				}
				else
				{
				
				}
			break;
			
			case "deleteSignListMainList" : 
				fn_commSearch(gfn_getCommTypeInit(), "getSignListMainList");
			break;
		}
	}
}


/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType, svcId)
{
	switch (svcId) 
	{
		case "getSignListMainList":
			if (sType == gfn_getCommTypeInit())
			{		
				ds_inqCndt.clearData();
				
				var sTabPageId = tab_sign.tabpages[tab_sign.tabindex].name;
				var sSttDt = cal_sttDt.value;
				var sEndDt = cal_endDt.value;
				var sAccntNo = edt_accntNo.value;
				var sSignGbn1 = cmb_signGbn1.value;
				var sSignGbn2 = cmb_signGbn2.value;
				var sSendStat = cmb_sendStat.value;
				var sCompStat = cmb_signComp.value;
				var sResnYn = cmb_resnYn.value;
				var sDelFg = cmb_delFg.value;
				var sSendMan = edt_sendMan.value;
				var sMemNm = edt_memNm.value;
				var sCell = edt_cell.value;
				var sBefEmpleCd = gv_bef_emple_cd;
				
				var nRow = ds_inqCndt.addRow();
				ds_inqCndt.setColumn(nRow, "svc_id", sTabPageId);
				ds_inqCndt.setColumn(nRow, "stt_dt", sSttDt);
				ds_inqCndt.setColumn(nRow, "end_dt", sEndDt);
				ds_inqCndt.setColumn(nRow, "accnt_no", sAccntNo);
				ds_inqCndt.setColumn(nRow, "sign_gbn1", sSignGbn1);
				ds_inqCndt.setColumn(nRow, "sign_gbn2", sSignGbn2);
				ds_inqCndt.setColumn(nRow, "send_stat", sSendStat);
				ds_inqCndt.setColumn(nRow, "comp_stat", sCompStat);
				ds_inqCndt.setColumn(nRow, "resn_yn", sResnYn);
				ds_inqCndt.setColumn(nRow, "del_fg", sDelFg);
				//ds_inqCndt.setColumn(nRow, "send_man", sSendMan);
				ds_inqCndt.setColumn(nRow, "send_man","");
				ds_inqCndt.setColumn(nRow, "mem_nm", sMemNm);
				ds_inqCndt.setColumn(nRow, "cell", sCell);
				if(!gfn_isNull(edt_bef_emple_nm.value)) {
					ds_inqCndt.setColumn(nRow, "bef_emple_cd", sBefEmpleCd);
				}
								
				//gv_bef_emple_cd                
				ds_inqCndt.setColumn(nRow, "orderBy"       , "SLM.SIGN_NO");
				ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
			}
		break;
	}
	
	return true;
}

function fn_preSave(sType, svcId)
{
	switch (svcId) 
	{
		case "deleteSignListMainList":
			if (sType == gfn_getCommTypeInit())
			{		
				ds_inqCndt.clearData();
				
				var sSignNo = ds_listSignListMain.getColumn(ds_listSignListMain.rowposition, "sign_no");
				var sAccntNo = ds_listSignListMain.getColumn(ds_listSignListMain.rowposition, "accnt_no");
				var sSignGbn1 = ds_listSignListMain.getColumn(ds_listSignListMain.rowposition, "sign_gbn1");
				var sSignGbn2 = ds_listSignListMain.getColumn(ds_listSignListMain.rowposition, "sign_gbn2");
				var sSendStat = ds_listSignListMain.getColumn(ds_listSignListMain.rowposition, "send_stat");
				
				var nRow = ds_inqCndt.addRow();
				
				ds_inqCndt.setColumn(nRow, "sign_no", sSignNo);
				ds_inqCndt.setColumn(nRow, "accnt_no", sAccntNo);
				ds_inqCndt.setColumn(nRow, "sign_gbn1", sSignGbn1);
				ds_inqCndt.setColumn(nRow, "sign_gbn2", sSignGbn2);
				ds_inqCndt.setColumn(nRow, "send_stat", sSendStat);
                
				ds_inqCndt.setColumn(nRow, "orderBy"       , "SLM.SIGN_NO");
				ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
			}
		break;
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, svcId) 
{
	if(!fn_preSearch(sType, svcId)) 
	{
		return false;
	}
	
	switch(svcId) 
	{
		case "getCodeSignGbnFull" :
