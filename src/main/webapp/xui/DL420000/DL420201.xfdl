function fn_preSearch(sType,svcid, gbn) {
	if (sType == gfn_getCommTypeInit())
	{
		switch(svcid) {
			case "srchOcbProdTransList": 
			var stt_dt = cal_stt_dt.value+"";
			stt_dt = stt_dt.substring(0,6);
			
			var end_dt = cal_end_dt.value+"";
			
			//end_dt = gfn_addMonth(end_dt,1);
			end_dt = end_dt.substring(0,6);
			
			// 조회조건 및 default 정렬
			ds_inqCndt.keystring = ""; // 정렬컬럼 초기화
			
			ds_inqCndt.clearData();
			ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(0, "accnt_no", edt_accnt_no.value);
			ds_inqCndt.setColumn(0, "mem_nm", edt_mem_nm.value);
			ds_inqCndt.setColumn(0, "prod_cd", edt_prod_cd.value);
			
			ds_inqCndt.setColumn(0, "stt_dt", stt_dt);
			ds_accntInqCndt.setColumn(0, "orderBy", "seq");
			ds_accntInqCndt.setColumn(0, "orderDirection", "asc");
	
			break;
			
			case "selectCustaccntList":
				ds_accntInqCndt.keystring = ""; // 정렬컬럼 초기화
				ds_accntInqCndt.clearData();
				ds_accntInqCndt.addRow();
				
				var srch_typ = ""
				var srch_vl = "";
				var option = "";
				
				if (gbn == "accnt_no") {
					srch_typ = gbn;
					srch_vl = edt_accnt_no.value;
				} else if (gbn == "mem_nm"){
					srch_typ = gbn;
					srch_vl = edt_mem_nm.value;
				}
				
				ds_accntInqCndt.setColumn(0, "srch_typ", srch_typ);
				ds_accntInqCndt.setColumn(0, "srch_vl", srch_vl);
				ds_accntInqCndt.setColumn(0, "option", option);
				ds_accntInqCndt.setColumn(0, "ns_yn", "N");
				ds_accntInqCndt.setColumn(0, "popup_yn", "N");
				ds_accntInqCndt.setColumn(0, "orderBy", "mem_nm");
				ds_accntInqCndt.setColumn(0, "orderDirection", "asc");
				return_bool = true;
			break;
			
			case "searchProdList":
			ds_prodInqCndt.keystring = ""; // 정렬컬럼 초기화
			
			ds_prodInqCndt.clearData();
			ds_prodInqCndt.addRow();
			ds_prodInqCndt.setColumn(0, "prod_cd", edt_prod_cd.value);
			ds_prodInqCndt.setColumn(0, "prod_nm", edt_prod_nm.value);
			ds_prodInqCndt.setColumn(0, "del_fg", "N");
			ds_prodInqCndt.setColumn(0, "popup_yn", "N");
			
			ds_prodInqCndt.setColumn(0, "orderBy", "order_seq");
			ds_prodInqCndt.setColumn(0, "orderDirection", "asc");
			
			break;
		}
	}
	return true;
}

