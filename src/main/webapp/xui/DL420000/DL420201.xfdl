			}
		}
	}
}

// 탭에 따른 전송 대상자 
function fn_sendOcbMember(typ)
{
	var chkfg = false;
	var chk = 0;
	var tabinfo = "";
	switch (typ){
		case 'reg' :
			for(var i=0;i<ds_reg_list.rowcount;i++){
				if(ds_reg_list.getColumn(i, "_chk") == 1) { // 체크 여부 확인
					ds_reg_list.updatecontrol = false;
					for (var i=0; i<ds_reg_list.rowcount; ++i) {
						if ("1" != ds_reg_list.getColumn(i, "_chk")) {
							ds_reg_list.setRowType(i, Dataset.ROWTYPE_NORMAL);
						}else {
							chk = chk + 1;
						}
					}
				ds_reg_list.updatecontrol = true;
				}
			}
			tabinfo = "transOcbProd";
		break;
		case 'del' :
		
			for(var i=0;i<ds_del_list.rowcount;i++){
				if(ds_del_list.getColumn(i, "_chk") == 1) { // 체크 여부 확인
					ds_del_list.updatecontrol = false;
					for (var i=0; i<ds_del_list.rowcount; ++i) {
						if ("1" != ds_del_list.getColumn(i, "_chk")) {
							ds_del_list.setRowType(i, Dataset.ROWTYPE_NORMAL);
						}else {
							chk = chk + 1;
						}
					}
				ds_del_list.updatecontrol = true;
				}
			}
			tabinfo = "transOcbProdDel";
		break;
		
		case 'add' :
		
			for(var i=0;i<ds_add_list.rowcount;i++){
				if(ds_add_list.getColumn(i, "_chk") == 1) { // 체크 여부 확인
					ds_add_list.updatecontrol = false;
					for (var i=0; i<ds_add_list.rowcount; ++i) {
						if ("1" != ds_add_list.getColumn(i, "_chk")) {
							ds_add_list.setRowType(i, Dataset.ROWTYPE_NORMAL);
						}else {
							chk = chk + 1;
						}
					}
				ds_add_list.updatecontrol = true;
				}
			}
			tabinfo = "transOcbProdAdd";
		break;
	}
		
	if(chk < 1){
		alert("전송할 회원을 선택 후 이용하세요");
	}else {
		fn_commSave(tabinfo);
	}
}


