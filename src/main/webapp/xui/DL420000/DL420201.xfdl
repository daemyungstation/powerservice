//조회
function fn_commSearch(sType, searchInfo, gbn) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
        case "srchOcbProdTransList": 
			if (!fn_preSearch(sType,"srchOcbProdTransList")) return false;
			
			switch(tab_ocb.tabindex){
				//최초 충전
				case 0 :
					szSvcid       = "srchOcbProdRegList";
					szController  = "/dlw/ocb-prod/trans-reg-list";
					szParam       = "sendType='reg'";
				break;
				
				//충전 취소
				case 1 : 
					szSvcid       = "srchOcbProdDelList";
					szController  = "/dlw/ocb-prod/trans-del-list";
					szParam       = "sendtype='regdel'";
				break;
				
				//적립
				case 4 : 
					szSvcid       = "srchOcbProdAddList";
					szController  = "/dlw/ocb-prod/trans-add-list";
					szParam       = "sendtype='sav'";
				break;
				
				//적립취소
				case 5 : 
					szSvcid       = "srchOcbProdDelList";
					szController  = "/dlw/ocb-prod/trans-del-list";
					szParam       = "sendtype='savdel'";
				break;
				
				//사용
				case 2 : 
					szSvcid       = "srchOcbProdAddList";
					szController  = "/dlw/ocb-prod/trans-add-list";
					szParam       = "sendtype='use'";
				break;
				
				//사용취소
				case 3 : 
					szSvcid       = "srchOcbProdDelList";
					szController  = "/dlw/ocb-prod/trans-del-list";
					szParam       = "sendtype='usedel'";
				break;
			}
			
			szInDs        = "ds_input=ds_inqCndt";
			szOutDs       = "ds_list_tmp=ds_output";
			
			szRetCallback = "fn_callBack";
        break;
        
		case "selectCustaccntList":
			if (!fn_preSearch(sType,"selectCustaccntList", gbn)) return false;
			szSvcid       = "selectCustaccntList";
			szController  = "/dlw/cust/cust-acnt/list";
			szInDs        = "ds_input=ds_accntInqCndt";
			szOutDs       = "ds_accnt_info=ds_output";
			szParam       = "";
			szRetCallback = "fn_callBack";
		break;
		
        case "searchProdList":
			if (!fn_preSearch(sType,"searchProdList")) return false;
			szSvcid       = "searchProdList";
			szController  = "/dlw/prod/list";
			szInDs        = "ds_input=ds_prodInqCndt";
			szOutDs       = "ds_prodList=ds_output";
			szParam       = "";
			szRetCallback = "fn_callBack";
        break;
    }
    // 트랜젝션 공통함수 호출
    if ( searchInfo == "srchOcbProdTransList" )
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
    else
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


//저장
function fn_commSave(saveInfo) {
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
	if(!fn_preSave("transOcbProd")) return false;
	
	var p_szParam = "";
	
	switch(tab_ocb.tabindex){
		case 0 :
			p_szParam = "sendtype=reg";
		break;
		case 1 :
			p_szParam = "sendtype=regdel";
		break;
		case 4 :
			p_szParam = "sendtype=sav";
		break;
		case 5 :
			p_szParam = "sendtype=savdel";
		break;
		case 2 :
			p_szParam = "sendtype=use";
		break;
		case 3 :
			p_szParam = "sendtype=usedel";
		break;
	}
    
    switch(saveInfo) {  
		//충전
        case "transOcbProd": 
			szSvcid       = "transOcbProdReg";
			szController  = "/dlw/ocb-prod/trans-reg";
			szInDs        = "ds_input=ds_reg_list::U ds_input2=ds_rgsn_ocb_send";
			szParam       = p_szParam;
		break;
        case "transOcbProdDel":
			szSvcid       = "transOcbProdDel";
			szController  = "/dlw/ocb-prod/trans-del";
			szInDs        = "ds_input=ds_del_list::U";
			szParam       = p_szParam;
		break;
        case "transOcbProdAdd":
			szSvcid       = "transOcbProdAdd";
			szController  = "/dlw/ocb-prod/trans-reg";
			szInDs        = "ds_input=ds_add_list::U ds_input2=ds_rgsn_ocb_send";
			szParam       = p_szParam;
		break;
    }
    szOutDs       = "";
	szRetCallback = "fn_callBack";
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}


