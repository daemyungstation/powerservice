    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var gv_pay_type = "";
var gv_coupon_inh_no = "";
var gv_update_yn = "";



/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL420707P02_onload(obj:Form, e:LoadEventInfo)
{	
	gfn_initForm(obj);

	gv_coupon_inh_no = p_coupon_inh_no; // 신규 및 수정 구분	
	gv_update_yn = p_update_yn;
	
	if(gv_update_yn == 'Y'){
		btn_save.enable = true;

		cbo_srch_svcCl.enable = true;
		edt_coupon_no1.enable = true;
		edt_coupon_no2.enable = true;
		cal_isu_dt.enable = true;
		cbo_use_yn.enable = true;		
	}else{
		btn_save.enable = false;
		
		cbo_srch_svcCl.enable = false;
		edt_coupon_no1.enable = false;
		edt_coupon_no2.enable = false;
		cal_isu_dt.enable = false;
		cbo_use_yn.enable = false;
	}
		
	fn_selectOptSvcList();
	fn_commSearch();
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {	
			case "saveCouponInfoMst":
				alert("쿠폰정보가 수정 되었습니다.");
				opener.fn_commSearch(gfn_getCommTypeInit()); // 쿠폰 최조 조회
				close();
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_pre_save(sType)
{	
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬

		if (gfn_isNull(cbo_srch_svcCl.value)) {
			alert("쿠폰종류를 입력 하세요");
			return false;
		}

		if (gfn_isNull(edt_coupon_no1.value)) {
			alert("쿠폰번호1을 입력 하세요");
			return false;
		}
		
	}
	
	return true;
}


//쿠폰종류 전 처리
function fn_pre_selectOptSvcList()
{
	ds_cond.clearData();
	ds_cond.addRow();
	
	ds_cond.setColumn(0, "isMMS", "N");
	return true;
}
/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 쿠폰별 콤보박스 리스트
function fn_selectOptSvcList()
{	
	// 유효성 체크
	if ( !fn_pre_selectOptSvcList() ) {
		return;
	}
	
	var szSvcid       = "selectOptSvcList";
    var szController  = "/common/selectOptSvcList";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_svcCl=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function fn_commSearch()
{
	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "coupon_inh_no", gv_coupon_inh_no);

	var szSvcid       = "selectCouponDetail";
    var szController  = "/dlw/couponInfo/selectCouponDetail";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_couponInfo=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


// 상품분류 신규등록
function fn_saveCouponInfoMst(obj:Button,  e:ClickEventInfo)
{
	if (!fn_pre_save(gfn_getCommTypeInit())) return false;
	ds_couponInfo.setColumn(ds_couponInfo.rowposition, "dml_mode", "update");
		
	var szSvcid       = "saveCouponInfoMst"; 
	var szController  = "/dlw/couponInfo/saveCouponInfoMst";
	var szInDs        = "ds_input=ds_couponInfo";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";

	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);	
}



/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/



/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 닫기
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}



