//저장 전 월청구금액 유효성 검사
function fn_preProdSave()
{

	var st_no, end_no, month_pay_amt, membership_fee;
		
	if(edt_prod_cd.value ==null){
		alert("상품코드를 입력하세요");
		return false;
	}else if(edt_prod_nm.value ==null){
		alert("상품명을 입력하세요");
		return false;
	}else if(cbo_sale_cl.value ==null){
		alert("판매구분을 입력하세요");
		return false;
	}
	
	if(cbo_longtermYn.value == "Y" && gfn_isNull(edt_midInfo1.value)) {
		alert("MID는 전산담당자에게 문의바랍니다.");
		return false;
	}
	
	
	if(ds_list_amt.rowcount>0){
	for (var i=0; i<ds_list_amt.rowcount; ++i) {
		ds_list_amt.setColumn(i, "st_no");
		st_no = ds_list_amt.getColumn(i, "st_no");
		end_no = ds_list_amt.getColumn(i, "end_no");
		month_pay_amt = ds_list_amt.getColumn(i, "month_pay_amt");
		
			if ( gfn_isNull(st_no) ) {
				alert("시작회차를 입력하세요");
				ds_list_amt.rowposition = i;
				return false;
			}
			if ( gfn_isNull(end_no) ) {
				alert("종료회차를 입력하세요");
				ds_list_amt.rowposition = i;
				return false;
			}
			if ( gfn_isNull(month_pay_amt) ) {
				alert("월 청구금액을 입력하세요");
				ds_list_amt.rowposition = i;
				return false;
			}
			
			
			trace(st_no+'/'+end_no);
			
			if (st_no > end_no) {
				alert("시작회차가 종료회차보다 큽니다");
				ds_list_amt.rowposition = i;
				return false;
			}
			
			if (!gfn_isNull(edt_membership_fee.value) && i == 0) {
				if(st_no != 1 || end_no != 1) {
					alert('멤버십가입비 설정 시 1회차 월납입금 별도 기입이 필요합니다.');
					return false;
				}
			}
		}
	}

	return true;

}


