/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL420802_onload(obj:Form, e:LoadEventInfo)
{

	grd_list.useGridPopup = 'Y';
	gfn_initForm(obj);
	
	gv_p_gubun = p_gubun; // 신규 및 수정 구분

	gfn_getList("dlwCd", "85,101,111,0128,0129,0130,0187,0188,0189"); // 공통코드조회
	
	if (p_gubun == "update"){
		if (gfn_isNull(p_prod_cd)) {
			alert("상품코드가 없습니다.");
			return false;
		} else {
			gv_prod_cd = p_prod_cd;
			p_prod_cd = null;
		}
		if (!gfn_isNull(p_pay_type)) {
			gv_pay_type = p_pay_type;
			p_pay_type = null;
		}
		gfn_getList("dlwCd", "85,101,111,0128,0129,0130,0187,0188,0189"); // 공통코드조회
	} else {
		fn_init();
			alert("상품코드는 영문자 및 숫자 혼용 2자리까지만 입력가능합니다");
	}
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
	trace('------------------에러 콜백');
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("85", "SELECT", ds_prodCl, gds_DmlCd);			// 상품구분
			//gfn_getCode("101", "SELECT", ds_membershipKind, gds_DmlCd);	// 멤버쉽카드종류
			gfn_getCode("111", "SELECT", ds_prodType, gds_DmlCd);		// 청구방법
			gfn_getCode("0130", "SELECT", ds_prodGubun, gds_DmlCd);		// 상품분류
			gfn_getCode("0187", "SELECT", ds_sectionf, gds_DmlCd);		// 섹션1
			gfn_getCode("0188", "SELECT", ds_sectiont, gds_DmlCd);		// 섹션2
			gfn_getCode("0189", "SELECT", ds_sectionthr, gds_DmlCd);	// 섹션3
			gfn_getCode("0128", "SELECT", ds_joinType, gds_DmlCd);		// 결합상품 유형
			gfn_getCode("0129", "SELECT", ds_joinGiftCardSort, gds_DmlCd); // 결합상품 상품권 종류
			
			fn_setMembershipCd();
			break;
			
			case "srchProdDtpt":
			
				trace(ds_list_amt.saveXML());
			if (p_gubun == "update"){
				fn_postSearch();
			}
			break;
			
			case "srchMembershipCd":
			if (p_gubun == "update"){
				fn_commSearch();
			}
			break;
			
			case "newProd":
			if (p_gubun == "update"){
				alert("상품정보가 수정되었습니다.");
				fn_commSearch();
			} else {
				alert("상품정보가 등록되었습니다.");
				fn_commSearch();
			}			
			break;
			
			case "delProd":
				alert("상품정보가 삭제되었습니다.");
				close();
			break
			
			case "delProdDtl":
				alert("선택한 회차별 월청구금액이 삭제되었습니다.");
				fn_commSearch();
			break;
		}
	}
}

