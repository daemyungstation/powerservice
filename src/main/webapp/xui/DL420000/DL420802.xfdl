//회차별 월청구금액 삭제
function btn_del_dtl(obj:Button,  e:ClickEventInfo)
{
		var st_no = ds_list_amt.getColumn(gv_row, "st_no");
		var p_seq = ds_list_amt.getColumn(gv_row, "seq");
		var cf = confirm("정말 삭제하시겠습니까?");
		if (cf == false) return;


		var chkfg = false;

		for(var i=0;i<ds_list_amt.rowcount;i++)
		{
			if(ds_list_amt.getColumn(i, "chk") == 1) { // 체크 여부 확인
				chkfg = true;
				break;
			}
		}	
		// 체크 여부확인
		if (chkfg == false){
			alert("삭제할 대상을 선택하세요.");
			return;
		}

	if(p_seq != null && p_seq !== " "){

		
		ds_delete_dtl.addRow();
		ds_delete_dtl.setColumn(0, "seq", p_seq );
		ds_delete_dtl.setColumn(0, "prod_cd", gv_prod_cd );
		ds_delete_dtl.setColumn(0, "chk", 1);
		trace(ds_delete_dtl.saveXML());

		var szSvcid       = "delProdDtl";
		var szController  = "/dlw/prod/delProdDtl";
		var szInDs        = "ds_input=ds_delete_dtl";
		var szOutDs       = "";
		var szParam       = "";
		var szRetCallback = "fn_callBack";

		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);	
		
		}else {		
				trace(ds_list_amt.getRowCount());
				
				for(var i=ds_list_amt.rowcount-1; i>=0; i--) {
					if ("1" == ds_list_amt.getColumn(i, "chk")){
						ds_list_amt.deleteRow(i);
					}
				}
				alert("삭제되었습니다.");
	   }
	
}

