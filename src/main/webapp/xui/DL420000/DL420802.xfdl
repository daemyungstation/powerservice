// 조회
function fn_preSearch()
{
	if (gfn_isNull(gv_prod_cd)) {
		alert("상품코드가 없습니다.");
		close();
		return false;
	}
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 상품 상세조회
function fn_commSearch()
{
	if (!fn_preSearch()) return false;
	
	var szSvcid       = "srchProdDtpt";
    var szController  = "/dlw/prod/dtpt";
    var szInDs        = "";
    var szOutDs       = "ds_prod=ds_output ds_list_amt=ds_output_list";
    var szParam       = "prod_cd="+gv_prod_cd+" pay_type="+gv_pay_type;
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 멤버쉽카드코드 조회
function fn_setMembershipCd()
{
	var szSvcid       = "srchMembershipCd";
