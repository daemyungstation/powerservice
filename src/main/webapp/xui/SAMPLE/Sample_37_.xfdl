include "LIB::lib_All.xjs";

var fv_totalCnt = 0;		// 전체건수

function SuggestSample_onload(obj:Form, e:LoadEventInfo)
{
	// 시스템 공통함수 호출
	gfn_commInit(obj);

	// Local Data 이용시 코드값 psudocode로 변환처리
	var count = dsFind.getRowCount();
	var i;
	for(i=0;i<count;i++)
	{
		var text = dsFind.getColumn(i,"text");
		var code = gfn_getSpliceTextE(text);
		dsFind.setColumn(i,"pseudocode",code);
	}
}

// 데이타 필터
function fn_searchText(sInputText)
{
	var text = sInputText;
	if(text.length > 0)
	{
		var etext = gfn_getSpliceTextE(text);
		fdsFind.filter("pseudocode.toString().indexOf('"+etext+"') == 0 || code.toString().indexOf('"+etext+"') == 0");
		
		if(fdsFind.rowcount > 0) 
			lst_Find.visible = true;
		else {
			edt_Input.setFocus();
			lst_Find.visible = false;	
		}
	}
	else
	{
		fdsFind.filter("pseudocode == 0");
		lst_Find.visible = false;
	}
}

function ListBox00_onitemclick(obj:ListBox, e:ListBoxClickEventInfo)
{
	edt_Input.value = e.itemtext;
	lst_Find.visible = false;
}

function Edit03_onkeydown(obj:Edit, e:KeyEventInfo)
{
	var objList 	= lst_Find;
	
	if(e.keycode == 13) {
		fn_searchText(obj.text);
	} else if(e.keycode == 40) {
		if(objList.visible == true) {
			objList.setFocus();
			objList.index = 0;
		} else {
			var sInputText = String(obj.value).trim();
			if(gfn_getLength(sInputText) > 0) {
				fn_searchText(sInputText);
				objList.setFocus();
				objList.index = 0;
			}
		}
	}
}

function ListBox00_onkeydown(obj:ListBox, e:KeyEventInfo)
{
	if(e.keycode == 13) {
		edt_Input.value = fdsFind.getColumn(obj.index, "codetext");
		edt_Input.setFocus();
		lst_Find.visible = false;	
	} else if(e.keycode == 38) {
		if(obj.index <= 0) {
			edt_Input.setFocus();
			lst_Find.visible = false;	
		}
	} else if(obj.index < 0){
		edt_Input.setFocus();
		lst_Find.visible = false;	
	}
}

