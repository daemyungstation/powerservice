function lst_Find4_onkeydown(obj:ListBox, e:KeyEventInfo)
{
	var objInput 	= Grid03;
	var objList 	= lst_Find4;
	var objDs		= ds_PrdtInfo2;	
	
	if(e.keycode == 13) {
		this.ds_List2.setColumn(obj.row, obj.col, objDs.getColumn(obj.index, "PRDT_CDNM"));
		ds_List2.rowposition = objList.row;
		objInput.setCellPos(objList.col);
		objInput.setFocus();	
		obj.visible = false;	
	} else if(e.keycode == 38) {
		if(obj.index <= 0) {
			ds_List2.rowposition = objList.row;
			objInput.setCellPos(objList.col);
			objInput.setFocus();
			obj.visible = false;	
		}
	} else if(obj.index < 0){
		ds_List2.rowposition = objList.row;
		objInput.setCellPos(objList.col);
		objInput.setFocus();
		obj.visible = false;	
	}
}

