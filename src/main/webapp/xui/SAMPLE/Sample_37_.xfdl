	gfn_setKwData_sqlId("getListPrdtInfoTest");
	gfn_setKwData_trscCode("TST0000905");

	gfn_tranDIS(gfn_getCommTypeAppend() 
				, "ds_PrdtInput=ds_PrdtInput"
				, "ds_PrdtInfo=datas"
				, ""
				, false);	
			
	objInput.setFocus();
	
	if(objDs.rowcount > 0) {
		if(objDs.rowcount == 1) {
			objInput.value = objDs.getColumn(0, "PRDT_CDNM");
			objList.setFocus();
			objInput.setFocus();
			objList.visible = false;		
		} else  {
			objList.visible = true;
			objList.index = 0;
		}	
	} else {
		objInput.setFocus();
		objList.visible = false;	
	}				
