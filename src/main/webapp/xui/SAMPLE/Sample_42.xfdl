function fn_resize(obj:Form, nCx, nCy)
{
	var nFullW 		= nCx-20;
	var nFullH 		= nCy-25;
	var nLeft 		= 0;
	var nStaticTop 	= 33;
	var nDivTop		= 62;
	var nDivW 		= parseInt(nCx/7);
	var nDivH 		= parseInt((nCy-62)/6);
	
// 	var nMoveL = (nFullW/2) - 100;
// 	btn_Year_Down.move(nMoveL, btn_Year_Down.position.top);
// 	sta_calMT_Year.move(nMoveL, sta_calMT_Year.position.top);
// 	btn_Year_Up.move(nMoveL, btn_Year_Up.position.top);
// 	btn_Month_Down.move(nMoveL, btn_Month_Down.position.top);
// 	sta_calMT_Month.move(nMoveL, sta_calMT_Month.position.top);
// 	btn_Month_Up.move(nMoveL, btn_Month_Up.position.top);
	
	// 타이틀영역 위치/이동처리
	sta_mtTilBg.position.width = nFullW;
	var nCommentL = nFullW - sta_Comment.position.width - 19;
	sta_Comment.move(nCommentL, sta_Comment.position.top);
	
	for(var i=0;i<=41;i++)
	{
		var objDiv 		= eval("div_Day_" + i);		
		
		if(i <= 6) {
			var objDayTitle = eval("sta_DayTitle_" + i);
			
			objDayTitle.move(nLeft, nStaticTop);
			objDiv.move(nLeft, nDivTop);
			
			if(i==6) {
				objDayTitle.position.right 	= nFullW;
				objDiv.position.right 		= nFullW;
			} else {
				objDayTitle.position.width 	= nDivW;
				objDiv.position.width 		= nDivW;
			}
			
			objDiv.position.height 			= nDivH;
			
			nLeft	 	+= nDivW-1;

		} else {
		
			var nMod = i%7;
			if(nMod == 0) {
				nDivTop += parseInt(nDivH)-1;		
				nLeft = 0;
			} 
			
			objDiv.move(nLeft, nDivTop);
			
			if (nMod == 6) { 
				objDiv.position.right 		= nFullW;
			} else {
				objDiv.position.width 		= nDivW;
			}	
			
			if(i<35) {
				objDiv.position.height 		= nDivH;
			} else {
				objDiv.position.bottom 		= nFullH;
			}	
			
			nLeft	 	+= nDivW-1;
			
		}
	}
}

function fn_setComment(sTitle, sDay)
{
	sta_Comment.text = sTitle+ " <fs v='11'><fc v='#feca5c'>" + sDay + "</fc></fs>일";
