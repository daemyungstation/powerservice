    <Script type="xscript4.0"><![CDATA[
include "LIB::lib_All.xjs";

var fv_Today;
var fv_CurDate;

function Comm_MonthCalender_onload(obj:Form, e:LoadEventInfo)
{
	fn_resize(obj, obj.position.width, obj.position.height);

	fv_Today = gfn_getToday();
	//fv_Today = "20130201";
	
	fn_setComment("대상일", "25");
	fn_setMonthInfo(fv_Today);
	
	// 범례처리
	var nCellIdx = 0;
	for(var i=0;i<ds_SGbn.rowcount;i++)
	{
		ds_View.setColumn(0, nCellIdx, ds_SGbn.getColumn(i, "SICon"));  
		nCellIdx++;
		
		ds_View.setColumn(0, nCellIdx, ds_SGbn.getColumn(i, "SNm")); 
		nCellIdx++;		
	}
	
	fn_setMonthInfo("20130201");
}

function fn_setMonthInfo(sDate)
{
	fv_CurDate = sDate;

	sta_calMT_Year.text 	= sDate.substr(0,4) + "년";
	sta_calMT_Month.text 	= parseInt(sDate.substr(4,2)) + "월";
	
	fn_setMonthDate(sDate.substr(0,6));
}

function fn_setMonthDate(sYYYYMM)
{
	var nDay = gfn_getDay(sYYYYMM+"01");		//요일(0:일,1:월,2:화,3:수,4:목,5:금,6:토)
	var sLastDay = String(gfn_getLastDay(sYYYYMM)).substr(6,2);
	var nIndex = nDay;
	
	trace("nDay::"+nDay);
	trace("sLastDay::"+sLastDay);
	trace("nIndex::"+nIndex);

	// 초기화 처리
	for(var i=0;i<=41;i++)
	{
		var objDiv = eval("div_Day_"+i);
		
		objDiv.ds_schList.clearData();
		objDiv.sta_ModifyYN.visible = false;	
	}

	// 이전월부분 처리
	if(nDay != 0) {
		var sPreYYYYMM = gfn_addMonth(sYYYYMM+"01", -1);
		var nPreMonthLastDay = parseInt(String(gfn_getLastDay(sPreYYYYMM)).substr(6,2));
		
 		for(var i=nDay-1;i>=0;i--)
		{
			var objDiv = eval("div_Day_"+i);
			objDiv.userdate = sPreYYYYMM.substr(0,6) + String(i).padLeft(2, "0");
			
			fn_setDay(objDiv, nPreMonthLastDay, "#c6c6c6");
			nPreMonthLastDay--;
		}
	}
	
	// 현재월 처리
	for(var i=1;i<=sLastDay;i++)
	{
		var objDiv = eval("div_Day_"+nIndex);
		
		objDiv.userdate = sYYYYMM + String(i).padLeft(2, "0");
		
		// 데이타셋팅-----------------------------------------
		
		var objDs = objDiv.ds_schList;
		
		ds_schList.filter("SDate=='"+objDiv.userdate+"'");
		for(var k=0;k<ds_schList.rowcount;k++)
		{
			var sICon = ds_SGbn.lookup("SCd", ds_schList.getColumn(k, "IGbn"), "SICon");
		
			var nRow = objDs.addRow();
			objDs.setColumn(nRow, "ICon", 	sICon);
			objDs.setColumn(nRow, "Title", 	"마감일 " + gfn_right(ds_schList.getColumn(k, "EDate"), 2) + "일");
			objDs.setColumn(nRow, "Time", 	ds_schList.getColumn(k, "ETime"));
		}
		if(ds_schList.findRow("ModifyYN", "Y") != -1) objDiv.sta_ModifyYN.visible = true;
		ds_schList.filter("");
		
		// 데이타셋팅 END-----------------------------------------
		
		//objDiv.sta_day.text = i;
		fn_setDay(objDiv, i, fn_getDayColor(objDiv.userdate));
		
		// 오늘날짜 예외처리
		if(objDiv.userdate == fv_Today) {
			objDiv.class = "div_CalSchList_Today";
			objDiv.sta_day.font = "Dotum,9,bold";
		} else {
			objDiv.class = "div_CalSchList";
		}		
		
		nIndex++;
	}	
	
	// 다음월 처리
	var sNextYYYYMM = gfn_addMonth(sYYYYMM+"01", 1);
	var nDay = 1;
	for(var i=nIndex;i<=41;i++)
	{
		var objDiv = eval("div_Day_"+i);
		objDiv.userdate = sNextYYYYMM.substr(0,6) + String(i).padLeft(2, "0");
		fn_setDay(objDiv, nDay, "#c6c6c6");
		nDay++;
	}
	
	// userdate 확인용 Loop
// 	for(var i=0;i<=41;i++)
// 	{
// 		var objDiv = eval("div_Day_"+i);
// 		trace("Date::" + i + " " +objDiv.userdate);
// 	}
}

function fn_getDayColor(sYYYYMMDD)
{
	var nWeekDay = gfn_getDay(sYYYYMMDD);
	var rtnColor;
	
	if(nWeekDay == 0)	// 일요일
		rtnColor = "#ff0000";
	else if(nWeekDay == 6)	// 토요일
		rtnColor = "#c6c6c6";
	else
		rtnColor = "dafault";
		
	return rtnColor;
}

function fn_setDay(objDiv, nDay, sDayColor)
{
	objDiv.sta_day.color 	= sDayColor;
	objDiv.sta_day.text 	= nDay;
}

function fn_setDayFont(obj)
{
	for(var i=0;i<41;i++)
	{
		var objDiv = eval("div_Day_"+i);
		if(obj == objDiv) {
			objDiv.sta_day.font = "Dotum,9,bold";
		} else {
			if(objDiv.userdate == fv_Today) {
				objDiv.sta_day.font = "Dotum,9,bold";
			} else {
				objDiv.sta_day.font = "Dotum,9";
			}	
		}
	}
}

function btn_Year_Down_onclick(obj:Button,  e:ClickEventInfo)
{
	var sDate = String(parseInt(fv_CurDate.substr(0,4)) - 1) + fv_CurDate.substr(4,4);
	fn_setMonthInfo(sDate);
}

function btn_Year_Up_onclick(obj:Button,  e:ClickEventInfo)
{
	var sDate = String(parseInt(fv_CurDate.substr(0,4)) + 1) + fv_CurDate.substr(4,4);
	fn_setMonthInfo(sDate);
}

function btn_Month_Down_onclick(obj:Button,  e:ClickEventInfo)
{
	var sDate = gfn_addMonth(fv_CurDate, -1);
	fn_setMonthInfo(sDate);
}

function btn_Month_Up_onclick(obj:Button,  e:ClickEventInfo)
{
	var sDate = gfn_addMonth(fv_CurDate, 1);
	fn_setMonthInfo(sDate);
}

function btn_Today_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_setMonthInfo(gfn_getToday());
}

