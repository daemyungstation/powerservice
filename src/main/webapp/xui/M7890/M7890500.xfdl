/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function M7890500_onload(obj:Form, e:LoadEventInfo)
{   
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	//gfn_athrPgm(gfn_gdsCd_admin()); // 권한
	fn_setAthrPgmBtn(); // 권한에 따른 버튼 설정
	grd_list.summarytype = "top";
//	fn_init();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID

			
			fn_init(); // 검색부 초기화
			fn_commSearch(gfn_getCommTypeInit()); // 상품 조회
			break;
			
			case "acuonlist": // 조회
			ds_Ylist.clearData();
			gfn_appendSearchDs(ds_Ylist, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리

			sta_TotalCnt.text = "전체가입자 :" + tc_mem + " 명 / 실행 대상자 : " + tc_silmem  + " 명";
		//	gfn_setTotalRowCount(ds_list_prod, sta_TotalCnt, tc_prod); // 페이지건수 표출
			break;
			
 			case "srchProdExcelList": // 엑셀다운
			gfn_exportGridToExcel(grd_list_excel,"상품조회_"+gfn_getTodayFull());
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{	
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_prod.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "prod_nm", edt_srch_prod_nm.value);
		ds_inqCndt.setColumn(0, "del_fg", "N");
		ds_inqCndt.setColumn(0, "acuon_yn",cbo_acuon_yn.value);
		ds_inqCndt.setColumn(0, "orderBy", "");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 애큐온관리현황
function fn_commSearch(sType)
{
	//if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	//if(!gfn_pageSearch(sType, ds_list_prod, sta_TotalCnt, 50, tc_prod)) return;
	
	var szSvcid       = "acuonlist";
    var szController  = "/acuo/trans/yList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
    
  

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 엑셀다운로드
function fn_srchExcelDownList()
{	
	//gfn_exportGridToExcel(grd_list, "애큐온현황LIST"+gfn_getTodayFull()+"");	
	trace(grd_list);
	gfn_exportGridToExcel(grd_list, "Acuon_"+gfn_getTodayFull()+"");
