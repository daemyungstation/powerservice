//Right(suct) 저장전
function fn_preRightSave()
{
	if(gfn_isNull(cbo_suctDivCdEdit.value)){
		alert("자료유형을 선택하세요.");
		return false;
	}
	
	if(gfn_isNull(edt_suctShrdNmEdit.value)){
		alert("항목명칭을 입력하세요.");
		return false;
	}
	
	if(gfn_isNull(rdo_useYn.value)){
		alert("사용여부를 선택하세요.");
		return false;
	}
	
	if(cbo_suctDivCdEdit.value == "10"){
		if(gfn_isNull(edt_faqSuctAddrNm.value)){
			alert("FAQ를 선택하세요.");
			return false;
		}
	}else if(cbo_suctDivCdEdit.value == "20"){
		if(gfn_isNull(edt_urlSuctAddrNm.value)){
			alert("URL을 입력하세요.");
			return false;
		}
		var pattern = /^((https?):\/\/)?([a-z0-9-]+\.)+([a-z0-9-]+\.)+[a-z0-9]{2,4}.*$/;
		var hmpgAddr = edt_urlSuctAddrNm.value;
		if( !pattern.test(hmpgAddr)){
			alert("URL을 확인하세요");
			return false;
		}
	}
	
	var useYnCnt = 0;
	
	
	if(ds_suctView.getColumn(ds_suctView.rowposition,'suct_id') != null){
		var suct_id = ds_suctView.getColumn(ds_suctView.rowposition,'suct_id');
		for (var i = 0; ds_suctList.rowcount> i ; i++){
			if(suct_id != ds_suctList.getColumn(i,'suct_id')){
				if(ds_suctList.getColumn(i,"use_yn") =="Y"){
					useYnCnt++;
				}
			}
		}	
	}else{
		for (var i = 0; ds_suctList.rowcount> i ; i++){
			if(ds_suctList.getColumn(i,"use_yn") =="Y"){
				useYnCnt++;
			}
		}
	}
	
	if (ds_suctSetList.getColumn(ds_suctSetList.rowposition,"suct_ppup_div_cd") == "10" && useYnCnt >= 1 && rdo_useYn.value == 'Y') {
		alert("'단일' 속성은 1개의 세부항목만 사용가능합니다.");
		return false;
	}
	
	return true;
}

