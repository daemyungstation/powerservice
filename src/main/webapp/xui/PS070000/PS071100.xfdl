//버튼 클릭이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
		case btn_search : //조회버튼
			fn_commSearch(gfn_getCommTypeInit()); // 조회
		break;
		
		case btn_init : //초기화버튼
			fn_init();
		break;
		
		case btn_create: //left 신규버튼 클릭
			ds_suctSetView.clearData();
			ds_suctSetView.addRow();
			
			ds_leftOrderList.insertRow(0);
			ds_leftOrderList.setColumn(ds_leftOrderList.rowposition,"name",parseInt(leftMaxOrder)+1);
			ds_leftOrderList.setColumn(ds_leftOrderList.rowposition,"id",parseInt(leftMaxOrder)+1);
			
			fn_oderInit_left(); // 올드 순서 셋팅
			
			ds_suctList.clearData();//하위유형 초기화
			ds_suctView.clearData();//하위유형 상세부 초기화
			
			fn_changeComponent("C");
		break;
		
		case btn_update: //left 수정
			fn_oderInit_left(); // 올드 순서 셋팅
			fn_changeComponent("U");
		break;
		
		case btn_delete : //삭제 
			if(confirm("선택된 내용을 삭제하시겠습니까?")) {
				fn_leftDeleteData();
			}
		break;
		
		case btn_save: //left 저장
			 fn_leftSaveData();
		break;
		
		case btn_cancel: //left 취소 
			fn_copyRow();
			fn_changeComponent("R");
		break;
		
		case btn_create_right : //right 신규
			ds_suctView.clearData();
			ds_suctView.addRow();
			
			ds_rightOrderList.insertRow(0);
			ds_rightOrderList.setColumn(ds_rightOrderList.rowposition,"name",parseInt(rightMaxOrder)+1);
			ds_rightOrderList.setColumn(ds_rightOrderList.rowposition,"id",parseInt(rightMaxOrder)+1);
			
			ds_suctView.setColumn(ds_suctView.rowposition, "suct_set_id", ds_suctSetList.getColumn(ds_suctSetList.rowposition,"suct_set_id"));
			
			ds_suctView.addColumn("suct_ppup_div_cd","STRING");
			ds_suctView.setColumn(ds_suctView.rowposition, "suct_ppup_div_cd", ds_suctSetList.getColumn(ds_suctSetList.rowposition,"suct_ppup_div_cd"));
			
			fn_oderInit_right();
			
			fn_changeComponent("SC");
		break;
		
		case btn_update_right : //right 수정
			fn_oderInit_right();
			
			fn_changeComponent("SU");
		break;
		
		case btn_delete_right: //삭제
			if(confirm("선택된 내용을 삭제하시겠습니까?")) {
				fn_rightDeleteData();
			}
		break;
		
		case btn_cancel_right: //right 취소버튼 
			fn_copyRow_right();
			fn_changeComponent("SR");
		break;
		
		case btn_faqPop : //FAQ 질문
			var sPrmr =  {openerFlag:true};
			gfn_openSelectFaqPop(sPrmr,false);
		break;
		
		case btn_save_right : //저장
			fn_rightSaveData();
		break;
		
		default : 
			trace("btn을 확인하세요");
		break;
	}
}

//콤보박스 변경시 이벤트
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(obj){
		case cbo_suctDivCdEdit : 
			
			if(obj.value == "10" ){
				stc_suctAddrNm.text = "FAQ질문";
				btn_faqPop.visible = true;
				edt_urlSuctAddrNm.visible = false;
				edt_faqSuctAddrNm.visible = true;
				edt_faqSuctAddrNm.position.width = edt_urlSuctAddrNm.position.width - btn_faqPop.position.width - 2 ;
				edt_faqSuctAddrNm.move(btn_faqPop.position.right+2,btn_faqPop.position.top-1);
				edt_urlSuctAddrNm.value = null;
				edt_faqSuctAddrNm.value = null;
			}else if(obj.value == "20" ){
				stc_suctAddrNm.text = "URL";
				btn_faqPop.visible = false;
				edt_faqSuctAddrNm.visible = false;
				edt_urlSuctAddrNm.visible = true;
				edt_urlSuctAddrNm.move(stc_suctAddrNm.position.right+2,btn_faqPop.position.top-1);
				edt_urlSuctAddrNm.readonly = false;
				edt_urlSuctAddrNm.value = null;
				edt_faqSuctAddrNm.value = null;
			}
		break;
	}
}

//그리드 셀클릭 이벤트 
function grd_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj){
		case grd_suctSet:
			fn_copyRow();
			fn_changeComponent("R"); // left 컴포넌트
		break
		
		case grd_suct :
			fn_copyRow_right();
			fn_changeComponent("SR");
		break;
	}
}

