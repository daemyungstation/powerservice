//right old 순서 셋팅
function fn_oderInit_right()
{
	ds_suctView.addColumn("old_expe_sqnc","INT");
	if(ds_suctView.getColumn(ds_suctView.rowposition,"expe_sqnc")==null){
		ds_suctView.setColumn(ds_suctView.rowposition,"old_expe_sqnc",parseInt(rightMaxOrder)+1);
