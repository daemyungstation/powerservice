//Right Contents------------------------------------------------------------------------------------------
//Right(suct) 조회결과 처리
function fn_postRightSearch()
{
	if(ds_suctList.rowcount < 1) {
		fn_copyRow_right();
		fn_changeComponent("SR");
	}else{
		fn_copyRow_right();
		fn_changeComponent("SR");
	}
	return;
}

//리스트 데이터셋 뷰 데이터셋에 카피
function fn_copyRow_right(){
	if(ds_suctList.rowcount<1){
		ds_suctView.clearData();
		ds_suctView.addRow();
		fn_rightMaxOrderSearch(ds_suctSetList.getColumn(ds_suctSetList.rowposition,"suct_set_id"));//순서조회
		
	}else{
		ds_suctView.clearData();
		ds_suctView.addRow();
		ds_suctView.copyRow(ds_suctView.rowposition,ds_suctList,ds_suctList.rowposition);
		
		//등록 수정일시 데이터타입에 맞게
 		var rgsnDttmStr = edt_rgsn_dttm_right.value;
 		edt_rgsn_dttm_right.value = gfn_strDttm(rgsnDttmStr);
 			
 		var amntDttmStr = edt_amnt_dttm_right.value;
 		edt_amnt_dttm_right.value = gfn_strDttm(amntDttmStr);
 		
 		
 		fn_rightMaxOrderSearch(ds_suctSetList.getColumn(ds_suctSetList.rowposition,"suct_set_id"));//순서조회
	}
}
