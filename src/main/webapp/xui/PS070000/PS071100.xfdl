//조회후 처리
function fn_postSearch()
{
	if(ds_suctSetList.rowcount < 1) {
		fn_copyRow();
		
		fn_changeComponent("R");
	}else{
		fn_copyRow();
		fn_leftMaxOrderSearch();//최대 갯수 조회
		fn_changeComponent("R");
	}
	return;
}

//리스트 데이터셋 뷰 데이터셋에 카피
function fn_copyRow(){
	if(ds_suctSetList.rowcount<1){
		ds_suctSetView.clearData();
		ds_suctSetView.addRow();
		
		fn_leftMaxOrderSearch();//최대 갯수 조회
	}else{
		ds_suctSetView.clearData();
		ds_suctSetView.addRow();
		ds_suctSetView.copyRow(ds_suctSetView.rowposition,ds_suctSetList,ds_suctSetList.rowposition);
		
		//등록 수정일시 데이터타입에 맞게
 		var rgsnDttmStr = edt_rgsn_dttm.value;
 		edt_rgsn_dttm.value = gfn_strDttm(rgsnDttmStr);
 			
 		var amntDttmStr = edt_amnt_dttm.value;
 		edt_amnt_dttm.value = gfn_strDttm(amntDttmStr);
 		
 		fn_leftMaxOrderSearch();//최대 갯수 조회
 		
 		fn_rightSuctSearch(gfn_getCommTypeInit(),ds_suctSetView.getColumn(ds_suctSetView.rowposition,'suct_set_id')); // 하위 조회
	}
}


