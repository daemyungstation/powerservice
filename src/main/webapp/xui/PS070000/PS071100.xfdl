//권한
function fn_setAthr(){
	if(rgsnYn != "Y"){
		btn_create.enable = false;
		btn_create_right.enable = false;
	}
	
	if(inqYn != "Y"){
		btn_search.enable = false;
	}
	
	if(amntYn != "Y"){
		btn_update.enable = false;
		btn_update_right.enable = false;
