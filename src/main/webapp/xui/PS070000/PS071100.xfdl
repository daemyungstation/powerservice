    <Script type="xscript4.0"><![CDATA[/***************************************************************************
 * Program ID    : PS071100.xfdl
 * Program Name  : 퀵링크관리
 * Author        : 정 훈
 * Created On    : 2016.02.11
 * 개    요 	 :
 *
 * =========================================================================
 * 변경일자     수정자       내    용
 * =========================================================================
 * 2016.02.11    정   훈      최초작성
 
 **************************************************************************/

/********************************************************************************************/
/* 1. Include Library include                                                               */
/********************************************************************************************/
include "LIB::lib_All.xjs";




/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트,전역변수,초기화) 처리                         	*/
/********************************************************************************************/
var total_count = 0;
var total_count01 = 0;
var leftMaxOrder = 0; //순서 카운트(Left)
var leftDupCount = 0;
var leftDupFlag = false;

var rightMaxOrder = 0; //순서 카운트(Right)
var rightDupCount = 0;
var rightDupFlag = false;


//권한
var rgsnYn; //등록권한
var inqYn; //조회권한
var amntYn; //수정권한
var dltnYn; //삭제권한
var pritYn; //출력권한
var xlsYn; //엑셀권한
var athrPpupYn; //팝업오픈권한
var xlsRrnoYn; //엑셀 개인정보 권한 여부 

function PS071100_onload(obj:Form, e:LoadEventInfo)
{
	gfn_athrPgm(gfn_gdsCd_admin()); //권한조회
	//KMS050 : 표시위치, KMS060 : 자료 유형
	gfn_getList("cd","KMS050,KMS060","fn_callBack",null,"srchCdSet");   // 코드셋
	gfn_createPagingDataset(obj);
	fn_commSearch(gfn_getCommTypeInit()); // 조회
	
}

//초기화 
function fn_init()
{	
	edt_srchVal.value =null;
	cbo_suctExpeStnCd.index=0;
	cbo_srchTyp.index = 0;
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
	if(errcd != 0){
		trace("에러발생 svcid : "+svcid+"   errmsg :"+errmsg+ "   errcd :"+errcd);
	}else{
		switch(svcid){
			case "srchCdSet": //코드셋 콜백
				gfn_getCode("KMS050", "ALL", ds_suctExpeStnCd); //표시위치 조회 
				gfn_getCode("KMS050", "SELECT", ds_suctExpeStnCdEdit); //표시위치 상세
				gfn_getCode("KMS060", "", ds_suctDivCdEdit); //자료유형
				cbo_suctExpeStnCd.index = 0;
				cbo_suctExpeStnNmEdit.index = 0;
				cbo_suctDivCdEdit.index =0;
			break;
			
			case "srchSuctSet": //left 퀵링크 조회 
				gfn_appendSearchDs(ds_suctSetList, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_suctSetList, sta_TotalCnt, total_count); // 페이지건수 표출
				fn_postSearch();
			break;
			
			case "srchLeftMaxOrder" : //left 최대갯수 콜백
				ds_leftOrderList.clearData();
				for(var i = leftMaxOrder; i > 0; i--){
					ds_leftOrderList.addRow();
					ds_leftOrderList.setColumn(ds_leftOrderList.rowposition,"name",i);
					ds_leftOrderList.setColumn(ds_leftOrderList.rowposition,"id",i);
				}
			break;
			
			case "srchLeftDupCount" : //Left(suctSet) 중복갯수
				if(leftDupCount > 0 && confirm("중복되는 순서가 있습니다. 순서를 자동증가하고 등록하시겠습니까?\n자동증가된 순서는 목록을 다시 조회하면 표시됩니다.")){
					leftDupFlag = true;
				}
			break;
			
			case "saveSuctSet" : //left 저장 콜백
				fn_commSearch(gfn_getCommTypeInit()); //재조회
				leftDupFlag = false; //초기화
				leftDupCount = 0; //초기화
			break;
			
			case "deleteLeftSuctSet" : //Left(suctSet) 삭제콜백
				fn_commSearch(gfn_getCommTypeInit()); // 조회
			break;
			
			//Right Contents------------------------------------------------------------------------------------------
			case "srchSuct" : //Right(suct) 조회 콜백
				gfn_appendSearchDs(ds_suctList, ds_list01_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_suctList, sta_TotalCnt01, total_count01); // 페이지건수 표출
				fn_postRightSearch();
			break;
			
			case "srchRightMaxOrder" : //Right(suct) 최대 갯수 콜백
				ds_rightOrderList.clearData();
				for(var i = rightMaxOrder; i > 0; i--){
					ds_rightOrderList.addRow();
					ds_rightOrderList.setColumn(ds_rightOrderList.rowposition,"name",i);
					ds_rightOrderList.setColumn(ds_rightOrderList.rowposition,"id",i);
				}
			break;
			
			case "deleteRightSuct": //Right(suct) 삭제콜백
				fn_rightSuctSearch(gfn_getCommTypeInit(),ds_suctSetList.getColumn(ds_suctSetList.rowposition,"suct_set_id"));//재조회
			break;
			
			case "srchRightDupCount" : //Right(suct) 중복갯수
				if(rightDupCount > 0 && confirm("중복되는 순서가 있습니다. 순서를 자동증가하고 등록하시겠습니까?\n자동증가된 순서는 목록을 다시 조회하면 표시됩니다.")){
					rightDupFlag = true;
				}
			break;
			
			case "saveSuct": //Right(suct) 저장콜백
				var suct_set_id = ds_suctSetList.getColumn(ds_suctSetList.rowposition,"suct_set_id");
				rightDupFlag = false; //초기화
				rightDupCount = 0; //초기화
				fn_rightSuctSearch(gfn_getCommTypeInit(),suct_set_id); // 하위 조회
			break;
			
			default :
			trace("svcid를 확인하세요");
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
//Left Contents------------------------------------------------------------------------------------------
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_suctSetList.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "suct_expe_stn_cd", cbo_suctExpeStnCd.value);
		ds_inqCndt.setColumn(0, "searchType", cbo_srchTyp.value);
		ds_inqCndt.setColumn(0, "searchValue", edt_srchVal.value);
		ds_inqCndt.setColumn(0, "orderBy", "expe_sqnc");
		ds_inqCndt.setColumn(0, "orderDirection", "asc");
	}
	return true;
}

//Left(suctSet) 저장전
function fn_preSave()
{
	if(gfn_isNull(cbo_suctExpeStnNmEdit.value)){
		alert("표시위치를 선택하세요.");
		return false;
	}
	
	if(gfn_isNull(rdo_suctPopupDivCd.value)){
		alert("팝업속성을 선택하세요.");
		return false;
	}
	
	if(gfn_isNull(edt_suctSetNmEdit.value)){
		alert("링크명칭을 입력하세요.");
		return false;
	}
	
	if(gfn_isNull(edt_ppupScrnNm.value)){
		alert("자료제목을 입력하세요.");
		return false;
	}
	
	if(gfn_isNull(rdo_useYnSet.value)){
		alert("사용여부를 선택하세요.");
		return false;
	}
	
	if(gfn_isNull(cbo_expeSqncSet.value)){
		alert("순서를 선택하세요.");
		return false;
	}
	
	var useYnCnt = 0;
	for (var i = 0; ds_suctList.rowcount> i ; i++){
		if(ds_suctList.getColumn(i,"use_yn") =="Y"){
			useYnCnt++;
		}
	}
	
	if(ds_suctSetView.getColumn(ds_suctSetView.rowposition,"suct_ppup_div_cd")=="10"&& useYnCnt>1 ){
		alert("'단일' 속성은 1개의 세부항목만 사용가능합니다.");
		return false;
	}

	return true;
}

