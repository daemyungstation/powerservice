/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("0152", "ALL", ds_goods_typ, gds_DmlCd); // 물품구분
			fn_init(); // 검색부 초기화
		break;
		
		case "srchEventSrvProd": 
			if(tab_main.tabindex == 5 || tab_main.tabindex == 6 || tab_main.tabindex == 7 || tab_main.tabindex == 8 || tab_main.tabindex == 9 || tab_main.tabindex == 10 || tab_main.tabindex == 11){
				fn_create_grid1(); // 장례용품/현장발주/장례식장/협력업체/기타용품/전체품목
			}
		break;
		
		case "srchBranchList":
			ds_branch_list.insertRow(0);
			ds_branch_list.setColumn(0, "cd_nm", "전체");
		break;
	}
}

