// 직영 그리드 동적 생성
function fn_create_grid1()
{
	var v_Formats = "";
	var v_Columns = "";
	var v_Rows = "";
	var v_BandHead = "";
	var v_BandBody = "";

	if(tab_main.tabindex == 7 || tab_main.tabindex == 11){
		v_Columns += '<Column size="150" />\n';
	}else{
		v_Columns += '<Column size="100" />\n';
	}
	
	v_Rows += '<Row size="30" band="head" />\n';
	v_Rows += '<Row size="23" />\n';
	
	v_BandHead += '<Cell rowspan="1" colspan="1" text="순번"/>\n';
	
	v_BandBody += '<Cell displaytype="normal" expr="currow+1"/>\n';
	//v_BandBody += '<Cell col="1" text="bind:gubun"/>\n';
	
	// 포맷 생성
	if(tab_main.tabindex == 11){
		var v_HeadText = '';
		var v_HeadColIdx = 11;
		var v_BodyColIdx = 11;	
	}else{
		var v_HeadText = '';
		var v_HeadColIdx = 8;
		var v_BodyColIdx = 8;
	}
	
	for (var i=0; i<ds_t6_head.getRowCount(); i++) {
		
		// Column 생성
		if(tab_main.tabindex == 7 || tab_main.tabindex == 11){
			v_Columns += '<Column size="150" />\n';
		}else{
			v_Columns += '<Column size="100" />\n';
		}
		// Head 생성
		if (v_HeadText != ds_t6_head.getColumn(i, "gds_nm")) {
			v_HeadText = ds_t6_head.getColumn(i, "gds_nm");
			
			var v_Colspan = ds_t6_head.getCaseCount("gds_nm=='" + v_HeadText + "'");
			
			// Row 1
			v_BandHead += '<Cell col="'+v_HeadColIdx+'" colspan="'+v_Colspan+'" text="'+v_HeadText+'"/>\n';
			v_HeadColIdx++;
		}
		
		// Body 생성
		v_BandBody += '<Cell style="align:center;" col="'+v_BodyColIdx+'" displaytype="number" text="bind:'+ds_t6_head.getColumn(i, "gds_cd")+'"/>\n';
		v_BodyColIdx++;
	}

	v_Formats = '<Formats>\n';
	v_Formats += '<Format id="default">\n';
	v_Formats += '<Columns>\n';
	v_Formats += '<Column size="100" />\n'; // 회원번호
	v_Formats += '<Column size="100" />\n'; // 회원명
	v_Formats += '<Column size="100" />\n'; // CP명
	v_Formats += '<Column size="100" />\n'; // 상품명
	v_Formats += '<Column size="150" />\n'; // 행사기간
	v_Formats += '<Column size="170" />\n'; // 행사장소
	v_Formats += '<Column size="180" />\n'; // 장지
	if(tab_main.tabindex == 11){
		v_Formats += '<Column size="150" />\n'; // 합계
		v_Formats += '<Column size="150" />\n'; // 재고사용분	
		v_Formats += '<Column size="150" />\n'; // 수의금액
	}
	v_Formats += v_Columns;
	v_Formats += '</Columns>\n';
	v_Formats += '<Rows>\n';
	v_Formats += v_Rows;
	v_Formats += '</Rows>\n';
	v_Formats += '<Band id="head">\n';
	v_Formats += '<Cell col="1" colspan="1" text="회원번호"/>\n';
	v_Formats += '<Cell col="2" colspan="1" text="회원명" />\n';
	v_Formats += '<Cell col="3" colspan="1" text="CP명"/>\n';
	v_Formats += '<Cell col="4" colspan="1" text="상품명"/>\n';
	v_Formats += '<Cell col="5" colspan="1" text="행사기간"/>\n';
	v_Formats += '<Cell col="6" colspan="1" text="행사장소"/>\n';
	v_Formats += '<Cell col="7" colspan="1" text="장지"/>\n';
	if(tab_main.tabindex == 11){
		v_Formats += '<Cell col="8" colspan="1" text="수의금액"/>\n';
		v_Formats += '<Cell col="9" colspan="1" text="재고사용분"/>\n';
		v_Formats += '<Cell col="10" colspan="1" text="합계"/>\n';
	}
	v_Formats += v_BandHead;
	v_Formats += '</Band>\n';	
	v_Formats += '<Band id="body">\n';
	v_Formats += '<Cell style="align:center;" col="1" displaytype="text" text="bind:accnt_no"/>\n';
	v_Formats += '<Cell style="align:center;" col="2" displaytype="text" text="bind:mem_nm" expr="expr:gfn_SetNameGrdSecu(mem_nm)"/>\n';
	v_Formats += '<Cell style="align:center;" col="3" displaytype="normal" text="bind:evt_mngr_nm"/>\n';
	v_Formats += '<Cell style="align:left;" col="4" displaytype="normal" text="bind:prod_nm"/>\n';
	v_Formats += '<Cell style="align:center;" col="5" displaytype="normal" text="bind:edate"/>\n';
	v_Formats += '<Cell style="align:left;" col="6" displaytype="normal" text="bind:event_place"/>\n';
	v_Formats += '<Cell style="align:left;" col="7" displaytype="normal" text="bind:cemetery"/>\n';
	if(tab_main.tabindex == 11){
		v_Formats += '<Cell style="align:center;" col="8" displaytype="number" text="bind:samt"/>\n';
		v_Formats += '<Cell style="align:center;" col="9" displaytype="number" text="bind:jamt"/>\n';
		v_Formats += '<Cell style="align:center;" col="10" displaytype="number" text="bind:tot_amt"/>\n';
	}
	v_Formats += v_BandBody;
	v_Formats += '</Band>\n';		
	v_Formats += '</Format>\n';	
	v_Formats += '</Formats>\n';
	
	trace(v_Formats);
	
	if(tab_main.tabindex == 5){
		tab_main.tabpage6.grd_list_t6.enableredraw = false;
		tab_main.tabpage6.grd_list_t6.formats = v_Formats;
		tab_main.tabpage6.grd_list_t6.enableredraw = true;
	}else if(tab_main.tabindex == 6){
		tab_main.tabpage7.grd_list_t6.enableredraw = false;
		tab_main.tabpage7.grd_list_t6.formats = v_Formats;
		tab_main.tabpage7.grd_list_t6.enableredraw = true;
	}else if(tab_main.tabindex == 7){
		tab_main.tabpage8.grd_list_t6.enableredraw = false;
		tab_main.tabpage8.grd_list_t6.formats = v_Formats;
		tab_main.tabpage8.grd_list_t6.enableredraw = true;
	}else if(tab_main.tabindex == 8){
