//조회전 처리 조회조건셋팅
function fn_preSearch(sType) {

	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	
	if ( gfn_isEmpty(stt_dt.value) ) {
		alert("시작일자를 입력해주십시오.");
		return false;
	}	
	
	if ( gfn_isEmpty(end_dt.value) ) {
		alert("종료일자를 입력해주십시오.");
		return false;
	}		
	
	var tab_id = tab_main.tabindex;
	
	// 장례용품 ~ 행사품목전체 일시 외주 검색 x
	if(tab_main.tabindex == 5 || tab_main.tabindex == 6 || tab_main.tabindex == 7 || tab_main.tabindex == 8 || tab_main.tabindex == 9 || tab_main.tabindex == 10 || tab_main.tabindex == 11){
		if(cbo_emp.value == '0002'){
			alert("직영보고서 현황입니다.")
			return false;
		}
	}

	ds_inqCndt.setColumn(0, "dt_typ", cbo_dt_typ.value);
	ds_inqCndt.setColumn(0, "stt_dt", stt_dt.value);
	ds_inqCndt.setColumn(0, "end_dt", end_dt.value);
	ds_inqCndt.setColumn(0, "custmr_cd", edt_custmr_cd.value);
	ds_inqCndt.setColumn(0, "custmr_nm", edt_custmr_nm.value);
	
	ds_inqCndt.setColumn(0, "accnt_no", edt_accnt_no.value);
	ds_inqCndt.setColumn(0, "cp_nm", edt_cp_nm.value);
	ds_inqCndt.setColumn(0, "emp_gubun", cbo_emp.value);
	ds_inqCndt.setColumn(0, "prod_cd", edt_prod_cd.value);
	
	ds_inqCndt.setColumn(0, "tab_id", tab_id);
		
	return true;
}

// 조회
function fn_commSearch(sType) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";

	if(!fn_preSearch(sType)) return false;
	//if(!gfn_pageSearch(sType, ds_goods_list, sta_TotalCnt, 500, tc_prod)) return;

	szSvcid       = "srchEventSrvProd";
	szController  = "/dlw/evnt/srvprod-list";
	szInDs        = "ds_input=ds_inqCndt";
	if(tab_main.tabindex == 0){
		szOutDs       = "ds_t1_list=ds_output1";
	}else if(tab_main.tabindex == 1){
		szOutDs       = "ds_t2_list=ds_output2";
	}else if(tab_main.tabindex == 2){
		szOutDs       = "ds_t3_list=ds_output3";
	}else if(tab_main.tabindex == 3){
		szOutDs       = "ds_t4_list=ds_output4";
	}else if(tab_main.tabindex == 4){
		szOutDs       = "ds_t5_list=ds_output5";
	}else if(tab_main.tabindex == 5 || tab_main.tabindex == 6 || tab_main.tabindex == 7 || tab_main.tabindex == 8 || tab_main.tabindex == 9 || tab_main.tabindex == 10 || tab_main.tabindex == 11){
		szOutDs       = "ds_t6_head=ds_output6h ds_t6_list=ds_output6";
	}
	szParam       = "";
	szRetCallback = "fn_callBack";
		

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

