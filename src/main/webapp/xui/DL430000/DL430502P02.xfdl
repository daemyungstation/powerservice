/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var fv_init_cnt		= 0;

var fv_mode			= "";
var fv_wh_out_no		= "";
var fv_wh_in_no2	= "";

var fv_out_cl		= "";
var fv_out_dt		= "";
var fv_emple_nm		= "";
var fv_emple_no		= "";


/* 그리드 금액계산 관련 */
var fv_gds_cd_ds 			= null; // 품목코드 조회후 값을 입력할 데이터셋(그리드와 바인드된 데이터셋)
var fv_gds_cd_ds_row 		= -1;	// 데이터셋의 행 index
var fv_gds_cd_col_id 		= "";	// 코드 컬럼ID
var fv_gds_nm_col_id 		= "";	// 코드명 컬럼ID
var fv_gds_unit_cost_col_id = "";	// 단가 컬럼ID
var fv_gds_unit_col_id		= "";
var fv_gds_standard_col_id 	= "";
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/

function frm_onload(obj:Form, e:LoadEventInfo)
{	
	if ( !gfn_isEmpty(p_wh_out_no) ) {
		fv_wh_out_no = p_wh_out_no;
	}
	if ( !gfn_isEmpty(p_out_cl) ) {
		fv_out_cl = p_out_cl;
	}
	if ( !gfn_isEmpty(p_out_dt) ) {
		fv_out_dt = p_out_dt;
	}
	if ( !gfn_isEmpty(p_emple_nm) ) {
		fv_emple_nm = p_emple_nm;
	}
	if ( !gfn_isEmpty(p_emple_no) ) {
		fv_emple_no = p_emple_no;
	}
	
	if ( gfn_isEmpty(fv_wh_out_no) ) {
		fv_emple_no = gfn_getUserInfo("user_id");
	}
	
	if ( !gfn_isEmpty(p_mode) ) {
		fv_mode = p_mode;
	} else {
		fv_mode = "insert";
	}
	
	fn_getWhCdList();
	
	gfn_getList("dlwCd","07,0163,0150,0151","fn_callBack",false);   // 대명코드조회 (07:관계코드)
		
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("07"	, "SELECT"	, ds_07		, gds_DmlCd); // 코드조회결과 필터
			gfn_getCode("0163"	, ""		, ds_0163	, gds_DmlCd); // 코드조회결과 필터
			gfn_getCode("0150"	, ""		, ds_0150	, gds_DmlCd); // 코드조회결과 필터
			gfn_getCode("0151"	, ""		, ds_0151	, gds_DmlCd); // 코드조회결과 필터
			gfn_getCode("00"	, ""		, ds_00		, gds_DmlCd); // 코드조회결과 필터			
			break;
		
		case "search":
			fn_postSearch();
			break;			

		case "getWhCdList":
			fn_postGetWhCdList();
			break;
			
		case "mergeWhOutMst":
			fn_postSave();
			break;
			
		case "delete":
			fn_postDelete();
			break;
	}
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch()
{
	if ( gfn_isEmpty(fv_wh_out_no) ) {
		return false;
	}
	
	ds_cond.clearData();
	ds_cond.addRow();	
	
	ds_cond.setColumn(0, "wh_out_no", fv_wh_out_no);
	
	return true;
}

//조회
function fn_search()
{	
	// 호출 건수 설정 및 조회여부 설정
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "search";
    var szController  = "/evnt/stockMng/selectOutWhList";
    var szInDs        = "ds_input=ds_cond";
    
    var szOutDs       = "ds_list=ds_output";
    
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//조회 결과 
function fn_postSearch()
{
	if(ds_list.rowcount < 1) {
		trace("조회 결과가 없습니다.");
		return;
 	}
}

//저장 전 처리
function fn_preSave()
{
	var i;
	var emple_nm 	= edt_emple_nm.value;
	var out_dt 		= cal_out_dt.value + "";
	var out_cl 		= cbo_out_cl.value;

	if( gfn_isEmpty(out_cl) ) {
		alert("출고구분은 필수입력 항목입니다.");
		cbo_out_cl.setFocus();
		return false;
	}
	
	if( gfn_isEmpty(fv_emple_no) ) {
		alert("담당자는 필수입력 항목입니다.");
		edt_emple_nm.setFocus();
		return false;
	}
	
	if( gfn_isEmpty(out_dt) ) {
		alert("출고일자는 필수입력 항목입니다.");
		cal_out_dt.setFocus();
		return false;
	}
	
	for(i=0; i<ds_list.rowcount; ++i) {
		if ( gfn_isEmpty(ds_list.getColumn(i, "wh_cd")) ) {
			alert("출고창고를 선택해주십시오.");
			return false;
		}
		
		if ( gfn_isEmpty(ds_list.getColumn(i, "gds_cd")) ) {
			alert("품목을 선택해주십시오.");
			return false;
		}
		
		if ( gfn_isEmpty(ds_list.getColumn(i, "qty")) || 0 == ds_list.getColumn(i, "qty") ) {
			alert("수량을 입력해주십시오.");
			return false;
		}
	}
	
	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "wh_out_no"		, fv_wh_out_no	);		
	ds_cond.setColumn(0, "emple_nm"		, emple_nm		);
	ds_cond.setColumn(0, "emple_no"		, fv_emple_no	);
	ds_cond.setColumn(0, "out_dt"		, out_dt			);
