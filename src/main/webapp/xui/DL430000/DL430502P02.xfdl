	var gds_cd, gds_nm, cost, unit, standard;
	
	// trace(ds_gds_tmp.saveXML());

	if (1 == ds_gds_tmp.rowcount) {
		gds_cd 		= ds_gds_tmp.getColumn(0, "gds_cd");
