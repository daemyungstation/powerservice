    fn_init();
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("02", "ALL", ds_evt_typ, gds_DmlCd); // 코드조회결과 필터
			cbo_event.index = 0;
			fn_init(); // 검색부 초기화
		break;
		
		case "srchMngripList":
			gfn_setTotalRowCount(ds_mngr_list, sta_TotalCnt, tc_prod, null, true, null); // 페이지건수 표출
		break;

	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch(sType) {

	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	var typ = "";
	
	ds_inqCndt.setColumn(0, "dt_typ", cbo_dt_typ.value);
	ds_inqCndt.setColumn(0, "stt_dt", cal_stt_dt.value);
	ds_inqCndt.setColumn(0, "end_dt", cal_end_dt.value);
	ds_inqCndt.setColumn(0, "mem_nm", edt_mem_nm.value);
	ds_inqCndt.setColumn(0, "mngr_cd", edt_mngr_cd.value);
	ds_inqCndt.setColumn(0, "ji_stt_dt", ji_stt_dt.value);
	ds_inqCndt.setColumn(0, "ji_end_dt", ji_end_dt.value);
	
	if(chk_1.isChecked() == true){
		typ = '\'0001\''
	}
	if(chk_2.isChecked() == true){
		typ += ',\'0002\''
	}
	if(chk_3.isChecked() == true){
		typ += ',\'0003\''
	}
	if(chk_4.isChecked() == true){
		typ += ',\'0004\''
	}

	if(typ.substring(0,1) == ','){
		var oztyp = typ.substr(1);
	}else{
		var oztyp = typ;
	}
	
	oz_typ = oztyp;
	//alert(oztyp);

	ds_inqCndt.setColumn(0, "job_duty", typ);

	return true;
}

// 조회
function fn_commSearch(sType) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";

	if(!fn_preSearch(sType)) return false;
	if(!gfn_pageSearch(sType, ds_mngr_list, sta_TotalCnt, 500, tc_prod)) return;

	szSvcid       = "srchMngripList";
	szController  = "/dlw/evnt/mngrip-list";
	szInDs        = "ds_input=ds_inqCndt";
	szOutDs       = "ds_mngr_list=ds_output";
	szParam       = "";
	szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

//조회 결과 
function fn_postSearch()
{
	if(ds_mngr_list.rowcount < 1) {
		trace("조회 결과가 없습니다.");
 	}
	return;
}


