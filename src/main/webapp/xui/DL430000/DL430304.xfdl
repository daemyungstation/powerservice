//초기화
function fn_init()
{
	var objDate = new Date();
	strDate = objDate.toFormatString("%Y%m");
	endDate = objDate.toFormatString("%Y%m%d");
	
	cal_stt_dt.value = strDate;
	cal_end_dt.value = endDate;
	edt_mem_nm.value = "";
	edt_mngr_cd.value = "";
	edt_mngr_nm.value = "";
	ji_stt_dt.value = "";
	ji_end_dt.value = "";
	chk_all.value = false;
	chk_1.value = false;
	chk_2.value = false;
	chk_3.value = false;
	chk_4.value = false;		
}

// 체크박스 선택 이벤트
function fn_chked_typ(){
	if(chk_all.isChecked() == true) {
		chk_1.value = true;
		chk_2.value = true;
		chk_3.value = true;
		chk_4.value = true;
	}else if(chk_all.isChecked() == false){
		chk_1.value = false;
		chk_2.value = false;
		chk_3.value = false;
		chk_4.value = false;	
	}
}

function fn_chked_evt(){

	if(chk_1.isChecked() == false || chk_2.isChecked() == false || chk_3.isChecked() == false || chk_4.isChecked() == false){
		chk_all.value = false;
	}else if(chk_1.isChecked() == true && chk_2.isChecked() == true && chk_3.isChecked() == true && chk_4.isChecked() == true){
		chk_all.value = true;
	}
}
