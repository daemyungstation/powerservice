	ds_quest.updatecontrol = false;
	for (var i=0; i<ds_quest.rowcount; ++i) {
		if ("1" == ds_quest.getColumn(i, "satsfctn1")) 
		{
			ds_quest.setColumn(i, "satsfctn", "0001");
