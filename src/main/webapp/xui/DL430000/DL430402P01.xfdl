/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("07", "SELECT", ds_07, gds_DmlCd); // 코드조회결과 필터
			gfn_getCode("0163", "", ds_0163, gds_DmlCd); // 코드조회결과 필터
			
			fn_init(); // 검색부 초기화
			
			fn_search(); // 행사정보 조회
			
			if ( gfn_isEmpty(fv_seq) ) // 수정모드일 경우는 모니털링 보고서 상세를 조회하기 때문에 질문지 조회가 필요없음
			{
				fn_getMonitorQuestList(); // 질문지 조회
				ds_list.addRow();
			}
			break;
		
		case "search":
			fn_postSearch();
			// 종합평가 불만 조회 시 visible 처리
			if(rad_total_assessmnt.value == '0004'){
				rad_total_badoption.visible = true;
				stc_step_bit_txt.visible = true;
				stc_step_bigo_txt.visible = true;
				rad_step_bit.visible = true;
				stc_19.visible = true;
				step_bigo.visible = true;	
			}			
			break;
		
		case "getMonitorQuestList":
			fn_postGetMonitorQuestList();
			break;
			
		case "saveMontrRpt":
			fn_postSave();
			break;
			
		case "getEventAccnt":
			fn_postGetEventAccnt();
			break;
			
			
	}
}


