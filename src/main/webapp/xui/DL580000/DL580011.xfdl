    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_list = 0;	// 전체건수

var xAccntNo;

var xID;
var xMemNo;
var xConsTxt;
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL520008_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin()); // 권한
	//gfn_getList("dlwCd","0114","fn_callBack",false);
	
	fn_init(); // 초기화
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {		
			case "getOnlnMemberList": // 홈페이지 회원조회
				gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_list); // 페이지건수 표출
				
				if (cbo_search_gbn.value == "04"){
					// 탈퇴회원 조회 후 
					div_resn.visible = true;					
				} else {
					div_resn.visible = false;
					// 조회결과 존재시 하단 정보창 DS COPY
					if(ds_list.rowcount > 0){
						xID = ds_list.getColumn(ds_list.rowposition,"id");					
						fn_commSearch(gfn_getCommTypeAppend(), "getOnlnMemberDtlList");
					} else {
						alert("조회 내역이 없습니다.");
						ds_list_dtl.clearData();					
					}				
				}
				
			break;
			
			case "getOnlnMemberDtlList": // 홈페이지 회원 상세
				SetComponent(); //버트 및 컴퍼넌트 설정
			break;
								
			case "updateMemberInfo": // 저장처리
				alert("저장 완료");
				fn_commSearch(gfn_getCommTypeAppend(), "getOnlnMemberDtlList");
			break;
			
			case "deleteExtSpecial": // 삭제처리
				alert("삭제 완료");
				fn_commSearch(gfn_getCommTypeInit(), "getOnlnMemberList"); // 조회
			break;
			
			case "getOnlnMemberListExcel": // 엑셀
				gfn_exportGridToExcel(grd_list_excel, "특수고객청구관리_"+gfn_getTodayFull()+"");
			break;				
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{
	// 조회조건 및 default 정렬
	ds_inqCndt.clearData();
	ds_list.keystring = ""; // 정렬컬럼 초기화
	
	if(gfn_isNull(edt_id.value) && gfn_isNull(edt_name.value)) {
		if(gfn_isNull(cal_srchSttDt.value) || gfn_isNull(cal_srchEndDt.value)) {
			alert("검색기간은 필수항목 입니다.");
			return false;
		}
		if(gfn_getDayDiff(cal_srchSttDt.value, cal_srchEndDt.value) > 7) {
			alert("날짜 범위는 7일을 초과 할 수 없습니다.");
			return false;
		}
	}	
	if(gfn_getDayDiff(cal_srchSttDt.value, cal_srchEndDt.value) < 0) {
		alert("시작일은 종료일을 초과 할 수 없습니다.");
		return false;
	}

	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "stt_dt", cal_srchSttDt.value);
	ds_inqCndt.setColumn(0, "end_dt", cal_srchEndDt.value);
	ds_inqCndt.setColumn(0, "search_gbn", cbo_search_gbn.value);
	ds_inqCndt.setColumn(0, "id", edt_id.value);
	ds_inqCndt.setColumn(0, "name", edt_name.value);
	ds_inqCndt.setColumn(0, "srch_type", cbo_srchDateType.value);
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회
function fn_commSearch(sType, searchInfo)
{
    switch(searchInfo) {
    
		// 조회
        case "getOnlnMemberList":
			if (!fn_preSearch(sType)) return false;
			
			// 호출 건수 설정 및 조회여부 설정
			if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 2000, tc_list)) return;
		
			var szSvcid       = "getOnlnMemberList";
			var szController  = "/dlw/onlnMng/getOnlnMemberList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
		
		// 조회
        case "getOnlnMemberDtlList":					
        if (!fn_preSearch(sType)) return false;
        
			var szSvcid       = "getOnlnMemberDtlList";
			var szController  = "/dlw/onlnMng/getOnlnMemberDtlList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_dtl=ds_output ds_pay_list=ds_output2";
			var szParam       = "sID=" + xID;
			var szRetCallback = "fn_callBack";
		break;		
		
		// 엑셀 조회
        case "getOnlnMemberListExcel":
			var szSvcid       = "getOnlnMemberListExcel";
			var szController  = "/dlw/onlnMng/getOnlnMemberList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_excel=ds_output";
			var szParam       = "excel_yn=Y";
			var szRetCallback = "fn_callBack";
		break;
	}

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{	
	var sToday = gfn_getToday();
	var sStartDate = gfn_addDate(sToday, -7);
	
	cal_srchSttDt.value = sToday;
	cal_srchEndDt.value = sToday;
		
	cbo_search_gbn.value = "01";
	edt_id.value = "";
}

function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
	fn_commSearch(gfn_getCommTypeInit(), "getOnlnMemberList"); // 조회
}

function fn_save(g_mode){
	
	// 유효성체크
	if(!fn_preSave(g_mode)) return false;
	
	if(!confirm("저장하시겠습니까?")) return false;
				
	var szSvcid       = "updateMemberInfo";
    var szController  = "/dlw/onlnMng/MemInfoUpdate/"+g_mode;
    var szInDs        = "ds_input=ds_list_dtl";
    var szOutDs       = "";
    var szParam       = "constxt=" + xConsTxt +" accnt_no=" + tab_Main.tab01.edt_accnt_no.value;
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 저장전
function fn_preSave(g_mode)
{
	var sConsTxt = "";
	switch(g_mode){
		case "email" :
			var sEmailA = tab_Main.tab01.edt_web_email.value; //email web
			var sEmailB = tab_Main.tab01.edt_mb_email.value; //email dlcc
			xConsTxt = "[홈페이지]EMAIL변경됨(" + sEmailB + ">> )";
			ds_list_dtl.setColumn(0,"mb_email",sEmailA);
		break;
		
		case "cell" :
			var sCellA = tab_Main.tab01.edt_web_cell.value; //cell web
			var sCellB = tab_Main.tab01.edt_mb_cell.value; //cell dlcc
			xConsTxt = "[홈페이지]전화번호변경됨(" + sCellB + ">>)";
			ds_list_dtl.setColumn(0,"cell",sCellA);
		break;
				
		case "cival" :
			var sCivalA = tab_Main.tab01.edt_web_ci.value; //ci web
			var sCivalB = tab_Main.tab01.edt_mb_ci.value; //ci dlcc
			xConsTxt = "[홈페이지]CI값변경됨";
			ds_list_dtl.setColumn(0,"ci",sCivalA);			
		break;	
					
		case "home" :
			var sHomeZip = tab_Main.tab01.edt_web_zip.value; //home web
			var sHomeAddr = tab_Main.tab01.edt_web_addr.value; //home
			var sHomeAddr2 = tab_Main.tab01.edt_web_addr2.value; //home
			
			var sHomeZipB = tab_Main.tab01.edt_mb_zip.value; //home web
			var sHomeAddrB = tab_Main.tab01.edt_mb_addr.value; //home
			var sHomeAddr2B = tab_Main.tab01.edt_mb_addr2.value; //home			
												
			xConsTxt = "[홈페이지]주소변경됨(" + sHomeZipB + "/" +  sHomeAddrB + "/" + sHomeAddr2B + ">>)";
			ds_list_dtl.setColumn(0,"home_zip",sHomeZip);			
			ds_list_dtl.setColumn(0,"home_addr",sHomeAddr);	
			ds_list_dtl.setColumn(0,"home_addr2",sHomeAddr2);				
		break;	
		case "use_n" :
			ds_list_dtl.setColumn(0,"use_yn","N");			
		break;
		case "use_y" :
			ds_list_dtl.setColumn(0,"use_yn","Y");			
		break;		
		case "comp" :
			var sAccntNo = tab_Main.tab01.edt_accnt_no.value; //accnt_no
			var sStat = tab_Main.tab01.edt_send_stat.value; //accnt_no
			if(gfn_isNull(sAccntNo)){
				alert("선택된 회원번호가 없습니다.");
				return false;
			}			
			if(gfn_isNull(sStat) || sStat == "Y"){
				alert("업데이트할 대상이 없습니다.");
				return false;
			}			
		break;				
	}
			
	return true;
}

function SetComponent(){
	var sMemNo = tab_Main.tab01.edt_mem_no.value; // mem_no
			
	if (gfn_isNull(sMemNo)){	
		tab_Main.tab01.btn_mem_no.visible = true;
		tab_Main.tab01.btn_email.visible = false;
		tab_Main.tab01.btn_cell.visible = false;
		tab_Main.tab01.btn_home.visible = false;
		tab_Main.tab01.btn_cival.visible = false;
	} else {	
		var sEmailA = tab_Main.tab01.edt_web_email.value; //email web
		var sEmailB = tab_Main.tab01.edt_mb_email.value; //email dlcc		
		var sCellA = tab_Main.tab01.edt_web_cell.value; //cell web
		var sCellB = tab_Main.tab01.edt_mb_cell.value; //cell dlcc	
		var sCivalA = tab_Main.tab01.edt_web_ci.value; //ci web
		var sCivalB = tab_Main.tab01.edt_mb_ci.value; //ci dlcc
		var sHomeZip = tab_Main.tab01.edt_web_zip.value; //home web
		var sHomeAddr = tab_Main.tab01.edt_web_addr.value; //home
		var sHomeAddr2 = tab_Main.tab01.edt_web_addr2.value; //home
		var sHomeZipB = tab_Main.tab01.edt_mb_zip.value; //dlcc zip
		var sHomeAddrB = tab_Main.tab01.edt_mb_addr.value; //home
		var sHomeAddr2B = tab_Main.tab01.edt_mb_addr2.value; //home		
		
		//email 정보 다름
		if(sEmailA != sEmailB){
			tab_Main.tab01.btn_email.visible = true;
		} else {
			tab_Main.tab01.btn_email.visible = false;
		}
		//cell 정보 다름
		if(sCellA != sCellB){
			tab_Main.tab01.btn_cell.visible = true;
		} else {
			tab_Main.tab01.btn_cell.visible = false;
		}
		//ci 정보 다름
		if(sCivalA != sCivalB){
			tab_Main.tab01.btn_cival.visible = true;
		} else {
			tab_Main.tab01.btn_cival.visible = false;
		}
		
		var sHomeAddr = sHomeZip + " " + sHomeAddr + " " + sHomeAddr2;
		var sHomeAddrB = sHomeZipB + " " + sHomeAddrB + " " + sHomeAddr2B;
		if(sHomeAddr != sHomeAddrB){
			tab_Main.tab01.btn_home.visible = true;
		} else {
			tab_Main.tab01.btn_home.visible = false;
		}		
	}
	//계정상태값 확인 
	var sUseYn = ds_list_dtl.getColumn(0,"use_yn");
	var sSearchGbn = cbo_search_gbn.value;
	if (sSearchGbn == '03'){
		tab_Main.tab01.btn_useyn.text = "휴면해제";		
		tab_Main.tab01.stc_useyn.visible = false;
	} else {	
		if(sUseYn == 'Y'){
			tab_Main.tab01.btn_useyn.text = "계정차단";
			tab_Main.tab01.stc_useyn.visible = false;
		} else {
			tab_Main.tab01.btn_useyn.text = "계정허용";
			tab_Main.tab01.stc_useyn.visible = true;
		}
	}
	
	var iPaycnt = ds_pay_list.rowcount;
	if(iPaycnt <= 0) {
		tab_Main.tab01.btn_paychange.enable = false;		
	} else {
		tab_Main.tab01.btn_paychange.enable = true;		
	}
}

