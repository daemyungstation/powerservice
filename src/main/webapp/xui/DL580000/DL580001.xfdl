function fn_changedColumn(){	
	var arrText = "";
	var asisData = "";
	var tobeData = "";
	var returnVal = false;
	for(var i=0; i<ds_dtl_list.colcount; i++){
		asisData = ds_dtl_list.getOrgColumn(0,i);	
		tobeData = ds_dtl_list.getColumn(0,i);		
