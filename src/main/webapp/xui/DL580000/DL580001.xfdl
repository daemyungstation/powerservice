		btn_excl.visible = false;
	}
	//fn_commSearch(gfn_getCommTypeInit(), "getMallMemberList"); // 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
// 			case "selectDlwCode" :
// 				gfn_getCode("0114", "SELECT", ds_kstbit, gds_DmlCd); // 코드조회결과 필터
// 			break;
			
			case "getMallMemberList": // 특수회원 조회
				gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_list); // 페이지건수 표출
				trace("tc_list : " + tc_list);
			
				// 조회결과 존재시 하단 정보창 DS COPY
				if(ds_list.rowcount > 0){
					ds_dtl_list.clearData();
					ds_dtl_list.addRow();
					ds_dtl_list.copyRow(0, ds_list, ds_list.rowposition);
					
					// 수정, 삭제버튼 enable
					btn_update.enable = true;
					//btn_delete.enable = true;
				} else {
					alert("조회 내역이 없습니다.");
					ds_dtl_list.clearData();
					
					// 수정, 삭제버튼 disable
					btn_update.enable = false;
					//btn_delete.enable = false;
				}
				
				g_mode = "";
				// 컴포넌트 컨트롤
				fn_controlComponents();
			break;
			
			case "updateMallMember": // 저장처리
				if(gfn_isNull(g_reason_msg)) {
					alert("저장 완료");
					fn_commSearch(gfn_getCommTypeInit(), "getMallMemberList"); // 조회
				} else {
					alert(g_reason_msg); // 실패 메시지 리턴
					g_reason_msg = "";
				}
			break;
			
			case "getMallMemberListExcel": // 엑셀
				//gfn_exportGridToExcel(grd_list, "특수고객청구관리_"+gfn_getTodayFull()+"");
				
				//var colDS = gfn_excelPrepareExcelDS("grd_list");
				//gfn_excelDown("특수고객청구관리_"+gfn_getTodayFull()+"", "XLS_DLCC", "DlwPayMap.getSpecialAcntList", "DlwPayMap.getCntSpecialAcntList", ds_inqCndt, true, colDS, true);
				
				gfn_exportGridToExcel(grd_list_excel, "특수고객청구관리_"+gfn_getTodayFull()+"");
			break;
				
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list.keystring = ""; // 정렬컬럼 초기화
		
		if(gfn_isNull(edt_srch_accnt_no.value)) {
			if(gfn_isNull(cal_srchSttDt.value) || gfn_isNull(cal_srchEndDt.value)) {
				alert("검색기간은 필수항목 입니다.");
				return false;
			}
			if(gfn_getDayDiff(cal_srchSttDt.value, cal_srchEndDt.value) > 7) {
				alert("날짜 범위는 7일을 초과 할 수 없습니다.");
				return false;
			}
			if(gfn_getDayDiff(cal_srchSttDt.value, cal_srchEndDt.value) < 0) {
				alert("시작일은 종료일을 초과 할 수 없습니다.");
				return false;
			}
		}		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
		ds_inqCndt.setColumn(0, "stt_dt", cal_srchSttDt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srchEndDt.value);
		//ds_inqCndt.setColumn(0, "bankr_gbn", cbo_bankr_gbn.value); // B-파산, R-회생
		ds_inqCndt.setColumn(0, "del_fg", cbo_delFg.value);
		ds_inqCndt.setColumn(0, "orderBy", "GOODS_ID");
		ds_inqCndt.setColumn(0, "orderDirection", "ASC");
		ds_inqCndt.setColumn(0, "sController", "");
		ds_inqCndt.setColumn(0, "dsName", "");
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회
function fn_commSearch(sType, searchInfo)
{
    switch(searchInfo) {
    
		// 조회
        case "getMallMemberList":
			if (!fn_preSearch(sType)) return false;
			
			// 호출 건수 설정 및 조회여부 설정
			if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 200, tc_list)) return;
		
			var szSvcid       = "getMallMemberList";
			var szController  = "/dlw/mallMng/getMallMemberList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
		
		// 엑셀 조회
        case "getMallMemberListExcel":
			var szSvcid       = "getMallMemberListExcel";
			var szController  = "/dlw/mallMng/getMallMemberList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_excel=ds_output";
			var szParam       = "excel_yn=Y";
			var szRetCallback = "fn_callBack";
		break;
	}

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{
	edt_srch_accnt_no.value = "";
	//cbo_bankr_gbn.index = 0; // 사유 : 전체
	cbo_delFg.index = 0; // 삭제여부 : 아니오
	
	var sToday = gfn_getToday();
	var sStartDate = gfn_addDate(sToday, -7);
	
	cal_srchSttDt.value = sStartDate;
	cal_srchEndDt.value = sToday;
	
	if(GV_SYSTEMDIV == "DLMNG") 
	{
		btn_update.visible = true;
	}
	else
	{
		btn_update.visible = true;
		
		var sOpenerName = this.getOwnerFrame().getOwnerFrame().name;
		
		if(sOpenerName == "initForm")
	    {
	        xAccntNo = pAccntNo; // 팝업으로 열릴경우
	        edt_srch_accnt_no.value = xAccntNo;
	        fn_commSearch(gfn_getCommTypeInit(), "getMallMemberList"); // 조회
	    }
	    else
	    {
	        xAccntNo = ""; // 프레임 탭으로 열릴경우 (상담화면에선 "FrameSet2" 관리화면에선 "FrameSet1")
	    }
	}
}

