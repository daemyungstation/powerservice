{
	ds_evltPlan.clearData();
	ds_evltPlan.addRow();
	ds_list_evlpInfo.clearData();
	ds_list_trprInfo.clearData();
	
	ds_evltPlan.setColumn(0, "evlt_prgr_stat_cd", "10");
	
	fn_saveMode();
}

//수정버튼클릭
function btn_update_onclick(obj:Button,  e:ClickEventInfo)
{
	btn_status.visible = false;
	btn_save.visible = true;
	btn_cancel.visible = true;
	
	edt_evlt_plan_nm2.readonly = false; //평가계획명
	var sStatCd = ds_list_evltPlan.getColumn(ds_list_evltPlan.rowposition, "evlt_prgr_stat_cd");
	if(sStatCd == "10"){
		msk_evlt_bas_yrmn.readonly = false;//평가월
	}
	msk_dist_end_dt.readonly = false;//이의제기마감
	edt_goal_call_cnt.readonly = false//목표콜
}

//취소버튼클릭
function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_readMode();
	fn_commSearch2(gfn_getCommTypeInit());
}

//저장버튼클릭
function btn_Save_onclick(obj:Button,  e:ClickEventInfo)
{
	if(gfn_isNull(edt_evlt_plan_nm2.value)){
		alert("평가계획명을 입력하세요.");
		edt_evlt_plan_nm2.setFocus();
		return false;
	}
	if(gfn_isNull(edt_evst_nm.value)){
		alert("평가지명을 선택하세요.");
		return false;
	}
	if(gfn_isNull(msk_evlt_bas_yrmn.value)){
		alert("평가월을 선택하세요.");
		msk_evlt_bas_yrmn.setFocus();
		return false;
	}
	if(gfn_isNull(msk_dist_end_dt.value)){
		alert("이의제기마감일을 선택하세요.");
		msk_dist_end_dt.setFocus();
		return false;
	}
	if(gfn_isNull(edt_goal_call_cnt.value)){
		alert("목표콜을 입력하세요.");
		edt_goal_call_cnt.setFocus();
		return false;
	}
	
	fn_save();
}

//진행상태 변경버튼 클릭
function btn_status_onclick(obj:Button,  e:ClickEventInfo)
{
	if (!confirm("평가계획 진행상태를 변경하시겠습니까?")) {
		return false;
	}
	
	var sStatCd = ds_evltPlan.getColumn(0, "evlt_prgr_stat_cd");
	if(sStatCd == "20"){//진행
		//평가완료된 건수 체크
		fn_checkFinishCnt();
	}else if(sStatCd == "30"){//이의제기
		//이의제기 건수 체크
		fn_request();
	}else{
		ds_evltPlan.setColumn(0, "evlt_prgr_stat_cd", toNumber(sStatCd) + 10);
		fn_save();
	}
	
}

//상단 삭제버튼 클릭
function btn_Delete_onclick(obj:Button,  e:ClickEventInfo)
{
	var nDeleteCnt = ds_list_evltPlan.getCaseCount("rowCheck=='1' && evlt_prgr_stat_cd == '10'");
	if (nDeleteCnt < 1) {
		alert("삭제할 평가계획을 선택하세요.");
		return;
	}	
	
	if (!confirm("삭제하시겠습니까?")) {
		return false;
	}
	
	fn_delete();
}

