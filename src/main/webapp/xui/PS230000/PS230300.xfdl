// Search Validate Check
function fn_preSearch(sType:String) {
	switch(sType) {
	case gfn_getCommTypeInit() :
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "evlt_plan_nm", edt_evlt_plan_nm.value);//평가지명
		ds_inqCndt.setColumn(0, "evlt_prgr_stat_cd", Combo_statusTypCd.value);//진행상태
		break;
	}
	
	return true;
}

// Search2 Validate Check
function fn_preSearch2(sType:String) {
	switch(sType) {
	case gfn_getCommTypeInit() :
		ds_inqCndt_left.clearData();
		ds_inqCndt_left.addRow();
		//평가계획ID
		ds_inqCndt_left.setColumn(0, "evlt_plan_id", ds_list_evltPlan.getColumn(ds_list_evltPlan.rowposition, "evlt_plan_id"));
		break;
	}
	
	return true;
}

// Search3 Validate Check
function fn_preSearch3(sType:String) {
	var bResult = true;
	switch(sType) {
	case gfn_getCommTypeInit() :
		var sEvlpId = ds_list_evlpInfo.getColumn(ds_list_evlpInfo.rowposition, "evlp_id");
		if (!gfn_isNull(sEvlpId)) {
			ds_inqCndt_right.clearData();
			ds_inqCndt_right.addRow();
			//평가계획ID
			ds_inqCndt_right.setColumn(0, "evlt_plan_id", ds_list_evltPlan.getColumn(ds_list_evltPlan.rowposition, "evlt_plan_id"));
			ds_inqCndt_right.setColumn(0, "evlp_id", sEvlpId);
		} else {
			bResult = false;
		}
		break;
	}
	
	return bResult;
}

