    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_cnt = 0; // 전체건수
var request_cnt= 0; // 이의제기 건수

var bRgsn = false;
var bAmnt = false;
var bDltn = false;

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS230300_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_getList("cd","EVL010");   //코드조회
	
	gfn_athrPgm(gfn_gdsCd_admin()); //권한조회
	bRgsn = "Y" == ds_athrPgmFilter.getColumn(0, 'rgsn_yn');
	bAmnt = "Y" == ds_athrPgmFilter.getColumn(0, 'amnt_yn');
	bDltn = "Y" == ds_athrPgmFilter.getColumn(0, 'dltn_yn');
	
	fn_commSearch(gfn_getCommTypeInit());
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
		case "selectCode":
			gfn_getCode("EVL010", "ALL", ds_statusTypCd); //진행상태
			Combo_statusTypCd.index = 0;
			break;
		
		case "getEvltPlanList":
			gfn_appendSearchDs(ds_list_evltPlan, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_evltPlan, sta_totalCnt, tc_cnt); // 페이지건수 표출
			
			fn_readMode();//읽기모드
			break;	
		
		case "getEvlpInfoList":
			ds_list_evlpInfo.addColumn("rowCheck", "string", 256);
			fn_commSearch3(gfn_getCommTypeInit());
			break;	
			
		case "getEvltTrprInfolist":
			ds_list_trprInfo.addColumn("rowCheck", "string", 256);
			break;		
		
		case "saveEvltPlan":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "deleteEvltPlan":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "getUserFinishCount":
			//trace(ds_list_tmp.saveXML());
			gfn_appendSearchDs(ds_list_userFinish, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			if(ds_list_userFinish.rowcount > 0){
				var sUser = "";
				//alert((ds_list_userFinish.rowcount));
				if(ds_list_userFinish.rowcount == 1){
					sUser = ds_list_userFinish.getColumn(0, "user_nm");
				}else if(ds_list_userFinish.rowcount > 1){
					for(i=0; i<ds_list_userFinish.rowcount; i++){
						sUser += ds_list_userFinish.getColumn(i, "user_nm") + ", ";
					}
				}
				alert("평가자 [ " + sUser + " ] 이 평가중입니다.");
			}else{
				var sStatCd = ds_evltPlan.getColumn(0, "evlt_prgr_stat_cd");
				ds_evltPlan.setColumn(0, "evlt_prgr_stat_cd", ds_statusTypCd.getColumn(ds_statusTypCd.findRow("cd", ds_evltPlan.getColumn(0, "evlt_prgr_stat_cd"))+1, "cd"));
				fn_save();
			}
			
			break;
		
		case "requestEvltAckdDtl":
			if(request_cnt > 0){
				alert("이의제기 요청을 처리하여주십시요.");
			}else{
				var sStatCd = ds_evltPlan.getColumn(0, "evlt_prgr_stat_cd");
				ds_evltPlan.setColumn(0, "evlt_prgr_stat_cd", ds_statusTypCd.getColumn(ds_statusTypCd.findRow("cd", ds_evltPlan.getColumn(0, "evlt_prgr_stat_cd"))+1, "cd"));
				fn_save();
			}
			break;
		
		case "deleteEvlpInfo":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "deleteTrprInfo":
			fn_commSearch3(gfn_getCommTypeInit());
			break;
		}	
 	}		
}

