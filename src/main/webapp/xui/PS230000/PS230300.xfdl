//평가자목록 삭제
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	var nDeleteCnt = ds_list_evlpInfo.getCaseCount("rowCheck=='1'");            
	if (nDeleteCnt < 1) {
		alert("삭제할 평가자를 선택하십시요.");
		return;
	}	
	
	if (!confirm("평가자를 삭제하시겠습니까?")) {
		return false;
	}
	
	fn_delete2();
}

