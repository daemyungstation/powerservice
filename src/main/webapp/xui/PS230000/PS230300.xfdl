/********************************************************************************************/
/**   6. 사용자함수
/********************************************************************************************/
//읽기모드
function fn_readMode()
{
	btn_status.visible = false;
	btn_save.visible = false;
	btn_cancel.visible = false;
	btn_evstSearch.visible = false;
	
	edt_evlt_plan_nm2.readonly = true; //평가계획명
	edt_evst_nm.readonly = true;//평가지명
	msk_evlt_bas_yrmn.readonly = true;//평가월
	msk_dist_end_dt.readonly = true;//이의제기마감
	edt_goal_call_cnt.readonly = true//목표콜
	

	var bStat = "10" == ds_list_evltPlan.getColumn(ds_list_evltPlan.rowposition, "evlt_prgr_stat_cd");	
	
	ds_evltPlan.clearData();
	ds_evltPlan.addRow();
	
	if (ds_list_evltPlan.rowposition > -1) {
		ds_evltPlan.copyRow(0, ds_list_evltPlan, ds_list_evltPlan.rowposition);
		
		var sStatCd = ds_list_evltPlan.getColumn(ds_list_evltPlan.rowposition, "evlt_prgr_stat_cd");
		var nRow = ds_statusTypCd.findRow("cd", sStatCd);
		if (ds_statusTypCd.rowcount > nRow+1 ) {
			btn_status.text = ds_statusTypCd.getColumn(nRow+1, "cd_nm");
			btn_status.visible = bAmnt && true;
		} else {
			btn_status.text = "";
			btn_status.visible = bAmnt && false;
		}

		//btn_update.visible = bAmnt && bStat && true;
		btn_new.visible = bRgsn && bStat && true;
		btn_new2.visible = bRgsn && !btn_new.visible;
		
		fn_commSearch2(gfn_getCommTypeInit());
	} else {
	//	btn_update.visible = bAmnt  && false;
		btn_new.visible = bRgsn && false;
		btn_new2.visible = bRgsn && !btn_new.visible;
	}
}

//저장모드
function fn_saveMode()
{
	btn_save.visible = true;
	btn_cancel.visible = true;
	btn_evstSearch.visible = true; //평가지명 조회버튼
	
	btn_new.visible = false;
	btn_new2.visible = false;
//	btn_update.visible = false;
	btn_status.visible = false;
	
	edt_evlt_plan_nm2.readonly = false; //평가계획명
	edt_evst_nm.readonly = false;//평가지명
	msk_evlt_bas_yrmn.readonly = false;//평가월
	msk_dist_end_dt.readonly = false;//이의제기마감
	edt_goal_call_cnt.readonly = false//목표콜
	
	ds_list_evlpInfo.clearData();
	ds_list_trprInfo.clearData();
}
