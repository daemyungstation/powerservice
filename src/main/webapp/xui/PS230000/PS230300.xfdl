// edit onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		fn_commSearch(gfn_getCommTypeInit());
	}
}
function ds_event(obj:Dataset,  e)
{
	var bRowCnt = obj.rowcount > 0;
	var bRowPos = obj.rowposition > -1;
	
	var bStat = "10" == ds_list_evltPlan.getColumn(ds_list_evltPlan.rowposition, "evlt_prgr_stat_cd");
	switch(obj.name) {
	case "ds_list_evltPlan" :
		btn_Delete.visible = bDltn && bRowCnt;
		break;
	
	case "ds_list_evlpInfo" :
		btn_delete.visible = bDltn && bStat && bRowCnt;
		
		btn_add2.visible = bRgsn && bStat && bRowPos;
		break;
		
	case "ds_list_trprInfo" :
		btn_delete2.visible = bDltn && bRowCnt;
		break;
		
	case "ds_evltPlan" :
		btn_add.visible = bRgsn && bStat && !gfn_isNull(obj.getColumn(obj.rowposition, "evlt_plan_id"));
		break;
	}
	// trace("Dataset >>" + obj.name + ", event >> " + obj.rowposition);
}

