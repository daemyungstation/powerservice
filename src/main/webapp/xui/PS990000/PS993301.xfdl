		} else if ("btn_accnt_no" == obj.name || "edt_accnt_no" == obj.name){ // 회원번호
			srch_typ = "accnt_no";
		} else if ("btn_mem_no" == obj.name || "edt_mem_no" == obj.name){ // 고유번호
