    <Script type="xscript4.0"><![CDATA[/**************************************************************************
 [ PART 1 ]공통 js 파일 include
***************************************************************************/
include "LIB::lib_All.xjs";

/**************************************************************************
 [ PART 2 ] Form에서 사용될 전역변수를 선언
**************************************************************************/
var tc_pur = 0;

/***************************************************************************
 * 함  수  명		: Empl_onload()
 * 기      능		: 폼 onload이벤트 
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function PS993402_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/***************************************************************************
 * 함  수  명		: btn_srch_onclick()
 * 기      능 		: 조회버튼 클릭
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/***************************************************************************
 * 함  수  명		: fn_init()
 * 기      능 		: 초기화
 ***************************************************************************/
function fn_init()
{	
	edt_srch_com_nm.value = "";
}

/***************************************************************************
 * 함  수  명		: fn_preSearch()
 * 기      능		: 조회 전 처리 조회조건 설정
 * 수  정  일      : 2016.01.22 
 ***************************************************************************/
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_purComp.keystring = ""; // 정렬컬럼 초기화
	
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "cd_nm", edt_srch_com_nm.value);
		//ds_inqCndt.setColumn(0, "orderBy", "cd");
		// 매입처명으로 정렬
		ds_inqCndt.setColumn(0, "orderBy", "cd_nm");
		ds_inqCndt.setColumn(0, "grp_cd", "S021");
	}
	return true;
}

/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능 		: 조회
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function fn_commSearch(sType)
{	
    if (!fn_preSearch(sType)) return false;	
	
	// 호출 레코드 수
	if(!gfn_pageSearch(sType, ds_purComp, sta_TotalCnt, 20, tc_pur)) return;
	
	var szSvcid       = "selectPurList";
    var szController  = "/dlw-syst/cd/cd-list";
    var szInDs        = "ds_input=ds_inqCndt";
    //var szOutDs       = "ds_purComp=ds_output";
    var szOutDs       = "ds_purComp_temp=ds_output";
    var szParam       = "svcid=selectPurList";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/***************************************************************************
 * 함  수  명		: fn_srchCallBack()
 * 기      능		: 조회 콜백 처리
 ***************************************************************************/
//function fn_srchCallBack(svcid, errcd, errmsg) {
function fn_callBack(svcid, errcd, errmsg) {
  
    if (errcd < 0) {
		alert("조회를 실패하였습니다.\n" + errmsg);
    } else {
		switch(svcid) {
			case "selectPurList": // 공통코드 조회 callBack Service ID
			    gfn_appendSearchDs(ds_purComp, ds_purComp_temp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_purComp, sta_TotalCnt, tc_pur); // 페이지건수 조회 처리
			break;
		}
    }
}

/***************************************************************************
 * 함  수  명		: fn_postSearch()
 * 기      능		: 처리결과
 ***************************************************************************/
 /*
