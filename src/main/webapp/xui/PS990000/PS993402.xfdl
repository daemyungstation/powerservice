{
	if(ds_purComp.rowcount < 1) {
		alert("조회 결과가 없습니다.");
	    return;
	} else {
		grd_list.setFocus();
	}
	
	// 팝업 파라미터가 존재하고 조회결과가 1건인 경우 바로 선택
	if (!gfn_isNull(gv_com_nm) && ds_purComp.rowcount == 1) {
		fn_selGridRow();
	}
}
*/

/***************************************************************************
 * 함  수  명		: fn_selGridRow()
 * 기      능		: 그리드 행 선택
 ***************************************************************************/
function fn_selGridRow()
{
	var com_cd = ds_purComp.getColumn(ds_purComp.rowposition, "cd");
	var com_nm = ds_purComp.getColumn(ds_purComp.rowposition, "cd_nm");
	
	opener.gv_pur_comp_cd = com_cd;
	opener.edt_pur_comp_nm.value = com_nm;
	//opener.gv_pur_comp_cd = cd;
	//opener.edt_pur_comp_nm.value = cd_nm;
	close();
}

/***************************************************************************
 * 함  수  명		: btn_close_onclick()
 * 기      능		: x 버튼 이벤트
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

/***************************************************************************
 * 함  수  명		: btn_init_onclick()
 * 기      능		: 초기화 버튼클릭시
 * 수  정  일      : 2016.01.22
 ***************************************************************************/
function btn_Init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

/***************************************************************************
 * 함  수  명		: grd_list_oncellclick()
 * 기      능		: 선택 버튼 이벤트
 ***************************************************************************/
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.cell == "3") {
	    // 그리드 행 선택
		fn_selGridRow();
	}
}

/***************************************************************************
 * 기      능		: (공통) 그리드 onheadclick이벤트 발생시에 전체체크(체크박스), 칼럼정렬 기능 추가
 ***************************************************************************/
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/dlw-syst/cd/cd-list");
	ds_inqCndt.setColumn(0, "dsName", "ds_output");
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_purComp, sta_TotalCnt, 20, tc_pur)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_purComp, sta_TotalCnt, tc_pur); // 페이지건수 표출
}

/***************************************************************************
 * 기      능		: (공통) 그리드 칼럼사이즈 변경시 Hidden 처리
						단, 칼럼사이즈를 0으로 변경한 후에는 공통기능버튼
							(칼럼 보이기/숨기기)으로 다시 보이기 처리
 ***************************************************************************/
function grd_list_oncolresized(obj:Grid, e:GridSizeChangedEventInfo)
{
	if(e.oldvalue == 0) {
		obj.setRealColSize(e.formatindex, 0);
	}
}

/***************************************************************************
 * 기      능		: (공통) 엔터 치면 조회실행
 ***************************************************************************/
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) {
		fn_commSearch(gfn_getCommTypeInit());
	}
}

/***************************************************************************
 * 기      능		: (공통) 그리드 마지막행 이후 스크롤시 Append 조회 처리
						페이징처리 대상인 경우만 해당
 ***************************************************************************/
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

//onactivate
function PS993402_onactivate(obj:Form, e:ActivateEventInfo)
{
	if (!gfn_isNull(this.userParam)) {
		gv_com_nm = this.userParam.p_com_nm;
		edt_srch_com_nm.value = gv_com_nm;
		fn_commSearch(gfn_getCommTypeInit()); // 조회
	
		// 폼에 parameter 초기화한다.
		this.userParam = null;
	}
}

function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_resetColsize(grd_list);
}

// 엔터시 자동선택/입력
function grd_prodList_onkeydown(obj:Grid, e:KeyEventInfo)
{
	if (e.keycode == 13 && ds_purComp.rowcount > 0) {
		fn_selGridRow();
	}
}
]]></Script>
