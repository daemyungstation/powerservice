function PS996100_onload(obj:Form, e:LoadEventInfo)
{   
	 if ( p_gubun =="2" ) {    // 값이있으면 조회
	     report_grp_nm.value =p_authgrpnm;
	     use_yn.value =p_useyn;
	     report_grp_cd.value =p_authgrpcd;
	     Pv_gubun=p_gubun;
	     lvl.value  =p_lvl
	 } else {                   // 값이없으면 신규
	     Pv_gubun=p_gubun;
	 }
	
 } 

function fn_callBack(svcid, errcd, errmsg) 
{   
     
	if (errcd != 0) {
		return;
	}
     
	switch (svcid) {
		
		case "report_grp_insert":	//
		   alert("자료권한그룹 저장 되었습니다.");
			close();
			break;
			
	
	}
}

function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	 
    var szSvcid = ""
    var szController = ""
    var szInDs = ""
    var szOutDs = ""
    var szParam = ""
    var szRetCallback = ""
    
	 ds_in.clearData();
	 ds_in.addRow();
	 ds_in.setColumn(0,"report_grp_cd", report_grp_cd.value);
	 ds_in.setColumn(0,"report_grp_nm", report_grp_nm.value);
	 ds_in.setColumn(0,"useyn", use_yn.value);
	 ds_in.setColumn(0,"gubun", Pv_gubun);
	 ds_in.setColumn(0,"lvl", lvl.value);

	 szSvcid       = "report_grp_insert";
	 szController  = "/dlw/dept/report_grp_insert";
	 szInDs        = "ds_input=ds_in";
	 szOutDs       = ""
	 szParam       = "";
	 szRetCallback = "fn_callBack";
		
		// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}



//조회 결과
function fn_postSearch()
{
	
		if(ds_in.rowcount < 1) {
