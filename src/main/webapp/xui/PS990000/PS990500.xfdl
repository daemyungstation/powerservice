/********************************************************************************************/
/* 1. Include Library include                                                               */
/********************************************************************************************/
include "LIB::lib_All.xjs";




/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트,전역변수,초기화) 처리                         	*/
/********************************************************************************************/
var total_count = 0;

//온로드
function PS990500_onload(obj:Form, e:LoadEventInfo)
{
	gfn_getList("cd","KMS030","fn_callBack",null,"srchCdSet"); 
	gfn_createPagingDataset(obj); 
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

//초기화
function fn_init()
{
	cbo_bswr_div.index = 0;
	cbo_srch_word.index = 0;
	edt_srch_word.value = null;
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
	 if (errcd != 0) {
		trace(errcd);
		return false;
	} else {
		switch(svcid){
			case "srchCdSet" : //코드셋 코드 콜백
			gfn_getCode("KMS030", "ALL", ds_bswr_div);
			cbo_bswr_div.index = 0;
			break;
			
			case "selectNobdList" ://조회 콜백
				gfn_appendSearchDs(ds_nobd_list, ds_list_tmp);
				gfn_setTotalRowCount(ds_nobd_list, sta_TotalCnt, total_count);
				fn_postSearch();	
			break;
		}
	}
}




/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType)
{
	//if(!gfn_preSearchDs(ds_ancm_mtr, sType)) return false;
	
	//TO-DO:조회조건 체크
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "searchBoard", cbo_srch_word.value);
	ds_inqCndt.setColumn(0, "nobd_bswr_div_cd", cbo_bswr_div.value);
	ds_inqCndt.setColumn(0, "word", edt_srch_word.value);
	ds_inqCndt.setColumn(0, "bzpt_div_yn", "Y");
	
	ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
	ds_inqCndt.setColumn(0, "orderDirection", "desc");

	return true;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
function fn_commSearch(sType)
{	
    if (!fn_preSearch(sType)) return false;	
	
	// 호출 레코드 수
	if(!gfn_pageSearch(sType, ds_nobd_list, sta_TotalCnt, 40, total_count)) return; 
	
	var szSvcid       = "selectNobdList";
    var szController  = "/knowledge/nobd/list/all";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}




/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//조회후 처리
function fn_postSearch()
{
	if(ds_nobd_list.rowcount < 1) {
		sta_TotalCnt.text = "조회 결과가 없습니다.";
	} else {

	}
	return;
}


/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
//버튼 클릭 이벤트 
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name){
		case "btn_Close" : // 타이틀 x 버튼
		close();
		break;
		
		case "btn_Search" : // 조회버튼
		fn_commSearch(gfn_getCommTypeInit()); // 조회
		break;
		
		case "btn_Init" : //초기화버튼
		fn_init();
		break;
	}
}

//페이징 
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

//정렬
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (!fn_preSearch()) return false;
	ds_inqCndt.setColumn(0, "sController", "/knowledge/nobd/list/all");
	ds_inqCndt.setColumn(0, "dsName", "ds_output");
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_nobd_list, sta_TotalCnt, 40, total_count)) return;
	gfn_grdHeadClick(obj, e, ds_inqCndt, null, ds_list_tmp ,ds_nobd_list);
	gfn_setTotalRowCount(ds_nobd_list,  sta_TotalCnt, total_count); // 페이지건수 표출
}

//그리드 더블클릭 이벤트
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var nobd_typ_id = ds_nobd_list.getColumn(grd_list.selectendrow, "nobd_typ_id"); 
	var nobd_id = ds_nobd_list.getColumn(grd_list.selectendrow, "nobd_id"); 

	var sPrmr = {nobd_typ_id:nobd_typ_id, nobd_id:nobd_id};	
	gfn_openDtailNobdPop(sPrmr,true,true);
}
]]></Script>
