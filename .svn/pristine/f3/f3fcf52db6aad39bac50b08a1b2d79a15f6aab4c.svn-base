/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
				// 메시지목록(알림톡) 탭 설정
				gfn_getCode("WEB130", "ALL", ds_chatCntsDivCdEdit); //채널구분
				gfn_getCode("WEB120", "SELECT", ds_chat_tmp_Edit); // 템플릿코드
				tab.tabpage1.cbo_chat_msg_typ_cd.index = 0;
				
				// 메시지목록(LMS) 탭 설정
				gfn_getCode("KMS070", "ALL", ds_chat_msg_typ_cd_lms); //메시지유형
				tab.tabpage2.cbo_chat_msg_typ_cd.index = 0;
 			break;
 			
 			case "getInfobankSmsRejectList":
				fn_postGetInfobankSmsRejectList();
 			break;
 			
 			case "getChatCntsList":
	
				gfn_appendSearchDs(ds_list_chatCnts, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_chatCnts, tab.tabpage1.sta_totalCnt, total_count); // 페이지건수 표출
			
				fn_mem_srch(); // 행사정보 조회
			
				/******************************************************
				2017-09-06 김준호 알림톡 가상계좌 데이터 가져오는 fn 작성
				*******************************************************/
				fn_vrtl_accnt_info();
			
			break;
			
			case "getChatCntsList_lms":
				//trace(ds_list_tmp.saveXML());
				gfn_appendSearchDs(ds_list_chatCnts_lms, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_chatCnts_lms, tab.tabpage2.sta_totalCnt, total_count); // 페이지건수 표출
				
				fn_mem_srch_2(); // 회원정보 조회
			break;
			
			case "sendMsg":
				alert("발송되었습니다.");
				fv_reject_include_cnt = -1; // 수신거부 체크 플래그 초기화
			break;
			
			case "getEvntChatInfo":
				//trace(ds_meminfo.saveXML());
			break;
			/************************************
			2017-09-06 김준호 알림톡 가상계좌 콜백 확인
			************************************/
			case "getVrtlChatInfo":
				// 데이터셋 결과 확인
				//alert(ds_vrtlinfo.saveXML());
			
			break;
			
 		}
 	}
} 		

