//tab 변경시
function tab_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{
	if(obj.tabindex == "0"){
		sendmode = "single";//단건
		edt_clph_no.enable = true;
		edt_cust_nm.enable = true;
		ds_list_excel.clearData();//tab전화시 그리드 초기화
		
		chk_kakao.value = true;
		chk_kakao.visible = true;
		chk_lms.value = false;
		chk_lms.visible = true;
		chk_lms.enable = true;
		chk_lms.position2 = "absolute l:689 w:100 t:36 h:20";
		
		// 메시지, 발신번호 수정 불가
		if(chk_kakao.value ==true) {
			TextArea_chat_msg_cntn.readonly = true;
			edt_cntr_rprs_tlno.enable = false;
		}
	}else if(obj.tabindex == "1"){ // 메시지목록(LMS) 
		sendmode = "single";//단건
		edt_clph_no.enable = true;
		edt_cust_nm.enable = true;
		ds_list_excel.clearData();//tab전화시 그리드 초기화
		
		// 메시지, 발신번호 수정 가능
		TextArea_chat_msg_cntn.readonly = false;
		edt_cntr_rprs_tlno.enable = true;
		
		chk_kakao.value = false;
		chk_kakao.visible = false;
		//chk_lms.visible = false;
		chk_lms.value = true;
		chk_lms.enable = false;
		chk_lms.position2 = "absolute l:577 w:100 t:36 h:20";
	}else{
		sendmode = "multi";//일괄(동보)
		edt_clph_no.value = "";
		edt_cust_nm.value = "";
		edt_clph_no.enable = false;
		edt_cust_nm.enable = false;
		
		chk_kakao.visible = true;
		chk_lms.visible = true;
		chk_lms.enable = true;
		chk_lms.position2 = "absolute l:689 w:100 t:36 h:20";
		
		// 메시지목록(LMS) 에서 작성 후, 일괄발송시 LMS로 표시
		if (e.preindex == 1) {
			chk_lms.value = true;
			chk_kakao.value = false;
		}
		
		// 일괄통보 탭 변경이전값을 가지고 서비스 분리 (send or newsend)
		g_broadcast_tab_gb = e.preindex;
		//trace("g_broadcast_tab_gb : " + g_broadcast_tab_gb);
	}
	
	//trace("e.preindex : " + e.preindex   +  "    /    " + "e.postindex : " + e.postindex  );
	// 알림톡 <----------> LMS  변경시 메시지내용 및 발신번호 초기화
	if( e.preindex == 0 && e.postindex == 1 
	 || e.preindex == 1 && e.postindex == 0 ) 
	{
		TextArea_chat_msg_cntn.value = "";
		edt_cntr_rprs_tlno.value = "";
	}
}

