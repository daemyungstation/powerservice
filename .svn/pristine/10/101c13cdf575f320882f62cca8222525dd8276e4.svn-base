    <Script type="xscript4.0"><![CDATA[/**************************************************************************
 [ PART 1 ]공통 js 파일 include
***************************************************************************/
include "LIB::lib_All.xjs";

/**************************************************************************
 [ PART 2 ] Form에서 사용될 전역변수를 선언
**************************************************************************/
var tc_bank = 0;

var gv_bank_nm = "";

/***************************************************************************
 * 함  수  명		: bank_onload()
 * 기      능		: 폼 onload이벤트 
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function bank_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	
	if(!gfn_isNull(p_bank_nm)) {
		gv_bank_nm = p_bank_nm;
		bank_nm.value = gv_bank_nm;
		p_bank_nm = null;
	}
	
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/***************************************************************************
 * 함  수  명		: btn_srch_onclick()
 * 기      능 		: 조회버튼 클릭
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/***************************************************************************
 * 함  수  명		: fn_init()
 * 기      능 		: 초기화
 ***************************************************************************/
function fn_init(typ)
{	
	if (typ == "Y" && !gfn_isNull(p_bank_cd)) {
		bank_nm.value = p_cust_acnt_nm;
		fn_commSearch(gfn_getCommTypeInit());
	} else {
		bank_nm.value = "";
	}
}

/***************************************************************************
 * 함  수  명		: fn_preSearch()
 * 기      능		: 조회 전 처리 조회조건 설정
 * 수  정  일      : 2016.01.22 
 ***************************************************************************/
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_bank.keystring = ""; // 정렬컬럼 초기화
	
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "cd_nm", bank_nm.value);
		ds_inqCndt.setColumn(0, "grp_cd", "63");
		ds_inqCndt.setColumn(0, "orderBy", "cd");
		ds_inqCndt.setColumn(0, "orderDirection", "asc");
	}
	return true;
}

/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능 		: 조회
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function fn_commSearch(sType)
{	
    if (!fn_preSearch(sType)) return false;	
	
	// 호출 레코드 수
	if(!gfn_pageSearch(sType, ds_bank, sta_TotalCnt, 500, tc_bank)) return; 
	
	var szSvcid       = "selectBankList";
    var szController  = "/dlw-syst/cd/cd-list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "svcid=selectBankList";
    var szRetCallback = "fn_srchCallBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/***************************************************************************
 * 함  수  명		: fn_srchCallBack()
 * 기      능		: 조회 콜백 처리
 ***************************************************************************/
function fn_srchCallBack(svcid, errcd, errmsg) {
  
    if (errcd < 0) {
		alert("조회를 실패하였습니다.\n" + errmsg);
    } else {
		gfn_appendSearchDs(ds_bank, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		gfn_setTotalRowCount(ds_bank, sta_TotalCnt, tc_bank); // 페이지건수 조회 처리
		fn_postSearch();	
    }
}

/***************************************************************************
 * 함  수  명		: fn_postSearch()
 * 기      능		: 처리결과
 ***************************************************************************/
function fn_postSearch()
{
	if(ds_bank.rowcount < 1) {
		alert("조회 결과가 없습니다.");
	}
	return;
}

/***************************************************************************
 * 함  수  명		: btn_close_onclick()
 * 기      능		: x 버튼 이벤트
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

/***************************************************************************
 * 함  수  명		: btn_init_onclick()
 * 기      능		: 초기화 버튼클릭시
 * 수  정  일      : 2016.01.22
 ***************************************************************************/
function btn_Init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init("N");
}

/***************************************************************************
 * 함  수  명		: grd_list_oncellclick()
 * 기      능		: 선택 버튼 이벤트
 ***************************************************************************/
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.cell == "3"){
		var cd = ds_bank.getColumn(grd_bankList.selectendrow, "cd");
		var cd_nm = ds_bank.getColumn(grd_bankList.selectendrow, "cd_nm");
		
		var ds_prenInqCndt = new Dataset;
		ds_prenInqCndt.name = "ds_prenInqCndt";
		ds_prenInqCndt.addColumn("cd", "string");
		ds_prenInqCndt.addColumn("cd_nm", "string");
		
		var nRow = ds_prenInqCndt.addRow();
		ds_prenInqCndt.setColumn(nRow, "cd", cd);
		ds_prenInqCndt.setColumn(nRow, "cd_nm", cd_nm);
		
		close(ds_prenInqCndt);
		close();
	}
}

