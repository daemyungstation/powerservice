}

// 그리드 스크롤 이벤트
function tab_deductionMgmt_tp_createAndOtherCorp_grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend(), "selectResnDayCloseList");
}

// 엑셀시
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_list.getRowCount() > 0) 
	{
		if(excelGb == "1"){ // 해약환급데일리작업 테이블에 데이터가 없을때
			var colDS = gfn_excelPrepareExcelDS("grd_excel");
			gfn_excelDown("해약환급데일리작업", "XLS_DLCC", "DlwItoMngMap.getResnDayCloseList", "DlwItoMngMap.getResnDayCloseListCount", ds_cond, true, colDS, true);
		}else if(excelGb == "2"){ // TB_RESN_DAY_CLOSE (해약환급데일리작업 테이블에 데이터가 있을때)
			var colDS = gfn_excelPrepareExcelDS("grd_excel");
			gfn_excelDown("해약환급데일리작업", "XLS_DLCC", "DlwItoMngMap.getResnDayCloseList2", "DlwItoMngMap.getResnDayCloseListCount2", ds_cond, true, colDS, true);
		}													                                    
	}
	else
	{
		alert("엑셀다운로드 할 대상이 없습니다.");
	}
}

// 생성버튼 클릭시
function btn_insert_onclick(obj:Button,  e:ClickEventInfo)
{
	var yyyy = String(calResnProcDay.value).substring(0,4);
	var mm = String(calResnProcDay.value).substring(4,6);
	var dd = String(calResnProcDay.value).substring(6,8);

	var cf = confirm("해약처리일 " + yyyy+"년 " + mm+"월 "+dd+"일로 생성하시겠습니까?");
	
	if (cf == false) return;
	
	var szSvcid       = "saveResnDayClose";
    var szController  = "/dlw/itomng/saveResnDayClose";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

// 삭제버튼 클릭시
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	var cf = confirm("정말 삭제하시겠습니까?");
	
	if (cf == false) return;
	
	var szSvcid       = "deleteResnDayClose";
    var szController  = "/dlw/itomng/deleteResnDayClose";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

// 해약처리일 변경시 초기화
function calResnProcDay_onchanged(obj:Calendar, e:ChangeEventInfo)
{	
	sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";

	ds_list.clearData();	
	btn_insert.enable = false;
	btn_delete.enable = false;
