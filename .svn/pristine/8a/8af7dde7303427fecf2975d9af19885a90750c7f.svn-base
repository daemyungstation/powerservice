    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt = 0;   //전체 건수
var excelGb = ""; // 액셀다운로드할때 조회된 쿼리구분
var gvMenuId;  // 프로그램 로그 추가 2017.12.04

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL563000_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정;
	fn_init();
	
	gvMenuId = GV_LOGMENUID; // 프로그램 로그
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) 
	{
		switch (svcid) 
		{			
			case "selectResnDayCloseList" :
			    
			break;
		}
	} 
	else 
	{
		switch (svcid) 
		{
			case "selectResnDayCloseList" :				
				if(ds_listTemp.rowcount > 0){
					ds_list.filter("");
					gfn_appendSearchDs(ds_list, ds_listTemp);
					gfn_setTotalRowCount(ds_list, sta_TotalCnt, nTotalListCnt, null, true, null); 
					
					excelGb = "1";
					
					btn_insert.enable = false;
					btn_delete.enable = true;
				}else{
					fn_commSearch2(gfn_getCommTypeInit);
				}
			break;
		
			case "selectResnDayCloseList2" :								
				if(ds_listTemp.rowcount > 0){
					ds_list.filter("");
					gfn_appendSearchDs(ds_list, ds_listTemp);
					gfn_setTotalRowCount(ds_list, sta_TotalCnt, nTotalListCnt, null, true, null);				
					
					excelGb = "2";	
					
					btn_insert.enable = true;
					btn_delete.enable = false;
				}else{
					alert("조회된 데이터가 없습니다.");
				}
			break;
			
			case "saveResnDayClose" :												
				ds_list.clearData();
				btn_insert.enable = false;				
				sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
				alert("저장되었습니다.");			
					
				fn_commSearch(gfn_getCommTypeInit()); // 조회
			break;
			
			case "deleteResnDayClose" :												
				ds_list.clearData();
				btn_delete.enable = false;
				sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
				alert("삭제되었습니다.");				
				
				fn_commSearch(gfn_getCommTypeInit()); // 조회
			break;						
		}
	}
}
/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType)
{		 
	if (sType == gfn_getCommTypeInit())
	{			
		if(gfn_IsNull(calResnProcDay.value))
		{
			alert("해약처리일은 필수 입력입니다.");
			return false;
		}				
	}
	
	return true;
}
/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 해약환급데일리 테이블에서 조회
function fn_commSearch(sType) 
{
	if(!fn_preSearch(sType)) 
	{
		return false;
	}

	if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 50, nTotalListCnt))
	{
		return;
	}
	
	var szSvcid       = "selectResnDayCloseList";
	var szController  = "dlw/itomng/selectResnDayCloseList";
	var szInDs        = "ds_input=ds_cond";
	var szOutDs       = "ds_listTemp=ds_output";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 해약환급데일리 테이블에 데이터가 없을때
function fn_commSearch2(sType) 
{
	if(!fn_preSearch(sType)) 
	{
		return false;
	}

	if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 50, nTotalListCnt))
	{
		return;
	}
	
	var szSvcid       = "selectResnDayCloseList2";
	var szController  = "dlw/itomng/selectResnDayCloseList2";
	var szInDs        = "ds_input=ds_cond";
	var szOutDs       = "ds_listTemp=ds_output";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//초기화
function fn_init()
{		
    //  검색기간 설정
	var sToday = gfn_getToday();
	sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
	calResnProcDay.value = sToday;	
	ds_list.clearData();	
	btn_insert.enable = false;
	btn_delete.enable = false;
}

//조회 결과
function fn_postSearch(svcid)
{
	switch (svcid) 
	{
		case "getNonPayResultList" :
	        if(ds_listNonPayResult.getRowCount() <= 0)
	        {
		        alert("조회된 데이터가 없습니다.");
		        return false;
	        }
		break;
	}
}

