function fn_searchBatchAtchFile_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	var count = ds_BatchAtchFileList.rowcount;
	
	ds_cgfDetailList.enableevent 	= false;
	ds_batchDetailList.enableevent 	= false;
	for(var i =0; i<count; i++){
		if(ds_BatchAtchFileList.getColumn(i,"btchFileTypCd") == "S"){
			var row = ds_cgfDetailList.addRow();
			ds_cgfDetailList.setColumn(row,"btchFilePathNm",ds_BatchAtchFileList.getColumn(i,"btchFilePathNm"));
			ds_cgfDetailList.setColumn(row,"btchFileNm",ds_BatchAtchFileList.getColumn(i,"btchFileNm"));
			ds_cgfDetailList.setColumn(row,"batchId",ds_BatchAtchFileList.getColumn(i,"batchId"));
			ds_cgfDetailList.setColumn(row,"fileSrno",ds_BatchAtchFileList.getColumn(i,"fileSrno"));
		}else{
			var row = ds_batchDetailList.addRow();
			ds_batchDetailList.setColumn(row,"btchFilePathNm",ds_BatchAtchFileList.getColumn(i,"btchFilePathNm"));
			ds_batchDetailList.setColumn(row,"btchFileNm",ds_BatchAtchFileList.getColumn(i,"btchFileNm"));
			ds_batchDetailList.setColumn(row,"batchId",ds_BatchAtchFileList.getColumn(i,"batchId"));
			ds_batchDetailList.setColumn(row,"fileSrno",ds_BatchAtchFileList.getColumn(i,"fileSrno"));
		}
	}
	ds_cgfDetailList.enableevent 	= true;
	ds_batchDetailList.enableevent 	= true;	
	
	ds_cgfDetailList.applyChange();
	ds_batchDetailList.applyChange();
	
	ds_inputSchd.setColumn(0,"batchId",ds_btchInfoList.getColumn(0,"batchId"));
	ds_inputSchd.setColumn(0,"jobSeCode",ds_btchInfoList.getColumn(0,"jobSeCode"));
	ds_inputSchd.setColumn(0,"searchCondition","A");
	fn_searchSchdList("INIT");
