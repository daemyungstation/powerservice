	if(e.columnid == "trscTypeNm" && e.newvalue != "EJB"){
		ds_list.setColumn(e.row, "trscWayNm", "");  // 트랜잭션 모드
		ds_list.setColumn(e.row, "trscTimOutSecn", ""); // Timeout 초
	}
	
	if(e.columnid == "srvcItemDivNm" && e.newvalue != "DIS"){
		ds_list.setColumn(e.row,"srvcReslDataNm","");
	}
	
	if(e.columnid == "srvcItemDivNm" 
	    && ds_list.getColumn(e.row, "srvcItemDivNm") != ds_list.getColumn(e.row, "srvcItemDivNmOld")
	   && (e.newvalue == "SC" || e.newvalue == "BT" || e.newvalue == "DIS" )) {
		alert("SC,DIS,BT는 트랜잭션관리에서 등록하시기 바랍니다.");
		ds_list.setColumn(e.row, "srvcItemDivNm", ds_list.getColumn(e.row, "srvcItemDivNmOld"));
		return;
	}
	
	
