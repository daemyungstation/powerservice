/***************************************************************************
 * 함  수  명		: fn_preSave()
 * 기      능		: 저장 전 처리
 ***************************************************************************/
function fn_preSave(sType)
{
	if(!gfn_preSaveDs(ds_list, sType)) return false;

	for(var i = 0 ; i < ds_list.getRowCount() ; i++){
		if (ds_list.getColumn(i, gfn_getColNmStatus()) == gfn_getRowTypeInsert() || ds_list.getColumn(i, gfn_getColNmStatus()) == gfn_getRowTypeUpdate()){
			if(ds_list.getColumn(i, "srvcItemDivNm") == "SC"  ) {
				if(ds_list.getColumn(i, "srvcOperNm") == "" || unescape(ds_list.getColumn(i, "srvcOperNm")) == "undefined"){
					alert(i+1 +"행, 서비스 구분이 SC인 경우 오퍼레이션 명은 필수 입니다.");
					return false;
				}
				if (ds_list.getColumn(i, "trscTypeNm") == "" || unescape(ds_list.getColumn(i, "trscTypeNm")) == "undefined"){
					alert(i+1 +"행, 서비스 구분이 SC인 경우 트랜잭션 타입명은 필수 입니다.");
					return false;
				}
				if(ds_list.getColumn(i, "trscTypeNm") == "EJB" ){
					if (ds_list.getColumn(i, "trscWayNm") == "" || unescape(ds_list.getColumn(i, "trscWayNm")) == "undefined"){
						alert(i+1 +"행, 트랜잭션타입이 EJB인 경우 트랙잭션 모드는 필수 입니다.");
						return false;
					}
				}
				if(ds_list.getColumn(i, "trscTypeNm") == "BMT" ||  ds_list.getColumn(i, "trscTypeNm") == "CMT" ){
					if (ds_list.getColumn(i, "trscTimOutSecn") == "" || unescape(ds_list.getColumn(i, "trscTimOutSecn")) == "undefined"){
						alert(i+1 +"행, 트랜잭션타입이 BMT/CMT 경우 Timeout은 필수 입니다.");
						return false;
					}
				}				
			}
		}
		
		if(ds_list.getColumn(i,"srvcItemDivNm") == "DIS" ) {
			var cntSql = 0;
			var cntData = 0;
			if(ds_list.getColumn(i,"srvcOperNm") != null){
				var sqlVl = ds_list.getColumn(i,"srvcOperNm");
				var arrSqlVl = sqlVl.split(",");
				
				// sql id 중복 검사
				if (arrSqlVl.length > 1) {
					for (var m = 0; m < arrSqlVl.length -1; m ++) {
						for (var n = m + 1; n < arrSqlVl.length; n ++) {
							if (arrSqlVl[m] == arrSqlVl[n]) {
								alert((i +1) +  " 열에 중복된 sql id가 있습니다.\n[" + arrSqlVl[m] + "][" + arrSqlVl[n] + "]");
								return false;
							}
						}
					}
				}
				cntSql = arrSqlVl.length;
			}
			if(ds_list.getColumn(i,"srvcReslDataNm") != null
			    && ds_list.getColumn(i,"srvcReslDataNm") != ""){
				var dataVl = ds_list.getColumn(i,"srvcReslDataNm");
				var arrDataVl = dataVl.split(",");
				  
				// data set 명 중복 검사
				if (arrDataVl.length > 1) {
					for (var m = 0; m < arrDataVl.length -1; m ++) {
						for (var n = m + 1; n < arrDataVl.length; n ++) {
							if (arrDataVl[m] == arrDataVl[n]) {
								alert((i +1) +  " 열에 중복된 sql id가 있습니다.\n[" + arrDataVl[m] + "][" + arrDataVl[n] + "]");
								return false;
							}
						}
					}
				}

				// sqlid와 dataset개수 검사
				cntData = arrDataVl.length;
				if(cntSql != cntData){
					alert((i +1) +  " 열에 DIS 데이터셋 수가 부족합니다.\nSqlMap ID[" + cntSql + "]개, DIS데이터 셋[" +  cntData + "]개");
					return false;
				}

			}
			
		}
	}
		
		
	
	//TO-DO:필수 입력 체크 등

	return true;
}

