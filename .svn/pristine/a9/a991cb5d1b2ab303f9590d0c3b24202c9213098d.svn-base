function fn_setCaption()
{
	var srvcItemDivNm = this.ds_input.getColumn(0, "srvcItemDivNm");
	
	var nRow = dsl_srvcItem00.findRow("code", srvcItemDivNm);
	
	if(nRow != -1) {
		Div00.stc_tranNm.text 		= dsl_srvcItem00.getColumn(nRow, "tranNm");
		Div00.stc_serviceNm.text 	= dsl_srvcItem00.getColumn(nRow, "serviceNm");
		Div00.stc_classNm.text 		= dsl_srvcItem00.getColumn(nRow, "classNm");
		Div00.stc_operationNm.text 	= dsl_srvcItem00.getColumn(nRow, "operationNm");
		
		div_body.grd_list.setCellProperty("head", 4, "text", dsl_srvcItem00.getColumn(nRow, "tranNm"));
		div_body.grd_list.setCellProperty("head", 5, "text", dsl_srvcItem00.getColumn(nRow, "serviceNm"));
		div_body.grd_list.setCellProperty("head", 6, "text", dsl_srvcItem00.getColumn(nRow, "classNm"));
		div_body.grd_list.setCellProperty("head", 7, "text", dsl_srvcItem00.getColumn(nRow, "operationNm"));
	}
}

function Div00_cbo_bzdpDivCd_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	fn_setCaption();
}

