/***************************************************************************
 * 기      능		: 폼 onload이벤트
 ***************************************************************************/
function FrmDev_onload(obj:Form, e:LoadEventInfo)
{
	// 시스템 공통함수 호출
	gfn_commPopInit(obj);

	// 공통코드 조회
	fn_getCommCode();

	// 폼 공통함수 호출
	fn_init();

}

//**************************************************************************
// [ PART 4 ] Main Process Event 정의
//**************************************************************************

/***************************************************************************
 * 함  수  명		: fn_init()
 * 기      능		: 폼 초기화 공통함수
 ***************************************************************************/
function fn_init()
{
	//개인/사업자 정보
	var nRow = ds_input.rowposition;

	ds_input.setColumn(nRow, "persBzopDiv"         , "1");      				//개인 사업자 구분
	ds_input.setColumn(nRow, "rrnoBzopNo" 	       , "7711112222222");			//주민등록번호
	ds_input.setColumn(nRow, "inqUsg"              , "01");						//조회 용도
	ds_input.setColumn(nRow, "credInqDivCd"        , "1");						//신용 조회 구분 코드
	ds_input.setColumn(nRow, "csno"                , "12345678");				//고객 번호
	ds_input.setColumn(nRow, "corpDivCd"           , "10");						//법인 구분 코드
}

/***************************************************************************
 * 함  수  명		: fn_getCommCode()
 * 기      능 		: 공통코드 조회
 ***************************************************************************/
function fn_getCommCode()
{
	// 10001 개인 법인 구분 코드
	// 10005 KIS용도 코드
	// 10026 신용 조회 구분 코드
	// 10505 법인 구분 코드
	// 10025 신용 관리 사무소 구분 코드
	gfn_getCommCodeToCombo(cboPersBzopDiv, "10001", gfn_getComCodeDefaultSelect(), "00", dsCbo_PersBzopDiv);
	gfn_getCommCodeToCombo(cboInqUsg, "10005", gfn_getComCodeDefaultSelect(), "00", dsCbo_InqUsg);
	gfn_getCommCodeToCombo(cboCredInqDivCd, "10026", gfn_getComCodeDefaultSelect(), "00", dsCbo_CredInqDivCd);
	gfn_getCommCodeToCombo(cboCorpDivCd, "10505", gfn_getComCodeDefaultSelect(), "00", dsCbo_CorpDivCd);
	gfn_getCommCodeToCombo(cboCredAdmnOfcDivCd, "10025", gfn_getComCodeDefaultSelect(), "00", dsCbo_CredAdmnOfcDivCd);
}

/***************************************************************************
 * 함  수  명		: fn_commInit()
 * 기      능		: (공통버튼)폼 초기화 프로세스
 ***************************************************************************/
function fn_commInit(sType)
{
	gfn_initDs(ds_input);
}



