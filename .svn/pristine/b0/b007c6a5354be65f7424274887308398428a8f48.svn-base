		cnt = ds_upload.getCaseCount("chk == '1'");
		if (cnt < 1) {
			alert("선택된 파일이 없습니다.");
			return;
		}
		
		if (!confirm("정말로 삭제 하시겠습니까?")) {
			return false;
		}
		
		ds_list_tmp.clearData();
		gfn_cpChkRow(ds_upload, ds_list_tmp);
		
	}else{
		// 사용안함
	}
	
	return true;
}

function fn_delete(obj:Button,  e:ClickEventInfo)
{
	var cnt;
	
	if (!fn_preDelete()) {
		return;
	}
	
	if (tab_main.tabindex == 0) {
	
		btn_del.enable = false;
	
		var szSvcid       = "hsUploadDataDelete";
		var szController  = "/dlw/prod/hsUploadDataDelete";
		var szInDs        = "ds_input=ds_list_tmp:U";
		var szOutDs       = "";
		var szParam       = "";
		var szRetCallback = "fn_callBack";
		
	}else{
		// 사용안함
	}	
	
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function fn_postDelete() {
	btn_del.enable = true;
	alert("삭제하였습니다.");
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (tab_main.tabindex == 0) {
	
