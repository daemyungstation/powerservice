// 다운로드 대상 파일압축
function fn_mkRecZip()
{
	// 유효성 체크
	if ( !fn_preMkRecZip() ) {
		return;
	}
	
	btn_down.enable = false;
	
	var szSvcid       = "mkRecZip";
    var szController  = "/dlw/prod/mkRecZip";
    var szInDs        = "ds_input=ds_list_tmp:U";
    var szOutDs       = "ds_zip=ds_output";
    var szParam       = "";
