			return;
		} else {
			tab_main.tabpage0.grd_list.formatid = "excel";
			gfn_exportGridToExcel(tab_main.tabpage0.grd_list, "파일업로드_"+gfn_getTodayFull()+"");
			tab_main.tabpage0.grd_list.formatid = "default";
		}
		
	}else{
		if (ds_upload.rowcount < 1) {
			alert("다운로드할 내역이 없습니다.");
			return;
		} else {
			gfn_exportGridToExcel(tab_main.tabpage1.grd_list, "업로드실패이력_"+gfn_getTodayFull()+"");
