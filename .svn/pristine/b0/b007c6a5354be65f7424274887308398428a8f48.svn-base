// 원본 데이터셋에서 체크된 레코드만 다른 데이터셋으로 복사한다.
function gfn_cpChkRow(src:Dataset, dst:Dataset, chkCol, chkVal, strColInfo) {
	var nPos = -1;
	var i;
	
	if (undefined == chkCol || "" == chkCol) {
		chkCol = "chk";
	}
	if (undefined == chkVal || "" == chkVal) {
		chkVal = "1";
	}
	
	for (i=0; i<src.rowcount; i++) {
		if (chkVal == src.getColumn(i, chkCol)) {
			nPos = dst.addRow();
			
			if ( !gfn_isEmpty(strColInfo) ) {
				dst.copyRow(nPos, src, i, strColInfo);
			} else {
				dst.copyRow(nPos, src, i);
			}
		}
	}
}


