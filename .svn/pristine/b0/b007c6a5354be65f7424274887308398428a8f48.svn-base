// Callback(gfn_Transaction 호출 후 처리내용) 
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
			alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} else {
		switch (svcid) {
		    // 공통
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
				gfn_getCode("0169", "ALL", ds_cl2, gds_DmlCd); // 코드조회결과 필터
				gfn_getCode("S016", "ALL", ds_cl3, gds_DmlCd); // 코드조회결과 필터
				fn_init(); // 검색부 초기화
				
				if (!gfn_isEmpty(g_accnt_no)) {
					stt_dt.value = "";
					end_dt.value = "";
					edt_accnt_no.value = g_accnt_no;					
					fn_commSearch(gfn_getCommTypeInit()); // 조회
				}
			break;
			
		    case "mkRecZip": // 파일일괄다운로드 대상파일 압축
			fn_postMkRecZip();
			break;
			
		    case "srchFileUploadList": // 파일업로드 조회
			fn_postSearch();
			break;
			
			case "srchUploadFailList": // 업로드실패이력 조회
			fn_postSearch();
			break;
			
			case "hsUploadDataDelete": // 삭제
			fn_postDelete();
			break;
		}
	}
}
/***************************************************************************
 * 함  수  명		: fn_preSearch()
 * 기      능		: 조회 전 처리 조회조건 설정
 * 수  정  일      : 2016.01.22 
 ***************************************************************************/
function fn_preSearch(sType)
{
	var sToday = gfn_getToday();
	var accnt_no 	= edt_accnt_no.value;
	var mem_nm 		= edt_mem_nm.value;
	
	// 조회조건 및 default 정렬
	ds_inqCndt.clearData();
	ds_inqCndt.addRow(); // 정렬컬럼 초기화
	
	var tab_id = tab_main.tabindex;
	
	
	if ( gfn_isEmpty(stt_dt.value) ) {
		if ( !gfn_isEmpty(g_accnt_no) && !gfn_isEmpty(edt_accnt_no.value) ) {
			stt_dt.value = "20110101";
		} else {
			alert("시작일자를 입력해주십시오.");
			return false;		
		}
	}	
	
	if ( gfn_isEmpty(end_dt.value) ) {
		if ( !gfn_isEmpty(g_accnt_no) && !gfn_isEmpty(edt_accnt_no.value) ) {
			end_dt.value = sToday;
		} else {
			alert("종료일자를 입력해주십시오.");
			return false;
		}
	}
	
	var sttDt = gfn_getCalVal(stt_dt);
	var endDt = gfn_getCalVal(end_dt);
	// var diff = Math.abs(gfn_getMonthDiff(sttDt, endDt));
	var diff = Math.abs(gfn_getDayDiff(sttDt, endDt));	
	
	if ( gfn_isEmpty(accnt_no) && gfn_isEmpty(mem_nm) ) {
		if (diff > 30) {
			alert("회원명, 회원번호 값이 없을경우는 검색기간이 1개월을 초과할 수 없습니다.");
			return false;
		}
	}

	if(chk_random_yn.isChecked() == true){
		r_yn = "01";
	}else{
		r_yn = "";
	}

	ds_inqCndt.setColumn(0, "dt_typ", cbo_dt_typ.value);
	ds_inqCndt.setColumn(0, "stt_dt", stt_dt.value);
	ds_inqCndt.setColumn(0, "end_dt", end_dt.value);
	ds_inqCndt.setColumn(0, "file_cl", cbo_file_cl.value);
	ds_inqCndt.setColumn(0, "cl2", cbo_cl2.value);
	ds_inqCndt.setColumn(0, "cl3", cbo_cl3.value);
	ds_inqCndt.setColumn(0, "mem_nm", edt_mem_nm.value);
	ds_inqCndt.setColumn(0, "accnt_no", edt_accnt_no.value);
	ds_inqCndt.setColumn(0, "random_yn", r_yn);
	
	ds_inqCndt.setColumn(0, "tab_id", tab_id);
	
	return true;
}

