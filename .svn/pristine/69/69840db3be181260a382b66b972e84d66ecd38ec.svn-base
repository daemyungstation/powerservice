                        </if>
                        <if test="cd_nm != null and cd_nm != ''">
                            AND CD_NM LIKE '%' || #{cd_nm} || '%'
                        </if>
                        <if test="cd != null and cd != ''">
                            AND COM_CD = #{cd}
                        </if>
                        ) A
