    </insert>
    
    <insert id="DlwNewTypeMainPopMap.insertDlwCardAnxtSrvc"  parameterType="map" >
        <!-- DM sql-id : insertDlwCardAnxtSrvc -->
        INSERT /* DlwNewTypeMainPopMap.insertDlwCardAnxtSrvc */
        INTO CARD_NEW_CANCL_APP
             ( SEQ
                  , ACCNT_NO
                  , CARD_CD
                  , CARD_NO
                  , APP_DT
                  , INV_TG_DT
                  , APP_CL
                  , EXPIRE_DATE
                  , REG_DM
                  , REG_MAN
                  , UPD_DM
                  , UPD_MAN
                  , CARD_BIRTH
                  , INI_YN 
