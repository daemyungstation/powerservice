//버튼 및 컨포넌트들 제어
function fn_componentStatChange()
{
	if(ds_list_examBasi.rowcount > 0){
		var sExamPrgrStatCd = ds_list_examBasi.getColumn(ds_list_examBasi.rowposition, "exam_prgr_stat_cd");
		//var sExamPrgrStatCd = "10";
		if(sExamPrgrStatCd == "10"){//준비중
			btn_new.position2.left = 274;
			btn_new.visible = true;
			btn_update.visible = true;
			btn_add.position2.right = 75;
			btn_add.visible = true;
			btn_delete.visible = true;
			btn_delete2.visible = true;
			btn_status.text = "확정";
			btn_status.visible = true;
		}else if(sExamPrgrStatCd == "20"){//확정
			btn_new.position2.left = 422;
			btn_new.visible = true;
			btn_update.visible = false;
			btn_add.position2.right = 1;
			btn_add.visible = false;
			btn_delete.visible = false;
			btn_delete2.visible = false;
			btn_status.text = "채점중";
			btn_status.visible = true;
		}else if(sExamPrgrStatCd == "30"){//채점중
			btn_new.position2.left = 422;
			btn_new.visible = true;
			btn_update.visible = false;
			btn_add.visible = false;
			btn_delete.visible = false;
			btn_delete2.visible = false;
			btn_status.text = "종료";
			btn_status.visible = true;
		}else{//종료
			btn_new.position2.left = 422;
			btn_new.visible = true;
			btn_update.visible = false;
			btn_add.visible = false;
			btn_delete.visible = false;
			btn_delete2.visible = false;
			btn_status.text = "";
			btn_status.visible = false;
		}
	}
}

