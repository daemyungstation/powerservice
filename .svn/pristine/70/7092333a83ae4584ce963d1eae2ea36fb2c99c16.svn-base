    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_cust_memo = 0; // 전체건수
//var tc_cnt = 0; // 전체건수

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS240200_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_getList("cd","EXM050,EXM060");   //코드조회
	fn_init();
	fn_commSearch(gfn_getCommTypeInit());
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
 			gfn_getCode("EXM050", "SELECT", ds_TypCd); //문제배열
			gfn_getCode("EXM060", "ALL", ds_statusTypCd); //진행상태
			break;
 			
 			case "getExamBasiList":
 			//trace(ds_list_tmp.saveXML());
 			gfn_appendSearchDs(ds_list_examBasi, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_examBasi, sta_totalCnt, tc_cust_memo); // 페이지건수 표출
			
			if(ds_list_examBasi.rowcount > 0){
				fn_commSearch2();
			}else{
				ds_list_tmp2.clearData();
				btn_new.visible = true;
				btn_status.visible = false;
				btn_update.visible = false;
				btn_delete.visible = false;
				btn_Save2.visible = false;
				btn_cancel.visible = false;
				btn_add.visible = false;
			}
			break;
 			
 			case "getExamTrprList":
 			//trace(ds_list_tmp.saveXML());
 			gfn_appendSearchDs(ds_list_examTrpr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
 		 	//gfn_setTotalRowCount(ds_list_examTrpr, sta_totalCnt2, tc_cnt); // 페이지건수 표출
 		 			
 			ds_list_tmp2.clearData();
			ds_list_tmp2.addRow();
			ds_list_tmp2.copyRow(0, ds_list_examBasi, ds_list_examBasi.rowposition);
			
			var sExamSttDttm = ds_list_tmp2.getColumn(0, "exam_stt_dttm");
			//####-##-## ##:## 날짜형식으로 변환
			var exam_stt_dttm = sExamSttDttm.substr(0,4)+"-"+sExamSttDttm.substr(4,2)+"-"+sExamSttDttm.substr(6,2)+" "+sExamSttDttm.substr(8,2)+":"+sExamSttDttm.substr(10,2);			
			var exam_stt_yyyymmdd = sExamSttDttm.substr(0,8);
			var exam_stt_ss = sExamSttDttm.substr(8,2)
			var exam_stt_mm = sExamSttDttm.substr(10,2);
			
			ds_list_tmp2.setColumn(0, "exam_stt_dttm", exam_stt_dttm);
			ds_list_tmp2.setColumn(0, "exam_stt_yyyymmdd", exam_stt_yyyymmdd);
			ds_list_tmp2.setColumn(0, "exam_stt_ss", exam_stt_ss);
			ds_list_tmp2.setColumn(0, "exam_stt_mm", exam_stt_mm);
			
			var sExamEndDttm = ds_list_tmp2.getColumn(0, "exam_end_dttm");
			//####-##-## ##:## 날짜형식으로 변환
			var exam_end_dttm = sExamEndDttm.substr(0,4)+"-"+sExamEndDttm.substr(4,2)+"-"+sExamEndDttm.substr(6,2)+" "+sExamEndDttm.substr(8,2)+":"+sExamEndDttm.substr(10,2);
			var exam_end_yyyymmdd = sExamEndDttm.substr(0,8);
			var exam_end_ss = sExamEndDttm.substr(8,2)
			var exam_end_mm = sExamEndDttm.substr(10,2);
			
			ds_list_tmp2.setColumn(0, "exam_end_dttm", exam_end_dttm);
			ds_list_tmp2.setColumn(0, "exam_end_yyyymmdd", exam_end_yyyymmdd);
			ds_list_tmp2.setColumn(0, "exam_end_ss", exam_end_ss);
			ds_list_tmp2.setColumn(0, "exam_end_mm", exam_end_mm);
			
			
			var sRgsnDttm = ds_list_tmp2.getColumn(0, "rgsn_dttm");
			var sAmntDttm = ds_list_tmp2.getColumn(0, "amnt_dttm");
			
			//####-##-## ##:## 날짜형식으로 변환
			var rgsn_dttm = sRgsnDttm.substr(0,4)+"-"+sRgsnDttm.substr(4,2)+"-"+sRgsnDttm.substr(6,2)+" "+sRgsnDttm.substr(8,2)+":"+sRgsnDttm.substr(10,2);
			var amnt_dttm = sAmntDttm.substr(0,4)+"-"+sAmntDttm.substr(4,2)+"-"+sAmntDttm.substr(6,2)+" "+sAmntDttm.substr(8,2)+":"+sAmntDttm.substr(10,2);
			
			ds_list_tmp2.setColumn(0, "rgsn_dttm", rgsn_dttm);
			ds_list_tmp2.setColumn(0, "amnt_dttm", amnt_dttm);
 			
			break;
			
			case "deleteExamTrpr":
			fn_commSearch(gfn_getCommTypeInit());
			break;
						
			case "saveExamBasi":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "deleteExamBasi":
			fn_commSearch(gfn_getCommTypeInit());
			break;
 		}	
 	}		
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_list_examBasi.clearData();
		ds_inqCndt.clearData();
		//ds_list_examPrbm.clearData();
		ds_list_examBasi.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "exam_nm", edt_exam_nm.value);//직무평가명
		ds_inqCndt.setColumn(0, "exam_prgr_stat_cd", cbo_statusTypCd.value);//진행상태
		ds_inqCndt.setColumn(0, "dateType", Combo_dateType.value);
		ds_inqCndt.setColumn(0, "exam_stt_dttm", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "exam_end_dttm", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "orderBy", "AMNT_DTTM");
		ds_inqCndt.setColumn(0, "orderDirection", "DESC");
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//좌측조회
function fn_commSearch(sType)
{
	btn_update.visible = true;
	btn_delete.visible = true;
	btn_new.visible = true;
	btn_Save2.visible = false;
	btn_cancel.visible = false;
	
	edt_exam_nm2.readonly = true; //직무평가명
	cbo_prbm_arnm_nm.readonly = true;//문자배열
	btn_pop.visible = false;//팝업버튼
	cal_exam_stt_yyyymmdd.readonly = true;//시작일
	cbo_exam_stt_ss.readonly = true;//시작시간
	cbo_exam_stt_mm.readonly = true;//시작분
	cal_exam_end_yyyymmdd.readonly = true;//종료일
	cbo_exam_end_ss.readonly = true;//종료시간
	cbo_exam_end_mm.readonly = true;//종료분
	edt_exam_expi.readonly = true;//직무평가설명
	
	stc_rgsr_nm.visible = true;
	Static_rgsr_nm.visible = true;
	stc_rgsn_dttm.visible = true;
	Static_rgsn_dttm.visible = true;
	stc_rgsn.visible = true;
	stc_amnd_nm.visible = true;
	Static_amnd_nm.visible = true;
	stc_amnt_dttm.visible = true;
	Static_amnt_dttm.visible = true;
	stc_amnt.visible = true;
	
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_examBasi, sta_totalCnt, 25, tc_cust_memo)) return;
	
	var szSvcid       = "getExamBasiList";
    var szController  = "/exam/exam-basi/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//좌측저장
function fn_save(type)
{
	if(gfn_isNull(edt_exam_nm2.value)){
		alert("직무평가명을 입력하세요.");
		return false;
	}
	if(gfn_isNull(cbo_prbm_arnm_nm.value)){
		alert("문제배열을 선택하세요.");
		return false;
	}
	if(gfn_isNull(edt_emsh_nm.value)){
		alert("평가지명을 선택하세요.");
		return false;
	}
	if(gfn_isNull(cal_exam_stt_yyyymmdd.value)){
		alert("시작일을 입력하세요.");
		return false;
	}
	if(gfn_isNull(cbo_exam_stt_ss.value)){
		alert("시작시간을 선택하세요.");
		return false;
	}
	if(gfn_isNull(cbo_exam_stt_mm.value)){
		alert("시작 분을 선택하세요.");
		return false;
	}
	if(gfn_isNull(cal_exam_end_yyyymmdd.value)){
		alert("종료일을 선택하세요.");
		return false;
	}
	if(gfn_isNull(cbo_exam_end_ss.value)){
		alert("종료시간을 선택하세요.");
		return false;
	}
	if(gfn_isNull(cbo_exam_end_mm.value)){
		alert("종료 분을 선택하세요.");
		return false;
	}
	
	var exam_stt_dttm = cal_exam_stt_yyyymmdd.value+cbo_exam_stt_ss.value+cbo_exam_stt_mm.value;
	var exam_end_dttm = cal_exam_end_yyyymmdd.value+cbo_exam_end_ss.value+cbo_exam_end_mm.value;
	ds_list_tmp2.setColumn(0,"exam_stt_dttm", exam_stt_dttm);
	ds_list_tmp2.setColumn(0,"exam_end_dttm", exam_end_dttm);
	
	if(type == "status"){
		var statCd = ds_list_tmp2.getColumn(0,"exam_prgr_stat_cd");
		statCd = toNumber(statCd)+10;
		ds_list_tmp2.setColumn(0,"exam_prgr_stat_cd_change", statCd);
	}
	
	var szSvcid       = "saveExamBasi";
    var szController  = "/exam/exam-basi/save";
    var szInDs        = "ds_input=ds_list_tmp2";
    var szOutDs       = "";
    var szParam       = "";
