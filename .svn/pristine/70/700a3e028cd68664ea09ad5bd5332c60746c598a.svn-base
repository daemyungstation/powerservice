/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {			
			case "srchRltmCardLog": 
			alert("고객청구결과관리 조회를 실패하였습니다.\n" + errmsg);
			break;
			
			/*
			case "pymnCancelNicePay":
			alert("NicePay 결제승인 취소를 실패하였습니다.\n" + errmsg);
			break;
			
			case "pymnRetryNicePay":
			alert("NicePay 결제승인 재결제를 실패하였습니다.\n" + errmsg);
			break;
			*/
	    }
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode" :
				gfn_getCode("0120", "SELECT", ds_cardCorp, gds_DmlCd); // 코드조회결과 필터
			break;
			
			case "srchRltmCardLog": 
		    gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		    gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_log, null, true, null); // 페이지건수 표출
		    fn_postSearch(svcid);
		
		    
			break;
			
			case "srchRltmCardLogTotal":
				//gfn_exportGridToExcel(grd_list00, "realCardLogList_"+gfn_getTodayFull()+"");
				//var dataAthrQury = ds_list_excel.getColumn(0, "dataAthrQury");
				//ds_inqCndt.setColumn(0, "dataAthrQury" , dataAthrQury);
				
				var saveFileNm = "";
				if(cbo_payMthd.value == "06")
					saveFileNm = "카드결제로그";
				else if(cbo_payMthd.value == "04")
					saveFileNm = "CMS결제로그";
				
				var colDS = gfn_excelPrepareExcelDS("grd_list01");
				//gfn_excelDown("카드결제로그_NEW", "XLS_DLCC", "DlwPayMap.getDlwRltmCardLogList", "DlwPayMap.getDlwRltmCardLogCount", ds_inqCndt, true, colDS, true);
				gfn_excelDown(saveFileNm, "XLS_DLCC", "DlwPayMap.getReqRltmCardLogList", "DlwPayMap.getReqRltmCardLogCount", ds_inqCndt, true, colDS, true);
			break;
			
			case "pymnCancNicePay":
			if ("03" == rslt_cd) {
				alert("결제승인 취소 및 입금취소 처리가 완료되었습니다");
			} else {
			//result_msg
				if(!gfn_isNull(rslt_cd)) {
					alert("["+rslt_cd+"]"+rslt_nm+"\n결제승인 취소에 실패하였습니다. \n3회 이상 실패시 전산담당자에게 연락바랍니다.");
				} else {
					if(!gfn_isNull(rslt_msg )) {
						alert(rslt_msg);
					} else{
						alert("결제승인 취소에 실패하였습니다. \n3회 이상 실패시 전산담당자에게 연락바랍니다.");
					}
				}
			}
			fn_commSearch(gfn_getCommTypeInit(), "srchRltmCardLog"); // 조회
			break;
			case "selectCustaccntList":
			if(tc_custAcnt == 1) {
				fn_setAccntNo();
			} else if (tc_custAcnt > 1) {
				btn_accnt_onclick(null, "Y");
			} else {
				edt_accnt_no.value = "";
			}			
			break;
			
			case "pymnCancelNicePay":
				if ("03" == rslt_cd) {
					alert("결제승인 취소 및 입금취소 처리가 완료되었습니다");
				} else {
					if(!gfn_isNull(rslt_nm )) {
						alert(rslt_nm);
					}
				}
				fn_commSearch(gfn_getCommTypeInit(), "srchRltmCardLog"); // 조회
			break;
			
			case "sendCancelNicePayment" :
				if(returnVal == 1)
				{
					alert("전환결제가 취소되었습니다.");
					returnVal = 0;
					returnMassage = "";
					fn_commSearch(gfn_getCommTypeInit(), "srchRltmCardLog"); // 조회
				}
				else
				{
					alert(returnMassage);
					returnVal = 0;
					returnMassage = "";
					return false;
				}
			break; 
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType, svcid, gbn) {
    var return_bool = false;
    switch(svcid) {
        case "srchRltmCardLog":
			if (sType == gfn_getCommTypeInit())
			{
				var cReqDayStt = cal_sttDt.value;
				var sReqDayStt = new String(cReqDayStt);
				var cReqDayEnd = cal_endDt.value;
				var sReqDayEnd = new String(cReqDayEnd);
				
			    // 청구일 조건 없는 경우 회원번호 필수 체크
				if ( (cal_sttDt.value == null || cal_sttDt.value == null) && gfn_isNull(edt_accnt_no.value) )
				{
					alert("전체 검색 시 회원번호는 필수 입니다.(전산팀)");
					edt_accnt_no.setFocus();
					return false;
				}
				
				//if (gfn_getDayDiff(cal_sttDt.value, cal_endDt.value) > 3) {
				// 회원번호 조회조건이 없는 경우만 조회기간 기간 체크
				if ( gfn_getDayDiff(cal_sttDt.value, cal_endDt.value) > 3 && gfn_isNull(edt_accnt_no.value) )
				{
					alert("조회기간은 3일 이하로 설정해야합니다.");
					return false;
				}
				
				if(sReqDayStt.substr(0, 4) <= 2021 && cbo_searchYy.value == "1" && gfn_isNull(edt_accnt_no.value)) {
					alert('청구시작일 및 조회년도를 확인하시기 바랍니다.');
					return false;
				} else if (sReqDayStt.substr(0, 4) >= 2022 && cbo_searchYy.value == "2" && gfn_isNull(edt_accnt_no.value)) {
					alert('청구시작일 및 조회년도를 확인하시기 바랍니다.');
					return false;
				}
				
				if(sReqDayEnd.substr(0, 4) <= 2021 && cbo_searchYy.value == "1" && gfn_isNull(edt_accnt_no.value)) {
					alert('청구종료일 및 조회년도를 확인하시기 바랍니다.');
					return false;
				} else if (sReqDayEnd.substr(0, 4) >= 2022 && cbo_searchYy.value == "2" && gfn_isNull(edt_accnt_no.value)) {
					alert('청구종료일 및 조회년도를 확인하시기 바랍니다.');
					return false;
				}
			
				// 조회조건 및 default 정렬
				ds_inqCndt.keystring = ""; // 정렬컬럼 초기화
				ds_inqCndt.clearData();
				ds_inqCndt.addRow();
				
				ds_inqCndt.setColumn(0, "stt_req_dt", cal_sttDt.value);
				ds_inqCndt.setColumn(0, "end_req_dt", cal_endDt.value);
				ds_inqCndt.setColumn(0, "accnt_no", edt_accnt_no.value);
				ds_inqCndt.setColumn(0, "req_bit", cbo_reqBit.value);
				ds_inqCndt.setColumn(0, "result_kind", cbo_resultKind.value);
				ds_inqCndt.setColumn(0, "pay_mthd", cbo_payMthd.value);
				ds_inqCndt.setColumn(0, "del_fg", cbo_req_stat.value);
				ds_inqCndt.setColumn(0, "search_gbn", cbo_searchGbn.value);
				if(!gfn_isNull(edt_accnt_no.value)) {
					ds_inqCndt.setColumn(0, "search_yy", "3");
				} else {
					ds_inqCndt.setColumn(0, "search_yy", cbo_searchYy.value);
				}
				ds_inqCndt.setColumn(0, "orderBy", "REQ_DAY DESC, REG_DM DESC, REQ_PAY_NO DESC, ACCNT_NO");
				
				if(cbo_payMthd.value == '06') {
					ds_inqCndt.setColumn(0, "auth_yn", cbo_payAuth.value);
				} else {
					ds_inqCndt.setColumn(0, "auth_yn", "");
				}
			}
			return_bool = true;
        break;
        
		case "selectCustaccntList":
			ds_accntInqCndt.keystring = ""; // 정렬컬럼 초기화
			ds_accntInqCndt.clearData();
			ds_accntInqCndt.addRow();
			
			var srch_typ = ""
			var srch_vl = "";
			var option = "";
			
			if (gbn == "accnt_no") {
				srch_typ = gbn;
				srch_vl = edt_accnt_no.value;
			} 
					
			ds_accntInqCndt.setColumn(0, "srch_typ", srch_typ);
			ds_accntInqCndt.setColumn(0, "srch_vl", srch_vl);
			ds_accntInqCndt.setColumn(0, "option", option);
			ds_accntInqCndt.setColumn(0, "ns_yn", "N");
			ds_accntInqCndt.setColumn(0, "popup_yn", "N");
			ds_accntInqCndt.setColumn(0, "orderBy", "mem_nm");
			ds_accntInqCndt.setColumn(0, "orderDirection", "asc");
			return_bool = true;
		break;
    }
	return return_bool;
}

