/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, searchInfo, gbn) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
		// 조회
        case "srchRltmCardLog": 
        if (!fn_preSearch(sType,"srchRltmCardLog")) return false;
	    if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 500, tc_log)) return;
	    szSvcid       = "srchRltmCardLog";
		//szController  = "/req/card/nicepay-rltm-card-log/list";
		szController  = "/req/reqCust/nicepay-rltm-card-log/list";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_list_tmp=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        
        // 엑셀
		case "srchRltmCardLogTotal":
        if (!fn_preSearch(sType,"srchRltmCardLog")) return false;
	    szSvcid       = "srchRltmCardLogTotal";
		//szController  = "/req/card/nicepay-rltm-card-log/list";
		szController  = "/req/reqCust/nicepay-rltm-card-log/list";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_list_total=ds_output ds_list_excel=ds_output_excel";
		szParam       = "excel_fg=Y";
		szRetCallback = "fn_callBack";
		break;
		
		case "selectCustaccntList":
        if (!fn_preSearch(sType,"selectCustaccntList", gbn)) return false;
		szSvcid       = "selectCustaccntList";
		szController  = "/req/reqCust/cust-acnt/list";
		szInDs        = "ds_input=ds_accntInqCndt";
		szOutDs       = "ds_accnt_info=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
		break;
    }
    // 트랜젝션 공통함수 호출
    if( searchInfo == "srchRltmCardLog" )
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
    else
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
//저장
function fn_commSave(saveInfo) {

    var szSvcid = ""
    var szController = ""
    var szInDs = ""
    var szOutDs = ""
    var szParam = ""
    var szRetCallback = ""

    switch(saveInfo) {
        
        case "pymnCancelNicePay":		
		szSvcid       = "pymnCancelNicePay";
		szController  = "/req/reqCust/cancel-realtime";
		szInDs        = "ds_input=ds_rgsnCard";
		szOutDs       = "";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        
        case "pymnCancNicePay":
		if (!fn_preSave("pymnCancNicePay")) return false;
		szSvcid       = "pymnCancNicePay";
		szController  = "/req/reqCust/pymn-canc-nice";
		szInDs        = "ds_input=ds_rgsnCard";
		szOutDs       = "";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
    }

	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
