    <!-- 행사자 목록 조회 -->
    <select id="DlwEvntMap.getEvntMngrList" parameterType="map" resultType="resultMap">

            SELECT /* DlwEvntMap.getEvntMngrList */
                   A.IDNTT,
                   A.EVT_SEQ,
                   FN_COM_NM('0138',A.EVT_MNGR_GUBUN) MNGR_GUBUN,
                   A.EVT_MNGR_GUBUN AS JOB_DUTY,
                   B.REAL_EVT_MNGR_CD AS MNGR_CD,
                   A.EVT_MNGR_CD AS EVT_MNGR_CD,
                   B.EVT_MNGR_NM AS MNGR_NM,
                   A.WORK_DM,
                   A.START_TIME,
                   A.END_TIME,
                   A.NUMBER_OF_DAY,
                   A.OVER_TIME,
                   A.PAY_AMT,
                   A.PAY_ADD_AMT,
                   A.GIVE_PAY_DM,
                   (NVL(A.PAY_AMT,0) + NVL(A.PAY_ADD_AMT,0)) - NVL(TRUNC((NVL(A.PAY_AMT,0) + NVL(A.PAY_ADD_AMT,0)) * 0.03, -1),0) - NVL(TRUNC(TRUNC((NVL(A.PAY_AMT,0) + NVL(A.PAY_ADD_AMT,0)) * 0.03) * 0.1, -1),0) SAMT,
                   A.NOTE,
                   ' ' AS CHK,
