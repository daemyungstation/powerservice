             , REMAIN_AMT
             , EVENT_NOTE
             , PAY_CNT
             , PAY_DTL_CNT
             , PAY_DTL1_CNT
             , OUTSRC_CUSTMR_CD
             , (SELECT CUSTMR_NM FROM CUSTMR_MNG WHERE CUSTMR_CD = OUTSRC_CUSTMR_CD) AS OUTSRC_CUSTMR_NM
             , OUTSRC_AMT
