                                  GROUP BY TBL1.EVT_MNGR_NM, TBL1.EVT_MNGR_CD
                                ) TBL3
                        ) TBL4

                  UNION ALL

                  SELECT '합계' CL
                       , (SELECT CNT FROM TOT_TBL) CNT
                       , CASE WHEN (SELECT CNT FROM TOT_TBL) <![CDATA[>]]> 0
                              THEN '100.00%'
                              ELSE '0.00%'
                         END PCT
                  FROM DUAL

    </select>

    <!-- 종교 분석현황 -->
    <select id="DlwEvntMap.getEvntAnal6" parameterType="map" resultType="resultMap">

