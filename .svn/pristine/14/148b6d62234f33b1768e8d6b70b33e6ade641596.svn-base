                            FROM WH_MV_MST WMM
                                 INNER JOIN WH_MV_DTL WMD ON WMD.WH_MV_NO = WMM.WH_MV_NO
                           WHERE DEL_FG = 'N'
                             AND WH_OUT_NO IN (SELECT WH_OUT_NO
                                                 FROM WH_OUT_MST
                                                WHERE OUT_CL = '0002'
                                                  AND DEL_FG = 'N'
                                                  AND RPT_NO = (SELECT RPT_NO
                                                                  FROM FUNRL_RPT
                                                                 WHERE DEL_FG = 'N'
