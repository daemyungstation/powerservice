        SELECT /* DlwEvntMap.getCloseDataSaveYnChk */
               CASE WHEN NVL((SELECT CASE WHEN (RESN.RESN_CL = '02' OR (RESN.RESN_CL != '02' AND NVL(RESN.RESN_PROC_YN, 'N') = 'Y'))
