    <!-- 일자별 지부별 물품 사용 조회 - 헤더 -->
    <select id="DlwEvntMap.getGdsUseList_Head" parameterType="map" resultType="resultMap">
        SELECT /* DlwEvntMap.getGdsUseList_Head */
               WH_CD,
               WH_NM
        FROM WAREHOUSE_MNG
        WHERE DEL_FG = 'N'
          AND WH_USE_YN = 'Y'
