                                         </if>
                                         <if test="mngr_cd != null and mngr_cd != '' ">
                                             AND EMN.REAL_EVT_MNGR_CD = #{mngr_cd}
                                         </if>
                                       ) TBL2 ON TBL1.LOC = TBL2.LOC
