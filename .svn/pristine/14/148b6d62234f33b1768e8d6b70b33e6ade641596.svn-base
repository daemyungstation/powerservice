             <!-- , CASH_APPR_NO cashApprNo
             , APPR_AMT apprAmt -->
             , CARD_PAY_AMT
             , CASH_PAY_AMT
             , DEDUCT_AMT AS OUT_TOT_DED_AMT
             , NVL(CASE WHEN EVT_COST_OUT IS NULL
                           THEN (SELECT EVT_COST_OUT FROM PRODUCT WHERE PROD_CD = RIGHT(LEFT(#{accnt_no}, 6), 2))
