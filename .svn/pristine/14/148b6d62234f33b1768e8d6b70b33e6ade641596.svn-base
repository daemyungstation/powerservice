                      LEFT OUTER JOIN EVENT_MANAGER_MNG C ON B.EVT_MNGR_CD = C.EVT_MNGR_CD
                      LEFT OUTER JOIN WAREHOUSE_MNG D ON C.WH_CD = D.WH_CD
