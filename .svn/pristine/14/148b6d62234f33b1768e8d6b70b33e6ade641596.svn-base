               '' AS CHK,
               ROW_NUMBER()OVER(ORDER BY A.GDS_CD) RNUM,
               B.GDS_CD,
               B.GDS_NM,
               B.STANDARD,
               A.QTY,
               B.UNIT,
               A.UNIT_COST,
               A.AMT,
               #{reg_man} EMP_CD,
               FN_EMPLE_NM(#{reg_man}) EMP_NM
        FROM FUNRL_RPT_GOODS A
            LEFT OUTER JOIN EVENT_GOODS_MNG_MST B ON A.GDS_CD = B.GDS_CD
        WHERE  A.DEL_FG = 'N'
          AND A.SITE_BUY_YN = 'N'
          AND A.GDS_CL = '0001'
          AND NVL(A.FRG_CL,' ') IN('0001','0005')
          AND NVL(B.GDS_GRP_CD, ' ') != '0009'
          AND A.RPT_NO = #{rpt_no}
    </select>

