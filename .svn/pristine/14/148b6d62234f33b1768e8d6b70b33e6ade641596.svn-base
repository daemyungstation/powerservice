    <!-- 외주 행사 사용물품 등록 조회 -->
    <select id="DlwEvntMap.getEvtRptOutDtl5List" parameterType="map" resultType="resultMap">
    <!-- DM sql-id : sql-task.xml : selectEvtRptOutDtl5List -->


        SELECT /* DlwEvntMap.getEvtRptOutDtl5List */
               FOG.FUNRL_OUTSRC_NO AS RPT_NO
             , FOG.DTL_SEQ
             , FOG.GDS_CD
             , (SELECT GDS_NM FROM EVENT_GOODS_MNG_MST WHERE GDS_CD = FOG.GDS_CD) AS GDS_NM
             , FOG.QTY
