function Button01_onclick(obj:Button,  e:ClickEventInfo)
{
	this.ds_List.clearData();
	
	var nCellIndex = 0;
	
	for(var i=0;i<this.ds_Data.rowcount;i++)
	{
		if(i%6 == 0) {
			nCellIndex = 0;
			
			var nRowTitle = this.ds_List.addRow();
			var nRowValue = this.ds_List.addRow();
			
			ds_List.setColumn(nRowTitle, "lvl", 1);
			ds_List.setColumn(nRowValue, "lvl", 0);
		}
		
		ds_List.setColumn(nRowTitle, "Column"+nCellIndex, ds_Data.getColumn(i, "code"));
		ds_List.setColumn(nRowValue, "Column"+nCellIndex, ds_Data.getColumn(i, "value")+" 원");
		
		nCellIndex++;
	}
}


