//  			ds_inqCndt_save.setColumn(nRow, "sub_trgt_list_id", "STL202204013724183"); //개발 STL202204013724183
			ds_inqCndt_save.setColumn(nRow, "trgt_list_id", "TAR202204063724403"); //개발 TAR202204013724182
