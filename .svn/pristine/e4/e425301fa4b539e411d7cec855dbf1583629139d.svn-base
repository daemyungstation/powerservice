    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var nTotalListCnt = 0;
var nBatchSuccessCode = 0;
var gblDir = "";

var input1;
var input2;
var input3;

var gvMenuId;  // 프로그램 로그 추가 2017.12.14
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL450700_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gvMenuId = GV_LOGMENUID;
	gfn_getList("dlwCd","0193","fn_callBack",false);
	fn_init();
	//trace("화면명은 :::: 450700 이고 ::: " +gds_UserInfo.saveXML());
	//alert("버튼권한은 :: " + gds_UserInfo.getColumn(0, "user_ui_button"));
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		return false;
	}
	
	switch (svcid) 
	{
		case "selectDlwCode" :
			gfn_getCode("0193", "SELECT", ds_codeFileType, gds_DmlCd); // 코드조회결과 필터
		break;
		
		case "getMbidCsvFileList" : // 문자발송이력 조회		    			
		    gfn_appendSearchDs(ds_listMbidCsvFileList, ds_listMbidCsvFileListTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_listMbidCsvFileList, sta_totalCnt, nTotalListCnt); // 페이지건수 표출
			fn_postSearch(svcid);
		break;
		
		case "getMbidCsvFileListExcel" : 
			//gfn_exportGridToExcel(grd_listExcel, "고객청구산출현황_"+gfn_getTodayFull()+"");
			alert("엑셀 다운로드가 완료되었습니다.");
		break;
		
		case "batchMbidCsvFileMake" :
			if(nBatchSuccessCode == "0")
			{
				alert("배치성공!");
			}
			else
			{
				alert("배치실패");
			}
		break;
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType, sSvcId)
{
	switch (sSvcId) 
	{
		case "getMbidCsvFileList" :
		    if (sType == gfn_getCommTypeInit())
			{				
			    var cExtDt = cal_extDt.value;
				var sExtDt = new String(cExtDt);
				
				if(gfn_IsNull(sExtDt) == false && sExtDt.length == 8)
				{
					ds_inqCndt.clearData();
					ds_listMbidCsvFileList.keystring = ""; // 정렬컬럼 초기화
					
					var nRow = ds_inqCndt.addRow();
	
					ds_inqCndt.setColumn(nRow, "ext_dt"        , sExtDt);
					ds_inqCndt.setColumn(nRow, "file_type"     , cbo_fileType.value);
					ds_inqCndt.setColumn(nRow, "orderBy"       , "FILE_TYPE");
					ds_inqCndt.setColumn(nRow, "orderDirection", "ASC");
				}
				else
				{
					alert("조회날짜를 정확하게 입력하여주십시오.");
					return false;
				}
			}
			return true;
		break;
		
		default :
			alert("서비스명이 잘못되었습니다.");
			return false;
		break;
	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 문자발송이력 조회
function fn_commSearch(sType, sSvcId)
{
	if (!fn_preSearch(sType, sSvcId)) return false;

	// 호출 건수 설정 및 조회여부 설정
	if (!gfn_pageSearch(sType, ds_listMbidCsvFileList, sta_totalCnt, 100, nTotalListCnt)) return;

	var szSvcid       = "getMbidCsvFileList";
    var szController  = "/dlw/pay/getMbidCsvFileList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_listMbidCsvFileListTemp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	var sToday = gfn_getToday();
	var sStartDate = gfn_addDate(sToday, -3);

	cal_extDt.value  = sToday;
	cbo_fileType.index    = 0;
	
	ds_inqCndt.clearData();
	ds_listMbidCsvFileList.clearData();
	ds_listMbidCsvFileListTemp.clearData();
	ds_listMbidCsvFileListExcel.clearData();
}

// 조회결과
function fn_postSearch(sSvcId)
{
	switch (sSvcId) 
	{
		case "getMbidCsvFileList" :
			var nListCnt = ds_listMbidCsvFileList.getRowCount();
			if(nListCnt <= 0)
			{
				alert("조회된 데이터가 없습니다.");
				return false;
			}
		break;
	}
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 조회
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit(), "getMbidCsvFileList"); 
}

// 초기화
function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

// 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/dlw/pay/getMbidCsvFileList"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명

	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_listMbidCsvFileList, sta_totalCnt, 100, nTotalListCnt)) return;
	//gfn_grdHeadClick(obj, e, ds_inqCndt, null, ds_list_tmp ,ds_nobd_list);
	gfn_grdHeadClick(obj, e, ds_inqCndt, null, ds_listMbidCsvFileListTemp);
	gfn_setTotalRowCount(ds_listMbidCsvFileList, sta_totalCnt, nTotalListCnt); // 페이지건수 표출
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend(), "getMbidCsvFileList");
}

// onkeydown
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) 
	{
		fn_commSearch(gfn_getCommTypeInit(), "getMbidCsvFileList");
	}
}

function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_listMbidCsvFileList.getRowCount() > 0) 
	{		
		var colDS = gfn_excelPrepareExcelDS("grd_list");
		gfn_excelDown("출금이체의뢰내역", "XLS_DLCC", "DlwPayMap.getMbidCsvFileList", "DlwPayMap.getMbidCsvFileListCount", ds_inqCndt, true, colDS, true);
	} 
	else 
	{
		alert("엑셀다운로드 할 대상이 없습니다.");
	}		
}

function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.col == 9)
	{
		if(confirm("다운로드를 수행하시겠습니까?") == true)
		{
			var sExtDt    = gfn_getDateFormat(ds_listMbidCsvFileList.getColumn(ds_listMbidCsvFileList.rowposition, "ext_dt"), "");
			var sFileType = ds_listMbidCsvFileList.getColumn(ds_listMbidCsvFileList.rowposition, "file_type");
			var sFilePath = ds_listMbidCsvFileList.getColumn(ds_listMbidCsvFileList.rowposition, "file_path");
			var sFileName = ds_listMbidCsvFileList.getColumn(ds_listMbidCsvFileList.rowposition, "file_name");
			
			if ( (gfn_isEmpty(sExtDt) == true) && (gfn_isEmpty(sFileType) == true) && (gfn_isEmpty(sFilePath) == true) ) 
			{
				alert("파일추출일, 파일타입, 파일경로가 누락되었습니다.");
				return false;
			}
		
			var sUrl = application.services["SVC"].url;
			sUrl = sUrl + "/dlw/pay/mbidDownLoadFile?ext_dt=" + sExtDt + "&file_type=" + sFileType + "&file_path=" + sFilePath;
	   
			var dir = FileDialog00.open("EB21파일 다운로드", FileDialog.SELFOLDER);
			if (gfn_isEmpty(dir)) 
			{
				return;
			}

			gblDir = dir + "\\" + sFileName;
			VirtualFile00.open(gblDir, VirtualFile.openWrite);	
			var bSucc = HttpObj.download(sUrl, VirtualFile00 ,true);
		}
	}
}

function HttpObj_onload(obj:HttpObject, e:HttpObjLoadEventInfo)
{
	if (e.errorcode == 0) {
		alert("다운로드가 완료되었습니다."); 
		fn_commSearch(gfn_getCommTypeInit(), "getMbidCsvFileList"); 
	}
	else {
		alert("다운로드 중 오류가 발생하였습니다.");
	    //VirtualFile.delete(gblDir,false)
		var objExtCommon = new ExtCommon(); 
		var ret = objExtCommon.DeleteFile(gblDir); 
