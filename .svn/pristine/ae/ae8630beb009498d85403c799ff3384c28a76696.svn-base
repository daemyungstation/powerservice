 * 기      능		: (공통버튼)조회 프로세스

 ***************************************************************************/
function fn_commSearch(sType)
{
	ds_trscSystDtpt.clearData();

	if (!fn_preSearch(sType)) return false;

	// 페이지 처리 기본설정
	if(!gfn_pageSearch(gfn_getTranTypeMultiDIS(), sType, ds_trscSyst, div_body.sta_TotalCnt, null, fv_pasingDatasetList1)) return;

	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_trscCode("SYS00002R19");
	if (sType != gfn_getCommTypeAppend()) {
		gfn_tranMultiDIS(sType
						, "ds_input00=ds_input00 ds_pagingRequestDs1=ds_pagingRequestDs1"
						, "ds_pagingResponseDs1=ds_count ds_trscSyst_Temp=ds_trscSyst_Temp"
						, "fn_commSearch_callBack");
	} else {
		gfn_setKwData_sqlId("getListTrscSyst");
		gfn_tranMultiDIS(sType
						, "ds_input00=ds_input00 ds_pagingRequestDs1=ds_pagingRequestDs1"
						, "ds_trscSyst_Temp=ds_trscSyst_Temp"
						, "fn_commSearch_callBack");
	}

}

