function fn_getXML()
{
	if ( gfn_isNull(ds_input00.getColumn(0, "systAdmnDivCd")) ) {
		alert("'관리 구분'을 선택하여 주십시요.");
		return;
	}

	// KWData 설정
	gfn_kwDataInit();
	ds_DISXML.clearData();
	
	gfn_setKwData_trscCode("SYS00002R21");
	gfn_tranDIS(gfn_getCommTypeAppend()
				, "ds_input00=ds_input00"
				, "ds_DISXML=ds_DISXML"
				, "fn_getXML_callBack");
}

function fn_getXML_callBack()
{

	var divCd = ds_input00.getColumn(0,"systAdmnDivCd");

	// Create Object
	var fopendlg = new FileDialog;
	
	// Create Buffer
	var buf = new Buffer();
	var context = null;
	var nLen = 0;
	
	for(i = 0; i < ds_DISXML.getRowCount(); i++){
		context = ds_DISXML.getColumn(i,"context");
		// insert string to buffer
		nLen = buf.appendText(context);
		//trace("buf size :"+nLen);
	}
	
	// Choose file to open
	var virtualfile = fopendlg.open("DIS XML Save", FileDialog.SAVE,'C:','KRPAppWasMap'+divCd+'.xml'); 
	
	// Open VirtualFile
	virtualfile.open(VirtualFile.openWrite);


	// Write file by buffer
	var bWrite = virtualfile.write(buf.getText(0));
	trace("bWrite :"+bWrite);
	
	// Close virtualFile
	var bClose = virtualfile.close();
	trace("bClose :"+bClose);
	
	// destroy Buffer Object
	buf = null;
}

