function ds_trscSyst_canrowposchange(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	if ( gfn_isUpdateDs(ds_trscSystDtpt) ) {
		if(!confirm("현재 수정중인 데이타가 있습니다. 현재 수정 중인 데이타는 전체 초기화됩니다. \n\n초기화 하시겠습니까?")) {
			return false;
		}
	}
	return true;
}

function ds_trscSystDtpt_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var fflag = ds_trscSystDtpt.getColumn(e.row, "fflag");
	if (fflag == gfn_getRowTypeInsert() ) {
		if (gfn_isNull(ds_trscSystDtpt.getColumn(e.row, "systAtrbCntn"))) {
			ds_trscSystDtpt.setColumn(obj.rowposition, gfn_getColNmChk(), undefined);
			ds_trscSystDtpt.setColumn(obj.rowposition, gfn_getColNmStatus(), "");
		} else {
			ds_trscSystDtpt.setColumn(obj.rowposition, gfn_getColNmChk(), 1);
			ds_trscSystDtpt.setColumn(obj.rowposition, gfn_getColNmStatus(), gfn_getRowTypeInsert());
		}
	} else {
		ds_trscSystDtpt.enableevent = false;

		var chk = ds_trscSystDtpt.getColumn(e.row, gfn_getColNmChk());
		var fflag = ds_trscSystDtpt.getColumn(e.row, "fflag");

		if (chk == "1" && fflag == gfn_getRowTypeUpdate() ) {
			ds_trscSystDtpt.setColumn(e.row, gfn_getColNmStatus(), gfn_getRowTypeUpdate());
		}
		if (chk == "0" && fflag == gfn_getRowTypeUpdate() ) {
			ds_trscSystDtpt.setColumn(e.row, gfn_getColNmStatus(), gfn_getRowTypeDelete());
		}
		ds_trscSystDtpt.enableevent = true;
	}
	return true;
}

/***************************************************************************
 * 기      능		: 상위에 포지션이 변경되면 하위의 데이타를 새로 셀렉트함
 ***************************************************************************/
function ds_trscSyst_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	if (ds_trscSyst.getColumn(e.newrow, gfn_getColNmStatus()) != gfn_getRowTypeInsert()) {
		// 트랜젝션상세정보 초기화
		ds_trscSystDtpt.clearData();
		ds_input.setColumn(0, "trscSystIdntNo", ds_trscSyst.getColumn(e.newrow, "trscSystIdntNo"));
		fn_commDtptSearch();
	}
}

