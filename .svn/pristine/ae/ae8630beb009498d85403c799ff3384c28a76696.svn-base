	// KWData 설정
	gfn_kwDataInit();

	gfn_setKwData_trscCode("SYS00002R20");
	gfn_tranDIS(gfn_getCommTypeAppend()
				, "ds_input=ds_input"
				, "ds_trscSystDtpt=ds_trscSystDtpt"
				, "fn_commDtptSearch_callBack");
}

