function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd);
		return false;
	} 
	else 
	{
		switch (svcid) 
		{
			case "srchCdSet": //코드셋						
				gfn_getCode("CAM090", "ALL", ds_dpmsReslCdList,ds_cdSet); //통화결과
				
