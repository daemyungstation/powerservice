			    gfn_appendSearchDs(ds_uplusList, ds_uplusListTemp);
			    gfn_setTotalRowCount(ds_uplusList, sta_TotalCnt, tc_prod);
			    fn_postSearch(gfn_getCommTypeInit(), svcid);
			break;
			
			case "insertUplusList" :
				alert("데이터 가져오기가 완료되었습니다.");
				fn_commSearch(gfn_getCommTypeInit(), "getUplusMainList");
			break;
