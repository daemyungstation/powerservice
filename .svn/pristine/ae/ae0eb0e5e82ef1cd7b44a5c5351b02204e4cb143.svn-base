// VOC 조회
function fn_commSearch(sType, svcId)
{
	if (!fn_preSearch(sType, "getUplusMainList"))
	{
	    return false;
	}
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_uplusList, sta_TotalCnt, 50, tc_prod)) return;
	
	var szSvcid       = "getUplusMainList";
    var szController  = "/dlw/uplus/getUplusMainList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_uplusListTemp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, "", "", "S", gvMenuId);
}

// 검색 결과
function fn_postSearch(sType, svcId)
{
	switch(svcId) 
	{
		case "getUplusMainList" :
			if (sType == gfn_getCommTypeInit())	
			{
				if (ds_uplusList.getRowCount() < 1) 
				{
					//ds_uplusSingle.clearData();
					//fn_setCRUDComponentType("NULL");
					alert("조회된 데이터가 없습니다.");
				} 
			}
		break;
	}
	
	return true;
}

// 저장 버튼
function fn_commSave(sType, svcId)
{
	switch(svcId)
	{
	    case "insertUplusList":
			if (!fn_preSave(sType, "insertUplusList")) 
			{
				return false;
			}
			
			var szSvcid       = "insertUplusList";
			var szController  = "/dlw/uplus/insertUplusList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";

			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, "", "", "I", gvMenuId);
	    break;
	}
}
