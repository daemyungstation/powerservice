Alert
{
	margin 				: 0 0 20 0 ;
	background 			: URL('Image::Alert_page.png') stretch 12,8 ;
	border 				: 0 none ;
	buttonheight			: 28  ;
	buttonwidth			: 83  ;
	color				: #0072cf ;
	font 				: Malgun Gothic, 19, antialias ;
	icon				: URL('Image::ico_Alert.png') ;
	iconalign 			: left middle ;
	iconpadding 			: 10 0 0 0 ;
	okbuttontext			: OK ;
	titlebackground 		: URL('Image::Alert_title.png') stretch 9,10 ;
	titleheight 			: 27 ;
	padding 			: 34 46 19 28 ;
	titlecolor 			: #0068bc ;
	titlefont 			: Malgun Gothic, 12, antialias ;
	titletext 			: Alert ;
	titlepadding 			: 2 0 0 15 ;
}Alert>#okbutton
{
	align				: center middle ;
	background			: @gradation ;
	border				: 2 double #79abd2 #98c2e2 ;
	bordertype			: round 5 5 ;
	color				: #feffff ;
	font 				: Malgun Gothic, 8, bold antialias ;
	gradation			: linear 50,0 #94c1e4 50,100 #67a7d8 ;
	pusheddrawoffset 		: 0 0 ;
	shadow 				: drop 0,1 3 #8080807d ;
	focusborder 			: 0 none ;
	cursor 				: hand ;
	margin 				: -19 0 19 0 ;
}
Alert>#okbutton:mouseover
{
	background 			: #70acda ;
}
Alert>#okbutton:pushed
{
	background 			: #70acda ;
	border 				: 2 double #70acda #70acda ;
}
Alert>#closebutton
{
	background			: transparent ;
	border				: 0 none ;
	image 				: URL('Image::Alert_close.png') ;
	cursor 				: hand ;
	focusborder 			: 0 none ;
	pusheddrawoffset 		: 0 0 ;
}
