function fn_postSearchAll(sType)
{
	if(ds_listAll.rowcount < 1)	{

		//TO-DO: 조회 결과 메세지처리

	} else {
		
		//TO-DO: 조회 결과 메세지처리
		var fopendlg = new FileDialog;
		var path = fopendlg.open("FileSave", FileDialog.SELFOLDER);
		//trace(path+ds_info.getColumn(0,"fileNm"));
		trace(path);
		if(path != null){
			var virtualfile = new VirtualFile(path + "\\" + ds_info.getColumn(0,"fileNm"), VirtualFile.openWrite);

			var bSuccess;
			for(var i = 0; i < ds_listAll.rowcount; i++){
				bSuccess = virtualfile.write(ds_listAll.getColumn(i,"spczCntnStr"));
				//trace(ds_list.getColumn(i,"spczCntnStr"));
				if(bSuccess == false){
					i = ds_listAll.rowcount;
					alert("파일 작성중 에러가 발생 하였습니다.");
					return;
				}
			}
			trace("name:"+virtualfile.filename+"::size:"+virtualfile.getFileSize());
			var filePath = virtualfile.fullpath;
			alert(filePath + " 파일이 작성되었습니다.");
			virtualfile.close();
		}

	}
}



