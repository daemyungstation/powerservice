function fn_dataParsing(nRow) {
	var dataType = ds_list.getColumn(nRow,"dataDiv");
	//trace(dataType);
	var data = ds_list.getColumn(nRow,"spczCntnStr");
	
	ds_layout.filter("spczTypNm =='"+dataType+"'");
	var cnt = ds_layoutView.copyData(ds_layout,true);
	ds_layoutView.addColumn( "data", "string" );

	//trace(ds_layoutView.saveXML());
	//var pos = 0;
	
	for(var i =0 ;i < ds_layoutView.rowcount; i++){
	
		var len = ds_layoutView.getColumn(i,"spczLngt");
		var stt = ds_layoutView.getColumn(i,"sttStnVl");
		var colData = data.substr(stt,len);
		ds_layoutView.setColumn(i,"data",colData);
		//pos = pos + len
	}
}


