    <Script type="xscript4.0"><![CDATA[/**************************************************************************
 [ PART 1 ]공통 js 파일 include
***************************************************************************/
include "LIB::lib_All.xjs";

var gv_prod_cd = "";
var gv_prod_nm = "";

/**************************************************************************
 [ PART 2 ] Form에서 사용될 전역변수를 선언
**************************************************************************/
var tc_prod = 0;

/***************************************************************************
 * 함  수  명		: prod_onload()
 * 기      능		: 폼 onload이벤트 
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function prod_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	
	if(!gfn_isNull(p_prod_cd)) {
		p_prod_cd = p_prod_cd.toUpperCase();
		gv_prod_cd = p_prod_cd;
		prod_cd.value = gv_prod_cd;
		p_prod_cd = null;
	}
	
	if (p_mode == "edit"){
		fn_commSearch(gfn_getCommTypeInit()); // 조회
	}	
}

/***************************************************************************
 * 함  수  명		: btn_srch_onclick()
 * 기      능 		: 조회버튼 클릭
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/***************************************************************************
 * 함  수  명		: fn_init()
 * 기      능 		: 초기화
 ***************************************************************************/
function fn_init()
{	
	prod_cd.value = "";
	prod_nm.value = "";
	chk_saleCl.value = "N";
}

/***************************************************************************
 * 함  수  명		: fn_preSearch()
 * 기      능		: 조회 전 처리 조회조건 설정
 * 수  정  일      : 2016.01.22 
 ***************************************************************************/
function fn_preSearch(sType)
{
	var prodNm = prod_nm.value;
	if ( !gfn_isEmpty(prodNm) ) {
		prodNm = prodNm.toUpperCase();
	}
	
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_prod.keystring = ""; // 정렬컬럼 초기화
	
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "prod_cd", prod_cd.value);
		ds_inqCndt.setColumn(0, "prod_nm", prodNm);
		ds_inqCndt.setColumn(0, "sale_cl", chk_saleCl.value);
		ds_inqCndt.setColumn(0, "del_fg", "N");
				
		ds_inqCndt.setColumn(0, "orderBy", "order_seq");
		ds_inqCndt.setColumn(0, "orderDirection", "asc");
	}
	return true;
}

/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능 		: 조회
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function fn_commSearch(sType)
{	
    if (!fn_preSearch(sType)) return false;	
	
	// 호출 레코드 수
	if(!gfn_pageSearch(sType, ds_prod, sta_TotalCnt, 10, tc_prod)) return; 
	
	var szSvcid       = "searchProdList";
    var szController  = "/dlw/newTypeCons/product_list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_srchCallBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/***************************************************************************
 * 함  수  명		: fn_srchCallBack()
 * 기      능		: 조회 콜백 처리
 ***************************************************************************/
function fn_srchCallBack(svcid, errcd, errmsg) {
  
    if (errcd < 0) {
		alert("조회를 실패하였습니다.\n" + errmsg);
    } else {
		gfn_appendSearchDs(ds_prod, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		gfn_setTotalRowCount(ds_prod, sta_TotalCnt, tc_prod); // 페이지건수 조회 처리
		fn_postSearch();	
    }
}

/***************************************************************************
 * 함  수  명		: fn_postSearch()
 * 기      능		: 처리결과
 ***************************************************************************/
function fn_postSearch()
{
	if (ds_prod.rowcount < 1) {
		alert("조회 결과가 없습니다.");
		return;
	}
	
	// 팝업 파라미터가 존재하고 조회결과가 1건인 경우 바로 선택
	if ((!gfn_isNull(gv_prod_cd) || !gfn_isNull(gv_prod_nm)) &&
	    ds_prod.rowcount == 1) {
		fn_selProductChk();
	}
	
	if (ds_prod.rowcount > 1) {
		grd_prodList.setFocus();
	}
}

/***************************************************************************
 * 함  수  명		: fn_selProductChk(e:GridClickEventInfo)
 * 기      능		: 상품선택
 ***************************************************************************/
function fn_selProductChk(e:GridClickEventInfo) 
{
	if (ds_prod.rowcount < 1) {
		alert("조회 결과가 없습니다.");
	    return;
	}
	
    var nSelectRow = -1;
    if (e != null) {
        nSelectRow = e.row;
    } else {
        nSelectRow = ds_prod.rowposition;
    }
    if (nSelectRow == -1) {
        return;
    }
	opener.ds_prodList.clearData();
	opener.ds_prodList.addRow();
	opener.ds_prodList.copyRow(0, ds_prod, nSelectRow);

	opener.fn_prodChcResl(); // 상품 선택 결과 호출
	
	close();
}
/***************************************************************************
 * 함  수  명		: btn_close_onclick()
 * 기      능		: x 버튼 이벤트
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

/***************************************************************************
 * 함  수  명		: btn_init_onclick()
 * 기      능		: 초기화 버튼클릭시
 * 수  정  일      : 2016.01.22
 ***************************************************************************/
function btn_Init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

/***************************************************************************
 * 함  수  명		: grd_list_oncellclick()
 * 기      능		: 선택 버튼 이벤트
 ***************************************************************************/
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.cell == "7"){
	    fn_selProductChk(e);
	}
}
/***************************************************************************
 * 함  수  명		: grd_list_oncelldblclick()
 * 기      능		: 더블 클릭 이벤트
 ***************************************************************************/
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	fn_selProductChk(e);
}

/***************************************************************************
 * 기      능		: (공통) 그리드 onheadclick이벤트 발생시에 전체체크(체크박스), 칼럼정렬 기능 추가
 ***************************************************************************/
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/dlw/prod/list");
	ds_inqCndt.setColumn(0, "dsName", "ds_output");
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_prod, sta_TotalCnt, 10, tc_prod)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_prod, sta_TotalCnt, tc_prod); // 페이지건수 표출
}

/***************************************************************************
 * 기      능		: (공통) 그리드 칼럼사이즈 변경시 Hidden 처리
						단, 칼럼사이즈를 0으로 변경한 후에는 공통기능버튼
							(칼럼 보이기/숨기기)으로 다시 보이기 처리
 ***************************************************************************/
function grd_list_oncolresized(obj:Grid, e:GridSizeChangedEventInfo)
{
	if(e.oldvalue == 0) {
		obj.setRealColSize(e.formatindex, 0);
	}
}

/***************************************************************************
 * 기      능		: (공통) 엔터 치면 조회실행
 ***************************************************************************/
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) {
		fn_commSearch(gfn_getCommTypeInit());
	}
}

/***************************************************************************
 * 기      능		: (공통) 그리드 마지막행 이후 스크롤시 Append 조회 처리
						페이징처리 대상인 경우만 해당
 ***************************************************************************/
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
