    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_cust_memo = 0; // 전체건수
var sNewEmshId = ""; //부모창의 평가지 아이디
var sSelectcheck = "";//부모창의 평가문제 아이디들

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS240201_onload(obj:Form, e:LoadEventInfo)
{
	sNewEmshId = opener.ds_list_emsh.getColumn(opener.ds_list_emsh.rowposition, "emsh_id");
	
	for (var i=0; i<opener.ds_list_examPrbm.rowcount; i++) {
		if(opener.ds_list_examPrbm.getColumn(i, "rowCheck") == "1"){
			sSelectcheck += opener.ds_list_examPrbm.getColumn(i, "exam_prbm_id")+",";
		}
	}
	
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_init();
	fn_commSearch(gfn_getCommTypeInit());
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "getEmshList":
			//trace(ds_list_tmp.saveXML());
 			gfn_appendSearchDs(ds_list_emsh, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_emsh, sta_totalCnt, tc_cust_memo); // 페이지건수 표출						
			break;
			
			case "copyEmsh":
			opener.fn_commSearch(gfn_getCommTypeInit());
			close(); 
            break;
 		}	
 	}		
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_list_emsh.clearData();
		ds_inqCndt.clearData();
		ds_list_emsh.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "emsh_nm", edt_emsh_nm.value);
		ds_inqCndt.setColumn(0, "orderBy", "AMNT_DTTM");
		ds_inqCndt.setColumn(0, "orderDirection", "DESC");
	}
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_emsh, sta_totalCnt, 25, tc_cust_memo)) return;
	
	var szSvcid       = "getEmshList";
    var szController  = "/exam/emsh/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//복사
function fn_copy()
{
	if(!confirm("복사하시겠습니까?")) {
		return false;
	}

	ds_list_emshTmp.clearData();
	ds_list_emshTmp.addRow();
	ds_list_emshTmp.copyRow(0, ds_list_emsh, ds_list_emsh.rowposition);
	ds_list_emshTmp.setColumn(0, "new_emsh_id", ds_list_emshTmp.getColumn(0, "emsh_id"));//현재창의 평가지 아이디
	ds_list_emshTmp.setColumn(0, "emsh_id", sNewEmshId);//부모창의 평가지 아이디
	ds_list_emshTmp.setColumn(0, "selectcheck", sSelectcheck);//부모창의 평가문제 아이디들
	
	//trace(ds_list_emshTmp.saveXML());
	
	var szSvcid       = "copyEmsh";
    var szController  = "/exam/emsh/copy";
    var szInDs        = "ds_input=ds_list_emshTmp";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	edt_emsh_nm.value = "";
}

/***************************************************************************
 * 함  수  명		: btn_close_onclick()
 * 기      능		: x 버튼 이벤트
 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

//초기화
function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

//조회
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit());
}

//그리드헤더클릭
function Grid00_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	gfn_grdHeadClick(obj, e);
}

//그리드셀클릭
function Grid00_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.cell == "8"){
		fn_copy();
	}
}
