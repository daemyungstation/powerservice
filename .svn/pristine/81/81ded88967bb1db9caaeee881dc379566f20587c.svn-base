		gfn_exportGridToExcel(tab_perform.tp_performWeek.grd_list, "실적현황(주간보고)_" + gfn_getTodayFull() + "");
		gfn_exportGridToExcel(tab_perform.tp_performWeek.grd_listTotPerform, "전체실적현황_" + gfn_getTodayFull() + "");
	}
	else if(sTabPageId == "tp_performMonth")
	{
		gfn_exportGridToExcel(tab_perform.tp_performMonth.grd_list, "실적현황(월간접수실적)_" + gfn_getTodayFull() + "");
	}
	else
	{
			
	}
}

