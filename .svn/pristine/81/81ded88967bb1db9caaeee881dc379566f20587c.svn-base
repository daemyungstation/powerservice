    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt1;
var nTotalListCnt2;

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS101700_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj);
	fn_init();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(sSvcId, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd);
		return false;
	} 
	else 
	{
		switch (sSvcId) 
		{
			case "getPerformInfo":
				var sTabPageId = tab_perform.tabpages[tab_perform.tabindex].name;
				
				if(sTabPageId == "tp_performWeek") 
				{
					gfn_setTotalRowCountOfNotPaging(tab_perform.tp_performWeek.sta_TotalCnt, nTotalListCnt1); // 페이지건수 표출
		            fn_postSearch(sSvcId);
				} 
				else if(sTabPageId == "tp_performMonth") 
				{
					gfn_setTotalRowCountOfNotPaging(tab_perform.tp_performMonth.sta_TotalCnt, nTotalListCnt2); // 페이지건수 표출
		            fn_postSearch(sSvcId);
				} 
				else
				{
					alert("할당되지 아니한 업무영역입니다.");
					return false;
				}
			break;
			
			case "insertPerformInfo":
				fn_postSave(sSvcId);
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType, sSvcId)
{
	if (gfn_isNull(sSvcId) == true) 
	{
	    alert("서비스명이 올바르지 않습니다.");
	    return false;
	} 
	
	switch(sSvcId) 
	{
		case "getPerformInfo" :
		    var sSttRegDt = new String(cal_sttRegDt.value);
		    var sYyyyMm = cal_sttYyyyMm.getDate();
		    
			if(gfn_isNull(sSttRegDt) == true || gfn_isNull(sYyyyMm) == true)
			{
				alert("마감일자 또는 기준월을 정확히 입력하여 주십시오");
				return false;
			}
			
			var sTabPageId = tab_perform.tabpages[tab_perform.tabindex].name;
			var sSttRegStartDt = sYyyyMm + "01"; // 기준월의 첫날 
			var sSttRegEndDt = sYyyyMm + gfn_LastDateNum(sSttRegDt); // 기준월의마지막날
			var sPreYearMonth = gfn_addMonth(sYyyyMm, -12).substr(0,6); // 작년월
			var sPrePreMonth = gfn_addMonth(sYyyyMm, -1).substr(0,6); // 전월
			var sCurMonth = sYyyyMm; // 당월
			var sPreYearStartMonth = gfn_addMonth(sYyyyMm, -12).substr(0,4) + "01"; // 작년첫번째월
			var sPreYearEndMonth = gfn_addMonth(sYyyyMm, -12).substr(0,4) + "12"; // 작년마지막월
			var sCurStartMonth = sYyyyMm.substr(0,4) + "01"; // 금년첫번째월
			var sCurEndMonth = sYyyyMm.substr(0,4) + "12"; //금년마지막월
			
			trace(sSttRegDt + ":" + sSttRegStartDt + ":" + sSttRegEndDt + ":" + sPreYearMonth + ":" + sPrePreMonth 
			      + ":" + sCurMonth + ":" + sPreYearStartMonth + ":" + sPreYearEndMonth + ":" + sCurStartMonth 
			      + ":" + sCurEndMonth + ":" + sTabPageId);
			
			ds_inqCndt.clearData();
			var nRow = ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(nRow, "svc_id", sTabPageId);
			ds_inqCndt.setColumn(nRow, "stt_reg_dt", sSttRegDt);
			ds_inqCndt.setColumn(nRow, "stt_reg_start_dt", sSttRegStartDt);
			ds_inqCndt.setColumn(nRow, "stt_reg_end_dt", sSttRegEndDt);
			ds_inqCndt.setColumn(nRow, "pre_year_month", sPreYearMonth);
			ds_inqCndt.setColumn(nRow, "pre_pre_month", sPrePreMonth);
			ds_inqCndt.setColumn(nRow, "cur_month", sCurMonth);
			ds_inqCndt.setColumn(nRow, "pre_year_start_month", sPreYearStartMonth);
			ds_inqCndt.setColumn(nRow, "pre_year_end_month", sPreYearEndMonth);
			ds_inqCndt.setColumn(nRow, "cur_start_month", sCurStartMonth);
			ds_inqCndt.setColumn(nRow, "cur_end_month", sCurEndMonth);
		break;
	}
	
	return true;
}

function fn_preSave(sType, sSvcId)
{
	if (gfn_isNull(sSvcId) == true) 
	{
	    alert("서비스명이 올바르지 않습니다.");
	    return false;
	} 
	
	switch(sSvcId) 
	{
		case "insertPerformInfo" :
		    var sSttRegDt = new String(cal_sttRegDt.value);
		    
			if(gfn_isNull(sSttRegDt) == true)
			{
				alert("기준일자를 정확히 입력하여 주십시오");
				return false;
			}
			
			if(confirm("실적마감 산출을 실행하시겠습니까?") == false) 
	        {
		        return false;
	        }
			
			var sTabPageId = tab_perform.tabpages[tab_perform.tabindex].name;
			
			trace(sTabPageId + ":" + sSttRegDt);
			
			ds_inqCndt.clearData();
			var nRow = ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(nRow, "svc_id", sTabPageId);
			ds_inqCndt.setColumn(nRow, "stt_reg_dt", sSttRegDt);
		break;
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 실적 통계 조회
function fn_commSearch(sType, sSvcId)
{
	if (fn_preSearch(sType, sSvcId) == false) 
	{
		return false;
	}
	
	switch(sSvcId) 
	{
        case "getPerformInfo": 
			var szSvcid       = "getPerformInfo";
			var szController  = "/dlw/perform/ext/getPerformInfo";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listPerformWeek=ds_output1 ds_listPerformMonth=ds_output2 ds_listTotPerform=ds_outputTot";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;              
    }
}

function fn_commSave(sType, sSvcId)
{
	if (fn_preSave(sType, sSvcId) == false) 
	{
		return false;
	}
	
	switch(sSvcId) 
	{
        case "insertPerformInfo": 
			var szSvcid       = "insertPerformInfo";
			var szController  = "/dlw/perform/ext/insertPerformInfo";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;              
    }
}
/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	var sToday = gfn_getToday();
	var sMonth = gfn_sttMonth(sToday, 0);
	
	cal_sttRegDt.value = sToday;
	cal_sttYyyyMm.setDate(sMonth);
	
	tab_perform.tp_performWeek.sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
	tab_perform.tp_performMonth.sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
	
	ds_inqCndt.clearData();
	ds_listPerformWeek.clearData();
	ds_listPerformMonth.clearData();
	ds_listTotPerform.clearData();
}

// 조회 결과
function fn_postSearch(sSvcId)
{
	switch(sSvcId) 
	{
	    case "getPerformInfo" :
			var sTabPageId = tab_perform.tabpages[tab_perform.tabindex].name;
			
	        if(sTabPageId == "tp_performWeek")
	        {
				/*
				var sTotReg = tab_perform.tp_performWeek.edt_totReg.value;
				var sTotJoin = tab_perform.tp_performWeek.edt_totJoin.value;
				var sYearJoin = tab_perform.tp_performWeek.edt_yearJoin.value;
				var stotPre = tab_perform.tp_performWeek.edt_totPre.value;
				
				tab_perform.tp_performWeek.edt_totReg.value = gfn_setComma(sTotReg);
				tab_perform.tp_performWeek.edt_totJoin.value = gfn_setComma(sTotJoin);
				tab_perform.tp_performWeek.edt_yearJoin.value = gfn_setComma(sYearJoin);
				tab_perform.tp_performWeek.edt_totPre.value = gfn_setComma(stotPre);
				*/
	        }
			else if(sTabPageId == "tp_performMonth")
			{
				var nRow = ds_listPerformMonth.getRowCount();
				
				if(nRow <= 0)
				{
					alert("조회결과가 없습니다.");
				}
			}
			else
			{
			
			}
		break;
	}
}

function fn_postSave(sSvcId)
{
	switch(sSvcId) 
	{
	    case "insertPerformInfo" :
			alert("실적현황 마감산출이 완료되었습니다.");
			fn_commSearch(gfn_getCommTypeInit(), "getPerformInfo");
		break;
	}
}

