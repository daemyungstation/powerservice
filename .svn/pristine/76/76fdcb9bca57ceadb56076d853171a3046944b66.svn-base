//조회후 처리
function fn_postSearch()
{
	if(ds_list_tmp.rowcount < 1) {
		sta_totalCnt.text = "조회 결과가 없습니다.";
	}
	return;
}

function fn_unpyComponent(sFg){
	//납입방법
	stc_paymthd.visible = sFg;
	cbo_srch_pay_mthd.visible = sFg;
	
	//이체일자
	stc_ichae_dt.visible = sFg;
	edt_ichae_dt.visible = sFg;
	
	//당월회차
	stc_month_cnt.visible = sFg;
	edt_stt_month_cnt.visible = sFg;
	sta_andMonthCnt.visible = sFg;
	edt_end_month_cnt.visible =sFg;
	
	//실납입 회차
	stc_true_cnt.visible = sFg;
	edt_stt_true_cnt.visible = sFg;
	sta_andTrueCnt.visible = sFg;
	edt_end_true_cnt.visible = sFg;
	
	//연체회차
	sta_pay_mthd.visible = sFg;
	edt_stt_pay_state.visible= sFg;
	sta_andPayState.visible= sFg;
	edt_end_pay_state.visible= sFg;
	
	if(sFg == false){ // 유형별 조회조건이 다르기 때문에 추후 변경될수 있다. (억지로 셋팅 위치변경)
		stc_05.position.bottom = cbo_srch_cmpg_typ_cd1.position.bottom + 10
		btn_search.position.top = cbo_srch_cmpg_typ_cd1.position.top;
		btn_search.position.bottom = cbo_srch_cmpg_typ_cd1.position.bottom;
		btn_init.position.top = cbo_srch_cmpg_typ_cd1.position.top;
		btn_init.position.bottom = cbo_srch_cmpg_typ_cd1.position.bottom;
		
		btn_dbExtn.position.top = stc_05.position.bottom +5;
		btn_dbExtn.position.height = 20;
		
		btn_fileExtc.position.top = stc_05.position.bottom +5;
		btn_excelDw.position.top = stc_05.position.bottom +5;
		btn_fileExtc.position.height = 20;
		btn_excelDw.position.height = 20;
		
		sta_totalCnt.move(sta_totalCnt.position.left,stc_05.position.bottom +7);
		grd_list_extc.position.top = btn_dbExtn.position.bottom +3;
		stc_05.bringToPrev
	}
}
/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
		case btn_search: //조회버튼
			if(fn_preSrch()){
				fn_commSearch(gfn_getCommTypeInit()); // 조회	
			}
		break;
		
		case btn_init ://초기화 버튼
		   fn_init();
		break;
		
		case btn_close: //닫기버튼
			close();
		break;
		
		case btn_dbExtn: //DB 추출 선택 버튼
			if(ds_list_tmp.rowcount >= 1){
				if(resn_add == 'Y'){
					//alert('작업은 여기에');
					//fn_saveData_sub();
					gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
					//trace(ds_list.saveXML());
					fn_saveData_sub();
				}else{
					opener.ds_extc.assign(ds_list_tmp);
					opener.edt_file_or_db.value = cbo_srch_cmpg_typ_cd1.text+' > '+cbo_srch_cmpg_typ_cd2.text;
					close();
				}								
			}else{
				alert('조회된 대상고객이 없습니다.');
				return;
			}
		break;
		
		case btn_fileExtc : //파일추출 변경
		    if(!gfn_isNull(opener.edt_file_or_db.value)){
				if(confirm("추출유형을 변경하시면 선택된 값이 초기화 됩니다. /n/n변경하시겠습니까?")){
					opener.cbo_trgt_list_extc_typ_cd.enable= true;
					opener.cbo_trgt_list_extc_typ_cd.value = '10';
					opener.stc_fileOrDb.text = "File데이터";
					opener.edt_file_or_db.value= null;
					opener.ds_extc.clearData();
					close();
				}
		    }else{
				opener.cbo_trgt_list_extc_typ_cd.enable= true;
				opener.cbo_trgt_list_extc_typ_cd.value = '10';
				opener.stc_fileOrDb.text = "File데이터";
				close();
		    }
			
		break;
		
		case btn_excelDw : //엑셀다운로드
			if(ds_list_tmp.rowcount >= 1){
				gfn_exportGridToExcel(grd_list_extc,"추출고객 대상리스트_"+gfn_getTodayFull());
			}else{
				alert('조회된 대상고객이 없습니다.');
				return;
			}
		break;
	}
}

function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(obj){
		case cbo_srch_pay_mthd : 
			if(obj.index == 0 || obj.index == 1){
				edt_ichae_dt.value = null;
				edt_ichae_dt.enable = false;
			}else{
				edt_ichae_dt.enable = true;
			}
		break;
	}
}


function cbo_onkeydown(obj:Combo, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if(fn_preSrch()){
			fn_commSearch(gfn_getCommTypeInit()); // 조회	
		}
	}
}

function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if(fn_preSrch()){
			fn_commSearch(gfn_getCommTypeInit()); // 조회	
		}
	}
}

