//초기화 
function fn_init()
{	
	if(cmpg_typ_cd2 == '1100'){ //증서멤버쉽
		cal_srch_startd.value = gfn_sttMonth(gfn_getToday(),-1);
		var vEndDt = String(cal_srch_startd.value);
		cal_srch_endd.value =  gfn_getLastDay(vEndDt.substr(0,6));
	}else if(cmpg_typ_cd2 == '1200'){//가입승인 
		cal_srch_startd.value = gfn_addDate(gfn_getToday(),-1);
		cal_srch_endd.value =  gfn_addDate(gfn_getToday(),-1);
	}else if (cmpg_typ_cd =='2000'||cmpg_typ_cd == '3000'){
		cbo_srch_pay_mthd.index = 0;
		edt_ichae_dt.value = null;
		edt_stt_month_cnt.value = null;
		edt_end_month_cnt.value = null;
		edt_stt_true_cnt.value = null;
		edt_end_true_cnt.value = null;
		edt_stt_pay_state.value = null;
		edt_end_pay_state.value = null;
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
	if(errcd != 0){
		trace("에러발생 svcid : "+svcid+"   errmsg :"+errmsg+ "   errcd :"+errcd);
	}else{
		switch(svcid){
			case "srchHrchCdSet": //계층유형 코드셋
				gfn_getHrchCodeLoad("","ALL",ds_hrchCode1_srch,ds_hrchCdSet,1);
				cbo_srch_cmpg_typ_cd1.value = cmpg_typ_cd;
				gfn_getHrchCodeLoad(cbo_srch_cmpg_typ_cd1.value,"ALL",ds_hrchCode2_srch,ds_hrchCdSet);
				cbo_srch_cmpg_typ_cd2.value = cmpg_typ_cd2;
			break;
			
			case "srchExtcTrgt" : //추출대상 조회
				//gfn_appendSearchDs(ds_list_trgtList, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리				
				gfn_setTotalRowCount(ds_list_tmp, sta_totalCnt, total_count); // 페이지건수 표출
				fn_postSearch();
			break;
			
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		//ds_list_extcCndt.keystring = ""; // 정렬컬럼 초기화
	
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "cmpg_typ_cd", cbo_srch_cmpg_typ_cd1.value);
		ds_inqCndt.setColumn(0, "cmpg_typ_cd2", cbo_srch_cmpg_typ_cd2.value);
		
		if(cmpg_typ_cd2 == '1100'){// 해피콜 / 증서멤버쉽
			ds_inqCndt.setColumn(0, "stt_join_dt", cal_srch_startd.value);
			ds_inqCndt.setColumn(0, "end_join_dt", cal_srch_endd.value);
		}else if(cmpg_typ_cd2 == '1200'){//해피콜/가입승인
			ds_inqCndt.setColumn(0, "stt_reg_dt", cal_srch_startd.value);
			ds_inqCndt.setColumn(0, "end_reg_dt", cal_srch_endd.value);
		}else if(cmpg_typ_cd2 == '1300'){//해피콜/전후방반송
			ds_inqCndt.setColumn(0, "dsps_rsn_cd", cmpg_typ_cd2);
		}else if(cmpg_typ_cd2 == '1400'){//해피콜/전후방반송
			ds_inqCndt.setColumn(0, "dsps_rsn_cd", cmpg_typ_cd2);
		}else if(cmpg_typ_cd2 == '1500'){//해피콜/전후방반송
			ds_inqCndt.setColumn(0, "dsps_rsn_cd", cmpg_typ_cd2);
		}else if(cmpg_typ_cd2 == '1600'){//해피콜/전후방반송
			ds_inqCndt.setColumn(0, "dsps_rsn_cd", cmpg_typ_cd2);
		}else if(cmpg_typ_cd2 == '1700' && resn_add == 'Y'){//해약방어 + 해약건 추가 
			ds_inqCndt.setColumn(0, "resn_add", 'Y');			
			ds_inqCndt.setColumn(0, "trgt_list_id", trgt_list_id);	
			ds_inqCndt.setColumn(0, "sub_trgt_list_id", sub_trgt_list_id);	
