			tab_Event.removeChild("evntDrmn");
			tab_Event.insertTabpage("evntOtsr",3,"DL540000::DL540106.xfdl");
			tab_Event.evntOtsr.text = "행사 보고서(외주)";
		}else{
// 			tab_Event.insertTabpage("evntDrmn",2,"DL540000::DL540105.xfdl");
