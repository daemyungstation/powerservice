/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL540102_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	
	event_mode = p_mode;
	event_stat_fg = p_event_stat_fg;
	event_accnt_no = p_accnt_no;
	event_seq = p_seq;
	
	if( event_mode == "updateMain") {
		if (gfn_isNull(p_accnt_no) || gfn_isNull(p_seq)) {
			alert("회원정보를 확인해주세요.");
			close();
			return false;
		}
	} 
	
	fn_init();
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	} else {
		switch(svcid) {
			case "srchEvtDetail":
			//상단 정보 (회원, 행사키, 담당자..)
			ds_accnt_info.clearData();
			ds_accnt_info.addRow();
			ds_accnt_info.setColumn(0,"accnt_no", ds_evnt_list.getColumn(0, "accnt_no"));
			ds_accnt_info.setColumn(0,"mem_no", ds_evnt_list.getColumn(0, "mem_no"));
			ds_accnt_info.setColumn(0,"mem_nm", ds_evnt_list.getColumn(0, "mem_nm"));
			ds_accnt_info.setColumn(0,"prod_cd", ds_evnt_list.getColumn(0, "prod_cd"));
			

			
			//행사등록탭 데이터셋 설정 및 값 설정..
			tab_Event.tab_event_info.ds_evnt_list.clearData();
			tab_Event.tab_event_info.ds_evnt_list.addRow();
			tab_Event.tab_event_info.ds_evnt_list.copyRow(0,ds_evnt_list,0); // 탭 이벤트 내 데이터셋으로 복사
			tab_Event.tab_event_info.fn_setDetailEvnt(); 
				
			tab_crt();	
			
			//행사보고서(직영,외주) 등 설정.. >> 2차

			break;
			
			case "selectCustaccntList":
			if(tc_custAcnt == 1) {
				fn_setAccntNo();
			} else if (tc_custAcnt > 1) {
				btn_accnt_onclick(null, "Y");
			} else {
				edt_accnt_no.value = "";
				edt_mem_nm.value = "";
			}			
			break;
			
			case "srchInputCheck":
			if (gfn_transNullToNumber(eventChk) >= 1) {
				alert("행사처리 하신 회원입니다.");
				fn_clear();
			}
			if (gfn_transNullToNumber(resnChk) >= 1) {
				alert("해약처리 하신 회원입니다.");
				fn_clear();
			}
			if (gfn_transNullToNumber(taxtChk) >= 1) {
				alert("세무처리 하신 회원입니다.");
				fn_clear();
			} else {
				//회원정보 납입부금 가져오기
				fn_commSearch(gfn_getCommTypeInit(), "srchEvntAccnt");
			}
			break;
			
			case "srchEvntAccnt":
			edt_emple_no.value = ds_accnt_info.getColumn(0, "emple_no");
			edt_emple_nm.value = ds_accnt_info.getColumn(0, "emple_nm");
			edt_prod_nm.value = ds_accnt_info.getColumn(0, "prod_nm");
			edt_prod_cd.value = ds_accnt_info.getColumn(0, "prod_cd");
			edt_model_cl_nm.value = ds_accnt_info.getColumn(0, "model_cl_nm");
			tab_Event.tab_event_info.ds_evnt_list.setColumn(0, "emple_no", ds_accnt_info.getColumn(0, "emple_no"));
			tab_Event.tab_event_info.ds_evnt_list.setColumn(0, "emple_nm", ds_accnt_info.getColumn(0, "emple_nm"));
			
			ds_evnt_list.setColumn(0, "join_dt", ds_event_accnt.getColumn(0, "join_dt"));
			ds_evnt_list.setColumn(0, "no", ds_event_accnt.getColumn(0, "no"));

			tab_Event.tab_event_info.fn_setPayInfo(ds_event_accnt);
			break;
		}
	}
}


/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType, svcid, gbn) {
	
	switch (svcid) {
		case "srchEvtDetail":
		ds_inqCndt.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "seq", ds_evnt_info.getColumn(0, "seq"));
		ds_inqCndt.setColumn(0, "evt_seq", ds_evnt_info.getColumn(0, "seq"));
		ds_inqCndt.setColumn(0, "accnt_no", ds_evnt_info.getColumn(0, "accnt_no"));
		ds_inqCndt.setColumn(0, "event_stat_fg", event_stat_fg);
		break;
		
		case "selectCustaccntList":
		ds_accntInqCndt.keystring = ""; // 정렬컬럼 초기화
		ds_accntInqCndt.clearData();
		ds_accntInqCndt.addRow();
		
		var srch_typ = ""
		var srch_vl = "";
		var option = "";
		
		if (gbn == "accnt_no") {
			srch_typ = gbn;
			srch_vl = edt_accnt_no.value;
		} else if (gbn == "mem_nm"){
			srch_typ = gbn;
			srch_vl = edt_mem_nm.value;
		}
		
		ds_accntInqCndt.setColumn(0, "srch_typ", srch_typ);
		ds_accntInqCndt.setColumn(0, "srch_vl", srch_vl);
		ds_accntInqCndt.setColumn(0, "option", option);
		ds_accntInqCndt.setColumn(0, "ns_yn", "N");
		ds_accntInqCndt.setColumn(0, "popup_yn", "N");
		ds_accntInqCndt.setColumn(0, "orderBy", "mem_nm");
		ds_accntInqCndt.setColumn(0, "orderDirection", "asc");
		return_bool = true;
		break;
		case "selectCustaccntList":
		ds_accntInqCndt.keystring = ""; // 정렬컬럼 초기화
		ds_accntInqCndt.clearData();
		ds_accntInqCndt.addRow();
		
		var srch_typ = ""
		var srch_vl = "";
		var option = "";
		
		if (gbn == "accnt_no") {
			srch_typ = gbn;
			srch_vl = edt_accnt_no.value;
		} else if (gbn == "mem_nm"){
			srch_typ = gbn;
			srch_vl = edt_mem_nm.value;
		}
		
		ds_accntInqCndt.setColumn(0, "srch_typ", srch_typ);
		ds_accntInqCndt.setColumn(0, "srch_vl", srch_vl);
		ds_accntInqCndt.setColumn(0, "option", option);
		ds_accntInqCndt.setColumn(0, "ns_yn", "N");
		ds_accntInqCndt.setColumn(0, "popup_yn", "N");
		ds_accntInqCndt.setColumn(0, "orderBy", "mem_nm");
		ds_accntInqCndt.setColumn(0, "orderDirection", "asc");
		return_bool = true;
		break;
	}
	
	return true;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, searchInfo, gbn) {

	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
        case "srchEvtDetail":
        if (!fn_preSearch(sType, "srchEvtDetail")) return false;
	    szSvcid       = "srchEvtDetail";
		szController  = "/dlw/newType/evnt/detailList";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_evnt_list=ds_output ds_acDirMst_list=ds_output_2 ds_acOutMst_list=ds_output_3  ds_ac1_list=ds_output_4  ds_ac1B_list=ds_output_5"
					  + " ds_ac1C_list=ds_output_6 ds_ac1D_list=ds_output_7 ds_ac1E_list=ds_output_8 ds_ac2_list=ds_output_9 ds_ac3_list=ds_output_10"
					  + " ds_ac4_list=ds_output_11 ds_ac5_list=ds_output_12 ds_ac6_list=ds_output_13 ds_ac7_list=ds_output_14 ds_ac9_list=ds_output_15"
					  + " ds_ac10_list=ds_output_16 ds_dirPayInfo_list=ds_output_17  ds_outPayInfo_list=ds_output_18";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
		case "selectCustaccntList":
        if (!fn_preSearch(sType,"selectCustaccntList", gbn)) return false;
		szSvcid       = "selectCustaccntList";
		szController  = "/dlw/cust/cust-acnt/list";
		szInDs        = "ds_input=ds_accntInqCndt";
		szOutDs       = "ds_accnt_info=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
		break;
		case "srchInputCheck":
		szSvcid       = "srchInputCheck";
		szController  = "/dlw/newType/evnt/inputCheck";
		szInDs        = "";
		szOutDs       = "";
		szParam       = "accnt_no=" + ds_accnt_info.getColumn(0, "accnt_no");
		szRetCallback = "fn_callBack";
		break;
		case "srchEvntAccnt":
		szSvcid       = "srchEvntAccnt";
		szController  = "/dlw/newType/evnt/evntAccntList";
		szInDs        = "";
		szOutDs       = "ds_event_accnt=ds_output";
		szParam       = "accnt_no=" + ds_accnt_info.getColumn(0, "accnt_no");
		szRetCallback = "fn_callBack";
		break;
    }
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
}


