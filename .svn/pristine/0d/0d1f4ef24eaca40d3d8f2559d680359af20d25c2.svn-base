//초기화
function fn_init()
{
	//행사관련 권한 불러오기..
		
	//CMS 모듈 init
	
	if ("Y" == event_stat_fg) {
		tab_Event.tab_event_info.btn_delete.enable = false;
		tab_Event.tab_event_info.btn_save.enable = true;
		tab_Event.tab_event_info.btn_cncl_cms.enable = false;
		tab_Event.tab_event_info.ckb_cnl_work_yn.visible = false;
		tab_Event.tab_event_info.ckb_cnl_work_yn.value = "N";
	}
	
	if("update" == event_mode) {
		ds_evnt_info.clearData();
		ds_evnt_info.addRow();
		ds_evnt_info.copyRow(0,opener.ds_list,opener.ds_list.rowposition);
	} else if( event_mode == "updateMain") {
		ds_evnt_info.clearData();
		ds_evnt_info.addRow();
		ds_evnt_info.setColumn(0, "seq", event_seq);
		ds_evnt_info.setColumn(0, "accnt_no", event_accnt_no);
		event_mode = "update";
	} else if (event_mode == "new") { // 콜센터 화면 행사등록 시 기본 회원 조회까지!
		ds_accnt_info.clearData();
		ds_accnt_info.addRow();	
		ds_accnt_info.setColumn(0, "accnt_no", event_accnt_no);
		fn_setAccntNo(); // 행사회원 체크
		edt_accnt_onsrch(); // 회원정보 바로 조회
	}
	
	var sProdCd = ds_accnt_info.getColumn(0, "prod_cd");
	if(sProdCd == "EI" || sProdCd == "EJ")
	{
		if(confirm("두 구좌 상품으로 더블 (A) 상품의 행사를 먼저 진행하셔야 합니다.") == true)
		{
			
		}
		else
		{
			close();
		}
	}

	tab_Event.tab_event_info.DL540103_onload();
}

