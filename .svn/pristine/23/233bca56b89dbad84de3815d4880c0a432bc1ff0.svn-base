/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능		: (공통버튼)조회 프로세스

 ***************************************************************************/
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;

	//공통코드정보를 조회하기 위해 셋팅한 데이타
	ds_input.setColumn( 0, "inqCndt", "B");

	if (sType == "INIT") {
		ds_input.setColumn( 0, "comnCdId", "");
		ds_comnCdBasiList.clearData();
		ds_comnCdVlInfoList.clearData();
	}
	ds_input.setColumn( 0, "cdTypDivCd", "");

	// 페이지 처리 기본설정
	if(!gfn_pageSearch(gfn_getTranTypeMCI(), sType, ds_comnCdBasiList, div_body.sta_TotalCnt)) return;

	// KWData 설정
	gfn_kwDataInit();

	gfn_setKwData_trscCode("SYS00002R01");
	gfn_setKwData_trscType("");
	gfn_setKwData_screenId("");
	gfn_setKwData_bizGroupCode("");
	gfn_setKwData_systemCode("KRP_TEST");

	gfn_tranNMCI(sType
				, "ds_input=ds_input"
				, "ds_list_Temp=ds_list_Temp"
				, "fn_commSearch_callBack");
}

