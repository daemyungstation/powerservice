/***************************************************************************
 * 기      능		: (공통) 그리드 oncellclick이벤트 발생시에 로우상태 초기화 처리
 ***************************************************************************/
function div_body_grd_comnCdBasiList_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.col == 0) {
		gfn_rollbackRowDs(this[obj.binddataset], obj, e);
	} else {
		ds_input.setColumn( 0, "inqCndt", "V"); //공통코드값정보를 조회하기 위해 셋팅한 데이타
		ds_input.setColumn( 0, "comnCdId", ds_comnCdBasiList.getColumn(e.row, "comnCdId"));
		ds_input.setColumn( 0, "relnCdId", ds_comnCdBasiList.getColumn(e.row, "relnCdId"));
		ds_input.setColumn( 0, "comnCdSubId", ds_comnCdBasiList.getColumn(e.row, "comnCdId"));
		ds_input.setColumn( 0, "cdTypDivCd", ds_comnCdBasiList.getColumn(e.row, "cdTypDivCd"));

		if (ds_input.getColumn( 0, "cdTypDivCd") == "2") {
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 5, "edittype", "none");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 6, "edittype", "none");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 7, "edittype", "none");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 8, "edittype", "none");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 9, "edittype", "none");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 10, "edittype", "none");
			//div_body.grd_comnCdVlInfoList.setCellProperty("body", 11, "edittype", "none");

			div_body.Button00.visible = false;

		} else {
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 5, "edittype", "normal");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 6, "edittype", "normal");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 7, "edittype", "normal");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 8, "edittype", "normal");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 9, "edittype", "normal");
			div_body.grd_comnCdVlInfoList.setCellProperty("body", 10, "edittype", "normal");
			//div_body.grd_comnCdVlInfoList.setCellProperty("body", 11, "edittype", "combo");

			div_body.Button00.visible = true
		}

		// 공통코드값정보 초기화
		ds_comnCdVlInfoList.clearData();
		fn_commVlSearch(gfn_getCommTypeAppend());
	}
}

