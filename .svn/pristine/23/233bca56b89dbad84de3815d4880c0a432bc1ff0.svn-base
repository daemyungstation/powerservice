/***************************************************************************
 * 함  수  명		: fn_commSearch_callBack()
 * 기      능		: [CallBack함수] Transaction Callback함수
 ***************************************************************************/
function fn_commSearch_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 조회된 결과를 실제보여줄 데이타셋으로 Append처리
	gfn_appendSearchDs(ds_comnCdBasiList, ds_list_Temp);
	
	if(strSvcID != gfn_getCommTypeAppend()) gfn_setTotalRowCount(ds_comnCdBasiList, div_body.sta_TotalCnt);	// 페이지건수 조회 처리
	
	// 조회 후 처리
	fn_postSearch(strSvcID);
}

function fn_commVlSearch_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	//공통코드값정보를 조회하기 위해 셋팅한 데이타
	gfn_appendSearchDs(ds_comnCdVlInfoList, ds_list_Temp);

	// 조회 후 처리
	fn_postSearch(strSvcID);
}

/***************************************************************************
 * 함  수  명		: fn_commSave_callBack()
 * 기      능		: [CallBack함수] Transaction Callback함수
 ***************************************************************************/
function fn_commSave_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	}
	// 저장 후 처리
	ds_comnCdVlInfoList.clearData();
	fn_postSave(strSvcID);
}

/***************************************************************************
 * 함  수  명		: fn_commDelete_callBack()
 * 기      능		: [CallBack함수] Transaction Callback함수
 ***************************************************************************/
function fn_commDelete_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 삭제 후 처리
	fn_postDelete(strSvcID);
