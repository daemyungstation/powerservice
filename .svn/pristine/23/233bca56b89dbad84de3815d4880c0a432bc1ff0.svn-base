/***************************************************************************
 * 기      능		: 서브등록 팝업
 ***************************************************************************/
function div_body_Button00_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = ds_comnCdVlInfoList.findRow(gfn_getColNmChk(), "1");
	if (nRow == -1) {
		alert("등록할 데이타를 선택하세요.");
		return;
	}
	for (var i = 0; i < ds_comnCdVlInfoList.rowcount ; i++) {
		if (ds_comnCdVlInfoList.getColumn(i, gfn_getColNmChk()) == "" ) {
			alert(i+1 +"번째 Row를 먼저 저장하셔야 합니다.");
			return;
		}
		if (ds_comnCdVlInfoList.getColumn(i, gfn_getColNmStatus()) != "" ) {
			alert(i+1 +"번째 Row의 데이타가 변경되었습니다.\n변경된 데이타를 먼저 저장하셔야 합니다.");
			return;
		}
		if (ds_comnCdVlInfoList.getColumn(i, gfn_getColNmChk()) == "1" && ds_comnCdVlInfoList.getColumn(i, "dltnYn") == "Y" ) {
			alert(i+1 +"번째 Row의 삭제가 사용으로 변경되어야 서브등록이 가능합니다.");
			return;
		}
	}

	var strParam = "";
	//gfn_diaLog(dialog_id, nLeft, nTop, nWidth, nHeight, stFormURL, bShowTitle, strArgument, isModeless, isLayered, bAutoSize)
	var reVal = gfn_diaLog("SYS0000200201", -1, -1, 875, 675, "SYS::SYS0000200201.xfdl", false, strParam, false, false, true);
}

