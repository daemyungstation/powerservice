{
	// KWData 설정
	gfn_kwDataInit();

	gfn_setKwData_trscCode("SYS00002R03");
	gfn_setKwData_trscType("");
	gfn_setKwData_screenId("");
	gfn_setKwData_bizGroupCode("");
	gfn_setKwData_systemCode("KRP_TEST");

	gfn_tranNMCI(sType
				, "ds_input=ds_input"
				, "ds_list_Temp=ds_list_Temp"
				, "fn_commVlSearch_callBack");
}

/***************************************************************************
 * 함  수  명		: fn_commSave()
 * 기      능		: (공통버튼)저장 프로세스
 ***************************************************************************/
function fn_commSave(sType)
{
	if (!fn_preSave(sType)) return false;

	for (var i = 0; i <= ds_comnCdVlInfoList.rowcount; i++)
	{
		if ( ds_comnCdVlInfoList.getColumn(i, gfn_getColNmStatus()) == gfn_getRowTypeUpdate() )
		{
			ds_comnCdVlInfoList.setColumn(i, "cdTypDivCd", ds_input.getColumn(0, "cdTypDivCd"));
			ds_comnCdVlInfoList.setColumn(i, "relnCdId", ds_input.getColumn(0, "relnCdId"));
		}
	}

	// KWData 설정
	gfn_kwDataInit();

	gfn_setKwData_nameSpace("");
	gfn_setKwData_trscCode("SYS00002C04");
	gfn_setKwData_trscType("");
	gfn_setKwData_screenId("");
	gfn_setKwData_bizGroupCode("");
	gfn_setKwData_systemCode("KRP_TEST");

	gfn_tranNMCI(sType
				, "ds_ComnCdVlInfo=ds_comnCdVlInfoList:U"
				, ""
				, "fn_commSave_callBack");
}

