 * 기      능		: (공통버튼)신규/행추가 프로세스
 ***************************************************************************/
function fn_commAdd()
{
	if (ds_input.getColumn(0, "cdTypDivCd") != "2") {
		gfn_addDs(ds_comnCdVlInfoList);
		for (var i = 0; i < ds_comnCdVlInfoList.getRowCount(); i++) {
			if(ds_comnCdVlInfoList.getColumn(i, gfn_getColNmStatus()) == gfn_getRowTypeInsert()) {
				ds_comnCdVlInfoList.setColumn(i, "comnCdId", ds_input.getColumn(0, "comnCdSubId"));
				ds_comnCdVlInfoList.setColumn(i, "dltnYn", "N");
			}
		}
	} else {
		alert("서브코드인 경우엔 신규/행추가가 안됩니다.");
	}
}

