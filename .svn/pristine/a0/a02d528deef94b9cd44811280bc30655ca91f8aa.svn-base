// onkeydown
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) 
	{
		if (obj.name == "srch_accnt_no") 
		{
			fn_commSearch(gfn_getCommTypeInit(), "getMemberBasicAllAmtInfomation");
		} 
		else if (obj.name == "srch_mem_nm") 
		{
			fn_commSearch(gfn_getCommTypeInit(), "getMemberBasicAllAmtInfomation");
		}
		else if (obj.name == "srch_mem_no")
		{
			fn_commSearch(gfn_getCommTypeInit(), "getMemberBasicAllAmtInfomation");
		}
	}
}

