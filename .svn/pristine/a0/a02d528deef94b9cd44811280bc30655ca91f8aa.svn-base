	gfn_createPagingDataset(obj); 		// 페이징처리 위한 데이터셋 설정
	fn_init();
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd)
		return false;
	} 
	else 
	{
		switch (svcid) 
		{						
			case "getMemberBasicAllAmtInfomation":
				gfn_appendSearchDs(ds_list, ds_listTmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_TotalRows); // 페이지건수 표출
				fn_postSearch();
			break;
			
			case "insertUserProdInfo":
				alert("3시 배치가 완료되었습니다.");
			break;
			
			case "updateUnpyAcrs":
				alert("5시 배치가 완료되었습니다.");
			break;
			
			case "updateCnctAcrs":
				alert("5시 30분 배치가 완료되었습니다.");
			break;
			
			case "insertB2QTmJoinCust":
				alert("6시 배치가 완료되었습니다.");
			break;
			
			case "updateChatSndgHstrResl":
				alert("7시 배치가 완료되었습니다.");
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원 조회
function fn_commSearch(sType, searchInfo)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 100, tc_TotalRows)) return;
	
	switch(searchInfo) 
	{
        case "getMemberBasicAllAmtInfomation": 
			ds_cond.clearData();
			var nRow = ds_cond.addRow();
			ds_cond.setColumn(nRow, "accnt_no", srch_accnt_no.value);
			ds_cond.setColumn(nRow, "mem_nm", srch_mem_nm.value);
			ds_cond.setColumn(nRow, "mem_no", srch_mem_no.value);

			var szSvcid       = "getMemberBasicAllAmtInfomation";
			var szController  = "/web/shopping/getMemberBasicAllAmtInfomation";
			var szInDs        = "ds_input=ds_cond";
			var szOutDs       = "ds_listTmp=ds_output01";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
        break;
    }

	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	srch_accnt_no.value = "";
	srch_mem_nm.value   = "";
	srch_mem_no.value   = "";
	
	ds_listTmp.clearData();
	ds_list.clearData();
	sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
}
// 조회 validation check.
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (gfn_isNull(srch_mem_nm.value) == true && gfn_isNull(srch_accnt_no.value) == true && gfn_isNull(srch_mem_no.value) == true) {
			alert("[회원명], [회원번호], [고유번호] 중 하나는 반드시 입력해야 합니다.");
			return false;
		}
	}
	
	return true;
}

// 조회 결과
function fn_postSearch()
{
	if (ds_list.getRowCount() < 1) 
	{
		ds_list.clearData();
		alert("조회된 결과가 없습니다.");
	} 
	else 
	{
		for(var idx = 0; idx < ds_list.getRowCount(); idx++)
		{
			var nAcu_amt   = ds_list.getColumn(idx, "acu_amt");
			var nUse_amt   = ds_list.getColumn(idx, "use_amt");
			var nCnl_amt   = ds_list.getColumn(idx, "cnl_amt");
			var nTotal_amt = nAcu_amt - (nUse_amt - nCnl_amt);
						
			ds_list.setColumn(idx, "total_amt", nTotal_amt);
		}
	}
}

