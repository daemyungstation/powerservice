//조회
function fn_commSearch(sType, searchInfo) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
        case "srchWdrwHstrCard": 
      
        if (!fn_preSearch(sType,"srchWdrwHstrCard")) return false;
        if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 2000, tc_condMem)) return;
        
        
	    szSvcid       = "srchWdrwHstrCard";
		szController  = "/dlw/card/wdrw-hstr/list";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_list_tmp=ds_output ds_count_card=ds_output1";
		szParam       = "excel_fg=Y";
		szRetCallback = "fn_callBack";
        break;
        
        case "callWdrwReqCheck":
        
       // trace("wefwefwe");
	    szSvcid       = "callWdrwReqCheck";
		szController  = "/dlw/cms/call-wdrw-req-chk";
		szInDs        = "";
		szOutDs       = "ds_chkList=ds_output";
		szParam       = "inv_dt=" + cal_inv_dt.value + " wdrw_gubun=Card";
		szRetCallback = "fn_callBack";
        break;
    }
    // 트랜젝션 공통함수 호출
    if ( searchInfo == "srchWdrwHstrCard" )
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
	else
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
