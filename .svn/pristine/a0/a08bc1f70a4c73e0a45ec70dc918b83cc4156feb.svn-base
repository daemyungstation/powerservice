function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
			case "srchWdrwHstrCard":  // 출금이체의뢰내역 조회
			alert("출금이체의뢰내역 조회를 실패하였습니다.\n" + errmsg);
			break;
			
