//삭제전 체크여부 확인
function fn_preDelete()
{
	var chkfg = false;
	for(var i=0;i<ds_list.rowcount;i++)
	{
		if(ds_list.getColumn(i, "_chk") == 1) { // 체크 여부 확인
			ds_list.setRowType(i,"U");
			chkfg = true;
		}
	}
	return chkfg;
}

function fn_preSave(svcid, consTyp) {
    var return_bool = false;
    switch(svcid) {
        case "updatePeriod":        
		var wdrw_req_amt = ds_list.getColumn(current_row,"wdrw_req_amt");
		if(!isNaN(wdrw_req_amt) && gfn_transNullToNumber(wdrw_req_amt) > 50000) {
		} else {
			alert("5만원 이상만 가능합니다.");
			ds_list.setColumn(current_row,"install_period", 0);
		}
		return_bool = true;
        break;
    }
    return return_bool;

}
