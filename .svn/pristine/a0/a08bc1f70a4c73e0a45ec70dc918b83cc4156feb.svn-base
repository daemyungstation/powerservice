function spn_onmouseleave(obj:Spin, e:MouseEventInfo)
{
	obj.visible = false;
	spn_onkillfocus(obj);
}
function spn_onkeydown(obj:Spin, e:KeyEventInfo)
{	
	if (e.keycode == 9) // tab키 동작 시 
    { 
		obj.setCellPos(obj.position -1); //현재 포지션 이전 위치 이동 
		return false;
    } 
	if (e.keycode == 13) {
		spn_onmouseleave(obj);
	}
}
