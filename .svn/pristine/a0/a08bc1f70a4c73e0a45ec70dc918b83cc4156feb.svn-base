			fn_commSearch(gfn_getCommTypeInit(), "srchWdrwHstrCard"); // 조회
			break;
			
			case "updateCardPop":
			if (cardPop == "N")
			{
				cardPop = "Y";
			}
			else if(cardPop == "Y")
			{
				cardPop = "N";
			}
			fn_cardPopBtn();		
			break;
		}
	}
}

