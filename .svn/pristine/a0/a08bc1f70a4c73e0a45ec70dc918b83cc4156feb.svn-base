			break;
			
			case "deleteWdrwEb21":
			break;
			
			case "batchNiceRtCard":
			break;
			
			case "updateCardPop":
			alert("카드팝업여부등록이 실패했습니다.\n" + errmsg);			
			break;
	    }
		return false;
	} else {
		switch (svcid) {		
			case "srchWdrwHstrCard":  // 출금이체의뢰내역 조회
			//ds_list.clearData();
			gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
