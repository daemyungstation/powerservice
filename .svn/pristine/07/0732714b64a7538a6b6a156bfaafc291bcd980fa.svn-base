function fn_setCRUDComponentType(sType)
{	
	if(sType == "NULL") 
	{
		xSaveType = "select";
		stc_17.text = "우편발송고객기본정보";
		
		btn_insert.visible = true;
		btn_update.visible = true;		
		btn_save.visible = false;
		btn_cancel.visible = false;
		
		btn_insert.position2 = "absolute r:80 w:72 b:60 h:20";
		btn_update.position2 = "absolute r:6 w:72 b:60 h:20";
		
		edt_accntNo.enable = false;
		btn_accntNo.enable = false;
		edt_memNm.enable = false;
		edt_cell.enable = false;
		cbo_postSendOpt.enable = false;
		cbo_docType.enable = false;
		edt_homeZip.enable = false;
		btn_homeZip.enable = false;
		edt_homeAddr.enable = false;
		edt_homeAddr2.enable = false;
	} 
	else if(sType == "VIEW")
	{
		xSaveType = "select";
		stc_17.text = "우편발송고객기본정보";
		
		btn_insert.visible = true;
		btn_update.visible = true;
		btn_save.visible = false;
		btn_cancel.visible = false;
		
		btn_insert.position2 = "absolute r:80 w:72 b:60 h:20";
		btn_update.position2 = "absolute r:6 w:72 b:60 h:20";
		
		edt_accntNo.enable = false;
		btn_accntNo.enable = false;
		edt_memNm.enable = false;
		edt_cell.enable = false;
		cbo_postSendOpt.enable = false;
		cbo_docType.enable = false;
		edt_homeZip.enable = false;
		btn_homeZip.enable = false;
		edt_homeAddr.enable = false;
		edt_homeAddr2.enable = false;
	}
	else if(sType == "INSERT")
	{
	    xSaveType = "insert";
	    stc_17.text = "우편발송고객기본정보 입력";
	    
		btn_insert.visible = false;
		btn_update.visible = false;
		btn_save.visible = true;
		btn_cancel.visible = true;
		
		btn_save.position2 = "absolute r:80 w:72 b:60 h:20";
		btn_cancel.position2 = "absolute r:6 w:72 b:60 h:20";
		
		edt_accntNo.enable = false;
		btn_accntNo.enable = true;
		edt_memNm.enable = true;
		edt_cell.enable = true;
		cbo_postSendOpt.enable = true;
		cbo_postSendOpt.index = 0;
		cbo_docType.enable = true;
		cbo_docType.index = 0;
		edt_homeZip.enable = true;
		btn_homeZip.enable = true;
		edt_homeAddr.enable = true;
		edt_homeAddr2.enable = true;
		
		ds_listCertfSendAddrSingle.clearData();
	    ds_listCertfSendAddrSingle.addRow();
	    
	    xMemNo = pMemNo;
        xAccntNo = pAccntNo;
	    edt_memNo.value = xMemNo;
	    edt_accntNo.value = xAccntNo;
	    
	    fn_memberAccntNoInfo();
	}
	else if(sType == "UPDATE")
	{
		xSaveType = "update";
		stc_17.text = "우편발송고객기본정보 수정";
		
		btn_insert.visible = false;
		btn_update.visible = false;
		btn_save.visible = true;
		btn_cancel.visible = true;
		
		btn_save.position2 = "absolute r:80 w:72 b:60 h:20";
		btn_cancel.position2 = "absolute r:6 w:72 b:60 h:20";
		
		edt_accntNo.enable = false;
		btn_accntNo.enable = false;
		edt_memNm.enable = true;
		edt_cell.enable = true;
		cbo_postSendOpt.enable = true;
		cbo_docType.enable = true;
		edt_homeZip.enable = true;
		btn_homeZip.enable = true;
		edt_homeAddr.enable = true;
		edt_homeAddr2.enable = true;
	}
}]]></Script>
