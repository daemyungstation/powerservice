    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                   */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_ConsTyp.xjs";

var nTotalListCnt;
var xSaveType = "select";
var xSelectAccntInputType = "";
var xSaveAccntNo = "";
var xMemNo = "";
var xAccntNo = "";
var gv_dpost; 
var gv_daddr;	

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	    */
/********************************************************************************************/
function DL50000106P03_onload(obj:Form, e:LoadEventInfo)
{
	gfn_getList("dlwCd","0227,0228","fn_callBack",false);   // 대명코드조회 (0227=우편발송종류, 0228=우편발송서류구분)
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		    */
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) 
	{
		return false;
	}
	else
	{
		switch (svcid) 
		{
		    case "selectDlwCode": // 공통코드 조회 callBack Service ID
			    gfn_getCode("0227", "SELECT", ds_codeComboPostSendOpt, gds_DmlCd);
			    gfn_getCode("0228", "SELECT", ds_codeComboDocType, gds_DmlCd);
			    gfn_getCode("0227", "SELECT", ds_codePostSendOpt, gds_DmlCd);
			    gfn_getCode("0228", "SELECT", ds_codeDocType, gds_DmlCd);
			    fn_init();
			break;
			
			case "getSendCertfAccntAddrList" :
				if (ds_listCertfSendAddr.getRowCount() < 1) 
				{
					gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, 0);
					ds_listCertfSendAddrSingle.clearData();
					fn_setCRUDComponentType("NULL");
					alert("조회된 데이터가 없습니다.");
				} 
				else 
				{
					if(gfn_isNull(xSaveAccntNo) == false)
					{
						var nRow = ds_listCertfSendAddr.findRow("accnt_no", xSaveAccntNo);
						ds_listCertfSendAddr.rowposition = nRow;
						xSaveAccntNo = "";
					}
					
					gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, nTotalListCnt);
					fn_setCertfSendAddrSingle();
					fn_setCRUDComponentType("VIEW");
				}
			break;
			
			case "getMemberAccntDtlInfo" :
			    var sMemNo = ds_memberAccntDtlInfo.getColumn(0, "mem_no");
			    var sAccntNo = ds_memberAccntDtlInfo.getColumn(0, "accnt_no");
			    var sMemNm = ds_memberAccntDtlInfo.getColumn(0, "mem_nm");
			    var sCell = ds_memberAccntDtlInfo.getColumn(0, "cell");
			    var sHomeZip = ds_memberAccntDtlInfo.getColumn(0, "home_zip");
			    var sHomeAddr = ds_memberAccntDtlInfo.getColumn(0, "home_addr");
			    var sHomeAddr2 = ds_memberAccntDtlInfo.getColumn(0, "home_addr2");
			    
			    edt_memNo.value = sMemNo;
			    edt_accntNo.value = sAccntNo;
			    edt_memNm.value = sMemNm;
			    edt_cell.value = sCell;
			    edt_homeZip.value = sHomeZip;
			    edt_homeAddr.value = sHomeAddr;
			    edt_homeAddr2.value = sHomeAddr2;
			break;
			
			case "saveSendCertfAccntAddrList" :
				alert("저장이 완료 되었습니다.");
			    fn_commSearch(gfn_getCommTypeInit(), "getSendCertfAccntAddrList");
			break;
			
			case "deleteSendCertfAccntAddrList" :
				alert("삭제가 완료 되었습니다.");
				fn_commSearch(gfn_getCommTypeInit(), "getSendCertfAccntAddrList");
			break;
		}
	}
}

/********************************************************************************************/
/* 4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType, searchInfo)
{
	switch (searchInfo) 
	{
		case "getSendCertfAccntAddrList" :
			var sSttRegDt = cal_sttRegDt.value;
			var sEndRegDt = cal_EndRegDt.value;
			var sAccntNo = edt_srchAccntNo.value;
			var sPostSendOpt = cbo_srchPostSendOpt.value;
			var sDocType = cbo_srchDocType.value;
			var sMemNm = edt_srchMemNm.value;
			
			ds_inqCndt.clearData();
			ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(0, "stt_reg_dt", sSttRegDt);
			ds_inqCndt.setColumn(0, "end_reg_dt", sEndRegDt);
			ds_inqCndt.setColumn(0, "accnt_no", sAccntNo);
			ds_inqCndt.setColumn(0, "post_send_opt", sPostSendOpt);
			ds_inqCndt.setColumn(0, "doc_type", sDocType);
			ds_inqCndt.setColumn(0, "mem_nm", sMemNm);
		break;
		
		case "getMemberAccntDtlInfo" :
		    var sAccntNo = edt_accntNo.value;
		    
		    ds_inqCndt.clearData();
			ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(0, "accnt_no", sAccntNo);
		break;
	}
	
	return true;
}

function fn_preSave(sType, svcId)
{
	switch(svcId) 
	{		
		case "saveSendCertfAccntAddrList" :
			if (sType == gfn_getCommTypeInit())	
			{
				xSaveAccntNo = ds_listCertfSendAddrSingle.getColumn(0, "accnt_no");
				
				if(xSaveType == "update" || xSaveType == "insert")
				{
					var sAccntNo = ds_listCertfSendAddrSingle.getColumn(0, "accnt_no");
					var sHomeZip = ds_listCertfSendAddrSingle.getColumn(0, "home_zip");
					var sHomeAddr = ds_listCertfSendAddrSingle.getColumn(0, "home_addr");
					var sMemNm = ds_listCertfSendAddrSingle.getColumn(0, "mem_nm");
					var sCell = ds_listCertfSendAddrSingle.getColumn(0, "cell");
					var sPostSendOpt = ds_listCertfSendAddrSingle.getColumn(0, "post_send_opt");
					var sDocType = ds_listCertfSendAddrSingle.getColumn(0, "doc_type");
					
					if (gfn_isNull(sAccntNo))
					{
						alert("회원번호를 입력하여 주십시오.");
						edt_accntNo.setFocus();
						return false;
					} 
					if (gfn_isNull(sHomeZip))
					{
						alert("우편번호를 입력하여 주십시오.");
						edt_homeZip.setFocus();
						return false;
					}
					if (gfn_isNull(sHomeAddr))
					{
						alert("발송주소1을 입력하여 주십시오.");
						edt_homeAddr.setFocus();
						return false;
					}
					if (gfn_isNull(sMemNm))
					{
						alert("고객명을 입력하여 주십시오.");
						edt_memNm.setFocus();
						return false;
					}
					if (gfn_isNull(sCell))
					{
						alert("연락처를 입력하여 주십시오.");
						edt_cell.setFocus();
						return false;
					}
					if (gfn_isNull(sPostSendOpt))
					{
						alert("우편종류를 입력하여 주십시오.");
						cbo_postSendOpt.setFocus();
						return false;
					}
					if (gfn_isNull(sDocType))
					{
						alert("서류양식을 입력하여 주십시오.");
						cbo_docType.setFocus();
						return false;
					}
				}
			}
		break;
		
		case "deleteSendCertfAccntAddrList":
			var nChkRowCnt = ds_listCertfSendAddr.getCaseCount("chk == '1'");

			if(nChkRowCnt < 1)
			{
				alert("선택된 회원이 없습니다.");
				return false;
			}
			
			if(!confirm("삭제 하시겠습니까?"))
			{
			    return false;
			}
			
			ds_listCertfSendAddrInDs.clearData();
			var nCopyAddRow = 0;
			
			for(var idx = 0; idx < ds_listCertfSendAddr.getRowCount(); idx++)
			{
				if(ds_listCertfSendAddr.getColumn(idx, "chk") == "1")
				{
					nCopyAddRow = ds_listCertfSendAddrInDs.addRow();
					ds_listCertfSendAddrInDs.copyRow(nCopyAddRow, ds_listCertfSendAddr, idx);
					nCopyAddRow++;
				}
			}
		break;
	}
	
	return true;
}

/********************************************************************************************/
/* 5. 서비스 호출(gfn_Transaction)                                                   	    */
/********************************************************************************************/
function fn_commSearch(sType, searchInfo) 
{
   	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) 
    {
		case "getSendCertfAccntAddrList" :
			if (!fn_preSearch(gfn_getCommTypeInit(), "getSendCertfAccntAddrList"))
			{
				return false;
			}
	
			var szSvcid       = "getSendCertfAccntAddrList";
			var szController  = "/dlw/newTypePop/div6/getSendCertfAccntAddrList";
			var szInDs        = "ds_input=ds_inqCndt";
            var szOutDs       = "ds_listCertfSendAddr=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
	
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
		break;
		
		case "getMemberAccntDtlInfo" :
		    if (!fn_preSearch(gfn_getCommTypeInit(), "getMemberAccntDtlInfo"))
			{
				return false;
			}
	        
			var szSvcid       = "getMemberAccntDtlInfo";
			var szController  = "/dlw/newTypePop/div6/getMemberAccntDtlInfo";
			var szInDs        = "ds_input=ds_inqCndt";
            var szOutDs       = "ds_memberAccntDtlInfo=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
		break;
	}
}

function fn_commSave(sType, svcId)
{
	switch(svcId)
	{	    
		case "saveSendCertfAccntAddrList" :
			if (!fn_preSave(sType, "saveSendCertfAccntAddrList")) 
			{
				return false;
			}
			
			var szSvcid       = "saveSendCertfAccntAddrList";
			var szController  = "/dlw/newTypePop/div6/saveSendCertfAccntAddrList";
			var szInDs        = "ds_input=ds_listCertfSendAddrSingle";
			var szOutDs       = "";
			var szParam       = "save_type=" + xSaveType;
			var szRetCallback = "fn_callBack";

			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
		break;
		
		case "deleteSendCertfAccntAddrList" :
		    if (!fn_preSave(sType, "deleteSendCertfAccntAddrList")) 
			{
				return false;
			}
			
			var szSvcid       = "deleteSendCertfAccntAddrList";
			var szController  = "/dlw/newTypePop/div6/deleteSendCertfAccntAddrList";
			var szInDs        = "ds_input=ds_listCertfSendAddrInDs";
			var szOutDs       = "";
			var szParam       = "";
			var szRetCallback = "fn_callBack";

			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
		break;
	}
}

/********************************************************************************************/
/* 6. 사용자 함수                                                                   	    */
/********************************************************************************************/
function fn_init()
{
    edt_srchAccntNo.value = "";
    cbo_srchPostSendOpt.index = 0; 
    cbo_srchDocType.index = 0;
    edt_srchMemNm.value = "";
    
	ds_inqCndt.clearData();
	ds_listCertfSendAddr.clearData();
	ds_listCertfSendAddrSingle.clearData();
	ds_accnt_info.clearData();
	
	var sToday = gfn_getToday();
	var sSttRegDt = gfn_addDate(sToday, -2);
	var sEndRegDt = sToday;
	
	cal_sttRegDt.value = sSttRegDt;
	cal_EndRegDt.value = sEndRegDt;
	
	fn_commSearch(gfn_getCommTypeInit(), "getSendCertfAccntAddrList");
}
