/* 저장처리 */
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if(!confirm("저장하시겠습니까?")) return false;
	
	// 유효성체크
	if(!fn_preSave()) return false;
		
	var szSvcid       = "updateExtSpecial";
    var szController  = "/req/reqCust/CustBankRup/"+g_mode;
    var szInDs        = "ds_input=ds_dtl_list";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 저장전
function fn_preSave()
{

	if (gfn_isNull(edt_accnt_no.value)) {
		alert("회원번호가 없습니다.");
		edt_accnt_no.setFocus();
		return false;
	} else if (gfn_isNull(cbo_special_bit.value)) {
		alert("사유를 입력하세요.");
		cbo_special_bit.setFocus();
		return false;
	} else if ( gfn_isNull(edt_req_pay_gunsu.value) && cbo_special_bit.value == "01" ) {
		alert("청구건수를 입력하세요.");
		edt_req_pay_gunsu.setFocus();
		return false;
	}
	
	return true;
}

