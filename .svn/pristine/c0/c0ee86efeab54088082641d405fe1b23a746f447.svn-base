 ***************************************************************************/
function fn_commSave_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	} 
	// 저장 후 처리
	ds_list.clearData();
	fn_postSave(strSvcID);
}

function fn_rltnSave_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	}
	ds_list.clearData();
	ds_role_scrn_rltn_list.clearData();
	fn_rltnSearch();
}

