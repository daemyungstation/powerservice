		// 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		gfn_appendSearchDs(ds_list, ds_list_Temp);
	}
	
	// 검색결과 건수(현재건수 / 전체건수) 
    gfn_setTotalRowCount(ds_list, div_body.sta_TotalCnt); 

}

