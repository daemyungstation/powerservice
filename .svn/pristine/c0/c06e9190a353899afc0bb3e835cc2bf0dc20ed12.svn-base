/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace("전산실 문의[CALL BACK ERROR!!]");
	} else {
		switch(svcid){
			case "selectDlwCode" :
				gfn_getCode("07", "ALL", ds_rltn_typ, gds_DmlCd); // 코드조회결과 필터	
				gfn_getCode("84", "", ds_resl_cd, gds_DmlCd); // 코드조회결과 필터
														
				//CMS등록된 회원 조회
				fn_commSearch(gfn_getCommTypeInit(), "selectCustaccntList");
			break;
			
			case "selectCustaccntList" :
				//청구여부 데이터 
				gv_req_yn = ds_accnt_info.getColumn(0,"wdrw_stat");
				gv_dbl_stat = ds_accnt_info.getColumn(0,"double_stat");	
							
				if (gv_dbl_stat == "1") {
					btn_Change.visible = true;
				} else {
					btn_Change.visible = false;
				}
				
				trace("xxxxxxxxx > : " + gv_dbl_stat);
				
				//CMS등록된 회원여부
				switch(ds_accnt_info.getColumn(0,"cms_yn")){
				
					// CMS회원 정보가 입력된적 없음
					case "NONE" :
						gv_cms_yn = "N";
						fn_setComponent("ADD");							
					break;
										
					// CMS회원 정보가 있는데 삭제된 경우 
					case "Y" :
						gv_cms_yn = "U";
						fn_setComponent("ADD");					
					break;
															
					// CMS회원 정상인 회원이 있음											
					case "N" :
						gv_cms_yn = "Y";
						fn_setComponent("SEARCH");
					break;									
				}
																							
			break;	
			
			case "updateCms" :
				alert("정상적으로 변경 되었습니다.");
				close();
			break;
			
			case "deleteCms" :
				alert("정상적으로 삭제 되었습니다.");
				opener.fn_srchPymnHstr();
				close();
			break;	
			
			case "insertCms" :
				alert("정상적으로 등록 되었습니다.");
				opener.fn_srchPymnHstr();
				close();
			break;				
					
			case "selectBankList":
				if (ds_bank.rowcount > 1) {
					btn_Bank_onclick()
				} else if (ds_bank.rowcount == 1) {
					edt_bankCd.value = ds_bank.getColumn(0, "cd");
					edt_bankNm.value = ds_bank.getColumn(0, "cd_nm");
				} else {
					edt_bankCd.value = "";
					edt_bankNm.value = "";
				}
			break;					
		}									
	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, svcId) {
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";

	switch(svcId) {	
		//회원번호 기준으로 CMS정보 가져오기 		
		case "selectCustaccntList":
			fn_preSearch(svcId);
			
			szSvcid       = "selectCustaccntList";
			szController  = "/dlw/newTypePop/div4/cust-acnt/accnt_list";
			szInDs        = "ds_input=ds_accntInqCndt";
			szOutDs       = "ds_accnt_info=ds_output";
			szParam       = "pay_mthd=CMS";
			szRetCallback = "fn_callBack";
		break;
	}
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function fn_preSearch(sType) {
	switch(sType) {	
		case "selectCustaccntList":		
			ds_accnt_info.clearData();
			
			ds_accntInqCndt.clearData();
			ds_accntInqCndt.addRow();
			ds_accntInqCndt.setColumn(0, "accnt_no", gv_accnt_no);
		break;
	}
}

function fn_commSave(saveInfo) {

    var szSvcid = ""
    var szController = ""
    var szInDs = ""
    var szOutDs = ""
    var szParam = ""
    var szRetCallback = ""

    switch(saveInfo) {
		
		// CMS정보 업데이트 
        case "updateCms":         						
			if(!fn_preSave(saveInfo)) return false;
			szSvcid       = saveInfo;
			szController  = "/dlw/newTypePop/div4/update_cms";
			szInDs        = "ds_input=ds_accnt_info ds_input_old=ds_accnt_old";
			szOutDs       = "";
			szParam       = "";
			szRetCallback = "fn_callBack";  
        break;   
                  
		// CMS정보 삭제
        case "deleteCms": 
            if(!fn_preSave(saveInfo)) return false;
			szSvcid       = saveInfo;
			szController  = "/dlw/newTypePop/div4/cms-anxt-srvc/save";
			szInDs        = "ds_input=ds_accnt_info";
			szOutDs       = "";
			szParam       = "double_stat=" + gv_dbl_stat;
			szRetCallback = "fn_callBack";  
        break;   
                         
		// CMS정보 등록
        case "insertCms": 
            if(!fn_preSave(saveInfo)) return false;
			szSvcid       = saveInfo;
			szController  = "/dlw/newTypePop/div4/cms-anxt-srvc/save";
			szInDs        = "ds_input=ds_accnt_info";
			szOutDs       = "";
			szParam       = "double_stat=" + gv_dbl_stat;
			szRetCallback = "fn_callBack";  
        break;
        
        // CMS로그 등록
        case "insertCmsLog": 
//            if(!fn_preSave(saveInfo)) return false;
			szSvcid       = saveInfo;
			szController  = "/dlw/newTypePop/div4/cms-history-logs/save";
			szInDs        = "ds_input=ds_cms_log";
			szOutDs       = "";
			szParam       = "";
			szRetCallback = "fn_callBack";  
        break;   
    }

	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

function fn_preSave(svcid) {
	var return_bool = false;
	
	switch(svcid) {
		case "updateCms":			
			//변경된 필드가 있는지 확인
			if(!fn_changedColumn()){
				alert("변경된 정보가 없습니다.");
				return false;
			}
							
			if (gv_req_yn == "N"){
				return_bool = confirm("산출(청구)중인 회원입니다.[수정 시 해당 월 추가 산출 될수 있음]");
			} else {
				return_bool = true;
			}
		break;
		
		case "deleteCms":			
				
			//웹모듈 연동
			if(ckb_webModule.value == "N") {
				var resl = fn_rltmWebModuleRequest();
				if (!resl) return false;
			} 

			if (gv_req_yn == "N"){
				return_bool = confirm("산출(청구)중인 회원이지만 해당 회원의 CMS를 삭제 하시겠습니까?");				
			} else {
				return_bool = confirm("해당 회원의 CMS를 해지 하시겠습니까?");
			}					
		break;		
		
		case "insertCms":	
			// 생년월일 체크 	
			var sBrthMonDay = edt_brdt.value;
			if (!gfn_isNull(sBrthMonDay)) {
				var len = String(sBrthMonDay).length;
				if (len < 6 || len > 10) {
					alert("올바른 생년월일을 입력하세요");
					edt_brdt.setFocus();
					return false;
				} 
			} else {
				alert("올바른 생년월일을 입력하세요");
				edt_brdt.setFocus();
				return false;
			}
			// 관계 값 체크 			
			if (cbo_RltnCd.index == 0) {				
				alert("올바른 관계 값을 입력하세요");
				cbo_RltnCd.setFocus();
				return false;				
			}							
			// 은행코드 체크 			
			var sBankCd = edt_bankCd.value; 
			var len = String(sBankCd).length;
			if (len != 3) {
				alert("올바른 은행코드를 입력하세요");
				edt_bankCd.setFocus();
				return false;
			} 
			// 계좌번호 체크 			
			var sBankAccnt = edt_bankaccntNo.value; 
			if (gfn_isNull(sBankAccnt)) {
				alert("올바른 계좌번호를 입력하세요");
				edt_bankaccntNo.setFocus();
				return false;
			}								
			// 예금주 체크 			
			var sDepr = edt_depr.value; 
			if (gfn_isNull(sDepr)) {
				alert("올바른 예금주를 입력하세요");
				edt_depr.setFocus();
				return false;
			}	
			// 대리납 체크	(생년월일이 틀리거나 예금주명이 틀리면 관계값이 본인이 될수 없음
			
			var brth_mon_day = ds_accnt_info.getColumn(0,"brth_mon_day");			
			
			if (sBrthMonDay !=  brth_mon_day || edt_mem_nm.value != sDepr){
				if(cbo_RltnCd.index == 1){
					alert("대리납의 경우 관계 값이 본인이 될 수 없습니다.[변경요망]");
					cbo_RltnCd.setFocus();
					return false;
				}
			}												
			
            //웹모듈 연동
			if(ckb_webModule.value == "N") {
				var resl = fn_rltmWebModuleRequest();
				if (!resl) return false;				
			}
						
			if(cbo_RltnCd.index == 1){
				return_bool = confirm("해당 회원의 CMS를 신청 하시겠습니까?");
			} else {
				return_bool = confirm("해당 회원[대리납]의 CMS를 신청 하시겠습니까?");
			}
		break;			
	}
			
	//통합구좌의 경우 회원번호가 아닌 두구좌 통합 번호를 회원번호로 조건처리함
	if (gv_dbl_stat == '2'){
		ds_accnt_info.setColumn(0, "accnt_no", edt_cms_accnt.text);	
	}
	return return_bool;
}

