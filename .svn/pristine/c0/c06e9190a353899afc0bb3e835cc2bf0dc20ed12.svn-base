}

function btn_Bank_onclick(obj:Button,  e:ClickEventInfo)
{
	var sPrmr = {p_bank_nm:edt_bankNm.value};
	var reVal = gfn_diaLog("은행 조회", -1, -1, 884, 601, "PS990000::PS992500.xfdl", false, sPrmr, false, false, true);
	
	if( reVal != null ){
