//웹모듈 결과 요청
function fn_rltmWebModuleRequest()
{
	//결과조회에 사용할 변수 초기화
	aplc_div = "";
	tmpr_resl = "";
   
    var tmpr_resl = ""; //임시결과
    
    var aplc_div = "";  //신규, 해지 구분
    var idn_no = "";    //개인식별번호
    var accnt_no = "";   //회원번호(구좌)
    var bank_cd = edt_bankCd.value;    //은행코드
    var bank_accnt_no = edt_bankaccntNo.value; //은행계좌번호
        bank_accnt_no = bank_accnt_no.replace("-","");
                
    if(gv_cms_yn == "Y"){
        aplc_div = "4";
    } else {
        aplc_div = "3";
    }
    
    accnt_no = gv_accnt_no;
    idn_no = edt_brdt.value;

    /*
    if(rdo_cmsApcl.value == "1") {
        aplc_div = "3";
        //accnt_no = ds_srchCms.getColumn(0,"accnt_no");
        accnt_no = gv_accnt_no;
    } else if (rdo_cmsApcl.value == "2") {
        aplc_div = "4";
        //accnt_no = ds_srchCms.getColumn(0,"accnt_no");
        accnt_no = gv_accnt_no;
    } else {
        alert("신규, 해지여부를 선택해주세요.");
        return false;
    }
    */
    
    var file_gb = cbo_file_gb.value;
    var file_ext = cbo_ext.value;
    
    //임시구분값 >> aplc_div >> 2일경우 유효검사
    //aplc_div = "2";
     
    //웹모듈 부가서비스요청
	tmpr_resl = gfn_AdditionalServiceReq(aplc_div,bank_cd,bank_accnt_no,accnt_no,idn_no, file_gb, file_ext);
		
	//결과조회에 사용할 변수 저장
	tmp_aplc_div = aplc_div;
	tmp_tmpr_resl = tmpr_resl;
	
    //결과 조회
    var resl_bool = fn_rltmWebModuleResult();
    
    return resl_bool;
}

//웹모듈 결과 조회
function fn_rltmWebModuleResult()
{
    var FlReslCd = gfn_AdditionalServiceRes(tmp_aplc_div,tmp_tmpr_resl);
    var RlReslCd = FlReslCd.substring(0,4);
	//  sleep(2000);
	ds_cms_log.clearData();
            
    if(RlReslCd != "0000") {    
        ds_resl_cd.filter("(cd=='"+ RlReslCd +"')");
		var errMsg = ds_resl_cd.getColumn(0,"cd_nm");
		if (gfn_isNull(errMsg)) {
			alert("부가서비스 등록 실패");
		} else {
			alert(ds_resl_cd.getColumn(0,"cd_nm"));
		}
		if (ds_resl_cd.getColumn(0,"cd_nm") != "정상완료") {
			ds_cms_log.addRow();
			ds_cms_log.setColumn(0, "accnt_no", gv_accnt_no);
			ds_cms_log.setColumn(0, "cd", RlReslCd);
			ds_cms_log.setColumn(0, "cd_nm", gfn_Nvl(errMsg, "부가서비스 등록 실패"));
			
			fn_commSave("insertCmsLog");
			
			return false;
		} else {
			return true;
		}
		ds_resl_cd.filter("");
    } else {
		return true;
    }
}

//데이터 업데이트시 ASIS데이터와 TOBE데이터 비교 후 변경된 데이터를 넘김
function fn_changedColumn(){	
	var arrText = "";
	var asisData = "";
	var tobeData = "";
	var returnVal = false;
	for(var i=0; i<ds_accnt_info.colcount; i++){
		asisData = ds_accnt_info.getOrgColumn(0,i);	
		tobeData = ds_accnt_info.getColumn(0,i);		
		if(asisData == undefined) asisData = "";
		if(tobeData == undefined) tobeData = "";	
			
		if(asisData != tobeData){				
			returnVal = true;
		} 		
	}			
	return returnVal;
}

function fn_setComponent(sType){
	trace("#########컴퍼넌트(" + sType +") 세팅############");
	switch(sType) {	
		case "INIT":
			edt_mem_nm.readonly = true;
			edt_brdt.readonly = true;
			cbo_RltnCd.readonly = true;
			edt_bankCd.readonly = true;
			edt_bankaccntNo.readonly = true;
			edt_reg_dm.readonly = true;
			edt_depr.readonly = true;
			
			btn_del_cms.visible = false;
			btn_bank_search.visible = false;
			
			cbo_file_gb.readonly = true;
			cbo_ext.readonly = true;	
														
			gv_accnt_no = p_accnt_no;							
		break;
		
		case "SEARCH":
			//이전 데이터 저장
			ds_accnt_old.assign(ds_accnt_info);
			btn_del_cms.visible = true;
		break;	
			
		case "ADD":
			//이전 데이터 저장
			edt_mem_nm.readonly = true;
			edt_brdt.readonly = false;
			cbo_RltnCd.readonly = false;
			edt_bankCd.readonly = false;			
			edt_bankaccntNo.readonly = false;
			edt_depr.readonly = false;
			
			btn_bank_search.visible = true;
			
			cbo_file_gb.readonly = false;
			cbo_ext.readonly = false;
			
			rdo_tranDt.value = "05";
			cbo_RltnCd.index = 1;
			
			// CMS 증빙자료 초기화
			cbo_file_gb.value = '2'
			ds_ext.assign(ds_ext_wav);
			cbo_ext.index = "1";
			
			// 이름 및 생년월일 자동 생성 
			edt_brdt.value = ds_accnt_info.getColumn(0,"brth_mon_day");
			edt_depr.value = edt_mem_nm.value;
									
			// CMS기존에 등록이 되었던 회원의 경우 (CMS업데이트 NEW_CANCLE 인서트)
			if(gv_cms_yn == "U"){										
				edt_bankCd.value = "";		
				edt_bankNm.value = "";
				edt_bankaccntNo.value = "";
				edt_reg_dm.value = "";
				edt_reg_man.value = "";
			}																							
		break;				
	}

}

function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

function edt_accnt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == "13") {
		if (obj.value == "") {
			btn_accnt_onclick(obj);
		} else {
			var srch_typ = "";
			if (obj.name == "edt_accnt_no") {
				gv_accnt_no = edt_accnt_no.value;
			}
			fn_commSearch(gfn_getCommTypeInit(), "selectCustaccntList"); // 조회
		}
	}
}

function cbo_file_gb_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	ds_ext.clearData();
	if (obj.value == "1") {
		ds_ext.assign(ds_ext_img);
		cbo_ext.index = "0";
	} else {
		ds_ext.assign(ds_ext_wav);
		cbo_ext.index = "1";
	}
}

function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if (gv_cms_yn == "Y"){
		fn_commSave("updateCms");
	} else {
		fn_commSave("insertCms");
	}
}

function btn_del_cms_onclick(obj:Button,  e:ClickEventInfo)
{
