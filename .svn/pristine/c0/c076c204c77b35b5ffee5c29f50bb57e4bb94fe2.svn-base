    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS031200_onload(obj:Form, e:LoadEventInfo)
{
	fn_init();
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		tab_niceMgmt.niceMemberAuth.btn_excel.visible = true;
	} else {
		tab_niceMgmt.niceMemberAuth.btn_excel.visible = false;
	}
	
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(sSvcId, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd);
		return false;
	} 
	else 
	{
		switch(sSvcId) 
		{						
			case "getNiceAuthInformation":
				fn_postSearch(sSvcId);
			break;
			
			case "getNiceJoinInformation":
				fn_postSearch(sSvcId);
			break;
			
			case "sendResendKakaoAuth":
				fn_postSearch(sSvcId);
			break;
			
			case "sendResendKakaoJoin":
				fn_postSearch(sSvcId);
			break;
			
			case "deleteNiceJoinData":
				fn_postSearch(sSvcId);
			break;
		}
	}
}

// 조회 결과
function fn_postSearch(sSvcId)
{
	if(sSvcId == "getNiceAuthInformation")
	{
		if (ds_listAuth.getRowCount() < 1) 
		{
			ds_listAuth.clearData();
			alert("조회된 결과가 없습니다.");
		} 
	}
	else if(sSvcId == "getNiceJoinInformation")
	{
		if (ds_listJoin.getRowCount() < 1) 
		{
			ds_listJoin.clearData();
			alert("조회된 결과가 없습니다.");
		} 
	}
	else if(sSvcId == "sendResendKakaoAuth")
	{
		alert("Nice 인증문자재전송이 완료되었습니다.");
		fn_commSearch(gfn_getCommTypeInit(), "getNiceAuthInformation");
	}
	else if(sSvcId == "sendResendKakaoJoin")
	{
		alert("Nice 가입문자재전송이 완료되었습니다.");
		fn_commSearch(gfn_getCommTypeInit(), "getNiceJoinInformation");
	}
	else if(sSvcId == "deleteNiceJoinData")
	{
		alert("Nice 가입문자데이터 삭제가 완료되었습니다.");
		fn_commSearch(gfn_getCommTypeInit(), "getNiceJoinInformation");
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_validationCheck(sType, sSvcId)
{
	if (gfn_isNull(sSvcId) == true) 
	{
		alert("올바르지 않은 서비스 명입니다.");
		return false;
	} 
	else 
	{
		switch (sSvcId) 
		{						
			case "getNiceAuthInformation" :
				var sCalSttDt = new String(cal_stt_dt.value);
				var sCalEndDt = new String(cal_end_dt.value);
				
				if(sCalSttDt > sCalEndDt)
				{
					alert("조회의 시작일이 종료일보다 클수 없습니다.");
					return false;
				}
				
				if(sCalSttDt.length != 8 || sCalEndDt.length != 8)
				{
					alert("조회의 시작일과 종료일을 정확하게 입력하여 주십시오.");
					return false;
				}
				
				return true;
			break;
			
			case "getNiceJoinInformation" :
				var sCalSttDt = new String(cal_stt_dt.value);
				var sCalEndDt = new String(cal_end_dt.value);
				
				if(sCalSttDt > sCalEndDt)
				{
					alert("조회의 시작일이 종료일보다 클수 없습니다.");
					return false;
				}
				
				if(sCalSttDt.length != 8 || sCalEndDt.length != 8)
				{
					alert("조회의 시작일과 종료일을 정확하게 입력하여 주십시오.");
					return false;
				}
				
				return true;
			break;
			
			case "sendResendKakaoAuth" : 
				var nChkRowCnt = ds_listAuth.getCaseCount("chk == '1'");
				var sNiceNo = "";
				var sUppTxType = "";

				if(nChkRowCnt < 1)
				{
					alert("선택된 회원이 없습니다.");
					return false;
				}
				
				for(var dsIdx = 0; dsIdx < ds_listAuth.getRowCount(); dsIdx++)
				{
					if(ds_listAuth.getColumn(dsIdx, "chk") == "1")
					{
						sNiceNo = ds_listAuth.getColumn(dsIdx, "nice_no");
						sUppTxType = ds_listAuth.getColumn(dsIdx, "upp_tx_type");
					
						if(sUppTxType != "01" && sUppTxType != "02")
						{
							alert("인증문자재전송은 [1차접수완료], [본인인증] 상태에서만 가능합니다. 해당 row번호 : " + (dsIdx + 1));
							return false;
						}
					}
				}
				return true;
			break;
			
			case "sendResendKakaoJoin" :
				var nChkRowCnt = ds_listJoin.getCaseCount("chk == '1'");
				var sNiceNo1 = "";
				var sNiceNo2 = "";
				var sUppTxType1 = "";
				var sUppTxType2 = "";
				
				if(nChkRowCnt < 1)
				{
					alert("선택된 회원이 없습니다.");
					return false;
				}
				
				for(var dsIdx = 0; dsIdx < ds_listJoin.getRowCount(); dsIdx++)
				{
					if(ds_listJoin.getColumn(dsIdx, "chk") == "1")
					{
						sNiceNo1 = ds_listJoin.getColumn(dsIdx, "nice_no_1");
						sNiceNo2 = ds_listJoin.getColumn(dsIdx, "nice_no_2");
						sUppTxType1 = ds_listJoin.getColumn(dsIdx, "upp_tx_type_auth_1");
						sUppTxType2 = ds_listJoin.getColumn(dsIdx, "upp_tx_type_auth_2");
						
						if(sUppTxType1 != "03")
						{
							alert("고객인증/신용정보취득이 종료되지 않은 회원입니다. 해당 row번호 : " + (dsIdx + 1));
							return false;
						}
						
						if(sUppTxType2 != "04" && sUppTxType2 != "05")
						{
							alert("가입문자재전송은 [2차접수완료], [위탁처리] 상태에서만 가능합니다. 해당 row번호 : " + (dsIdx + 1));
							return false;
						}
					}
				}
				return true;
			break;
			
			case "deleteNiceJoinData" : 
				var nChkRowCnt = ds_listJoin.getCaseCount("chk == '1'");
				var sNiceNo1 = "";
				var sNiceNo2 = "";
				var sUppTxType1 = "";
				var sUppTxType2 = "";
				
				if(nChkRowCnt < 1)
				{
					alert("선택된 회원이 없습니다.");
					return false;
				}
				
				for(var dsIdx = 0; dsIdx < ds_listJoin.getRowCount(); dsIdx++)
				{
					if(ds_listJoin.getColumn(dsIdx, "chk") == "1")
					{
						sNiceNo1 = ds_listJoin.getColumn(dsIdx, "nice_no_1");
						sNiceNo2 = ds_listJoin.getColumn(dsIdx, "nice_no_2");
						sUppTxType1 = ds_listJoin.getColumn(dsIdx, "upp_tx_type_auth_1");
						sUppTxType2 = ds_listJoin.getColumn(dsIdx, "upp_tx_type_auth_2");
						
						if(sUppTxType1 != "03")
						{
							alert("고객인증/신용정보취득이 종료되지 않은 회원입니다. 해당 row번호 : " + (dsIdx + 1));
							return false;
						}
						
						if(sUppTxType2 != "04" && sUppTxType2 != "05" && gfn_isNull(sUppTxType2) == false)
						{
							alert("가입문자데이터 삭제는 [2차접수중], [2차접수완료], [위탁처리] 상태에서만 가능합니다. 해당 row번호 : " + (dsIdx + 1));
							return false;
						}
					}
				}
				return true;
			break;
		}
	}
}
/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원 조회
function fn_commSearch(sType, sSearchInfo)
{
	if (fn_validationCheck(sType, sSearchInfo) == false) 
	{
		return false;
	}
	switch(sSearchInfo) 
	{
        case "getNiceAuthInformation": 
			ds_cond.clearData();
			var nRow = ds_cond.addRow();
			
			ds_cond.setColumn(nRow, "stt_dt", cal_stt_dt.value);
			ds_cond.setColumn(nRow, "end_dt", cal_end_dt.value);
			ds_cond.setColumn(nRow, "mem_nm", edt_mem_nm.value);
			ds_cond.setColumn(nRow, "upp_tx_type", cbo_upp_tx_type.value);
			ds_cond.setColumn(nRow, "brth_mon_day", cal_brth_mon_day.value);
			ds_cond.setColumn(nRow, "clph_tlno", edt_clph_tlno.value);

			var szSvcid       = "getNiceAuthInformation";
			var szController  = "/web/nice/getNiceAuthInformation";
			var szInDs        = "ds_input=ds_cond";
			var szOutDs       = "ds_listAuth=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;
        
        case "getNiceJoinInformation": 
			ds_cond.clearData();
			var nRow = ds_cond.addRow();
			
			ds_cond.setColumn(nRow, "stt_dt", cal_stt_dt.value);
			ds_cond.setColumn(nRow, "end_dt", cal_end_dt.value);
			ds_cond.setColumn(nRow, "mem_no", edt_mem_no.value);
			ds_cond.setColumn(nRow, "accnt_no", edt_accnt_no.value);
			ds_cond.setColumn(nRow, "mem_nm", edt_mem_nm.value);
			ds_cond.setColumn(nRow, "upp_tx_type", cbo_upp_tx_type.value);
			ds_cond.setColumn(nRow, "brth_mon_day", cal_brth_mon_day.value);

			var szSvcid       = "getNiceJoinInformation";
			var szController  = "/web/nice/getNiceJoinInformation";
			var szInDs        = "ds_input=ds_cond";
			var szOutDs       = "ds_listJoin=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
			
			gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
        break;
    }
}


/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	ds_cond.clearData();
	ds_listAuth.clearData();
	ds_listJoin.clearData();
	
	var sToday = gfn_getToday();
	var sStartDay = gfn_addDate(sToday, -7);
	
	cal_stt_dt.value = sStartDay;
	cal_end_dt.value = sToday;
	
	tab_niceMgmt.tabindex = 0;
	
	tab_niceMgmt_onchanged(tab_niceMgmt);
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/

function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

// 검색 버튼 클릭
function btn_srch_onclick(obj:Button,  e:ClickEventInfo)
{
	if(tab_niceMgmt.tabindex == 0)
	{
		fn_commSearch(gfn_getCommTypeInit(), "getNiceAuthInformation");
	}
	else if(tab_niceMgmt.tabindex == 1)
	{
		fn_commSearch(gfn_getCommTypeInit(), "getNiceJoinInformation");
	}
}

// 그리드 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	gfn_grdHeadClick(obj, e);
}

function btn_resendKaKao_onclick(obj:Button,  e:ClickEventInfo)
{
	if(tab_niceMgmt.tabindex == 0)
	{
		if(fn_validationCheck(gfn_getCommTypeInit(), "sendResendKakaoAuth") == true)
		{
			ds_listAuthInDs.clearData();
			var nCopyAddRow = 0;
			
			for(var idx = 0; idx < ds_listAuth.getRowCount(); idx++)
			{
				if(ds_listAuth.getColumn(idx, "chk") == "1")
				{
					nCopyAddRow = ds_listAuthInDs.addRow();
					ds_listAuthInDs.copyRow(nCopyAddRow, ds_listAuth, idx);
					nCopyAddRow++;
				}
			}
		
			var szSvcid       = "sendResendKakaoAuth";
