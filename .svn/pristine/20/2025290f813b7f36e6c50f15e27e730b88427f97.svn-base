    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_memAddr = 0; // 전체 건수

// 권한 설정
var rgsnYn; 	// 등록권한
var amntYn; 	// 수정권한
var dltnYn; 	// 삭제권한

var gvMenuId;  // 프로그램 로그 추가 2017.12.04
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS993900_onload(obj:Form, e:LoadEventInfo)
{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.04 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin());
	gfn_getList("cd", "CUS170"); 	// 공통코드조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
 			gfn_getCode("CUS170", "SELECT", ds_srch_rltn_cd); // 고객관계코드
 			gfn_getCode("CUS170", "SELECT", ds_rltn_cd);
 			
 			fn_init("Y");
 			// 2017.12.14 조건 체크 후 조회 로 인한 삭제
			//fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "srchMemberAddr":
			gfn_appendSearchDs(ds_list_memAddr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_memAddr, sta_TotalCnt, tc_memAddr); // 페이지건수 표출
			fn_postSearch();
			break;
			
			case "saveMemberAddr":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "deleteMemberAddr":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_memAddr.keystring = ""; // 정렬컬럼 초기화
		
		// 2017.12.14 김찬영 추가...///////////////
		//alert(cbo_srch_rltn_cd.value);
		if (gfn_isNull(edt_srch_mem_nm.value))
		{
			alert("회원명을 반드시 입력해야 합니다.");
			edt_srch_mem_nm.setFocus();
			return false;
		}
		if (gfn_isNull(cbo_srch_rltn_cd.value))
		{
			alert("관계를 선택 해 주세요!");
			cbo_srch_rltn_cd.setFocus();
			return false;
		}
		
		/////////////////////////////////////////////
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "mem_nm", edt_srch_mem_nm.value);
		ds_inqCndt.setColumn(0, "mem_no", edt_srch_mem_no.value);
		if (cbo_srch_typ.value == "cell") {
			ds_inqCndt.setColumn(0, "cell", edt_srch_tel_no.value);
			ds_inqCndt.setColumn(0, "home_tel", "");
		} else if (cbo_srch_typ.value == "home_tel") {
			ds_inqCndt.setColumn(0, "cell", "");
			ds_inqCndt.setColumn(0, "home_tel", edt_srch_tel_no.value);
		}
		ds_inqCndt.setColumn(0, "rltn_cd", cbo_srch_rltn_cd.value);
		ds_inqCndt.setColumn(0, "home_zip", edt_srch_home_zip.value);
		ds_inqCndt.setColumn(0, "home_addr", edt_srch_home_addr.value);
		
		// 2016-07-29 부서 관계 없이 전체 조회
		// ds_inqCndt.setColumn(0, "bzpt_div_yn", "Y");
		
		ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

// 저장전
function fn_preSave()
{
	if (gfn_isNull(edt_mem_no.value)) {
		alert("회원고유번호가 없습니다.");
		return false;
	} else if (gfn_isNull(edt_mem_nm.value)) {
		alert("회원명이 없습니다.");
		return false;
	} else if (gfn_isNull(cbo_rltn_cd.value)) {
		alert("관계를 선택하세요.");
		cbo_rltn_cd.setFocus();
		return false;
	} 
	return true;
}

// 삭제전
function fn_preDelete()
{
	if (gfn_isNull(ds_memAddrInfo.getColumn(0, "mem_addr_id"))) {
		alert("삭제하려는 회원주소정보가 없습니다.");
		return false;
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회
function fn_commSearch(sType)
{

	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_memAddr, sta_TotalCnt, 15, tc_memAddr)) return;
	
	var szSvcid       = "srchMemberAddr";
    var szController  = "/member-addr/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

// 저장
function fn_saveMemberAddr()
{
	if (!fn_preSave()) return false;
	
	var szSvcid       = "saveMemberAddr";
    var szController  = "/member-addr/save";
    var szInDs        = "ds_input=ds_memAddrInfo";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

// 삭제
function fn_delMemberAddr()
{
	if (!fn_preDelete()) return false;

	if (confirm("해당 회원주소정보를 삭제하시겠습니까?")) {
		var szSvcid       = "deleteMemberAddr";
		var szController  = "/member-addr/delete";
		var szInDs        = "";
		var szOutDs       = "";
		var szParam       = "mem_addr_id=" + ds_memAddrInfo.getColumn(0, "mem_addr_id");
		var szRetCallback = "fn_callBack";
		
		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
	}
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init(sType)
{
	edt_srch_mem_nm.value = "";
	edt_srch_mem_no.value = "";
	edt_srch_tel_no.value = "";
	edt_srch_home_zip.value = "";
	edt_srch_home_addr.value = "";
	
	cbo_srch_typ.index = 0;
	cbo_srch_rltn_cd.index = 0;
	
	if (sType == "Y") {
		cbo_rltn_cd.index = 0;
	}
}

// 조회 결과
function fn_postSearch()
{
	if (ds_list_memAddr.rowcount < 1) {
		ds_memAddrInfo.clearData();
		fn_setBtn("I");
		mae_rgsn_dttm.mask = "";
		mae_amnt_dttm.mask = "";
		//mae_home_zip.mask = "";
	} else {
		fn_setMemberAddr();
	}
}

// 회원 주소정보 설정
function fn_setMemberAddr()
{
	ds_memAddrInfo.clearData();
	ds_memAddrInfo.addRow();
	ds_memAddrInfo.copyRow(0, ds_list_memAddr, ds_list_memAddr.rowposition);
	
	fn_setBtn("IUD");
	fn_setEnable("VIEW");
	
	mae_rgsn_dttm.mask = "####-##-## ##:##";
	mae_amnt_dttm.mask = "####-##-## ##:##";
	
	//gfn_SetHomeZipMask(mae_home_zip);
	gfn_phoneNumObj(edt_cell);
	gfn_phoneNumObj(edt_home_tel);
}

// 버튼 제어
function fn_setBtn(sType)
{
	if (sType == "I") {
		btn_update.visible = false;
		btn_save.visible = false;
		btn_cancel.visible = false;
		btn_delete.visible = false;
		btn_chng_hstr.visible = false;
		if (rgsnYn == "Y") {
			btn_insert.position2 = "absolute r:0 w:72 b:88 h:20";
			btn_insert.visible = true;
		} else {
			btn_insert.visible = false;
		}
	} else if (sType == "IUD") {
		btn_save.visible = false;
		btn_cancel.visible = false;
		
		btn_insert.visible = false;
		btn_update.visible = false;
		btn_delete.visible = false;
		
	    // 2016-08-04 삭제 버튼 숨김 처리
		if (rgsnYn == "Y" || amntYn == "Y" || dltnYn == "Y") {
		    /*
			btn_insert.position2 = "absolute r:222 w:72 b:88 h:20";
			btn_update.position2 = "absolute r:148 w:72 b:88 h:20";
			btn_delete.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_insert.visible = true;
			btn_update.visible = true;
			btn_delete.visible = true;
			*/
			btn_insert.position2 = "absolute r:148 w:72 b:88 h:20";
			btn_update.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_insert.visible = true;
			btn_update.visible = true;
		} else if (rgsnYn == "Y" || amntYn == "Y") {
			btn_insert.position2 = "absolute r:148 w:72 b:88 h:20";
			btn_update.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_insert.visible = true;
			btn_update.visible = true;
		} else if (rgsnYn == "Y" || dltnYn == "Y") {
		    /*
			btn_insert.position2 = "absolute r:148 w:72 b:88 h:20";
			btn_delete.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_insert.visible = true;
			btn_delete.visible = true;
			*/
			btn_insert.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_insert.visible = true;
		} else if (amntYn == "Y" || dltnYn == "Y") {
		    /*
			btn_update.position2 = "absolute r:148 w:72 b:88 h:20";
			btn_delete.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_update.visible = true;
			btn_delete.visible = true;
			*/
			btn_update.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_update.visible = true;
		} else if (rgsnYn == "Y") {
			btn_insert.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_insert.visible = true;
		} else if (amntYn == "Y") {
			btn_update.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_update.visible = true;
		} else if (dltnYn == "Y") {
		    /*
			btn_delete.position2 = "absolute r:74 w:72 b:88 h:20";
			btn_delete.visible = true;
			*/
		}
		btn_chng_hstr.position2 = "absolute r:0 w:72 b:88 h:20";
		btn_chng_hstr.visible = true;
	} else if (sType == "SC") {
		btn_update.visible = false;
		btn_insert.visible = false;
		btn_delete.visible = false;
		btn_chng_hstr.visible = false;
		btn_save.position2 = "absolute r:74 w:72 b:88 h:20";
		btn_cancel.position2 = "absolute r:0 w:72 b:88 h:20";
		btn_save.visible = true;
		btn_cancel.visible = true;
	}
}

// enable
function fn_setEnable(sType)
{
	if (sType == "VIEW"){
		btn_srchMemPop.enable = false;
		btn_homeZip.enable = false;
		cbo_rltn_cd.enable = false;
		edt_cell.enable = false;
		edt_home_tel.enable = false;
		edt_home_addr2.enable = false;
	} else {
		btn_srchMemPop.enable = true;
		btn_homeZip.enable = true;
		cbo_rltn_cd.enable = true;
		edt_cell.enable = true;
		edt_home_tel.enable = true;
		edt_home_addr2.enable = true;
	} 
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// button onclick
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_search") {				// 검색
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_init") {		// 초기화
		fn_init("N");
	} else if (obj.name == "btn_srchMemPop") {	// 회원조회팝업
	     sPrmr = {p_mem_nm:"", p_home_tel:"", p_cell:""}
		var reVal = gfn_diaLog("회원 조회", -1, -1, -1, -1, "PS020000::PS020402.xfdl", false, sPrmr, false, false, true);
		
		if( reVal != null ){
			if(reVal.getColumn(0, "mem_no") != ""){
				edt_mem_no.value = reVal.getColumn(0, "mem_no");
				edt_mem_nm.value = reVal.getColumn(0, "mem_nm");
			}
		}
	} else if (obj.name == "btn_homeZip") {		// 우편번호조회팝업	
		//var reVal = gfn_diaLog("우편번호조회1", -1, -1, -1, -1, "PS990000::PS994100.xfdl", false, {}, true, false, true, null, null, true);
		// 기존 주소검색 폐지 / 2017.04.10 부로 다음API사용
		var sPrmr = {};
		var reVal = gfn_diaLog("주소 조회", -1, -1, 884, 601, "PS990000::PS992099.xfdl", false, sPrmr, false, false, true);		
	} else if (obj.name == "btn_insert") {		// 등록버튼
		ds_memAddrInfo.clearData();
		ds_memAddrInfo.addRow();
		
		fn_setBtn("SC");
		fn_setEnable("INSERT");
		stc_txt_addr.text = "회원 주소정보 등록";
		
		mae_rgsn_dttm.mask = "";
		mae_amnt_dttm.mask = "";
		//mae_home_zip.mask = "";
		cbo_rltn_cd.index = 0;
	} else if (obj.name == "btn_update") {		// 수정버튼
		fn_setBtn("SC");
		fn_setEnable("UPDATE");
		stc_txt_addr.text = "회원 주소정보 수정";
	} else if (obj.name == "btn_save") {		// 저장버튼
		fn_saveMemberAddr();
	} else if (obj.name == "btn_cancel") {		// 취소버튼
		if (ds_list_memAddr.rowcount < 1) {
			ds_memAddrInfo.clearData();
			fn_setBtn("I");
			fn_setEnable("INSERT");
		} else {
			fn_setMemberAddr();
			fn_setBtn("IUD");
			fn_setEnable("VIEW");
		}
		stc_txt_addr.text = "회원정보 보기";
	} else if (obj.name == "btn_delete") {		// 삭제버튼
		fn_delMemberAddr();
	} else if (obj.name == "btn_chng_hstr") {	// 변경이력
	    var sPrmr = {p_mem_addr_id: ds_memAddrInfo.getColumn(0, "mem_addr_id")};
		var reVal = gfn_diaLog("고객주소변경이력", -1, -1, -1, -1, "PS990000::PS994800.xfdl", false, sPrmr, true, false, true, null, null, true);
	}
}

// 주소 값 등록(팝업창 호출)
function fn_daum_addr()
{
	mae_home_zip.value = gv_dpost; 
	edt_home_addr.value = gv_daddr;	
}

// onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		fn_commSearch(gfn_getCommTypeInit());
	}
}

// 그리드 헤더 사이즈 초기화
function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_resetColsize(grd_list);
}

// 그리드 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/member-addr/list"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_memAddr, sta_TotalCnt, 15, tc_memAddr)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_memAddr, sta_TotalCnt, tc_memAddr); // 페이지건수 표출
	
	fn_setMemberAddr();
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}


// 전화번호 설정
function phone_ontextchanged(obj:Edit,  e:TextChangedEventInfo)
{
	obj.value = gfn_phoneNumStr(obj.value);
	obj.setCaretPos(-1);
}

function phone_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	obj.value = gfn_phoneNumStr(obj.value);
}

// 그리드 셀 클릭
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	fn_setMemberAddr();
}
