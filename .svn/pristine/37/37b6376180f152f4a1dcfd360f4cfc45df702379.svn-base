// 납입회차 수정 시(추가금)
function pay_cnt_chng3(obj:Spin, e:SpinEventInfo)
{
	var ncnt1 = 0;
	var next_cnt = 0;
	var nSum = 0;
	var tcnt = 0;
	next_cnt = parseInt(g_tcnt3); // 다음 납입 회차
	ncnt1 = e.postvalue; // 납입예정회차
	tcnt = next_cnt + ncnt1;

	for (i=next_cnt; i < tcnt; i++){
		nSum += toNumber(ds_paydtl.getColumn(i, "all_amt")); 
	} 	

	pay_amt3.value = nSum || 0;	
}

// 납입회차 수정 시(추가금)
function pay_cnt_chng3_keyup(obj:Spin, e:KeyEventInfo)
{
	var ncnt1 = 0;
	var next_cnt = 0;
	var nSum = 0;
	var tcnt = 0;
	next_cnt = parseInt(g_tcnt3); // 다음 납입 회차
	ncnt1 = parseInt(sp_pay3.value); // 납입예정회차
	tcnt = next_cnt + ncnt1;

	if(parseInt(sp_pay3.value) > g_cnt3){
		alert("추가 입력회차가 잔여회차보다 더 많습니다.");
		sp_pay3.setFocus();
		return;
	}

	for (i=next_cnt; i < tcnt; i++){
		nSum += toNumber(ds_paydtl.getColumn(i, "all_amt")); 
	} 	

	pay_amt3.value = nSum || 0;	
}
/***************************************************************************
 * 함  수  명		: fn_srchCallBack()
 * 기      능		: 조회 콜백 처리
 ***************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
  
    if (errcd < 0) {
		alert("조회를 실패하였습니다.\n" + errmsg);
    } else {
		switch(svcid){
			case "searchMoveGoodsList":
				fn_postSearch();
			break;
			case "searchPayTotalList":
				pay_hap_null();
				fn_paymng_dtl(); // 입금회차에 따른 금액조회
			break;
			case "srchEvntPayDtl":
				//trace(ds_paydtl.saveXML())
			break;
			case "saveBasicAmt":
				fn_close();
			break;
		}
    }
}

