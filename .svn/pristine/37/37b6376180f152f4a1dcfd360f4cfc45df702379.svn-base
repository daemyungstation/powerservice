 * 수  정  일      : 2016.01.21 
 ***************************************************************************/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

/***************************************************************************
 * 기      능		: (공통) 그리드 칼럼사이즈 변경시 Hidden 처리
						단, 칼럼사이즈를 0으로 변경한 후에는 공통기능버튼
							(칼럼 보이기/숨기기)으로 다시 보이기 처리
 ***************************************************************************/
function grd_list_oncolresized(obj:Grid, e:GridSizeChangedEventInfo)
{
	if(e.oldvalue == 0) {
		obj.setRealColSize(e.formatindex, 0);
	}
}

function fn_close(){

	pay_cnt = sp_pay1.value;
	pay_dtl_cnt = sp_pay2.value;
	pay_dtl1_cnt = sp_pay3.value;
	pay_amt = pay_amt1.value;
	pay_dtl_amt = pay_amt2.value;
	pay_dtl1_amt = pay_amt3.value;
	
	alert("수정되었습니다.");
	opener.fn_setBasicCmsamt(pay_cnt, pay_dtl_cnt, pay_dtl1_cnt, pay_amt, pay_dtl_amt, pay_dtl1_amt);
	close();	
}


