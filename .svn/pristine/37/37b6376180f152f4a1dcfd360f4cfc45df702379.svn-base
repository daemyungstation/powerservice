// 납입회차 수정 시(상조부금)
function pay_cnt_chng(obj:Spin, e:SpinEventInfo)
{
	var ncnt1 = 0;
	var next_cnt = 0;
	var nSum = 0;
	var tcnt = 0;
	next_cnt = parseInt(g_tcnt1); // 다음 납입 회차
	ncnt1 = e.postvalue; // 납입예정회차
	tcnt = next_cnt + ncnt1;

	/*
	next_cnt ==> 입금처리시 시작회차
	tcnt     ==> 입금처리시 종료회차
	*/
	
	for (i=next_cnt; i < tcnt; i++){
		nSum += toNumber(ds_paydtl.getColumn(i, "mon_amt")); 
	} 	

	pay_amt1.value = nSum || 0;	

}

// 납입회차 수정 시(상조부금)
function pay_cnt_chng_keyup(obj:Spin, e:KeyEventInfo)
{
	var ncnt1 = 0;
	var next_cnt = 0;
	var nSum = 0;
	var tcnt = 0;
	next_cnt = parseInt(g_tcnt1); // 다음 납입 회차
	ncnt1 = parseInt(sp_pay1.value); // 납입예정회차
	tcnt = next_cnt + ncnt1;

	if(parseInt(sp_pay1.value) > g_cnt1){
		alert("상조부금 입력회차가 잔여회차보다 더 많습니다.");
		sp_pay1.setFocus();
		return;
	}

	/*
	next_cnt ==> 입금처리시 시작회차
	tcnt     ==> 입금처리시 종료회차
	*/
	
	for (i=next_cnt; i < tcnt; i++){
		nSum += toNumber(ds_paydtl.getColumn(i, "mon_amt")); 
	} 	

	pay_amt1.value = nSum || 0;	
}

