/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType, sSvcId)
{
	if (sType == gfn_getCommTypeInit())
	{
		var sTabPageId = tab_resn.tabpages[tab_resn.tabindex].name;	
		if(gfn_IsNull(div_monthSttCal.getDate()) == true || gfn_IsNull(div_monthEndCal.getDate()) == true){
			alert("기준 일자를 정확하게 입력하여 주십시오.");
			return false;
		}
		
		ds_inqCndt.clearData();
		//ds_listMangiList.keystring = ""; // 정렬컬럼 초기화
			
		var nRow = ds_inqCndt.addRow();
		
		var sToday = gfn_getToday();
		var sStartDate = gfn_addMonth(sToday, -1);
		
		ds_inqCndt.setColumn(nRow, "stt_dt" , div_monthSttCal.getDate());
		ds_inqCndt.setColumn(nRow, "end_dt" , div_monthEndCal.getDate());
		ds_inqCndt.setColumn(nRow, "ext_dt" , sStartDate);
		
		var sTabPageId = tab_resn.tabpages[tab_resn.tabindex].name;		
		
		if(sTabPageId == "tp_resn3" || sTabPageId == "tp_resn4"){
			ds_inqCndt.setColumn(nRow, "rf_rate", edt_empleNm.value);
		} else {
			ds_inqCndt.setColumn(nRow, "rf_rate", "");
		}
		
		ds_inqCndt.setColumn(nRow, "svc_id", sTabPageId);
		
	}
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
function fn_commSearch(sType, sSvcId)
{
	var szSvcid       = "";	
	var sTabPageId = tab_resn.tabpages[tab_resn.tabindex].name;		
	if (!fn_preSearch(sType)) return false;
	//if (!fn_preSearch(sType, sSvcId)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(sTabPageId == "tp_resn1"){
		szSvcid       = "getMangiResnList1";
	} else if(sTabPageId == "tp_resn2"){
		szSvcid       = "getMangiResnList2";
	} else if(sTabPageId == "tp_resn3"){
		szSvcid       = "getMangiResnList3";
	} else if(sTabPageId == "tp_resn4"){
		szSvcid       = "getMangiResnList4";
	}
	
	
	//var szSvcid       = "getMangiResnList";
    var szController  = "/dlw/mangi/ext/getMangiResnLIst";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_listMangiListTemp=ds_output ds_listMangiListTemp2=ds_output2 ds_listMangiListTemp3=ds_output3 ds_listMangiListTemp4=ds_output4";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

