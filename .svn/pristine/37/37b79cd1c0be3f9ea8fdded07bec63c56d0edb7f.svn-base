/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		return false;
	}
	
	switch (svcid) 
	{
		case "getMangiResnList1" : 
			ds_listMangiList.clearData();	    			
				    			
		    gfn_appendSearchDs(ds_listMangiList, ds_listMangiListTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			//gfn_setTotalRowCount(ds_listMangiList, sta_totalCnt, nTotalListCnt); // 페이지건수 표출		
			fn_postSearch(svcid);
		break;
		
		case "getMangiResnList2" : 
			ds_listMangiList2.clearData();	    			
			
		    gfn_appendSearchDs(ds_listMangiList2, ds_listMangiListTemp2); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			//gfn_setTotalRowCount(ds_listMangiList, sta_totalCnt, nTotalListCnt); // 페이지건수 표출		
			fn_postSearch(svcid);
		break;
		
		case "getMangiResnList3" : 
			ds_listMangiList3.clearData();	    			
			
		    gfn_appendSearchDs(ds_listMangiList3, ds_listMangiListTemp3); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			//gfn_setTotalRowCount(ds_listMangiList, sta_totalCnt, nTotalListCnt); // 페이지건수 표출		
			fn_postSearch(svcid);
		break;
		
		case "getMangiResnList4" : 
			ds_listMangiList4.clearData();	    			
			
		    gfn_appendSearchDs(ds_listMangiList4, ds_listMangiListTemp4); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			//gfn_setTotalRowCount(ds_listMangiList, sta_totalCnt, nTotalListCnt); // 페이지건수 표출		
			fn_postSearch(svcid);
