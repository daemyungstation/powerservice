                       ,PAY_AMT           /* 청구금액                                                                 */
                       ,REG_DM            /* 등록일                                                                   */
                       ,REG_MAN           /* 등록자                                                                   */
                       ,ICHAE_DT          /* 정기건 및 연체건 이체일                                                  */
                       ,REQ_PAY_GUNSU     /* 산출청구건수                                                             */
                       ,UPD_DM            /* 수정일                                                                   */
                       ,UPD_MAN           /* 수정자                                                                   */
                       ,DEL_FG            /* 청구여부(N:청구전 Y:삭제 R:청구 C:완료(변환/입금) F:완료(실패))          */
