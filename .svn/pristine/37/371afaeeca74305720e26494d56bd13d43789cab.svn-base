            USING DUAL MMDB
                  ON (MMDA.DEL_FG = 'N' AND MMDA.ACCNT_NO = #{accnt_no} AND MMDA.GIFT_CODE = #{gift_code})
            WHEN MATCHED THEN
                UPDATE
                      SET DELIVERY_CORP = #{delivery_corp}
                      ,INVOICE_NUM   = #{invoice_num}
                      ,TX_ETC        = #{tx_etc}
                      ,ORDR_DAY      = TO_DATE(#{ordr_day}, 'YYYYMMDD');
                      ,UPD_MAN       = #{rgsr_id}
                      ,UPD_DM        = SYSDATE
        END;
    </insert>

