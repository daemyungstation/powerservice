               ,SUM(CASE WHEN REQ_BIT = '01'     AND DEL_FG = 'X' AND RESULT_KIND NOT IN ('02','03') THEN PAY_AMT ELSE 0 END) AS X_JUNGI_AMT          /* 정기건 총액 (취소)        */
