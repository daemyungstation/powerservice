                 SELECT MWR.ACCNT_NO          /* 회원번호                                                                 */
                       ,MWR.PROD_CD           /* 상품코드                                                                 */
                       ,MWR.PAY_MTHD          /* 산출구분(CMS:04, CARD:06)                                                */
                       ,MWR.REQ_BIT           /* 산출상세구분(01:정기, 02:미출금, 03:임의 ,04:연체, 05:임시, 06:고객센터) */
                       ,MWR.STAT              /* 상태                                                                     */
