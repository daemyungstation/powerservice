                       ,TRUNC(TO_NUMBER(TO_CHAR(SYSDATE, 'YYYY')) - TO_NUMBER(SUBSTR(MBR.BRTH_MON_DAY, 0,4)) + 1, -1) AS AGE_GROUP                                    
