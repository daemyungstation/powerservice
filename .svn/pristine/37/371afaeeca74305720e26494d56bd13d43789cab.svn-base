            ) TBL INNER JOIN
                TB_MEMBER_BASIC_INFO_DAY MPA ON TBL.ACCNT_NO = MPA.ACCNT_NO INNER JOIN
                MEMBER MB ON MPA.MEM_NO = MB.MEM_NO AND MB.DEL_FG = 'N' INNER JOIN
                PRODUCT PD ON MPA.PROD_CD = PD.PROD_CD LEFT OUTER JOIN
