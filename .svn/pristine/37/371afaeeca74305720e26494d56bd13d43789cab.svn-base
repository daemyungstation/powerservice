                       ,MWR.BANK_FILIN_BRKDN  /* 통장기재내역                                                             */
                       ,MWR.PAY_AMT           /* 청구금액                                                                 */
                       ,MWR.REG_DM            /* 등록일                                                                   */
                       ,MWR.REG_MAN           /* 등록자                                                                   */
                       ,MWR.ICHAE_DT          /* 정기건 및 연체건 이체일                                                  */
                       ,MWR.REQ_PAY_GUNSU     /* 산출청구건수                                                             */
                       ,MWR.UPD_DM            /* 수정일                                                                   */
