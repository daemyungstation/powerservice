function Div00_cbo_searchDiv_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(e.postvalue =="USER"){
		Tab00.tabindex = 1;
		fn_changeSearchDiv(1);
	}else if(e.postvalue == "ORGZ"){
		Tab00.tabindex = 0;
		fn_changeSearchDiv(0);
	}
}

function Tab00_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{
	if(e.postindex == 0){
		Div00.cbo_searchDiv.index = 0;
		ds_input.setColumn( 0,"searchDiv", Div00.cbo_searchDiv.value);	
		fn_changeSearchDiv(0);
	}else if(e.postindex == 1){
		Div00.cbo_searchDiv.index = 1;
		ds_input.setColumn( 0,"searchDiv", Div00.cbo_searchDiv.value);	
		fn_changeSearchDiv(1);
	}
}

function fn_changeSearchDiv(idx){

	if(idx == 1){
		Div00.stc_01.text = "사용자명"
		Div00.stc_02.text = "사원번호"
		Div00.cbo_orgzTypDivCd.visible = false;
		Div00.Button00.visible = true;
		Div00.stc_02.visible = true;
		Div00.Edit01.visible = true;
		Div00.Edit00.visible = true;
		ds_input.clearData();
		ds_input.addRow();
		this.Div00.cbo_orgzTypDivCd.index = 0;
		ds_input.setColumn( 0,"orgzTypDivCd", Div00.cbo_orgzTypDivCd.value);	
		this.Div00.cbo_searchDiv.index = 1;
		ds_input.setColumn( 0,"searchDiv", Div00.cbo_searchDiv.value);	
	}else if(idx == 0){
		Div00.stc_01.text = "조직구분"
		//Div00.stc_02.text = "부서명"
		Div00.stc_02.visible = false;
		Div00.cbo_orgzTypDivCd.visible = true;
		Div00.Button00.visible = false;
		Div00.Edit01.visible = false;
		Div00.Edit00.visible = false;
		ds_input.clearData();
		ds_input.addRow();
	 	this.Div00.cbo_orgzTypDivCd.index = 0;
		ds_input.setColumn( 0,"orgzTypDivCd", Div00.cbo_orgzTypDivCd.value);	
		this.Div00.cbo_searchDiv.index = 0;
		ds_input.setColumn( 0,"searchDiv", Div00.cbo_searchDiv.value);	
		//Div00.Edit00.enable = true;
	}
}

