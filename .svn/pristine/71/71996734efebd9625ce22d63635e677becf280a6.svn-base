function Tab00_tabpage2_div_body00_btn_addOrgz_onclick(obj:Button,  e:ClickEventInfo)
{

	if (gfn_isNull(ds_orgz_rltn_list.getColumn(0, "orgzCd")) ) {
		alert("먼저 등록할 조직을 선택하세요.");
		ds_orgz_rltn_list.clearData();
		return;
	}
	/*
	if (gfn_isNull(ds_admn_rltn_list.getColumn(0, "menuCd")) ) {
		alert("선택된 메뉴엔 프로그램이 없습니다.\n다시 확인하시기 바랍니다.");
		ds_role_orgz_rltn_list.clearData();
		return;
	}
	*/
	var orgzCd = ds_orgz_rltn_list.getColumn(0, "orgzCd");
	var userId = ds_orgz_rltn_list.getColumn(0, "userId");
	var fRow = ds_userlist.findRowExpr("userId=='"+userId+"' && orgzCd=='"+orgzCd+"'");
	if (fRow == -1) {
		gfn_addCommCol(ds_list);
		var nRow = ds_userlist.addRow();
		
		ds_userlist.setColumn(nRow, gfn_getColNmStatus(), gfn_getRowTypeInsert());
		
		ds_userlist.setColumn(nRow, "orgzCd", orgzCd);
		ds_userlist.setColumn(nRow, "userId", userId);
		ds_userlist.setColumn(nRow, "blngOrgzCd", ds_input.getColumn(0, "blngOrgzCd"));		
		ds_userlist.setColumn(nRow, "userNm", ds_orgz_rltn_list.getColumn(0, "userNm"));
		//ds_userlist.setColumn(nRow, "jbttNm", ds_admn_rltn_list.getColumn(0, "jbttNm"));
		//ds_userlist.setColumn(nRow, "dtrsNm", ds_admn_rltn_list.getColumn(0, "dtrsNm"));
		ds_userlist.setColumn(nRow, "blngorgzNm", ds_orgz_rltn_list.getColumn(0, "orgzNm"));
		ds_userlist.setColumn(nRow, "vrtlUserYn", ds_orgz_rltn_list.getColumn(0, "vrtlUserYn"));
		ds_userlist.setColumn(nRow, "edtYn", "Y");
	} else {
		alert("이미 등록된 데이타가 있습니다.\n다시 확인하시기 바랍니다.");
	}
	//ds_role_orgz_rltn_list.clearData();
		
    ds_list.enableevent = true;	
}


function Tab00_tabpage2_div_body00_btn_delOrgz_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_userlist.enableevent = false;
	trace(ds_orgz_rltn_list.saveXML());
	if(ds_admn_rltn_list.rowcount < 1)	{
		alert("삭제할 데이타가 없습니다.");
	} else {
		var orgzCd = ds_orgz_rltn_list.getColumn(0, "orgzCd");
		var userId = ds_orgz_rltn_list.getColumn(0, "userId");
		var edtYn = ds_orgz_rltn_list.getColumn(0, "edtYn");
		var vrtlUserYn = ds_orgz_rltn_list.getColumn(0, "vrtlUserYn");
		//trace(orgzCd +  " : " + roleId + " : " + edtYn);
		if(edtYn == "N"){
			alert("삭제할 수 없는 사용자 입니다.");
			ds_userlistenableevent = true;
			return;
		}else if(vrtlUserYn == "N"){
			alert("가상사용자가 아닙니다. 사용자관리에서 처리하십시요. ");
			ds_userlist.enableevent = true;
			return;
		}else{
			var fRow = ds_userlist.findRowExpr("userId=='"+userId+"' && orgzCd=='"+orgzCd+"' && edtYn=='"+edtYn+"'");
			if (fRow != -1) {		
				var nRow = ds_orgz_rltn_list.getColumn(0, "selRow");
				if(ds_userlist.getColumn(nRow, gfn_getColNmStatus()) == gfn_getRowTypeInsert()) {
					ds_userlist.deleteRow(nRow);
				} else {	
					/*
					for(var j=0;j<ds_role_list.getColCount();j++)
					{
						ds_list.setColumn(nRow, ds_list.getColID(j), ds_list.getOrgColumn(nRow, ds_list.getColID(j)));
					}
					*/
					ds_userlist.setColumn(nRow, gfn_getColNmStatus(), gfn_getRowTypeDelete());
				}
			} else {
				alert("삭제할 데이타를 선택하시기 바랍니다.");
			}
		}
	}
	ds_orgz_rltn_list.clearData();
	
	ds_userlist.enableevent = true;
}


