 ***************************************************************************/
function div_body_grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.col == 0) {
		gfn_rollbackRowDs(this[obj.binddataset], obj, e);
	}else if(e.col == 4 && ds_list.getColumn(e.row,"vrtlUserYn") == "N"){
	
	//&& obj.getCellProperty("Body",(e.row * 7) + e.col,"edit") == "button"
		trace("change orgz");
		var strParam = {pv_info:""};
		//전문상세 조회 팝업
		var rtn = gfn_diaLog("searchOrgz", -1, -1, 989, 519, "SYS::SYS0000202302.xfdl", false, strParam,  false, false, true);
		//추가 입력된 내용 재 조회
		if(rtn instanceof Object) {
			trace("orgzCd:"+rtn["orgzCd"]);
			if(!gfn_isNull(rtn["orgzCd"])  && !gfn_isNull(rtn["orgzNm"])){
				ds_list.setColumn(e.row,"blngOrgzCd",rtn["orgzCd"]);
				ds_list.setColumn(e.row,"blngorgzNm",rtn["orgzNm"]);
			}
		}
	}else {
		ds_admn_rltn_list.clearData();
		ds_admn_rltn_list.addRow();
		ds_admn_rltn_list.setColumn(0, "orgzCd", ds_input_user.getColumn(0, "orgzCd"));
		ds_admn_rltn_list.setColumn(0, "blngOrgzCd", ds_list.getColumn(e.row, "blngOrgzCd"));
		ds_admn_rltn_list.setColumn(0, "userId", ds_list.getColumn(e.row, "userId"));
		ds_admn_rltn_list.setColumn(0, "userNm", ds_list.getColumn(e.row, "userNm"));
		ds_admn_rltn_list.setColumn(0, "jbttNm", ds_list.getColumn(e.row, "jbttNm"));
		ds_admn_rltn_list.setColumn(0, "dtrsNm",  ds_list.getColumn(e.row, "dtrsNm"));
		ds_admn_rltn_list.setColumn(0, "edtYn",   ds_list.getColumn(e.row, "edtYn"));
		ds_admn_rltn_list.setColumn(0, "vrtlUserYn",  ds_list.getColumn(e.row, "vrtlUserYn"));
		ds_admn_rltn_list.setColumn(0, "selRow", e.row);
		//trace(ds_admn_rltn_list.saveXML());
	}
}

