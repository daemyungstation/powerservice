//버튼 클릭이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
		case btn_search: //조회버튼
		fn_commSearch(gfn_getCommTypeInit());
		break;
		
		case btn_init: //초기화버튼
		fn_init();
		break;
		
		case btn_create: // 신규버튼 클릭시
		ds_extcCndt.clearData();
		ds_extcCndt.addRow();
		
		fn_changeComponent("C");//컴포넌트 상태변경
		fn_changeTable("1000");
		break;
		
		case btn_update : //수정버튼 클릭시
		fn_changeComponent("U");//컴포넌트 상태변경
		break;
		
		case btn_delete : // 삭제버튼 클릭시
		if(confirm("선택된 내용을 삭제하시겠습니까?")) {
			fn_deleteData();//트랜잭션 호출
		}
		break;
		
		case btn_save : //저장버튼 클릭시
		fn_saveData();
		break;
		
		case btn_cancel : //취소버튼클릭시
		fn_copyRow(ds_list_extcCndt.rowcount);
		fn_changeTable(ds_list_extcCndt.getColumn(ds_list_extcCndt.rowposition,'extc_cndt_typ_cd1'));
		fn_changeComponent("R");//컴포넌트 상태변경
		if(ds_list_extcCndt.getColumn(ds_list_extcCndt.rowposition,"extc_cndt_typ_cd1")== 5000)fn_commSearch_prodList();
		break;
		
		case btn_add : //추가버튼클릭시
		var sPrmr = {ds_copyData:ds_list_prod};	
		gfn_diaLog("상품 선택", -1, -1, 498, 538, "PS220000::PS220502.xfdl", false, sPrmr, true, false, true);
		break;
	}
}


//콤보박스 변경시 후속 입벤트 
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(obj){
		case cbo_srch_dateSelect: //검색조건 수정일자 콤보변환시
		gfn_dateSrchDefaultChangeEvnt(cbo_srch_dateSelect,cal_srch_sttDt,cal_srch_endDt);
		break;
		
		case cbo_hrchCode1 : // 캠페인유형 LV1 콤보 변환시
		gfn_getHrchCodeLoad(cbo_hrchCode1.value,"",ds_hrchCode2,null);
		cbo_hrchCode2.index = 0;
		fn_changeTable(cbo_hrchCode1.value);
		break;
		
		case cbo_dateSelect : //가입일자 기간콤보변경시
		gfn_dateSrchDefaultChangeEvnt(cbo_dateSelect,cal_join_dt_sttDt,cal_join_dt_endDt);
		if(cbo_dateSelect.value == 0){
			cal_join_dt_sttDt.enable = true;
			cal_join_dt_endDt.enable = true;
		}else{
			cal_join_dt_sttDt.enable = false;
			cal_join_dt_endDt.enable = false;
		}
		
		break;
	}
}

//데이터셋 변경시 일어나는 이벤트
function ds_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	switch(obj){
		case ds_list_extcCndt :
		fn_copyRow();
		gfn_getHrchCodeLoad(cbo_hrchCode1.value,"",ds_hrchCode2,null);
		fn_changeTable(ds_list_extcCndt.getColumn(ds_list_extcCndt.rowposition,'extc_cndt_typ_cd1'));
		fn_changeComponent("R");//컴포넌트 상태변경
		if(ds_list_extcCndt.getColumn(ds_list_extcCndt.rowposition,"extc_cndt_typ_cd1")== 5000)fn_commSearch_prodList();
		break;
	}
}


//그리드안 버튼 클릭
function grd__oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(obj == grd_list_prod){
		if(grd_list_prod.formatid == 1){
			if(e.cell == 3){
				if(confirm("선택한 행을 삭제 하시겠습니까?")){
					ds_list_prod.deleteRow(ds_list_prod.rowposition); 
				}
			}
		}
	}
}

//정렬
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (!fn_preSearch()) return false;
	ds_inqCndt.setColumn(0, "sController", "/campaign/extc-cndt/list");
	ds_inqCndt.setColumn(0, "dsName", "ds_output");
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_extcCndt, sta_totalCnt, 30, total_count)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	
	gfn_setTotalRowCount(ds_list_extcCndt, sta_totalCnt, total_count); // 페이지건수 표출
}
