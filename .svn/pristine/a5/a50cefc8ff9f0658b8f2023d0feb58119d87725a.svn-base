// 조회결과
function fn_postSearch()
{
	if (ds_list_extcCndt.rowcount < 1) {
	
		fn_copyRow(ds_list_extcCndt.rowcount);
		fn_changeTable(ds_list_extcCndt.getColumn(ds_list_extcCndt.rowposition,'extc_cndt_typ_cd1'));
		fn_changeComponent("R");
		sta_totalCnt.text ="조회 결과가 없습니다.";
		
	} else {
		fn_copyRow();
		gfn_getHrchCodeLoad(cbo_hrchCode1.value,"",ds_hrchCode2,null);
		fn_changeTable(ds_list_extcCndt.getColumn(ds_list_extcCndt.rowposition,'extc_cndt_typ_cd1'));
		fn_changeComponent("R");
		
		if(ds_list_extcCndt.getColumn(ds_list_extcCndt.rowposition,"extc_cndt_typ_cd1")== 5000)fn_commSearch_prodList();
		
	}

}

//리스트 데이터셋 뷰 데이터셋에 카피
function fn_copyRow(nCnt){

	if(nCnt<1){
		ds_extcCndt.clearData();
		ds_extcCndt.addRow();
	}else{
		ds_extcCndt.clearData();
		ds_extcCndt.addRow();
		ds_extcCndt.copyRow(ds_extcCndt.rowposition,ds_list_extcCndt,ds_list_extcCndt.rowposition);
		
		//등록 수정일시 데이터타입에 맞게
		var rgsnDttmStr = edt_rgsn_dttm.value;
		edt_rgsn_dttm.value = gfn_strDttm(rgsnDttmStr);
			
		var amntDttmStr = edt_amnt_dttm.value;
		edt_amnt_dttm.value = gfn_strDttm(amntDttmStr);
	}
}

