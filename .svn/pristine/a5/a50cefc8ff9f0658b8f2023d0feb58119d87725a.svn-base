/********************************************************************************************/
/* 1. Include Library include                                                               */
/********************************************************************************************/
include "LIB::lib_All.xjs";

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트,전역변수,초기화) 처리                         	*/
/********************************************************************************************/
var total_count = 0;

//온로드
function PS220500_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_getList("hrchCd","CAM990","fn_callBack",null,"srchHrchCdSet");   // 코드셋
	fn_init();
	fn_commSearch(gfn_getCommTypeInit());
}

// 초기화
function fn_init()
{
	edt_srch_extc_cndt_nm.value = null
	edt_srch_amnd_nm.value = null;
	cbo_srch_dateSelect.value = 3;
	cal_srch_sttDt.value = gfn_addDate(gfn_getToday(),-7);
	cal_srch_endDt.value = gfn_getToday();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
//콜백

function fn_callBack(svcid, errcd, errmsg) {
	if(errcd != 0){
		trace(errmsg + svcid);
	}else{
		switch(svcid){
			case "srchExtcCndt": //조회콜백
			gfn_appendSearchDs(ds_list_extcCndt, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_extcCndt, sta_totalCnt, total_count); // 페이지건수 표출
			fn_postSearch();	
			break;
			
			case "srchHrchCdSet": //계층유형 코드셋
			gfn_getHrchCodeLoad("","",ds_hrchCode1,null,1);
			//gfn_getCode("CAM030", "", ds_extcCndtTypCd); 
			break;
			
			case "deleteExtcCndt" : //삭제콜백
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "saveExtcCndt" : //저장콜백
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "srchProdList" : //조회 콜백
			
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_list_extcCndt.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "extc_cndt_nm", edt_srch_extc_cndt_nm.value);
		ds_inqCndt.setColumn(0, "startd", cal_srch_sttDt.value);
		ds_inqCndt.setColumn(0, "endd", cal_srch_endDt.value);
		ds_inqCndt.setColumn(0, "amnd_nm", edt_srch_amnd_nm.value);
		ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	return true;
}

function fn_preSaveData()
{
	if(gfn_isNull(cbo_hrchCode2.value)){
		alert("캠페인 유형을 선택하세요.");
		return false;
	}
	
	if(gfn_isNull(edt_extc_cndt_nm.value)){
		alert("추출조건명을 입력하세요.");
		return false;
	}
	
	if(cbo_hrchCode1.value == "5000"){
		if(gfn_isNull(cal_join_dt_sttDt.value)){
			alert("가입일자를 확인하세요.");
			return false;
		}
		
		if(gfn_isNull(cal_join_dt_endDt.value)){
			alert("가입일자를 확인하세요.");
			return false;
		}
	}
	return true;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType)
{	
    if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_extcCndt, sta_totalCnt, 30, total_count)) return; 
	
	var szSvcid       = "srchExtcCndt";
    var szController  = "/campaign/extc-cndt/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function fn_commSearch_prodList()
{	
	var szSvcid       = "srchProdList";
    var szController  = "/campaign/extc-cndt-item/prod-list";
    var szInDs        = "ds_input=ds_extcCndt";
    var szOutDs       = "ds_list_prod=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}



//삭제
function fn_deleteData(){
	var szSvcid       = "deleteExtcCndt";
    var szController  = "/campaign/extc-cndt/delete";
    var szInDs        = "ds_input=ds_extcCndt";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//저장
function fn_saveData(){
	if(!fn_preSaveData()) return false;
	
	var szSvcid       = "saveExtcCndt";
    var szController  = "/campaign/extc-cndt/save";
    if(cbo_hrchCode1.value == "5000"){
		var szInDs        = "ds_input=ds_extcCndt ds_input2=ds_list_prod";
    }else{
		var szInDs        = "ds_input=ds_extcCndt";
    }
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}



