    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var total_count = 0; // 전체건수
var team_cd = "";

var bRgsn = false;
var bAmnt = false;
var bDltn = false;
var bXls = false;

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS250100_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_getList("cd","EDC010,EDC020");   //코드조회
	gfn_athrPgm(gfn_gdsCd_admin()); //권한조회
	bRgsn = "Y" == ds_athrPgmFilter.getColumn(0, 'rgsn_yn');
	bAmnt = "Y" == ds_athrPgmFilter.getColumn(0, 'amnt_yn');
	bDltn = "Y" == ds_athrPgmFilter.getColumn(0, 'dltn_yn');
	bXls = "Y" == ds_athrPgmFilter.getColumn(0, 'xls_yn');
	

	fn_init();
	fn_commSearch(gfn_getCommTypeInit());
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
		case "selectCode":
			gfn_getCode("EDC010", "SELECT", ds_statusTypCd); //교육진행상태
			gfn_getCode("EDC020", "ALL", ds_edctTypCd); //교육유형
			gfn_getCode("EDC020", "SELECT", ds_edctTypCd_dtl); //교육유형2
			cbo_srch_typ_cd.index = 0;
			break;
		
		case "srchEdctCrs":
			gfn_appendSearchDs(ds_list_edctCrs, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_edctCrs, sta_totalCnt, total_count); // 페이지건수 표출
			
			// 모드 셋팅
			fn_setMode("R");
			break;
		
		case "srchEdctTrpr":
			gfn_appendSearchDs(ds_list_edctTrpr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			break;
		
		case "saveEdcuation":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "deleteEdctCrs":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "copyEdctCrs":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		
		case "deleteEdctTrpr":
			fn_commSearch(gfn_getCommTypeInit());
			break;
 		}	
 	}		
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit()) {
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "edct_crs_nm", edt_edct_crs_nm.value);
		ds_inqCndt.setColumn(0, "startd", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "endd", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "edct_typ_cd", cbo_srch_typ_cd.value);
	}
	
	return true;
}

// 조회 체크 - 하단
function fn_preSearch_sub(sType)
{
	if (sType == gfn_getCommTypeInit()) {
		ds_inqCndt_sub.clearData();
		ds_inqCndt_sub.addRow();
		var sEdctCrsId = ds_list_edctCrs.getColumn(ds_list_edctCrs.rowposition, "edct_crs_id");
		ds_inqCndt_sub.setColumn(0, "edct_crs_id", sEdctCrsId);
	}
	
	return true;
}

// 저장 체크
function fn_preSave(sType:String) {
	switch(sType) {
	case "state" :
		var sEdctPrgrStatCd = ds_edctCrs.getColumn(ds_edctCrs.rowposition, "edct_prgr_stat_cd");
		var nRow = ds_statusTypCd.findRow("cd", sEdctPrgrStatCd) + 1;
		
		ds_edctCrs.setColumn(ds_edctCrs.rowposition, "edct_prgr_stat_cd", ds_statusTypCd.getColumn(nRow, "cd"));
		break;
		
	case "save" :
		if (gfn_isNull(cbo_srch_typ_cd2.value)) {
			alert("교육유형을 선택하세요");
			return false;
		}
		if (gfn_isNull(cbo_srch_sratus_typ_cd.value)) {
			alert("진행상태를 선택하세요");
			return false;
		}
		if (gfn_isNull(edt_edct_crs_nm2.value)) {
			alert("교육명을 입력하세요");
			return false;
		}
		if (gfn_isNull(msk_edct_stt_dt.value) || gfn_isNull(msk_edct_end_dt.value)) {
			alert("교육기간을 선택하세요");
			return false;
		}
		if (gfn_isNull(cbo_edct_atnc_hr.value)) {
			alert("이수시간을 선택하세요");
			return false;
		}
		if (gfn_isNull(edt_edct_lctr_nm.value)) {
			alert("강사명을 입력하세요");
			return false;
		}
		if (gfn_isNull(edt_edct_scr.value)) {
			alert("할당점수를 입력하세요");
			return false;
		}
		if (gfn_isNull(edt_edct_plc_nm.value)) {
			alert("교육장소를 입력하세요");
			return false;
		}
		if (gfn_isNull(edt_edct_trgt_nm.value)) {
			alert("교육대상를 입력하세요");
			return false;
		}
		break;
	}

	return true;
}

// 삭제 체크
function fn_preDelete() {
	var nDeleteCnt = ds_list_edctCrs.getCaseCount("rowCheck=='1'");            
	if (nDeleteCnt < 1) {
		alert("삭제할 교육평가를 선택하세요.");
		return false;
	}	
	
	if(!confirm("교육평가를 삭제하시겠습니까?")) {
		return false;
	}
	
	var sEdctCrsId = "";
	for (var i=0; i<ds_list_edctCrs.rowcount; i++) {
		if(ds_list_edctCrs.getColumn(i, "rowCheck") == "1" && ds_list_edctCrs.getColumn(i, "edct_prgr_stat_cd") == "10"){
			sEdctCrsId += ds_list_edctCrs.getColumn(i, "edct_crs_id")+",";
		}
	}
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "selectcheck", sEdctCrsId);
	
	return true;
}

// 삭제 체크2
function fn_preDelete2() {
	var nDeleteCnt = ds_list_edctTrpr.getCaseCount("rowCheck=='1'");            
	if (nDeleteCnt < 1) {
		alert("제외할 교육대상자를 선택하세요.");
		return false;
	}	
	
	if(!confirm("교육대상자를 제외하시겠습니까?")) {
		return false;
	}
	
	var sEdctTrprId = "";
	for (var i=0; i<ds_list_edctTrpr.rowcount; i++) {
		if(ds_list_edctTrpr.getColumn(i, "rowCheck") == "1"){
			sEdctTrprId += ds_list_edctTrpr.getColumn(i, "edct_trpr_id")+",";
		}
	}
	
	var sEdctCrsId = ds_edctCrs.getColumn(ds_edctCrs.rowposition, "edct_crs_id");
	
	ds_inqCndt_sub.clearData();
	ds_inqCndt_sub.addRow();
	ds_inqCndt_sub.setColumn(0, "edct_crs_id", sEdctCrsId);
	ds_inqCndt_sub.setColumn(0, "selectcheck", sEdctTrprId);
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_edctCrs, sta_totalCnt, 50, total_count)) return;
	
	var szSvcid       = "srchEdctCrs";
    var szController  = "/edct-crs/edct-crs/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function fn_commSearch_sub(sType)
{
	// 조회조건 및 default 정렬
	ds_list_edctTrpr.clearData();
	if (!fn_preSearch_sub(sType)) return false;
		
	var szSvcid       = "srchEdctTrpr";
	var szController  = "/edct-trpr/edct-trpr/list";
	var szInDs        = "ds_input=ds_inqCndt_sub";
	var szOutDs       = "ds_list_tmp=ds_output";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}




//교육과정 상세 저장
function fn_saveData(sType)
{
	if (!fn_preSave(sType)) return false;
	
	var szSvcid       = "saveEdcuation";
	var szController  = "/edct-crs/edct-crs/save";
	var szInDs        = "ds_input=ds_edctCrs";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//상단 그리드 삭제
function fn_delete()
{
	if (!fn_preDelete()) return false;
	
	var szSvcid       = "deleteEdctCrs";
	var szController  = "/edct-crs/edct-crs/delete";
	var szInDs        = "ds_input=ds_inqCndt";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//하단 그리드 삭제
function fn_delete2(){	
	if (!fn_preDelete2()) return false;
	
	var szSvcid       = "deleteEdctTrpr";
	var szController  = "/edct-trpr/edct-trpr/delete";
	var szInDs        = "ds_input=ds_inqCndt_sub";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//복사
function fn_copy()
{
	if(!confirm("복사하시겠습니까?")) {
		return false;
	}
	
	var szSvcid       = "copyEdctCrs";
	var szController  = "/edct-crs/edct-crs/copy";
