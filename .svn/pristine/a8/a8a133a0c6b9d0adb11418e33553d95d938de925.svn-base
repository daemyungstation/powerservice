/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	cbo_srch_dateTyp.index = 4;
	cbo_srch_typ_cd.index = 0;
	edt_edct_crs_nm.value = "";
	fn_changeDateType();
}

// 검색부 콤보 변경
function combo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	fn_changeDateType();
}

// 날짜 선택
function fn_changeDateType()
{
	gfn_dateSrchDefaultChangeEvnt(cbo_srch_dateTyp, cal_srch_stt_dt, cal_srch_end_dt);
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
//그리드 셀 클릭 시 이벤트
function Grid_Main_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	fn_setMode("R");
}

// 그리드 헤더 클릭
function Grid_Main_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (!fn_preSearch()) return false;
	
	ds_inqCndt.setColumn(0, "sController", "/edct-crs/edct-crs/list");
	ds_inqCndt.setColumn(0, "dsName", "ds_output");
	ds_inqCndt.setColumn(0, "szCallback", "fn_setMode");
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_edctCrs, sta_totalCnt, 50, total_count)) return;
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_edctCrs, sta_totalCnt, total_count); // 페이지건수 표출
}

function Grid_Sub_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	gfn_grdHeadClick(obj, e);
}

//그리드 오버스크롤
function Grid_Main_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}


// edit onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		fn_commSearch(gfn_getCommTypeInit());
	}
}



//버튼 및 컨포넌트들 제어
function fn_setMode(mode:String) {
	mode = gfn_isNull(mode) ? "R" : mode;
	switch(mode) {
	case "C":
		ds_edctCrs.clearData();
		ds_edctCrs.addRow();
		cbo_srch_typ_cd2.index = 0;
		cbo_srch_sratus_typ_cd.index = 1;
		cbo_edct_atnc_hr.index = 0;
		
		ds_list_edctTrpr.clearData();
		break;
		
	case "R":
		ds_edctCrs.clearData();
		ds_edctCrs.addRow();
		
		if (ds_list_edctCrs.rowposition > -1) {
			ds_edctCrs.copyRow(0, ds_list_edctCrs, ds_list_edctCrs.rowposition);
			fn_commSearch_sub(gfn_getCommTypeInit());
		}
		break;
	case "U":
		break;
	}
	fn_setCompenent(mode);
}

function fn_setCompenent(mode:String)
{
	var bC = mode == "C";
	var bU = mode == "U";
	var bR = mode == "R";
	
	var bPos = ds_list_edctCrs.rowposition >= 0;
	var sEdctPrgrStatCd = ds_edctCrs.getColumn(ds_edctCrs.rowposition, "edct_prgr_stat_cd");
	var bMode = sEdctPrgrStatCd == "10";

	// 컴포넌트 컨트롤
	cbo_srch_typ_cd2.readonly = bR;			// 교육유형
	cbo_srch_sratus_typ_cd.readonly = true;	// 진행상태
	edt_edct_crs_nm2.readonly = bR;			// 교육명
	msk_edct_stt_dt.readonly = bR;			// 교육시작기간
	msk_edct_end_dt.readonly = bR;			// 교육종료기간
	cbo_edct_atnc_hr.readonly = bR;			// 이수시간
	edt_edct_lctr_nm.readonly = bR;			// 강사명
	edt_edct_scr.readonly = bR;				// 할당점수
	edt_edct_plc_nm.readonly = bR;			// 교육장소
	edt_edct_trgt_nm.readonly = bR;			// 교육대상
	TextArea_edct_cntn.readonly = bR;		// 설명

	// 버튼 컨트롤
	btn_delete.visible = bDltn & bR;
	btn_excel.visible = bXls & bR;
	
	btn_save.visible = !bR;
	btn_cancel.visible = !bR;
	
	btn_new.visible = bRgsn && bR;
	btn_update.visible = bMode && bPos && bAmnt && bR;
	btn_copy.visible = bPos && bRgsn && bR;
	
	
	btn_reg.visible = bMode && bRgsn && bR;
	btn_delete2.visible = bMode && bDltn && bR;

	// 교육상태버튼 제어
	if (bR) {
		var nRow = ds_statusTypCd.findRow("cd", sEdctPrgrStatCd) + 1;
		if (ds_statusTypCd.rowcount > nRow) {
			btn_state.text = ds_statusTypCd.getColumn(nRow, "cd_nm")
		} else {
			btn_state.text = "교육평가";
		}
		btn_state.visible = bAmnt;
	} else {
		btn_state.visible = false;
	}
	

}
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj) {
	// 교육과정 조회
	case btn_Search :
		fn_commSearch(gfn_getCommTypeInit());
		break;
		
	// 검색조건 초기화
	case btn_Init :
		fn_init();
		break;
		
	// 엑셀다운로드
	case btn_excel :
		gfn_exportGridToExcel(Grid_Main, "교육평과관리_"+gfn_getTodayFull()+"");
		break;
	
	// 교육과정 삭제
	case btn_delete :
		fn_delete();
		break;
	
	// 교육과정 신규등록
	case btn_new :
		fn_setMode("C");
		break;
	
	// 교육과정 수정
	case btn_update :
		fn_setMode("U");
		break;
	
	
	// 교육과정 복사
	case btn_copy :
		fn_copy();
		break;
	
	// 교육과정 저장
	case btn_save :
		fn_saveData("save");
		break;
		
	// 교육과정 취소
	case btn_cancel :
		fn_setMode("R");
		break;
	
	// 교육대상자 등록
	case btn_reg :
		var sPrmr = {};
		var reVal = gfn_diaLog("대상자 지정", -1, -1, 884, 601, "PS250000::PS250102.xfdl", false, sPrmr, false, false, true);
		break;
		
	// 교육대상자 삭제
	case btn_delete2 :
		fn_delete2();
		break;
		
	// 교육과정 상태 수정
	case btn_state :
		switch (ds_edctCrs.getColumn(ds_edctCrs.rowposition, "edct_prgr_stat_cd")) {
		case "10" :
			if (ds_list_edctTrpr.rowcount == 0){
				alert("교육대상자를 등록하세요");
				return false;
			}

			if (ds_list_edctTrpr.getCaseCount("edut=='미지정'")>0) {
				alert("교육대상자 목록의 교육시간을 확인하세요");
				return false;
			} 
			
			if (!confirm("교육평가를 진행하시겠습니까?")) {
				return false;
			}

			fn_saveData("state");
			break;
			
		case "20" :
			if (confirm("교육평가를 완료하시겠습니까?")) {
				fn_saveData("state");
			}

			break;
		case "30" :		
			var sPrmr = {};
			var reVal = gfn_diaLog("교육과정 평가", -1, -1, 884, 601, "PS250000::PS250101.xfdl", false, sPrmr, false, false, true);
			break;
		}
	}
}]]></Script>
