    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";
include "LIB::lib_ConsTyp.xjs"; //상담유형

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS100500_onload(obj:Form, e:LoadEventInfo)
{
	fn_init();
	gfn_createTeamUserInqDs(obj); // 팀에 따른 사용자 조회를 위한 데이터셋 설정
	gfn_getList("cd", "USR030");  // 공통코드조회
	gfn_getList("consTyp","","fn_callBack",null,"consTyp1");    // 상담유형대분류코드조회
	
	//(윌앤비전:T20100, 모든이에:T20200, 비투퀄리티:T20300)
	var sTeamCd = gds_UserInfo.getColumn(0, "bzpt_div");
	//var sTeamCd = "T20100";
	if(sTeamCd.substr(0,2)=="T2"){
		ds_hgrnTeamCd.addRow();
		ds_hgrnTeamCd.setColumn(0, "team_cd", "T20000");
		ds_hgrnTeamCd.setColumn(0, "team_nm", "외주TM업체");
		cbo_srch_hgrn_team_cd.index = 0;
		cbo_srch_hgrn_team_cd.enable = false;
		
		if(sTeamCd == "T20100"){
			sTeamNm = "윌앤비전";
		}else if(sTeamCd == "T20200"){
			sTeamNm = "모든이에";
		}else{
			sTeamNm = "비투퀄리티";
		}
		ds_teamCd.addRow();
		ds_teamCd.setColumn(0, "team_cd", sTeamCd);
		ds_teamCd.setColumn(0, "team_nm", sTeamNm);
		cbo_srch_team_cd.index = 0;
		cbo_srch_team_cd.enable = false;

		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_teamCd, "ALL");
		
	}else{
		gfn_getList("teamCd","", null, null, null, "ds_allTeamCd"); // 팀코드 조회
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "consTyp1":
				gfn_getConsTypLoad("", "ALL", ds_cons_typ);//상담대분류
				cbo_cons_typ_cd1.index = 0; // 초기화
			break;
			
			case "consTyp2":
				gfn_getConsTypLoad(cbo_cons_typ_cd1.value, "ALL", ds_cons_typ2);
			    cbo_cons_typ_cd2.index = 0; // 초기화
			break;
			
			case "consTyp3":
				gfn_getConsTypLoad(cbo_cons_typ_cd2.value, "ALL", ds_cons_typ3);
			    cbo_cons_typ_cd3.index = 0; // 초기화
			break;
						
			case "selectTeamCode":
				gfn_getTeamCodeLoad("", "ALL", ds_hgrnTeamCd, ds_allTeamCd, "1");
				var sTeamCd = gds_UserInfo.getColumn(0, "team_cd");
				sTeamCd = sTeamCd.substr(0,2)+"0000";
				cbo_srch_hgrn_team_cd.value = sTeamCd;
				cbo_onitemchanged(cbo_srch_hgrn_team_cd);
			break;
			
			case "selectUserCode":
				gfn_getUserCdLoad("", "" ,"ALL", ds_userId);
		
				cbo_srch_user_id.index = 0;
				
				if (ds_userId.rowcount > 1) {
					cbo_srch_user_id.enable = true;
				} else {
					cbo_srch_user_id.enable = false;
				}
			break;
		}
	}
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/

//초기화
function fn_init()
{
	//(윌앤비전:T20100, 모든이에:T20200, 비투퀄리티:T20300)
	var sTeamCd = gds_UserInfo.getColumn(0, "bzpt_div");
	//var sTeamCd = "T20100";
	if(sTeamCd.substr(0,2)=="T2"){
		cbo_srch_user_id.index = 0;
	}else{
		gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id);
	}
	
	cbo_cons_typ_cd1.index = 0;
	cbo_cons_typ_cd2.index = 0;
	cbo_cons_typ_cd3.index = 0;
	cbo_cons_typ_cd2.enable = false;
	cbo_cons_typ_cd3.enable = false;
	cbo_srch_dateTyp.index = 1;
	cbo_searchType.index = 0;
	fn_changeDateType();
}

// 검색부 콤보 변경tab1
function combo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	fn_changeDateType();
}

// 날짜 선택
function fn_changeDateType()
{
	gfn_dateSrchDefaultChangeEvnt(cbo_srch_dateTyp, cal_srch_stt_dt, cal_srch_end_dt);
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
//대분류 선택시 이벤트
function cbo_cons_typ_cd1_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	ConsTypSelect(cbo_cons_typ_cd1, cbo_cons_typ_cd2, cbo_cons_typ_cd3,"1");
}

//중분류 선택시 이벤트
function cbo_cons_typ_cd2_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	ConsTypSelect(cbo_cons_typ_cd1, cbo_cons_typ_cd2, cbo_cons_typ_cd3,"2");
}

// 콤보 값 변경
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{	
	if (obj.name == "cbo_srch_hgrn_team_cd") {
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "1", ds_allTeamCd, ds_teamCd, "ALL");
	} else if (obj.name == "cbo_srch_team_cd") {
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_teamCd, "ALL");
	} 
}

//조회버튼
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	var sFormId = "PS100500";
	var sCntrCd = gds_UserInfo.getColumn(0, "cntr_cd");
	var sStartd = cal_srch_stt_dt.value;
	var sEndd = cal_srch_end_dt.value;
	var sHgrnTeamCd = cbo_srch_hgrn_team_cd.value;
	var sTeamCd = cbo_srch_team_cd.value;
	var sUserId = cbo_srch_user_id.value;
	var sSearchType = cbo_searchType.value;
	var sConsTyp1Cd = cbo_cons_typ_cd1.value;
	var sConsTyp2Cd = cbo_cons_typ_cd2.value;
	var sConsTyp3Cd = cbo_cons_typ_cd3.value;
	
	var sPrmr = {form_id:sFormId
				,cntr_cd:sCntrCd
				,startd:sStartd
				,endd:sEndd
				,hgrn_team_cd:sHgrnTeamCd
				,team_cd:sTeamCd
				,user_id:sUserId
				,searchType:sSearchType
				,cons_typ1_cd:sConsTyp1Cd
				,cons_typ2_cd:sConsTyp2Cd
				,cons_typ3_cd:sConsTyp3Cd};

	var reVal = gfn_diaLog("OZ레포팅팝업", -1, -1, 884, 601, "PS990000::PS994500.xfdl", false, sPrmr, false, false, true);
}

//초기화버튼
function btn_Init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}]]></Script>
