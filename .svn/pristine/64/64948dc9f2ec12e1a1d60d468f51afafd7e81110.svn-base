// 부분입고. 부분입고취소
function fn_fInWhProc(mode) {
	var i,
		nPos,
		in_wh_qty,
		qty,
		in_wh_dt,
		proc_stat,
		tmpMode,
		sPrmr,
		ord_no;
		
	tmpMode = mode;
	proc_stat = ds_list.getColumn(ds_list.rowposition, "proc_stat");	
	ord_no = ds_list.getColumn(ds_list.rowposition, "ord_no");	
	var chkCnt = ds_list2.getCaseCount("chk == '1'");
	
	if (chkCnt < 1) {
		alert("선택하신 정보가 없습니다.\n상세품목을 선택해주십시오.");
		return;
	}
	
	if (chkCnt > 1) {
		alert("한 건만 선택해 주십시오");
		gfn_checkDs(grd_list2, "chk", false);
		return;
	}
	
	if ("dtl" == mode) {
		nPos = ds_list2.findRow("chk", "1");
		in_wh_qty 	= ds_list2.getColumn(nPos, "in_wh_qty");
		qty 		= ds_list2.getColumn(nPos, "qty");
		in_wh_dt 	= ds_list2.getColumn(nPos, "in_wh_dt");
		
		if (in_wh_qty == qty) {
			alert("입고완료 된 품목입니다.");
			return;
		}
		
		if (gfn_isEmpty(in_wh_dt)) {
			if ("0001" == proc_stat) {
				tmpMode = "newDtl";
			}
		} else {
			tmpMode = "updDtl";
		}
		
		gfn_copyCheckData(ds_list2, ds_list_tmp);
		// 상품상세 팝업 - 모달
		sPrmr = {p_ds:ds_list_tmp, p_mode:tmpMode, p_ord_no:ord_no};
		fn_openInWhPopup(sPrmr); // 
		
	} 
	else if ("dtlCncl" == mode) {
		nPos = ds_list2.findRow("chk", "1");
		in_wh_dt = ds_list2.getColumn(nPos, "in_wh_dt");
			
		if ( tmpMode == "newDtl" || gfn_isEmpty(in_wh_dt) ) {
			alert("미입고 된 품목은 입고취소 할 수 없습니다.");
			return;
		} 
		else {
			if (!confirm("부분 입고취소 하시겠습니까?")) {
				return;
			}
			gfn_copyCheckData(ds_list2, ds_list_tmp);
			fn_deleteWhInCncl('dtlCncl');
		}
	}
}

function fn_allChk(isChecked) {
	var i,
		oldRowType,
		chkVal;
	
	if (true == isChecked) {
		chkVal = "1";
	} else {
		chkVal = "";
	}
	ds_list.enableevent 	= false;
	ds_list.updatecontrol 	= false;
	for (i=0; i<ds_list.rowcount; ++i) {
		oldRowType = ds_list.getRowType(i);
		
		if ("0003" == ds_list.getColumn(i, "proc_stat")) {
			ds_list.setColumn(i, "chk", "");
		} 
		else {
			if ("0001" == ds_list.getColumn(i, "ord_cl")) {
				ds_list.setColumn(i, "chk", "");
			} else {
				ds_list.setColumn(i, "chk", chkVal);
			}
		}
		ds_list.setRowType(i, oldRowType);
	}
	ds_list.updatecontrol 	= true;
	ds_list.enableevent 	= true;
	grd_list.redraw();
}

function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	var txt = obj.getCellProperty( "head", e.cell, "text"); 
	if ( gfn_isEmpty(txt) ) {
		obj.setCellProperty( "head", e.cell, "text", "1"); 
		fn_allChk(true);
	} else {
		obj.setCellProperty( "head", e.cell, "text", ""); 
		fn_allChk(false);
	}
}

