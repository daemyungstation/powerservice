/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch()
{
	ds_cond.clearData();
	ds_cond.addRow();	
	
	var condCnt			= 0;
	var sFrDt 			= "";
	var sToDt 			= "";
		
	var custmr_cd 	= div_custmr.getCd();
	var gds_cd 		= div_gds.getCd();
	
	var ord_no 		= edt_ord_no.value;
	var wh_cd 		= cbo_wh_cd.value;
	var ord_cl 		= cbo_ord_cl.value;
	var proc_stat 	= cbo_proc_stat.value;
	
	if ( !gfn_isEmpty(cal_ord_start_dt.value) ) {
		sFrDt = cal_ord_start_dt.value;
	}	
	if ( !gfn_isEmpty(cal_ord_end_dt.value) ) {
		sToDt = cal_ord_end_dt.value;
	}
	
	if ( gfn_isEmpty(sFrDt) ) {
		alert("조회 시작일자는 필수 입력입니다.");
		cal_start_dt.setFocus();
		return false;
	}
	if ( gfn_isEmpty(sToDt) ) {
		alert("조회 종료일자는 필수 입력입니다.");
		cal_end_dt.setFocus();
		return false;
	}
	
	
	ds_cond.setColumn(0, "ord_start_dt"	, sFrDt		);
	ds_cond.setColumn(0, "ord_end_dt"	, sToDt		);		
	ds_cond.setColumn(0, "custmr_cd"	, custmr_cd	);
	ds_cond.setColumn(0, "gds_cd"		, gds_cd	);
	ds_cond.setColumn(0, "ord_no"		, ord_no	);
	ds_cond.setColumn(0, "wh_cd"		, wh_cd		);
	ds_cond.setColumn(0, "ord_cl"		, ord_cl	);
	ds_cond.setColumn(0, "proc_stat"	, proc_stat	);	
	
	return true;
}

//조회
function fn_search(obj, e)
{	
	// 호출 건수 설정 및 조회여부 설정
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "search";
    var szController  = "/evnt/stockMng/selectOrderMngList";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_list=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//조회 결과 
function fn_postSearch()
{
	var i;
	//저장 및 수정시 새창 안뜨도록 >> 추후 처리
	 // gfn_setTotCnt(ds_list, sta_TotalCnt); // 건수 표시
	 
	if(ds_list.rowcount < 1) {
		ds_list2.clearData();
		trace("조회 결과가 없습니다.");
 	} else {		
		if ( !gfn_isEmpty(fv_selected_ord_no) ) {
			fn_selectOrdDtlInfo(fv_selected_ord_no);
			for (i=0; i<ds_list.rowcount; ++i) {
				if (fv_selected_ord_no == ds_list.getColumn(i, "ord_no")) {
					ds_list.rowposition = i;
					break;
				}
			}
			fv_selected_ord_no = "";
		} else {
			fn_selectOrdDtlInfo(ds_list.getColumn(0, "ord_no"));
		}
		
 	}
	return;
}


