/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("0143", "SELECT", ds_0143, gds_DmlCd); // 코드조회결과 필터
			gfn_getCode("0144", "SELECT", ds_0144, gds_DmlCd); // 코드조회결과 필터
			fn_init(); // 검색부 초기화
			break;
		
		case "search":			
			fn_postSearch();
			break;
		
		case "getBranchList":
			fn_postGetBranchList();
			break;
		
		case "getWhCdList":
			fn_postGetWhCdList();
			break;
		
		case "selectOrdDtlInfo":
			fn_postSelectOrdDtlInfo();
			break;
		
		case "selectCustaccntList": // 회원조회 - 회원명에서 엔터키 입력 시 조회
			fn_postSelectCustaccntList();
			break;
			
		case "deleteWhInCncl": 
			fn_postDeleteWhInCncl();
			break;

	}
}

