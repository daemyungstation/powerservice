	var reVal = gfn_diaLog("입고처리", -1, -1, 884, 601, "DL430000::DL430501P02.xfdl", false, sPrmr, false, false, true, null, null, true);
}

function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	var nPos 		= e.row;
	var proc_stat 	= ds_list.getColumn(nPos, "proc_stat");
	var ord_cl 		= ds_list.getColumn(nPos, "ord_cl");	
	var chk 		= ds_list.getColumn(nPos, "chk");	
	if (2 == e.col) {
		system.setClipboard("CF_TEXT", ds_list.getColumn(nPos, "ord_no"));
	}
	
	fv_ord_no		= ds_list.getColumn(nPos, "ord_no");
	
	if ("0001" == proc_stat) {	// 발주
	
		if ("0001" == ord_cl) { // 현장발주
			btnInWhDtl.enabled = false;	// 부분 입고처리
			btnInWhAll.enabled = false;	// 일괄 입고처리
		} else {
			btnInWhDtl.enabled = false;
			btnInWhAll.enabled = false;
		}
		
		btnInWhCncl.enabled 	= false;		// 부분 입고취소
		btnInWhCnclAll.enabled 	= false;		// 일괄 입고취소
		
	} else if ("0003" == proc_stat) {	// 입고완료
		btnInWhDtl.enabled = false;
		btnInWhAll.enabled = false;
		btnInWhCncl.enabled = true;
		btnInWhCnclAll.enabled = true;
		
	} else { // 입고중
		btnInWhDtl.enabled = true;
		btnInWhAll.enabled = true;
		btnInWhCncl.enabled = true;
		btnInWhCnclAll.enabled = true;
	}
	
	var oldRowType = ds_list.getRowType(nPos);
	ds_list.enableevent = false;
	ds_list.updatecontrol = false;
	
	if ("0002" == ord_cl) { // 외주발주
		if ("0003" != proc_stat) {
			if( "1" == chk ){
				ds_list.setColumn(nPos, "chk", "");
			}else{
				ds_list.setColumn(nPos, "chk", "1");
			}
		}
	}
	ds_list.setRowType(nPos, oldRowType);	
	ds_list.updatecontrol = true;
	ds_list.enableevent = true;
	
	if (e.oldrow != e.row) {
		setTimer(fv_timerId, 30);
	}
}

function frm_ontimer(obj:Form, e:TimerEventInfo)
{
	fn_selectOrdDtlInfo(fv_ord_no);
	killTimer( fv_timerId );
}

