function fn_allChk2() {
	var isAllCheck = true;
	for (var i=0; i<ds_list2.rowcount; ++i) 
	{
		if ("0001" == ds_list2.getColumn(i, "ord_cl")) {	// 발주구분이 현장발주면 비활성화
			ds_list2.setColumn(i, "chk", "");
			isAllCheck = false;
		} else {
			ds_list2.setColumn(i, "chk", "1");			
		}
	}
	grd_list2.setCellProperty( "Head", 0, "expr", isAllCheck); 
}

function fn_fInWhProcAll(mode) {
	// mode = [all,  allCncl]
	
	if (ds_list2.rowcount < 1) {
		alert("처리할 대상이 없습니다.");
		return;
	}
	
	// fv_bCheck = true; // boolCl2 - 불필요	
	fn_allChk2();
	
	var proc_stat 	= ds_list.getColumn(ds_list.rowposition, "proc_stat");
	var ord_no 		= ds_list.getColumn(ds_list.rowposition, "ord_no");
	
	
	if ("all" == mode) {
		if ("0003" == proc_stat) { // 입고완료
			alert("이미 입고완료 되었습니다.");
			return;
		}
		
		if ("0001" == proc_stat) { // 발주
			mode = "newAll";
			
			gfn_copyCheckData(ds_list2, ds_list_tmp);
	
			if (ds_list_tmp.rowcount < 1) {
				alert("발주서 입고처리할 내역이 없습니다.");
				return;
			}
	
			// 상품상세 팝업 - 모달
			var sPrmr = {p_ds:ds_list_tmp, p_mode:mode, p_ord_no:ord_no};
			fn_openInWhPopup(sPrmr); // 
		}
	} 
	else if ("allCncl" == mode) {
		if ("0001" == proc_stat) { // 발주
			// 아래 2라인 불필요
			// fv_bCheck = false; // boolCl2  - 불필요
			// fn_allChk2();
			alert("미입고 된 발주는 일괄 입고취소 할 수 없습니다.");
			gfn_checkDs(grd_list2, "chk", false);
			// grd_list2.setCellProperty( "Head", 0, "expr", false); 
		} else {
			if (!confirm("일괄 입고취소 하시겠습니까?")) {
				return;
			}
			
			gfn_copyCheckData(ds_list2, ds_list_tmp);
			fn_deleteWhInCncl('allCncl'); // deleteWhInCncl
		}
	}
}

// 일괄입고취소
function fn_deleteWhInCncl(mode)
{
	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "mode", mode);
	
	var szSvcid       = "deleteWhInCncl";
    var szController  = "/evnt/stockMng/deleteWhInCncl"; // insrtCustmrMng, updateCustmrMng, deleteCustmr
    var szInDs        = "ds_input=ds_list_tmp:U ds_input2=ds_cond:U";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

