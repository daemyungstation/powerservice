function gfn_copyCheckData(src:Dataset, dst:Dataset, sCol) {
	dst.assign(src);
	dst.clearData();
	if (undefined == sCol || null == sCol || "" == sCol) {
		sCol = "chk";
	}
	
	for (var i=src.rowcount-1; i>=0; i--) {
		if ("1" == src.getColumn(i, sCol)) {
			nPos = dst.addRow();
			dst.copyRow(nPos, src, i);
		}
	}
}

