    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt = 0; // 전체건수
var nTotalListCntDept = 0; // 전체건수
var hgrn_team_cd = ""; // 상위상담그룹코드
var team_cd = "" // 상담그룹코드

var nPageNoCnt = 0;     // 페이지수(사원별)
var nPageNoCntDept = 0; // 페이지수(업체별)

var pageNo1 = 0;   // 현재페이지
var pageNo2 = 0;   // 현재페이지

var gvMenuId;  // 프로그램 로그 추가

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS031900_onload(obj:Form, e:LoadEventInfo)
{	
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_createPagingDataset(obj); 		// 페이징처리 위한 데이터셋 설정	
	
	gfn_getList("teamCd","", "fn_callBack", null, null, "ds_teamCd"); //팀코드조회
	
	fn_init(); // 검색부 초기화
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {			
			case "selectTeamCode": //팀코드 조회
				gfn_getTeamCodeLoad("", "ALL", ds_teamCd1, ds_teamCd, "1");
				
				//정훈 소스변경 업체간 검색조건 변경
				var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');	
				if(sUserAthrCd.substr(0,2)=='TM'){
					cbo_teamCd1.value = 'T20000';					
					cbo_teamCd1.enable = false;				
					gfn_getTeamCodeLoad(cbo_teamCd1.value, "ALL", ds_teamCd2, ds_teamCd);
					cbo_teamCd2.value = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
					cbo_teamCd2.enable = false;									
				}else{
					cbo_teamCd1.value = "";
					cbo_teamCd1_onitemchanged(cbo_teamCd1);
				}				
			break;
			
			case "selectUserTeamList": // 사원별 조회			 
				gfn_appendSearchDs(ds_mem_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_mem_list, Tab_Main.tabpage1.sta_totalCnt, nTotalListCnt, null, true, null); // 페이지건수 표출						

				if(ds_mem_list.rowcount < 1) 
				{
					alert("조회된 자료가 없습니다.");
					ds_mem_list.clearData();
				}			
			break;
			
			case "selectUserTeamListDept": // 업체별 조회			
				gfn_appendSearchDs(ds_dept_list, ds_dept_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_dept_list, Tab_Main.tabpage2.sta_totalCnt, nTotalListCntDept, null, true, null); // 페이지건수 표출							
				if(ds_dept_list.rowcount < 1) 
				{
					alert("조회된 자료가 없습니다.");
					ds_dept_list.clearData();
				}
			break;					
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	var sTabPageId = Tab_Main.tabpages[Tab_Main.tabindex].name;	
	
	if (sType == gfn_getCommTypeInit())	{
	
		if (gfn_isNull(cal_srch_stt_dt.value) || gfn_isNull(cal_srch_end_dt.value)) {
			alert("검색기간을 입력하세요.");
			return false;
		}
	
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		if (gfn_getMonthDiff(cal_srch_stt_dt.value, cal_srch_end_dt.value) > 2) {
			alert("검색기간은 3개월미만입니다.");
			return false;
		}
							
		// 호출 건수 설정 및 조회여부 설정
		if(sTabPageId == "tabpage1"){
			pageNo1 = 0;   // 현재페이지		
		} else if(sTabPageId == "tabpage2"){
			pageNo2 = 0;   // 현재페이지			
		}	
	}
	
	if (sType == gfn_getCommTypeAppend()){	
		if(sTabPageId == "tabpage1"){
			pageNo1 = pageNo1 + 1;
			gds_RequestCompVariable.setColumn(0, "pageNo", pageNo1);			
		} else if(sTabPageId == "tabpage2"){
			pageNo2 = pageNo2 + 1;
			gds_RequestCompVariable.setColumn(0, "pageNo", pageNo2);
		}
	}	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, searchInfo)
{	
	var szSvcid       = "";	
	var sTabPageId = Tab_Main.tabpages[Tab_Main.tabindex].name;		

	if (!fn_preSearch(sType)) return false;

	// 호출 건수 설정 및 조회여부 설정
	if(sTabPageId == "tabpage1"){
		if(!gfn_pageSearch(sType, ds_mem_list, Tab_Main.tabpage1.sta_totalCnt, 100, nTotalListCnt)) return;							
	} else if(sTabPageId == "tabpage2"){
		if(!gfn_pageSearch(sType, ds_dept_list, Tab_Main.tabpage2.sta_totalCnt, 100, nTotalListCntDept)) return;					
	}	

	switch(searchInfo) {
        case "selectUserTeamList":       
			var szSvcid       = "selectUserTeamList";
			var szController  = "dlw/itomng/selectUserTeamList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";						
        break;

        case "selectUserTeamListDept":        
			var szSvcid       = "selectUserTeamListDept";
			var szController  = "dlw/itomng/selectUserTeamListDept";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_dept_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";			
        break;        
    }     
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init(sType)
{
	//  검색기간 설정
	cbo_srch_dateTyp.index = 0;
	var sToday = gfn_getToday();
	cal_srch_stt_dt.value = sToday;
	cal_srch_end_dt.value = sToday;

	hgrn_team_cd = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'hgrn_team_cd');
	team_cd = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
	
	cbo_teamCd1.value = hgrn_team_cd;
	cbo_teamCd1_onitemchanged();
	cbo_teamCd2.value = team_cd;
	ds_mem_list.clearData();
	ds_dept_list.clearData();	
	
	Tab_Main.tabpage1.sta_totalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
	Tab_Main.tabpage2.sta_totalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";	
	
	Tab_Main.tabindex = 0;
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/

// 검색 버튼 클릭
function btn_srch_onclick(obj:Button,  e:ClickEventInfo)
{
	if(Tab_Main.tabindex == 0){
		fn_commSearch(gfn_getCommTypeInit(), 'selectUserTeamList');		// 사원별 조회
	}else{
		fn_commSearch(gfn_getCommTypeInit(), 'selectUserTeamListDept'); // 업체별 조회
	}
}

// 초기화 버튼 클릭
function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

// 상담그룹 콤보 변경시
function cbo_teamCd1_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	//조회 팀1콤보 변경시
	fn_changeTeam1Cd(cbo_teamCd1 , cbo_teamCd2, ds_teamCd2)
}

//상단 팀 코드 1변경
function fn_changeTeam1Cd(item1 , item2, inDs)
{
	if (!fn_preChngTeam1(item1 , item2, inDs)) return false;
	
	gfn_getTeamCodeLoad(item1.value, "ALL", inDs, ds_teamCd);
	//정훈 소스변경 업체간 검색조건 변경
	var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
	
	if(item1.value == 'T20000'){
		if(sUserAthrCd.substr(0,2)=='TM'){
			item2.value = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
			
			if(item2.value == "T20300" || item2.value == "T20100" || item2.value == "T20900"){
				//T20300 효성ITX , T20100 윌앤비전 , T20900 리딩아이
				ds_teamCd2.filter("team_cd == 'T20300' || team_cd == 'T20100' || team_cd == 'T20900' || team_cd == ''");
				item2.enable = true;
			}else{
				item2.enable = false;
			}
		}else{
			item2.index = "0";
			item2.enable = true;
		}
	}else{
		item2.index = "0";
		item2.enable = true;
	}
}

//팀코드 1
function fn_preChngTeam1(item1 , item2, inDs)
{
	if (gfn_isNull(item1.value)) {
		item2.index = "0";
		item2.enable = false;
		
		return false;
	}
	return true;
}

// 사원별 스크롤바 이벤트
function Tab_Main_tabpage1_grd_main_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend(), 'selectUserTeamList' );	
}

// 업체별 스크롤바 이벤트
function Tab_Main_tabpage2_grd_rec_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend(), 'selectUserTeamListDept'); // 업체별 조회
}

// 엑셀버튼 클릭
function fn_excel(obj:Button,  e:ClickEventInfo)
{
	var sTabPageId = Tab_Main.tabpages[Tab_Main.tabindex].name;
	if(sTabPageId == "tabpage1")
	{		
		if(ds_mem_list.getRowCount() > 0) 
		{
			if(cbo_srch_dateTyp.value == "01"){
				var colDS = gfn_excelPrepareExcelDS2("grd_excel_user","1"); // 접수일자
				gfn_excelDown("종합현황(접수일자사원별)", "XLS_DLCC", "DlwItoMngMap.selectUserTeamList", "DlwItoMngMap.selectUserTeamListCount", ds_inqCndt, true, colDS, true);	
			}else{
				var colDS = gfn_excelPrepareExcelDS2("grd_excel_user2","2"); // 가입일자
				gfn_excelDown("종합현황(가입일자사원별)", "XLS_DLCC", "DlwItoMngMap.selectUserTeamList", "DlwItoMngMap.selectUserTeamListCount", ds_inqCndt, true, colDS, true);	
			}																											
		}
		else
		{
			alert("엑셀다운로드 할 대상이 없습니다.");
		}	
	}
	else if(sTabPageId == "tabpage2")
	{	
		if(ds_dept_list.getRowCount() > 0) 
		{		
			if(cbo_srch_dateTyp.value == "01"){
				var colDS = gfn_excelPrepareExcelDS2("grd_excel_dept","3"); // 접수일자
				gfn_excelDown("종합현황(접수일자업체별)", "XLS_DLCC", "DlwItoMngMap.selectUserTeamListDept", "DlwItoMngMap.selectUserTeamListCountDept", ds_inqCndt, true, colDS, true);																							
				
			}else{
				var colDS2 = gfn_excelPrepareExcelDS2("grd_excel_dept2","4"); // 가입일자
				gfn_excelDown("종합현황(가입일자업체별)", "XLS_DLCC", "DlwItoMngMap.selectUserTeamListDept", "DlwItoMngMap.selectUserTeamListCountDept", ds_inqCndt, true, colDS2, true);																											
			}			
		}
		else
		{
			alert("엑셀다운로드 할 대상이 없습니다.");
		}
	}	
}

function gfn_excelPrepareExcelDS2(grdNM:String,sVal) {
	var colDS:Dataset = null;
	
	if(sVal == "1"){
		if (this["ds_tempExcelDown1"]) {			
			colDS = this["ds_tempExcelDown1"];		
		} else {
			colDS = new Dataset;
			colDS.name = "ds_tempExcelDown1";
			this.addChild(colDS.name, colDS);
		}
	}else if(sVal == "2"){
		if (this["ds_tempExcelDown2"]) {			
			colDS = this["ds_tempExcelDown2"];		
		} else {
			colDS = new Dataset;
			colDS.name = "ds_tempExcelDown2";
			this.addChild(colDS.name, colDS);
		}

	}else if(sVal == "3"){
		if (this["ds_tempExcelDown3"]) {			
			colDS = this["ds_tempExcelDown3"];		
		} else {
			colDS = new Dataset;
			colDS.name = "ds_tempExcelDown3";
			this.addChild(colDS.name, colDS);
		}
	}else if(sVal == "4"){
		if (this["ds_tempExcelDown4"]) {			
			colDS = this["ds_tempExcelDown4"];		
		} else {
			colDS = new Dataset;
			colDS.name = "ds_tempExcelDown4";
			this.addChild(colDS.name, colDS);
		}

	}

	colDS.addColumn("xls_colm_id", "string", 256);
	colDS.addColumn("xls_colm_nm", "string", 256);
	colDS.addColumn("xls_colm_type_cd", "string", 256);
	
	for (var i=0; i< this[grdNM].getCellCount("Head"); i++) {
		var colId = this[grdNM].getCellProperty("Body", i, "text")
		if (colId==null || colId.search("expr") > -1)
			continue;
			
		colId = colId.replace("bind:", "");
		colDS.addRow();
		colDS.setColumn(colDS.rowposition, "xls_colm_id", colId);
		colDS.setColumn(colDS.rowposition, "xls_colm_nm", this[grdNM].getCellProperty("Head", i, "text"));
		colDS.setColumn(colDS.rowposition, "xls_colm_type_cd", "10");
	}
	
	return colDS;
}]]></Script>
