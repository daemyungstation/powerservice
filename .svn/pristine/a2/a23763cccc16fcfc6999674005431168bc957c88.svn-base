/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		return false;
	}
	
	switch (svcid) 
	{
		case "DL410406DLWCD" :			
		    gfn_getCode("0201", "ALL", ds_codeLoanPurcCrco, ds_codeDlwCds); // 카드사명(여신)
		break;	
		
		case "getAcquResultDataList" : // 문자발송이력 조회		    			
