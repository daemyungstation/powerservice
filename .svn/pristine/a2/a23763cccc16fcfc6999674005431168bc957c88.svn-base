function fn_commSearch(sType, sSvcId)
{
	if (!fn_preSearch(sType, sSvcId))
	{
		return false;
	}
	
	switch(sSvcId) 
	{
        case "getAcquResultDataList": 
			if (!gfn_pageSearch(sType, ds_listAcquRslt, tab_result.tap_acquList.sta_totalCnt, 100, nTotalListCnt))
