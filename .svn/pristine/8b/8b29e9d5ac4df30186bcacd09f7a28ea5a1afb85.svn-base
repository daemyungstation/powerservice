    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
var sCdCnt = "";
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/

function PS090110_onload(obj:Form, e:LoadEventInfo)
{
	cbo_use_yn.index = 0;
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_commSearch();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
		
			case "searchCdSet":
				//trace(ds_list_tmp.saveXML());
				gfn_appendSearchDs(ds_list_cdSet, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				
				if(ds_list_cdSet.rowcount > 0){
					ds_cdSet.clearData();
					ds_cdSet.addRow();
					ds_cdSet.copyRow(0, ds_list_cdSet, ds_list_cdSet.rowposition);
					
					ds_inqCndt.clearData();
					ds_inqCndt.addRow();
					ds_inqCndt.setColumn(0, "cd_set_cd", ds_list_cdSet.getColumn(ds_list_cdSet.rowposition, "cd_set_cd"));
					
					fn_commSearch2();
					
				}else{
					ds_cdSet.clearData();
				}
				fn_readMode();
			break;
			
			case "getCdList":
				//trace(ds_list_tmp.saveXML());
				gfn_appendSearchDs(ds_list_cd, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				
				if(ds_list_cd.rowcount > 0){
					ds_cd.clearData();
					ds_cd.addRow();
					ds_cd.copyRow(0, ds_list_cd, ds_list_cd.rowposition);
					
					ds_cdSqnc.clearData();
					sCdCnt = ds_cd.getColumn(0, "cd_sqnc");
					var sRowCnt = ds_list_cd.rowcount;
					//alert(sRowCnt);
					//순서 콤보박스 증가
					for (var i=0; i<sRowCnt; i++) {
						//trace(i);
						ds_cdSqnc.addRow();
						ds_cdSqnc.setColumn(i, "cd", i+1);
						ds_cdSqnc.setColumn(i, "cd_nm", i+1);
					}
					cbo_cd_sqnc.index = toNumber(sCdCnt)-1;						
				}else{
					sCdCnt = "0";
					ds_cd.clearData();
				}

			break;
			
			case "saveCdSet":
				fn_commSearch();
			break;
			
			case "saveCd":
				fn_commSearch();
			break;
		}	
 	}		
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//좌측조회
function fn_commSearch()
{
	ds_list_cdSet.clearData();
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "cd_set_cd", edt_cd_set_cd.value);
	ds_inqCndt.setColumn(0, "cd_set_nm", edt_cd_set_nm.value);
	ds_inqCndt.setColumn(0, "use_yn", cbo_use_yn.value);
	ds_inqCndt.setColumn(0, "type_cd", "member");
	
	var szSvcid       = "searchCdSet";
    var szController  = "/syst/cd-set/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//우측조회
function fn_commSearch2()
{
	ds_list_cd.clearData();
	
	var szSvcid       = "getCdList";
    var szController  = "/syst/cd/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "type_cd='member'";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//좌측저장
function fn_save()
{
	var szSvcid       = "saveCdSet";
    var szController  = "/syst/cd-set/com_save";
    var szInDs        = "ds_input=ds_cdSet";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//우측저장
function fn_save2(sType)
{
	if(sType == "check"){
		ds_cd.setColumn(0, "dplc_flag", "Y");
		ds_cd.setColumn(0, "cd_sqnc_old", sCdCnt);
	}else{	
		ds_cd.setColumn(0, "dplc_flag", "N");
	}
	
	ds_cd.setColumn(0, "cd_set_cd", ds_cdSet.getColumn(0, "cd_set_cd"));
	
	var szSvcid       = "saveCd";
    var szController  = "/syst/cd/com_save";
    var szInDs        = "ds_input=ds_cd";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//읽기모드
function fn_readMode()
{
	//좌측
	btn_new.visible = true;
	btn_update.visible = true;
	btn_save.visible = false;
	btn_cancel.visible = false;
	
	edt_cd_set_cd2.readonly = true;//코드셋코드
	edt_cd_set_nm2.readonly = true;//코드셋명
	Radio_use_yn.readonly = true;//사용여부
	TextArea_cd_set_expi.readonly = true;//설명
		
	Mask_rgsn_dttm.mask = "####-##-## ##:##";
	Mask_amnt_dttm.mask = "####-##-## ##:##";
	
	//우측
	btn_new2.visible = true;
	btn_update2.visible = true;
	btn_save2.visible = false;
	btn_cancel2.visible = false;
	
	edt_cd.readonly = true;//코드
	edt_cd_nm.readonly = true;//코드명
	edt_adtl_cd.readonly = true;//추가코드
	edt_adtl_cd_nm.readonly = true;//추가코드명
	Radio_use_yn2.readonly = true;//사용여부
	cbo_cd_sqnc.readonly = true;//순서
	edt_cd_expi.readonly = true;//설명
	
	Mask_rgsn_dttm2.mask = "####-##-## ##:##";
	Mask_amnt_dttm2.mask = "####-##-## ##:##";
}	

//좌측저장모드
function fn_saveMode()
{
	btn_new.visible = false;
	btn_update.visible = false;
	btn_save.visible = true;
	btn_cancel.visible = true;
	
	edt_cd_set_nm2.readonly = false;//코드셋명
	Radio_use_yn.readonly = false;//사용여부
	TextArea_cd_set_expi.readonly = false;//설명
		
	stc_rgsr_nm.text = "";
	stc_amnd_nm.text = "";
	Mask_rgsn_dttm.mask = "";
	Mask_amnt_dttm.mask = "";
}

//우측저장모드
function fn_saveMode2()
{
	btn_new2.visible = false;
	btn_update2.visible = false;
	btn_save2.visible = true;
	btn_cancel2.visible = true;
	
	edt_cd_nm.readonly = false;//코드명
	edt_adtl_cd.readonly = false;//추가코드
	edt_adtl_cd_nm.readonly = false;//추가코드명
	Radio_use_yn2.readonly = false;//사용여부
	cbo_cd_sqnc.readonly = false;//순서
	edt_cd_expi.readonly = false;//설명
		
	stc_rgsr_nm2.text = "";
	stc_amnd_nm2.text = "";
	Mask_rgsn_dttm2.mask = "";
	Mask_amnt_dttm2.mask = "";
}
