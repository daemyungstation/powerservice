//좌측그리드 셀클릭
function Grid00_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	ds_cdSet.clearData();
	ds_cdSet.addRow();
	ds_cdSet.copyRow(0, ds_list_cdSet, ds_list_cdSet.rowposition);
	
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "cd_set_cd", ds_list_cdSet.getColumn(ds_list_cdSet.rowposition, "cd_set_cd"));
	
	fn_readMode();
	fn_commSearch2();
}

//우측그리드 셀클릭
function Grid01_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	ds_cd.clearData();
	ds_cd.addRow();
	ds_cd.copyRow(0, ds_list_cd, e.row);
	
	ds_cdSqnc.clearData();
	sCdCnt = ds_cd.getColumn(0, "cd_sqnc");
	var sRowCnt = ds_list_cd.rowcount;
	//alert(sRowCnt);
	//순서 콤보박스 증가
	for (var i=0; i<sRowCnt; i++) {
		//trace(i);
		ds_cdSqnc.addRow();
		ds_cdSqnc.setColumn(i, "cd", i+1);
		ds_cdSqnc.setColumn(i, "cd_nm", i+1);
	}
	cbo_cd_sqnc.index = toNumber(sCdCnt)-1;			
	fn_readMode();
}

//좌측신규등록버튼클릭
function btn_new_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_saveMode();
	ds_cdSet.clearData()
	ds_cdSet.addRow();
	Radio_use_yn.value = "Y";
	edt_cd_set_cd2.readonly = false;//코드셋코드
	
}

//좌측수정버튼클릭
function btn_update_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_cdSet.rowcount == 0){
		alert("수정할 대상이 없습니다.");
		return false;
	}
	fn_saveMode();
}

//좌측저장버튼클릭
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if(gfn_isNull(edt_cd_set_cd2.value)){
		alert("코드셋코드를 입력하세요");
		return false;
	}
	
	fn_save();
}

//취소버튼클릭
function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit());
}

//우측신규등록버튼클릭
function btn_new2_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_saveMode2();
	ds_cd.clearData()
	ds_cd.addRow();
	
	Radio_use_yn2.value = "Y";
	ds_cdSqnc.clearData();
	
	var sRowCnt = ds_list_cd.rowcount;
	var index = "";
	//순서 콤보박스 증가
	for (var i=0; i<toNumber(sRowCnt)+1; i++) {
		//trace(i);
		ds_cdSqnc.addRow();
		ds_cdSqnc.setColumn(i, "cd", i+1);
		ds_cdSqnc.setColumn(i, "cd_nm", i+1);
		index = i;
	}
	cbo_cd_sqnc.index = index;
	sCdCnt = cbo_cd_sqnc.value;
	edt_cd.readonly = false;//코드
}

//우측수정버튼클릭
function btn_update2_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_cd.rowcount == 0){
		alert("수정할 대상이 없습니다.");
		return false;
	}
	fn_saveMode2();
}

//우측저장버튼클릭
function btn_save2_onclick(obj:Button,  e:ClickEventInfo)
{
		
	var checkCnt = ds_list_cd.getCaseCount("cd_sqnc=='"+cbo_cd_sqnc.value+"'");
	
	if(checkCnt > 0){ 
		if(cbo_cd_sqnc.value != ds_list_cd.getColumn(ds_list_cd.rowposition, "cd_sqnc")){
			if(!confirm("중복되는 순서가 있습니다. 순서를 자동증가하고 등록하시겠습니까?\n자동증가된 순서는 목록을 다시 조회하면 표시됩니다.")) {
				return false;
			}
			fn_save2("check");		
		}else{
			fn_save2();	
		}	
	}else{
		fn_save2();
	}

}

//조회버튼클릭
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch();
}

//초기화
function btn_Init_onclick(obj:Button,  e:ClickEventInfo)
{
	edt_cd_set_cd.value = "";
	edt_cd_set_nm.value = "";
	cbo_use_yn.index = 0;
}

function Edit_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		fn_commSearch();
	}
}

