/* TitlebarForm */
TitlebarForm
{ 
	background	: @gradation;
	gradation 	: linear 0,0 #1c89d7 0,100 #235798 [6% #0073cb][90% #235798];
	border		: 0 none;
	bordertype 	: round 2 2 lefttop righttop;
	align		: left middle;
	font		: Dotum,10,bold;
	color		: #ffffff;
	icon		: theme://images/ico_title.png;
}
TitlebarForm:deactivate, TitlebarForm:disabled
{ 
	gradation 	: linear 0,0 #83c7f6 0,100 #6894cc [6% #78bbef]; 
}
TitlebarForm>#minbutton
{
	background	: transparent;
	border		: 0 none;
	bordertype	: normal ;
	margin		: -2 20 4 -30;
	image		: theme://images/btn_TitlebarForm_Min_N.png;
	imagealign	: center middle; 
}
TitlebarForm>#minbutton:mouseover
{
	image		: theme://images/btn_TitlebarForm_Min_O.png;
}
TitlebarForm>#minbutton:pushed
{
	image		: theme://images/btn_TitlebarForm_Min_P.png;
	pusheddrawoffset	: 0 0;
}
TitlebarForm>#minbutton:deactivate
{
	image		: theme://images/btn_TitlebarForm_Min_D.png;
}
TitlebarForm>#maxbutton
{
	background	: transparent;
	border		: 0 none;
	bordertype	: normal ;
	margin		: -2 11 4 -21;
	image		: theme://images/btn_TitlebarForm_Max_N.png;
	imagealign	: center middle; 
}
TitlebarForm>#maxbutton:mouseover
{
	image		: theme://images/btn_TitlebarForm_Max_O.png;
}
TitlebarForm>#maxbutton:pushed
{
	image		: theme://images/btn_TitlebarForm_Max_P.png;
	pusheddrawoffset	: 0 0;
}
TitlebarForm>#maxbutton:disabled
{
	image		: theme://images/btn_TitlebarForm_Max_D.png;
}
TitlebarForm>#maxbutton:deactivate
{
	image		: theme://images/btn_TitlebarForm_Max_D.png;
}
TitlebarForm>#normalbutton
{
	background	: transparent;
	border		: 0 none;
	bordertype	: normal ;
	margin		: -2 11 4 -21;
	image		: theme://images/btn_TitlebarForm_Nor_N.png;
	imagealign	: center middle; 
}
TitlebarForm>#normalbutton:mouseover
{
	image		: theme://images/btn_TitlebarForm_Nor_O.png;
}
TitlebarForm>#normalbutton:pushed
{
	image		: theme://images/btn_TitlebarForm_Nor_P.png;
	pusheddrawoffset	: 0 0;
}
TitlebarForm>#normalbutton:disabled
{
	image		: theme://images/btn_TitlebarForm_Nor_D.png;
}
TitlebarForm>#closebutton
{
	background	: transparent;
	border		: 0 none;
	bordertype	: normal ;
	margin		: -2 2 4 -12;
	image		: theme://images/btn_TitlebarForm_Close_N.png;
	imagealign	: center middle; 
}
TitlebarForm>#closebutton:mouseover
{
	image		: theme://images/btn_TitlebarForm_Close_O.png;
}
TitlebarForm>#closebutton:pushed
{
	image		: theme://images/btn_TitlebarForm_Close_P.png;
	pusheddrawoffset	: 0 0;
}
TitlebarForm>#closebutton:disabled
{
	image		: theme://images/btn_TitlebarForm_Close_D.png;
}
/* StatusbarForm */
StatusbarForm
{ 
	background	: #f2f2ef;
	border		: 1 solid #a8a8a2, 0 none, 0 none, 0 none;
	bordertype	: round 3 3 leftbottom rightbottom;
	font		: Dotum,9 ;
	padding		: 0 0 0 12;
	color		: #46463d;
	align		: left middle;
	gripimage	: URL('theme://images/img_grip.png');
	progressbargab	: 5;
	progressbarheight	: 20;
	progressbarwidth	: 260;
	zoomcombogap 		: 10;
	zoomcomboheight 	: 20;
	zoomcombowidth		: 80;
}
StatusbarForm>#progressbar
{ 
	background	: transparent;
	border		: 1 solid #a8a8a2;
	bartype		: normal;
	align		: center middle;
	color		: #444444 ;
	font		: Dotum, 9 ;
	padding		: 1 1 1 1;
	smooth		: true;
	barcolor	: cdcdc8;
	direction	: forward;
}
StatusbarForm>#zoomcombo
{
	background	: #f2f2ef;
	border		: 1 solid #a8a8a2;
	bartype		: normal;
	font		: Dotum,9;
	color		: #5a280b;
	align		: left middle;
	cursor		: ;
	padding		: 1 1 1 1;
	buttonsize	: 16;
	itembackground	: #ffffff;
	itemborder		: 0 none;
	itemcolor		: #6e6e63;
	itemheight		: 20;
	itempadding	: 0 5 0 8;
}
StatusbarForm>#zoomcombo:mouseover
{
	itembackground	: #f2f2ef;
	itemcolor		: #5a280b;
}
StatusbarForm>#zoomcombo:selected
{
	itembackground	: #e0e0d9;
	itemcolor		: #5a280b;
}
StatusbarForm>#zoomcombo>#comboedit
{
	background	: #f2f2ef url('theme://images/ico_zoomcombo.png') left middle;
	border		: 0 none;
	align		: left middle;
	font		: Dotum,9;
	color		: #5a280b;
	padding		: 0 0 0 20;
	caretcolor		: #3da2df;
	selectbackground	: #ffffff;
	selectcolor 		: #333333;	
}
StatusbarForm>#zoomcombo>#comboedit:mouseover, StatusbarForm>#zoomcombo>#comboedit:focused
{
	caretcolor		: #3da2df;
	compositecolor	: #ffffff;
	selectbackground	: #3da2df;
	selectcolor 		: #ffffff;
}
StatusbarForm>#zoomcombo>#dropbutton
{
	background	: @gradation url('theme://images/img_drop_N.png') stretch 6,6;
	gradation	: linear 0,0 #ffffff 0,100 #e3e3dd;
	border		: 1 solid #a8a8a2;
	margin		: 1 1 1 1;
	image		: theme://images/btn_drop_N.png;
	imagealign	: center middle;
	pusheddrawoffset 	: 0 0;
}
StatusbarForm>#zoomcombo>#dropbutton:mouseover
{
	background	: @gradation url('theme://images/img_drop_O.png') stretch 6,6;
	gradation	: linear 0,0 #ddddd6 0,100 #acaca6;
	image		: theme://images/btn_drop_O.png;
}
StatusbarForm>#zoomcombo>#dropbutton:pushed
{
	background	: #e1e1da;
	gradation	: linear 0,0 #ffffff 0,100 #e3e3dd;
	image		: theme://images/btn_drop_O.png;
}
StatusbarForm>#zoomcombo>#combolist
{
	background	: #ffffff;
	border		: 1 solid #a8a8a2;
	align		: left middle;
	font		: Dotum,9;
	color		: #6e6e63;
	itembackground	: #ffffff;
	itemborder		: 0 none;
	itemcolor		: #6e6e63;
	itemheight		: 20;
	itempadding	: 0 5 0 8;
	
}
/* vscrollbar */
*>#vscrollbar
{
	background	: #ecece8;
	border		: 0 none;
	scrollbarsize: 13;
	incbtnsize	: 13;
	decbtnsize	: 13;
}
*>#vscrollbar:mouseover, *>#vscrollbar:pushed, *>#vscrollbar:focused
{
	background	: #dadad2;
}
*>#vscrollbar>#incbutton, *>#vscrollbar>#decbutton, *>#vscrollbar>#trackbar
{
	background	: @gradation;
	gradation	: linear 0,0 #fbfbfa 100,0 #deded7;
	border		: 2 double #a8a8a2 #eeeeeb, 1 solid #a8a8a2;
	imagealign	: center middle;
	pusheddrawoffset: 0 0;
	focusborder	: 0 none;
}
*>#vscrollbar>#incbutton:mouseover,*>#vscrollbar>#decbutton:mouseover,*>#vscrollbar>#trackbar:mouseover
{
	gradation	: linear 0,0 #ddddd6 100,0 #aeaea8;
	border		: 2 double #a8a8a2 #c5c5bf, 1 solid #a8a8a2;
}
*>#vscrollbar>#incbutton:pushed, *>#vscrollbar>#decbutton:pushed, *>#vscrollbar>#trackbar:pushed
{
	gradation	: linear 0,0 #dbdbd4 100,0 #aeaea8;
	border		: 1 solid #909085;
}
*>#vscrollbar>#incbutton:disabled, *>#vscrollbar>#decbutton:disabled
{
	gradation	: linear 0,0 #f5f5f3 100,0 #e5e5df;
	border		: 1 solid #cacac5;
}
*>#vscrollbar>#incbutton
{
	image		: theme://images/btn_vinc_N.png;
	imagepadding: 2 0 0 0;
}
*>#vscrollbar>#incbutton:mouseover
{
	image		: theme://images/btn_vinc_O.png;
}
*>#vscrollbar>#incbutton:pushed
{
	image		: theme://images/btn_vinc_P.png;
}
*>#vscrollbar>#incbutton:disabled
{
	image		: theme://images/btn_vinc_D.png;
}
*>#vscrollbar>#decbutton
{
	image		: theme://images/btn_vdec_N.png;
}
*>#vscrollbar>#decbutton:mouseover
{
	image		: theme://images/btn_vdec_O.png;
}
*>#vscrollbar>#decbutton:pushed
{
	image		: theme://images/btn_vdec_P.png;
}
*>#vscrollbar>#decbutton:disabled
{
	image		: theme://images/btn_vdec_D.png;
}
