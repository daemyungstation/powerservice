/***** FrameObject Group Start *****/
/* MainFrame */
MainFrame
{
	background		: #235798 ;
	border			: 2 solid #235798 ;
	bordertype		: round 4 4;
	menubarheight	: 28;
	statusbarheight	: 25;
	titlebarheight	: 24;
	color			: #ffffff;
}
MainFrame:deactivate
{
	border			: 2 solid #6894cc;
}
/* Frame Group */
ChildFrame, VFrameSet, HFrameSet
{
	align		: left top ;
	background	: #ffffff ;
	border		: 0 none ;
}
FrameSet
{
	background	: url('theme://images/bg_FrameSet.png') stretch 12,12 left top;
}
ChildFrame
{
	background	: #ffffff;
	border		: 0 none;
}
ChildFrame>TitlebarForm
{
	background	: @gradation;
	gradation	: linear 0,0 #8f8f8b 0,100 #7f7f7b;
	font		: Dotum,9;
	color		: #ffffff;
	align		: left middle;
	padding		: 0 0 0 0;
	icon		: theme://images/ico_subtitle.png;
}
ChildFrame>TitlebarForm:deactivate
{
	gradation	: linear 0,0 #bbbbbb 0,100 #acacac;
}
ChildFrame>TitlebarForm>#minbutton
{
	background	: @gradation;
	gradation	: linear 0,0 #b2aca5 0,100 #635c52;
	border		: 1 solid #645f57;
	bordertype	: round 3 3;
	margin		: 1 0 3 0;
	imagealign	: center middle;
}
ChildFrame>TitlebarForm>#minbutton,
ChildFrame>TitlebarForm>#minbutton:mouseover, ChildFrame>TitlebarForm>#minbutton:pushed,
ChildFrame>TitlebarForm>#minbutton:disabled
{
	image		: theme://images/btn_SubTitlebarForm_Min_N.png;
	imagepadding: 3 0 0 2;
}
ChildFrame>TitlebarForm>#minbutton:mouseover
{
	gradation	: linear 0,0 #d3aa61 0,100 #614e30;
	border		: 1 solid #7e6c2e;
}
ChildFrame>TitlebarForm>#minbutton:pushed
{
	background	: #d6ab60;
	border		: 1 solid #685628;
}
ChildFrame>TitlebarForm>#minbutton:disabled
{
	gradation	: linear 0,0 #b6b5b5 0,100 #878685;
	border		: 1 solid #969696;
}
ChildFrame>TitlebarForm>#maxbutton
{
	background	: @gradation;
	gradation	: linear 0,0 #b2aca5 0,100 #635c52;
	border		: 1 solid #645f57;
	bordertype	: round 3 3;
	margin		: 1 0 3 0;
	imagealign	: center middle;
}
ChildFrame>TitlebarForm>#maxbutton,
ChildFrame>TitlebarForm>#maxbutton:mouseover, ChildFrame>TitlebarForm>#maxbutton:pushed,
ChildFrame>TitlebarForm>#maxbutton:disabled
{
	image		: theme://images/btn_SubTitlebarForm_Max_N.png;
	imagepadding: 1 0 0 0;
}
ChildFrame>TitlebarForm>#maxbutton:mouseover
{
	gradation	: linear 0,0 #d3aa61 0,100 #614e30;
	border		: 1 solid #7e6c2e;
}
ChildFrame>TitlebarForm>#maxbutton:pushed
{
	background	: #d6ab60;
	border		: 1 solid #685628;
}
ChildFrame>TitlebarForm>#maxbutton:disabled
{
	gradation	: linear 0,0 #b6b5b5 0,100 #878685;
	border		: 1 solid #969696;
}
ChildFrame>TitlebarForm>#normalbutton
{
	background	: @gradation;
	gradation	: linear 0,0 #b2aca5 0,100 #635c52;
	border		: 1 solid #645f57;
	bordertype	: round 3 3;
	margin		: 1 0 3 0;
	imagealign	: center middle;
}
ChildFrame>TitlebarForm>#normalbutton,
ChildFrame>TitlebarForm>#normalbutton:mouseover, ChildFrame>TitlebarForm>#normalbutton:pushed,
ChildFrame>TitlebarForm>#normalbutton:disabled
{
	image		: theme://images/btn_SubTitlebarForm_Normal_N.png;
	imagepadding: 1 0 0 0;
}
ChildFrame>TitlebarForm>#normalbutton:mouseover
{
	gradation	: linear 0,0 #d3aa61 0,100 #614e30;
	border		: 1 solid #7e6c2e;
}
ChildFrame>TitlebarForm>#normalbutton:pushed
{
	background	: #d6ab60;
	border		: 1 solid #685628;
}
ChildFrame>TitlebarForm>#normalbutton:disabled
{
	gradation	: linear 0,0 #b6b5b5 0,100 #878685;
	border		: 1 solid #969696;
}
ChildFrame>TitlebarForm>#closebutton
{
	background	: @gradation;
	gradation	: linear 0,0 #b2aca5 0,100 #635c52;
	border		: 1 solid #645f57;
	bordertype	: round 3 3;
	margin		: 1 0 3 0;
	imagealign	: center middle;
}
ChildFrame>TitlebarForm>#closebutton,
ChildFrame>TitlebarForm>#closebutton:mouseover, ChildFrame>TitlebarForm>#closebutton:pushed,
ChildFrame>TitlebarForm>#closebutton:disabled
{
	image		: theme://images/btn_SubTitlebarForm_Close_N.png;
}
ChildFrame>TitlebarForm>#closebutton:mouseover
{
	gradation	: linear 0,0 #d3aa61 0,100 #614e30;
	border		: 1 solid #7e6c2e;
}
ChildFrame>TitlebarForm>#closebutton:pushed
{
	background	: #d6ab60;
	border		: 1 solid #685628;
}
ChildFrame>TitlebarForm>#closebutton:disabled
{
	gradation	: linear 0,0 #b6b5b5 0,100 #878685;
	border		: 1 solid #969696;
}
