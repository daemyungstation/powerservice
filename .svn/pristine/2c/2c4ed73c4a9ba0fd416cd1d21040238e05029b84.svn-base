/* Tabpage */
Tabpage
{
	margin		: 0 0 0 0;
}
/* Splitter */
Splitter
{
	background	: @gradation;
	gradation	: linear 0,0 #dadad2 100,0 #ffffff;
	border		: 0 none;
	bordertype	: round 3 3;
}
/* Splitter = vertical */
/** LF Splitter */
Splitter[type="vertical"]
{
	background	: @gradation;
	gradation	: linear 0,0 #dadad2 0,100 #ffffff;
	border		: 0 none;
	bordertype	: round 3 3;
}
/* ProgressBar */
ProgressBar
{ 
	background	: #f7f7f5;
	border		: 1 solid #999999;
	font 		: Dotum,9 ;
	color		: #333333 ;
	align		: center middle;
	smooth		: true;
	direction	: forward;
	bartype		: image;
	progressimage 	: theme://images/img_progress.png;
	focusborder	: 0 none ;
}
ProgressBar:disabled
{
	border			: 1 solid #d5d5d5;
	background		: #ffffff ;
	progressimage  	: theme://images/img_progress_D.png;
	color			: #999999;
}
