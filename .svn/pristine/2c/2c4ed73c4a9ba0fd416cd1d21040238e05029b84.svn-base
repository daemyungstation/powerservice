}
Tab>#spinupbutton
{
	border		: 1 solid #a8a8a2;
	bordertype	: round 2 2 lefttop leftbottom;
	background	: @gradation;
	gradation	: linear 0,0 #ffffff 0,100 #deded7;
	image		: theme://images/btn_tabspinup_N.png;
	imagealign	: center middle;
	pusheddrawoffset	: 0 0;
	margin		: -1 -2 0 1;
}
Tab>#spinupbutton:mouseover
{
	border		: 1 solid #a8a8a2;
	gradation	: linear 0,0 #ddddd6 0,100 #adada7;
