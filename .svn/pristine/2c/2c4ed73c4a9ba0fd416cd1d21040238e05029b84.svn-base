/* ApplicationMenu */
ApplicationMenu
{
	background 		: @gradation;
	gradation		: linear 0,0 #fefefe 0,100 #dbdbd3;
	align			: center middle;
	border 			: 1 solid #a8a8a2;
	font 			: Dotum,9;
	color			: #5a280b;
	itemcolor 		: #5a280b;
	itempadding 	: 20 20 20 20 ;
	itembackground 	: transparent;
	itemgradation	: 0,0 #f8fbfdb3 0,100 #92c9f3b3;
	checkboximage 	: URL('theme://images/menu_check.png') ;
	popupbackground : #f2f2ef;
	popupborder 	: 1 solid #a8a8a2;
	popupbordertype : normal;
	popuppadding 	: 0 0 0 0;
	popupcolor 		: #46463d;
	popupfont 		: Dotum,9;
	popupitemheight : 20;
	popupitempadding: 1 6 1 8;
	popupitemborder : 1 solid #fcfcfb, 1 solid #fcfcfb, 1 solid #e7e7e2, 1 solid #fcfcfb;
	popupitembackground 	: #f2f2ef;
	popupitemalign 	: left middle;
	expandimage 	: URL('theme://images/img_TP_GNB_expand_N.png') ;
}
ApplicationMenu:mouseover
{
	itembackground 		: transparent url('theme://images/img_TP_GNB_O.png') stretch 10,0 left top;
	color				: #3da2df;
	popupitembackground	: @gradation;
	popupitemgradation	: linear 0,0 #94bdd5 0,100 #2f89cd [48% #4da3db][50% #3696d7];
	popupitemborder 	: 1 solid #4485ad, 3 double #fcfcfb #4485ad, 1 solid #4485ad, 3 double #fcfcfb #4485ad;
	popupitempadding 	: 1 4 1 6;
	popupcolor			: #ffffff;
	expandimage 		: URL('theme://images/img_TP_GNB_expand_O.png');
}
ApplicationMenu:selected
{
	color			: #ffffff;
	itembackground 	: url('theme://images/bg_applicationmenu_S.png') stretch 12,12 left top;
}
ApplicationMenu:disabled
{
	color			: #999999;
}

