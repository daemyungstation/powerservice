/***** Object Component Start *****/
/* Static */
Static
{
	background	: transparent;
	border		: 0 none ;
	align		: left middle;
	font 		: Dotum,9 ;
	color		: #46463d;
}
Static:disabled	
{
	color		: #aaaaaa ;
}
/* Edit */
Edit
{
	background	: #ffffff;
	border		: 1 solid #a6a6a9, 1 solid #d5d5d5, 1 solid #d5d5d5, 1 solid #a6a6a9;
	align		: left middle;
	font		: Dotum,9 ;
	color		: #333333;
	padding		: 0 5 0 5;
	caretcolor		: #3da2df;
	selectbackground	: #ffffff;
	selectcolor 		: #333333;
}
Edit:mouseover, Edit:focused
{
	background	: #fbf8f1;
	border		: 1 solid #89785e, 1 solid #c6a469, 1 solid #c6a469, 1 solid #89785e;
	color		: #000000;
	caretcolor		: #3da2df;
	compositecolor	: #ffffff;
	selectbackground	: #3da2df;
	selectcolor 		: #ffffff;
}
Edit:disabled
{
	background	: #f7f7f7;
	border		: 1 solid #d5d5d5;
	color		: #999999;
}
