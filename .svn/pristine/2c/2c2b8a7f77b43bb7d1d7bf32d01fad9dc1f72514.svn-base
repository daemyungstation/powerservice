}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
    if (errcd != 0) {
		trace(errcd);
		return false;
	} else {
		switch(svcid){
			case "srchTrgtList"://조회
				gfn_appendSearchDs(ds_list_trgtCustAlct, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_trgtCustAlct, sta_totalCnt, total_count); // 페이지건수 표출
				fn_postSearch();
			break;
			
			case "deleteTrgtList"://삭제
				opener.ds_list_trgtList.setColumn(opener.ds_list_trgtList.rowposition, 'cust_data_extc_cnt', total_cust_data_extc_cnt);
				opener.edt_cust_data_extc_cnt.value = gfn_commaStr(opener.ds_list_trgtList.getColumn(opener.ds_list_trgtList.rowposition, 'cust_data_extc_cnt'))+" 건"; //목록건수
				opener.ds_list_subTrgtList.setColumn(opener.ds_list_subTrgtList.rowposition,'cust_data_extc_cnt',
													parseInt(opener.ds_list_subTrgtList.getColumn(opener.ds_list_subTrgtList.rowposition,'cust_data_extc_cnt')) - delete_count);
				opener.fn_copyRow_sub();
				opener.fn_changeComponent_sub("R");
				
				delete_count = 0;
				fn_commSearch(gfn_getCommTypeInit()); // 조회
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		ds_list_trgtCustAlct.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "trgt_list_id", trgt_list_id);
		ds_inqCndt.setColumn(0, "sub_trgt_list_id", sub_trgt_list_id);
		ds_inqCndt.setColumn(0, "searchType", cbo_srchTyp.value);
		ds_inqCndt.setColumn(0, "searchValue", edt_srchVal.value);
		ds_inqCndt.setColumn(0, "cmpg_typ_cd", cmpg_typ_cd);
		ds_inqCndt.setColumn(0, "cmpg_typ_cd2",cmpg_typ_cd2);
		ds_inqCndt.setColumn(0, "del_yn",chk_del_yn.value);
		
		if(cmpg_typ_cd =='2000'||cmpg_typ_cd =='3000'){//미납
			
			ds_inqCndt.setColumn(0, "pay_mthd", cbo_srch_pay_mthd.value);
			ds_inqCndt.setColumn(0, "ichae_dt", edt_ichae_dt.value);
			
			ds_inqCndt.setColumn(0, "stt_month_cnt", edt_stt_month_cnt.value);
			ds_inqCndt.setColumn(0, "end_month_cnt",  edt_end_month_cnt.value);
			ds_inqCndt.setColumn(0, "stt_true_cnt", edt_stt_true_cnt.value);
			ds_inqCndt.setColumn(0, "end_true_cnt",  edt_end_true_cnt.value);
			ds_inqCndt.setColumn(0, "stt_pay_state", edt_stt_pay_state.value);
			ds_inqCndt.setColumn(0, "end_pay_state",  edt_end_pay_state.value);
		}
		ds_inqCndt.setColumn(0, "orderBy", "cust_nm");
		ds_inqCndt.setColumn(0, "orderDirection", "asc");
	}
	 
	return true;
}

function fn_preSrch(){
	if(cmpg_typ_cd =='2000' || cmpg_typ_cd == '3000'){
		if(!gfn_isNull(edt_stt_month_cnt.value)){
			if(gfn_isNull(edt_end_month_cnt.value)){
				alert("당월회차를 입력해 주세요.");
				return false;
			}
		}
		
		if(!gfn_isNull(edt_end_month_cnt.value)){
			if(gfn_isNull(edt_stt_month_cnt.value)){
				alert("당월회차를 입력해 주세요.");
				return false;
			}
		}
		
		if(edt_stt_month_cnt.value > edt_end_month_cnt.value){
			alert("당월회차를 확인해 주세요.");
			return false;
		}
		
		
		if(!gfn_isNull(edt_stt_true_cnt.value)){
			if(gfn_isNull(edt_end_true_cnt.value)){
				alert("실납입회차를 입력해 주세요.");
				return false;
			}
		}
		
		if(!gfn_isNull(edt_end_true_cnt.value)){
			if(gfn_isNull(edt_stt_true_cnt.value)){
				alert("실납입회차를 입력해 주세요.");
				return false;
			}
		}
		
		if(edt_stt_true_cnt.value > edt_end_true_cnt.value){
			alert("실납입회차를 확인해 주세요.");
			return false;
		}
		
		if(!gfn_isNull(edt_stt_pay_state.value)){
			if(gfn_isNull(edt_end_pay_state.value)){
				alert("연체회차를 입력해 주세요.");
				return false;
			}
		}
		
		if(!gfn_isNull(edt_end_pay_state.value)){
			if(gfn_isNull(edt_stt_pay_state.value)){
				alert("연체회차를 입력해 주세요.");
				return false;
			}
		}
		
		if(edt_stt_pay_state.value > edt_end_pay_state.value){
			alert("연체회차를 확인해 주세요.");
			return false;
		}
    }
    return true;
}

function fn_preDelete(){
	var trgtCustDtptIds = "";
	for(var i=ds_list_trgtCustAlct.rowcount-1;i>=0;i--) {
		if(ds_list_trgtCustAlct.getColumn(i, "_chk") == 1) {
			if(ds_list_trgtCustAlct.getColumn(i, 'delete_fg') == 'Y'){
				if (nobdIds == "") {
					trgtCustDtptIds = ds_list_trgtCustAlct.getColumn(i, "trgt_cust_dtpt_id");
				} else {
					trgtCustDtptIds += "," + ds_list_trgtCustAlct.getColumn(i, "trgt_cust_dtpt_id");
				}
				ds_list_trgtCustAlct.deleteRow(i);
			}else{
				alert("삭제가 불가능한 행이 포함되어 있습니다.");
				ds_list_trgtCustAlct.reset();
				return false;
			}
		}
	}
	return trgtCustDtptIds;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType)
{	
    if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_trgtCustAlct, sta_totalCnt, 300, total_count)) return; 
	
	var szSvcid       = "srchTrgtList";
    var szController  = "/campaign/trgt-cust/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

//삭제
function fn_deleteData(sType)
{	
	for(var i=ds_list_trgtCustAlct.rowcount-1;i>=0;i--) {
		if(ds_list_trgtCustAlct.getColumn(i, "_chk") == 1) {
			if(ds_list_trgtCustAlct.getColumn(i, 'delete_fg') == 'Y'){
				++delete_count;
				//ds_list_trgtCustAlct.setRowType(i,'D');
				ds_list_trgtCustAlct.deleteRow(i);
			}else{
				alert("삭제가 불가능한 행이 포함되어 있습니다.");
				delete_count = 0;
				ds_list_trgtCustAlct.reset();
				return false;
			}
		}
	}
	
	var szSvcid       = "deleteTrgtList";
	var szController  = "/campaign/trgt-cust/delete";
	var szInDs        = "ds_input=ds_list_trgtCustAlct::U";
	var szOutDs       = "";
	var szParam       = "sub_trgt_list_id="+ sub_trgt_list_id + " trgt_list_id="+trgt_list_id+" cmpg_typ_cd="+cmpg_typ_cd
	                    +" cmpg_typ_cd2="+cmpg_typ_cd2;
	var szRetCallback = "fn_callBack";
	
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

