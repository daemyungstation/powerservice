function fn_unpyComponent(sFg){
	//납입방법
	stc_paymthd.visible = sFg;
	cbo_srch_pay_mthd.visible = sFg;
	
	//이체일자
	stc_ichae_dt.visible = sFg;
	edt_ichae_dt.visible = sFg;
	
	//당월회차
	stc_month_cnt.visible = sFg;
	edt_stt_month_cnt.visible = sFg;
	sta_andMonthCnt.visible = sFg;
	edt_end_month_cnt.visible =sFg;
	
	//실납입 회차
	stc_true_cnt.visible = sFg;
	edt_stt_true_cnt.visible = sFg;
	sta_andTrueCnt.visible = sFg;
	edt_end_true_cnt.visible = sFg;
	
	//연체회차
	sta_pay_mthd.visible = sFg;
	edt_stt_pay_state.visible= sFg;
	sta_andPayState.visible= sFg;
	edt_end_pay_state.visible= sFg;
	
	if(sFg == false){ // 유형별 조회조건이 다르기 때문에 추후 변경될수 있다. (억지로 셋팅 위치변경)
		stc_05.position.bottom = cbo_srchTyp.position.bottom + 10
		btn_search.position.top = cbo_srchTyp.position.top;
		btn_search.position.bottom = cbo_srchTyp.position.bottom;
		btn_init.position.top = cbo_srchTyp.position.top;
		btn_init.position.bottom = cbo_srchTyp.position.bottom;
		
		btn_delete.position.top = stc_05.position.bottom +5;
		btn_delete.position.height = 20;
		
		sta_totalCnt.move(sta_totalCnt.position.left,stc_05.position.bottom +7);
		grd_list_trgtCust.position.top = btn_delete.position.bottom +3;
		stc_05.bringToPrev
	}
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/

//버튼클릭이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	 switch(obj){
		case btn_close://닫기버튼
			close();
		break;
		
		case btn_search: //조회버튼
			if(fn_preSrch()){
				fn_commSearch(gfn_getCommTypeInit()); // 조회
			}
		break;
		
		case btn_init: //초기화버튼
			fn_init();
		break;
		
		case btn_delete : //삭제버튼
			if(confirm("해당 내용을 삭제 하시겠습니까?")){
				fn_deleteData();
			}
		break;
	}
}

//정렬
function grd_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj){
		case grd_list_trgtCust:
			if (!fn_preSearch()) return false;
			ds_inqCndt.setColumn(0, "sController", "/campaign/trgt-cust/list");
			ds_inqCndt.setColumn(0, "dsName", "ds_output");
			if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_trgtCustAlct, sta_totalCnt, 300, total_count)) return;
			gfn_grdHeadClick(obj, e, ds_inqCndt);
			gfn_setTotalRowCount(ds_list_trgtCustAlct, sta_totalCnt, total_count); // 페이지건수 표출
		break;
		
		default :
			alert("obj 를 확인하세요");
		break ;
	}
}

//스크롤 오버
function grd_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}


//조회 버튼 
function cbo_onkeydown(obj:Combo, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if(fn_preSrch()){
			fn_commSearch(gfn_getCommTypeInit()); // 조회
		}
	}
	
}

function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if(fn_preSrch()){
			fn_commSearch(gfn_getCommTypeInit()); // 조회
		}
	}
}
