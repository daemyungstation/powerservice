/* 1. Include Library include                                                               */
/********************************************************************************************/
include "LIB::lib_All.xjs";


/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트,전역변수,초기화) 처리                         	*/
/********************************************************************************************/
var total_count = 0;
var total_count_pgmIndvFnct = 0;
var maxOrder = 0;
var dupCount = 0;
var dupFlag = false;

//권한
var rgsnYn; //등록권한
var inqYn; //조회권한
var amntYn; //수정권한
var dltnYn; //삭제권한
var pritYn; //출력권한
var xlsYn; //엑셀권한
var athrPpupYn; //팝업오픈권한
var xlsRrnoYn; //엑셀 개인정보 권한 여부 

//온로드
function PS090300_onload(obj:Form, e:LoadEventInfo)
{
	gfn_athrPgm(gfn_gdsCd_admin());
	
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_commSearch();//트리조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
//콜백
function fn_callBack(svcid, errcd, errmsg) {
    if (errcd != 0) {
		trace("errcd  :   " +errcd +"   \n errmsg   :   "+ errmsg +"    \n svcid   :   "+ svcid)
		return false;
	} else {
		switch (svcid) {
			case "getTreeView" : //트리뷰 조회
				gfn_appendSearchDs(ds_list_tree, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				ds_list_tree.rowposition = -1;
				fn_commSearch_pgm(gfn_getCommTypeInit());//조회 
			break;
			
			case "srchPgm": // 프로그램 조회
				gfn_appendSearchDs(ds_list_pgm, ds_list_tmp_pgm); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_pgm, sta_totalCnt, total_count); // 페이지건수 표출
				fn_postSearch();
			break;
			
			case "srchSequence": //순서 리스트
				ds_orderList.clearData();
				for(var i = maxOrder; i > 0; i--){
					ds_orderList.addRow();
					ds_orderList.setColumn(ds_orderList.rowposition,"name",i);
					ds_orderList.setColumn(ds_orderList.rowposition,"id",i);
				}
			break;
			
			case "deletePgm": //삭제
				ds_list_tree.clearData();
				fn_commSearch();
				fn_commSearch_pgm(gfn_getCommTypeInit());//조회 
			break;
			
			case "deletePgmIndvFnct" : //개별기능 삭제
				fn_commSearch_pgmIndvFnct(gfn_getCommTypeInit());//개별기능 조회 
			break;
			
			case "savePgm": //저장
				fn_commSearch_pgm(gfn_getCommTypeInit());//조회
				ds_list_tree.clearData();
				fn_commSearch();
				dupCount = 0;
				dupFlag = false;
			break;
			
			case "savePgmIndvFnct" ://개별기능 저장
				fn_commSearch_pgmIndvFnct(gfn_getCommTypeInit());//개별기능 조회 
			break;
			
			case "srchDupCount": //중복카운트
				if(dupCount > 0 && confirm("중복되는 순서가 있습니다. 순서를 자동증가하고 등록하시겠습니까?\n자동증가된 순서는 목록을 다시 조회하면 표시됩니다.")){
					dupFlag = true;
				}
			break;
			
			case "srchPgmIndvFnct": //개별기능 리스트 조회
				gfn_appendSearchDs(ds_list_pgmIndvFnct, ds_list_tmp_pgmIndvFnct); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_pgmIndvFnct, sta_totalCnt_pgmIndvFnct, total_count_pgmIndvFnct); // 페이지건수 표출
				fn_postSearch_pgmIndvFnct();
			break;
			
			case "srchPgmAthr" :
			
			break;
		}	
	}
}


/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
