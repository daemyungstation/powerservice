//조회 결과 
function fn_postSearch()
{
	if(ds_list_pgm.rowcount < 1) {
		sta_totalCnt.text ="조회 결과가 없습니다.";
 		fn_copyRow();
 		fn_setSequenceList();
 		fn_changeComponent("R"); //컴포넌트 셋팅
 	} else {
 		fn_copyRow();
 		fn_setSequenceList();
 		fn_changeComponent("R"); //컴포넌트 셋팅
	}
	return;
}

//조회결과 개별기능
