//sta클릭이벤트
function sta_onclick(obj:Static,  e:ClickEventInfo)
{
	switch(obj){
		case sta_collapse:
			this.grd_tree.treeinitstatus = "collapse,null";
		break;
		
	    case sta_expand:
			this.grd_tree.treeinitstatus = "expand,null";
	    break;
	}
}

//콤보변환 이벤트
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(obj){
		case cbo_gds_cd : //프로그램 콤보변환시 tree 검색
			ds_list_tree.clearData();
			fn_commSearch();
		break;
	}
}


//그리드 셀클릭 이벤트
function grd_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj){
		case grd_tree:
			fn_commSearch_pgm(gfn_getCommTypeInit());
		break;
		
		case grd_list:
			fn_copyRow();
			fn_setSequenceList();
			fn_changeComponent("R");
		break;
		
		case grd_list_pgmIndvFnct: //개별기능
			fn_copyRow_pgmIndvFnct();
			fn_changeComponent_pgmIndvFnct("R");
		break;
	}
}

//그리드 해더클릭 이벤트
function grd_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj){
		case grd_tree:
			ds_list_tree.rowposition = -1;
			fn_commSearch_pgm(gfn_getCommTypeInit());//조회 
		break;
	}
}

//버튼클릭이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
		case btn_create : //신규버튼
			ds_pgm.clearData();
			ds_pgm.addRow();
			ds_pgm.setColumn(ds_pgm.rowposition, 'gds_cd', cbo_gds_cd.value);
			ds_pgm.setColumn(ds_pgm.rowposition, 'hgrn_pgm_cd', ds_list_tree.getColumn(ds_list_tree.rowposition , "pgm_cd"));
			if(ds_list_tree.rowposition == -1){
				ds_pgm.setColumn(ds_pgm.rowposition, 'lvl_cnt', "1");
			}else{
				ds_pgm.setColumn(ds_pgm.rowposition, 'lvl_cnt', parseInt(ds_list_tree.getColumn(ds_list_tree.rowposition , "lvl_cnt"))+1);
			}
			//개별기능 클리어
			ds_list_pgmIndvFnct.clearData();
			ds_pgmIndvFnct.clearData();
			sta_totalCnt_pgmIndvFnct.text = "검색결과가 없습니다.";
			
			// 순서 +1 추가
			ds_orderList.insertRow(0);
			ds_orderList.setColumn(ds_orderList.rowposition,"name",parseInt(maxOrder)+1);
			ds_orderList.setColumn(ds_orderList.rowposition,"id",parseInt(maxOrder)+1);
			//old 순서 셋팅
			fn_setOldOrder();
			//컴포넌트 변경
			fn_changeComponent("C");
		break;
		
		case btn_create00 : //개별기능 신규버튼
			ds_pgmIndvFnct.clearData();
			ds_pgmIndvFnct.addRow();
			ds_pgmIndvFnct.setColumn(ds_pgm.rowposition, 'gds_cd', cbo_gds_cd.value);
			ds_pgmIndvFnct.setColumn(ds_pgm.rowposition, 'pgm_cd', ds_list_pgm.getColumn(ds_list_pgm.rowposition, 'pgm_cd'));
			fn_changeComponent_pgmIndvFnct("C");
		break;
		
		case btn_update : //수정버튼
			//개별기능 클리어
			ds_list_pgmIndvFnct.clearData();
			ds_pgmIndvFnct.clearData();
			sta_totalCnt_pgmIndvFnct.text = "검색결과가 없습니다.";
			
			//old 순서 셋팅
			fn_setOldOrder();
			fn_changeComponent("U");
		break;
		
		case btn_update00 : //개별기능 수정버튼
			fn_changeComponent_pgmIndvFnct("U");
		break;
		
		case btn_delete ://삭제버튼
			if(confirm("삭제하시겠습니까?")) {
				fn_deleteData();
			}
		break;
		
		case btn_delete00 ://개별기능 삭제버튼
			if(confirm("삭제하시겠습니까?")) {
				fn_deleteData_pgmIndvFnct();
			}
		break;
		
		case btn_save ://저장버튼
			fn_saveData();
		break;
		
		case btn_save00 ://개별기능 저장버튼
			fn_saveData_pgmIndvFnct();
		break;

		case btn_cancel ://취소버튼
			fn_copyRow();
			fn_setSequenceList();
			fn_changeComponent("R");
		break;
		
		
		case btn_cancel00 ://개별기능 취소버튼
			fn_copyRow_pgmIndvFnct();
			fn_changeComponent_pgmIndvFnct("R");
		break;
	}
}

