//리스트 데이터셋 뷰 데이터셋에 카피
function fn_copyRow(){
	if(ds_list_pgm.rowcount<1){
		ds_pgm.clearData();
		ds_pgm.addRow();
		ds_list_pgmIndvFnct.clearData();
		ds_pgmIndvFnct.clearData();
		sta_totalCnt_pgmIndvFnct.text = "조회 결과가 없습니다."
	}else{
		ds_pgm.clearData();
		ds_pgm.addRow();
		ds_pgm.copyRow(ds_pgm.rowposition,ds_list_pgm,ds_list_pgm.rowposition);
		
		fn_commSearch_pgmIndvFnct(gfn_getCommTypeInit());//개별기능 조회 
		
		//등록 수정일시 데이터타입에 맞게
		var rgsnDttmStr = edt_rgsn_dttm.value;
		edt_rgsn_dttm.value = gfn_strDttm(rgsnDttmStr);
			
		var amntDttmStr = edt_amnt_dttm.value;
		edt_amnt_dttm.value = gfn_strDttm(amntDttmStr);
	}
}


