//그리드 해더클릭 이벤트 
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	switch(obj){
		case grd_list: //프로그램 그리드
			if (!fn_preSearch_pgm()) return false;
			ds_inqCndt_pgm.setColumn(0, "sController", "/syst/pgm/list");
			ds_inqCndt_pgm.setColumn(0, "dsName", "ds_output");
			
			if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_pgm, sta_totalCnt, 50, total_count)) return;
			
			gfn_grdHeadClick(obj, e, ds_inqCndt_pgm);
			
			gfn_setTotalRowCount(ds_list_pgm, sta_totalCnt, total_count); // 페이지건수 표출
			
			fn_copyRow();
			fn_setSequenceList();
			fn_changeComponent("R"); //컴포넌트 셋팅
		break;
		
		case grd_list_pgmIndvFnct : //개별권한 그리드 
			if (!fn_preSearch_pgmIndvFnct()) return false;
			ds_inqCndt_pgmIndvFnct.setColumn(0, "sController", "/syst/pgm-indv-fnct/list");
			ds_inqCndt_pgmIndvFnct.setColumn(0, "dsName", "ds_output");
			
			if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_pgmIndvFnct, sta_totalCnt_pgmIndvFnct, 50, total_count_pgmIndvFnct)) return;
			
			gfn_grdHeadClick(obj, e, ds_inqCndt_pgmIndvFnct);
			
			gfn_setTotalRowCount(ds_list_pgmIndvFnct, sta_totalCnt_pgmIndvFnct, total_count_pgmIndvFnct); // 페이지건수 표출
			
			fn_copyRow_pgmIndvFnct();
			fn_changeComponent_pgmIndvFnct("R");
		break;
	}
}
