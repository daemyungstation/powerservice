//콜백
function fn_callBack(svcid, errcd, errmsg) {
  
	if (errcd < 0) {
		trace("svcid ::: "+svcid+"   /errmsg :::   "+errmsg);
	} else {
		switch(svcid){
			case "srchCertfInfo":
				if (ds_certf_mng.rowcount > 0) {
					tab_Menu.tabpage1.ActiveX01_01.Document.body.innerHTML= "<font style='font-size:12'>" + ds_certf_mng.getColumn(ds_certf_mng.rowposition,'terms_svc') + "</font>";
