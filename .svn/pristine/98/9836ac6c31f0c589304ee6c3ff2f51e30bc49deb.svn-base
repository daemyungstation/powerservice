// 회원 조회 콜백 함수
function fn_setAccntNo(){
	
	if (ds_accnt_info.rowcount < 1) {
		if ("btn_accnt_no" == fv_accnt_search_button) {
			tab_main.tabpage2.div_input.edt_accnt_no.value = "";
			tab_main.tabpage2.div_input.edt_mem_nm.value = "";
		}
	} 
	else {
		if ("btn_accnt_no" == fv_accnt_search_button) {
			var accnt_no 	= ds_accnt_info.getColumn(0, "accnt_no");
			var mem_nm 		= ds_accnt_info.getColumn(0, "mem_nm");
			var prod_cd 	= ds_accnt_info.getColumn(0, "prod_cd");
			var prod_nm 	= ds_accnt_info.getColumn(0, "prod_nm");			
			var emple_nm 	= ds_accnt_info.getColumn(0, "emple_nm");
			var join_stat 	= ds_accnt_info.getColumn(0, "join_stat");
			var pay_stat 	= ds_accnt_info.getColumn(0, "pay_stat");
			var join_dt 	= ds_accnt_info.getColumn(0, "join_dt");
			var tel 		= ds_accnt_info.getColumn(0, "wrpl_tel");
			
			tab_main.tabpage2.div_input.edt_accnt_no.value = accnt_no;
			tab_main.tabpage2.div_input.edt_mem_nm.value = mem_nm;
			tab_main.tabpage2.div_input.edt_prod_nm.value = prod_nm;
			ds_t2_mem.setColumn(0, "prod_cd", prod_cd);
			tab_main.tabpage2.div_input.edt_emple_nm.value = emple_nm;
			tab_main.tabpage2.div_input.edt_pay_stat.value = pay_stat;
			tab_main.tabpage2.div_input.edt_join_stat.value = join_stat;
			tab_main.tabpage2.div_input.cal_join_dt.value = join_dt;
			tab_main.tabpage2.div_input.edt_tel.value = tel;
		}
	}
}

