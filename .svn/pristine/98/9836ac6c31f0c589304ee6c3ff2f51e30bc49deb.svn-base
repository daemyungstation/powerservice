function fn_getImageFile(pic_file_nm, objImg) {	

	var sUrl = application.services["SVC"].url;
	var fileUrl = sUrl + "dlw/evnt/getClassPhoto?fileCode="
	
	//var fileUrl = application.services["fileUrl"].url;	
	//    fileUrl = fv_eventImgViewUrl + "?fileCode=" + pic_file_nm + "&dir=" + gfn_getEventPicPath();
	
	pic_file_nm = encodeURIComponent(pic_file_nm);
	fileUrl = fileUrl + pic_file_nm;
	
	//fileUrl 	= fv_eventImgViewUrl + "?fileCode=" + pic_file_nm + "&dir=" + gfn_getEventPicPath();
	trace("fileUrl : " + fileUrl);
	objImg.image = fileUrl;
}

