	gfn_initForm(obj);
	
	// 지부 조회
	// fn_getBranchList();
	
	// gfn_getList("dlwCd","02","fn_callBack",false);   // 대명코드조회 (02:행사종류)
	fn_init(); // 검색부 초기화
	
	fn_setAthrPgmBtn();	
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			
			// gfn_getCode("02", "ALL", ds_evt_typ, gds_DmlCd); // 코드조회결과 필터
			// cbo_event.index = 0;			
			// fn_init(); // 검색부 초기화
			break;
		
		case "search":
			//저장 및 수정시 새창 안뜨도록 >> 추후 처리
			btn_search.enable = true;
			fv_searchCallback();
			break;
		
		case "disconncectAccnt":
			fn_postDisconncectAccnt();
			break;
			
		case "conncectAccnt":
			fn_postConncectAccnt();
			break;

	}
}

// 선택파일 회원연결 전 처리
function fn_preConncectAccnt()
{
	
	var cnt = ds_t2_list.getCaseCount("chk == '1'");
	if (cnt < 1) {
		alert("선택한건이 없습니다.");
		return false;
	}	
	
	var accnt_no = tab_main.tabpage2.div_input.edt_accnt_no.value;
	if ( gfn_isEmpty(accnt_no) ) {
		alert("회원을 선택하세요.");
		return false;
	}
	
	ds_cond.clearData();
	var nPos = ds_cond.addRow();
	ds_cond.setColumn(nPos, "accnt_no", accnt_no);
	
	return true;
}



//선택파일 회원연결
// asis : conncectAccnt
function fn_conncectAccnt(obj, e)
{
	// 유효성 체크
	if ( !fn_preConncectAccnt() ) {
		return;
	}
	
	var szSvcid       = "conncectAccnt";
    var szController  = "/dlw/evnt/conncectAccnt";
    var szInDs        = "ds_input=ds_t2_list:U ds_input2=ds_cond";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 선택파일 회원연결 후 처리
function fn_postConncectAccnt() {
	alert("분류가 완료되었습니다.");
	
	ds_t2_mem.clearData();
	ds_t2_mem.addRow();
	
	fn_search();
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch()
{
	ds_cond.clearData();
	ds_cond.addRow();	
	
	var condCnt		= 0;
	var sFrDt 		= "";
	var sToDt 		= "";	 
	var sEvtMngrCd	= edt_evt_mngr_cd.value;
	
	if ( !gfn_isEmpty(cal_start_dt.value) ) {
		sFrDt = cal_start_dt.value;
	}	
	if ( !gfn_isEmpty(cal_end_dt.value) ) {
		sToDt = cal_end_dt.value;
	}
	
	if ( gfn_isEmpty(sFrDt) ) {
		alert("검색기간 시작일자는 필수 입력입니다.");
		cal_start_dt.setFocus();
		return false;
	}
	if ( gfn_isEmpty(sToDt) ) {
		alert("검색기간 종료일자는 필수 입력입니다.");
		cal_end_dt.setFocus();
		return false;
	}
	
	ds_cond.setColumn(0, "stt_dt"		, sFrDt				);
	ds_cond.setColumn(0, "end_dt"		, sToDt				);	
	ds_cond.setColumn(0, "evt_mngr_cd"	, sEvtMngrCd		);
	ds_cond.setColumn(0, "accnt_clsfy"	, tab_main.tabindex	);
	ds_cond.setColumn(0, "accnt_no"		, edt_accnt_no.value);
	
	if (tab_main.tabindex == 0) {
		//
    } else if (tab_main.tabindex == 1) {
		tab_main.tabpage1.img_view.image = "";
    } else if (tab_main.tabindex == 2) {
		tab_main.tabpage2.img_view.image = "";
		ds_cond.setColumn(0, "accnt_no"		, "");
    }
	
	return true;
}

//조회
function fn_search(obj, e)
{	
	// 호출 건수 설정 및 조회여부 설정
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "search";    
    var szController  = "/dlw/evnt/getEvntPicList"; // 의전행사 사진 분류 - 행사회원 조회
    
    if (tab_main.tabindex == 1) {
		szController  = "/dlw/evnt/getEvntPicSort"; // 의전행사 사진 분류 - 분류/미분류회원 조회
    } 
    else if (tab_main.tabindex == 2) {
		szController  = "/dlw/evnt/getEvntPicSort"; // 의전행사 사진 분류 - 분류/미분류회원 조회
    }
    
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "";
    
    if (tab_main.tabindex == 0) {
		szOutDs = "ds_t0_list=ds_output";
		fv_searchCallback = fn_postSearch0;
    } else if (tab_main.tabindex == 1) {
		szOutDs = "ds_t1_list=ds_output";
		fv_searchCallback = fn_postSearch1;
    } else if (tab_main.tabindex == 2) {
		szOutDs = "ds_t2_list=ds_output";
		fv_searchCallback = fn_postSearch2;
    } else {
		return;
    }
    
    btn_search.enable = false;
    
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

