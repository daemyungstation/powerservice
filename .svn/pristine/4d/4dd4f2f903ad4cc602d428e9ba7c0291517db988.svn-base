function tab_cms_onlbuttonup(obj:Tab, e:TabMouseEventInfo)
{		
	if(e.fromobject == "[object Tab]")
	{			
	    if (obj.tabindex == 0) {
		    fn_srchCnctList();
	    } 
		else if (obj.tabindex == 1) {
			fn_srchPyinHstr();
		} 
		else if (obj.tabindex == 2) {
			fn_srchPymnHstr();
		} 
		else if (obj.tabindex == 3) {
			fn_srchLgUplusPrepay();
		}
		else if (obj.tabindex == 4) {
			fn_srchPayTotInfo();
		}
		else if (obj.tabindex == 5) {
			fn_srchProdOptSvc();
		}
		else if (obj.tabindex == 6) {
			fn_srchAskHstr();
		} 
		else if (obj.tabindex == 7){
			fn_srchWdrwLog();
		}
		
		fn_setVisibleTabDtl();
				
	}
}

