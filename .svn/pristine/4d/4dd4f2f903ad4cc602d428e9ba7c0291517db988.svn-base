/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS02000106_onload(obj:Form, e:LoadEventInfo)
{
    fn_setVisibleTabDtl();
    
    parent.div_product.gv_accnt_no = gv_accnt_no;
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			// 해약정보 조회결과
			case "srchCnctList":
			    
				if (ds_list_cnct.rowcount > 0) {
					ds_list_cnct.rowposition = ds_list_cnct.rowcount - gv_rntm_cnt; // 납입회차 자동 선택
				}

				var cnt = 0;
				var hamt = 0;
				cnt = ds_list_cnct.getColumn(0, "no");
				hamt = ds_list_cnct.getColumn(0, "resn_amt");
				
				var hcamt = String(cnt) + "회 / " + gfn_commaStr(gfn_transNullToNumber(hamt));
				edt_hcamt.value = hcamt;
				
			break;
			
		}
	}
}

