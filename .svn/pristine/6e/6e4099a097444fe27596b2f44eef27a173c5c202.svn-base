/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var tc_chat_sndg_hstr = 0; // 문자발송이력 전체건수

// 권한 설정
var rgsnYn; 	// 등록권한
var amntYn; 	// 수정권한
var dltnYn; 	// 삭제권한
var xlsYn; 		// 엑셀권한

var gvMenuId;  // 프로그램 로그 추가 2017.12.14
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS040907_onload(obj:Form, e:LoadEventInfo)
{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.12 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	
	gfn_athrPgm(gfn_gdsCd_admin());
	fn_init();
	gfn_createTeamUserInqDs(obj); // 팀에 따른 사용자 조회를 위한 데이터셋 설정
	fn_commSearch(gfn_getCommTypeInit());
	
	// (윌앤비전:T20100, 모든이에:T20200, 비투퀄리티:T20300)
	var sTeamCd = gds_UserInfo.getColumn(0, "bzpt_div");
	// var sTeamCd = "T20100";
	if (sTeamCd.substr(0, 2) == "T2") {
		ds_hgrnTeamCd.addRow();
		ds_hgrnTeamCd.setColumn(0, "team_cd", "T20000");
		ds_hgrnTeamCd.setColumn(0, "team_nm", "외주TM업체");
		cbo_srch_hgrn_team_cd.index = 0;
		cbo_srch_hgrn_team_cd.enable = false;
		
		if (sTeamCd == "T20100") {
			sTeamNm = "윌앤비전";
		} else if (sTeamCd == "T20200") {
			sTeamNm = "모든이에";
		} else {
			sTeamNm = "비투퀄리티";
		}
		ds_teamCd.addRow();
		ds_teamCd.setColumn(0, "team_cd", sTeamCd);
		ds_teamCd.setColumn(0, "team_nm", sTeamNm);
		cbo_srch_team_cd.index = 0;
		cbo_srch_team_cd.enable = false;

		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_teamCd, "ALL");
	} else {
		gfn_getList("teamCd","", null, null, null, "ds_allTeamCd"); // 팀코드 조회
	}
	
	// 엑셀 버튼 표시 처리
	var sExcelAthrYn = rgsnYn; // "N";
	if (sExcelAthrYn == "N") {
	    btn_excel.visible = false;
	    btn_excel_upload.visible = false;
	    
	    btn_grdInit.position2.right = 2;
	}
	
	// 엑셀권한 
	if (xlsYn != "Y") {
		btn_excel.visible = false;
 	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		// alert(errcd);
		return false;
	}
	
	switch (svcid) {
		case "selectTeamCode":
			gfn_getTeamCodeLoad("", "ALL", ds_hgrnTeamCd, ds_allTeamCd, "1");
			cbo_srch_hgrn_team_cd.index = 0;
		break;
		
		case "selectUserCode":
			gfn_getUserCdLoad("", "" ,"ALL", ds_userId);
	
			cbo_srch_user_id.index = 0;
			
			if (ds_userId.rowcount > 1) {
				cbo_srch_user_id.enable = true;
			} else {
				cbo_srch_user_id.enable = false;
			}
		break;
		
		case "srchChatSndgHstr": // 문자발송이력 조회
		    // 휴대전화 포맷 설정
		    for (var i = 0; i < ds_list_tmp.rowcount; i++) {
		        ds_list_tmp.setColumn(i, "cell", gfn_phoneNumStr(ds_list_tmp.getColumn(i, "cell")));
		    }
		    gfn_appendSearchDs(ds_list_chatSndgHstr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		    
			gfn_setTotalRowCount(ds_list_chatSndgHstr, sta_totalCnt, tc_chat_sndg_hstr); // 페이지건수 표출
			fn_postSearch();
		break;
		
		case "srchChatSndgHstrExcel": // 문자발송이력 조회
		    gfn_exportGridToExcel(grd_list_excel, "문자발송이력_" + gfn_getTodayFull());
		break;
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
	
		if (cal_srch_stt_dt.value == null|| cal_srch_end_dt.value == null){
			alert("전체 조회의 경우 시스템 부하로 인하여 제한합니다.(전산팀)");
			return false;
		}

		if (gfn_getDayDiff(cal_srch_stt_dt.value, cal_srch_end_dt.value) > 31) {
			alert("날짜 범위는 31일을 초과 할 수 없습니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_chatSndgHstr.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		
		ds_inqCndt.setColumn(0, "cell", edt_srch_cell.value);
		ds_inqCndt.setColumn(0, "mem_nm", edt_srch_mem_nm.value);
		ds_inqCndt.setColumn(0, "hgrn_team_cd", cbo_srch_hgrn_team_cd.value);
		ds_inqCndt.setColumn(0, "team_cd", cbo_srch_team_cd.value);
		ds_inqCndt.setColumn(0, "sndg_chpr_id", cbo_srch_user_id.value);
		ds_inqCndt.setColumn(0, "chat_sndg_stat_cd", cbo_srch_chat_sndg_stat_cd.value);
		ds_inqCndt.setColumn(0, "chat_sndg_resl_cd", cbo_srch_chat_sndg_resl_cd.value);
		
		ds_inqCndt.setColumn(0, "orderBy", "sndg_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
		
		// 엑셀 조회조건
		ds_inqCndt_excel.clearData();
		ds_inqCndt_excel.addRow();
		ds_inqCndt_excel.copyRow(0, ds_inqCndt, ds_inqCndt.rowposition);
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 문자발송이력 조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if (!gfn_pageSearch(sType, ds_list_chatSndgHstr, sta_totalCnt, 30, tc_chat_sndg_hstr)) return;
	
	var szSvcid       = "srchChatSndgHstr";
    var szController  = "/chnl/chat-sndg-hstr/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	cbo_srch_dateTyp.index = 1;
	fn_changeDateType();
	
	edt_srch_cell.value = "";
	edt_srch_mem_nm.value = "";
	
	cbo_srch_chat_sndg_stat_cd.index = 0;
	cbo_srch_chat_sndg_resl_cd.index = 0;
	
	// (윌앤비전:T20100, 모든이에:T20200, 비투퀄리티:T20300)
	var sTeamCd = gds_UserInfo.getColumn(0, "bzpt_div");
	if (sTeamCd.substr(0, 2) == "T2") {
		cbo_srch_user_id.index = 0;
	} else {
		gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id);
	}
	
	/*
	cbo_srch_user_id.index = 0;
	cbo_srch_user_id.enable = false;
	cbo_srch_team_cd.index = 0;
	cbo_srch_team_cd.enable = false;
	cbo_srch_hgrn_team_cd.index = 0;
	*/
}

