// 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/chnl/chat-sndg-hstr/list"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명
	
	if (!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_chatSndgHstr, sta_totalCnt, 30, tc_chat_sndg_hstr)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_chatSndgHstr, sta_totalCnt, tc_chat_sndg_hstr); // 페이지건수 표출
	
	fn_setChatSndgHstr();
}

