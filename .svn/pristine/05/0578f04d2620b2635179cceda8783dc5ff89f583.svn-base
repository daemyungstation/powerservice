/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL460200_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	
	fn_setAthrPgmBtn();
	
    gfn_getList("dlwCd","01,95","fn_callBack",false);   // 대명코드조회 (01:회원1회분상태,85:상품구분,95:구분,0125:영업형태)
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
			alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} else {
		switch (svcid) {
		    // 공통
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("01", "ALL", ds_mem_stat, gds_DmlCd); 
			gfn_getCode("95", "ALL", ds_dept_kind, gds_DmlCd); 
			fn_init(); // 검색부 초기화
			break;
			
			case "srchCondPayList":
			//저장 및 수정시 새창 안뜨도록 >> 추후 처리
		    gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		    gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_condPay, null, true, null); // 페이지건수 표출
			fn_postSearch();
			break;
			
			case "srchCondPayTotalList":
			gfn_exportGridToExcel(grd_list00, "CondPayList_"+gfn_getTodayFull()+"");
			break;
			
			case "selectCustaccntList":
			if(tc_custAcnt == 1) {
				fn_setAccntNo();
			} else if (tc_custAcnt > 1) {
				btn_accnt_onclick(null, "Y");
			} else {
				edt_accnt_no.value = "";
			}			
			break;
		}
	}
}


/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType,svcid, gbn) {
	if (sType == gfn_getCommTypeInit())
	{
		switch(svcid) {
			case "srchCondPayList": 
			if (gfn_isNull(cal_stt_dt.value) || gfn_isNull(cal_end_dt.value)) {
				alert("날짜를 입력하세요.");
				return false;
			}
			if (cal_stt_dt.value > cal_end_dt.value) {
				alert("시작일이 종료일보다 큽니다.");
				return false;
			}
			
			// 조회조건 및 default 정렬
			ds_inqCndt.keystring = ""; // 정렬컬럼 초기화
			ds_inqCndt.clearData();
			ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(0, "stt_dt", cal_stt_dt.value);
			ds_inqCndt.setColumn(0, "end_dt", cal_end_dt.value);
			ds_inqCndt.setColumn(0, "prod_cd", edt_prod_cd.value);
			ds_inqCndt.setColumn(0, "prod_nm", edt_prod_nm.value);
			ds_inqCndt.setColumn(0, "accnt_no", edt_accnt_no.value);
			ds_inqCndt.setColumn(0, "dept_kind", cbo_dept_kind.value);
			ds_inqCndt.setColumn(0, "dept_cd", "");
			ds_inqCndt.setColumn(0, "affl_yn", cbo_affl_yn.value);
			ds_inqCndt.setColumn(0, "ocb_gubn", cbo_ocb_gubn.value);
			ds_inqCndt.setColumn(0, "pay_mthd", cbo_mem_stat.value);
			if (!gfn_isNull(edt_emple_no.value)) {
				ds_inqCndt.setColumn(0, "emple_nm", edt_emple_nm.value);
				ds_inqCndt.setColumn(0, "emple_no", edt_emple_no.value);
			} else {
				edt_emple_nm.value = "";
			}
			if (!gfn_isNull(edt_recom_emple_nm.value)) {
				ds_inqCndt.setColumn(0, "recom_emple_nm", edt_recom_emple_nm.value);
				ds_inqCndt.setColumn(0, "recom_emple_no", edt_recom_emple_no.value);
			} else {
				edt_recom_emple_nm.value = "";
			}			
			ds_inqCndt.setColumn(0, "orderBy", "join_dt");
			ds_inqCndt.setColumn(0, "orderDirection", "DESC");			
			break;
			
			case "selectCustaccntList":
			ds_accntInqCndt.keystring = ""; // 정렬컬럼 초기화
			ds_accntInqCndt.clearData();
			ds_accntInqCndt.addRow();
			
			var srch_typ = ""
			var srch_vl = "";
			var option = "";
			
			if (gbn == "accnt_no") {
				srch_typ = gbn;
				srch_vl = edt_accnt_no.value;
			} 
					
			ds_accntInqCndt.setColumn(0, "srch_typ", srch_typ);
			ds_accntInqCndt.setColumn(0, "srch_vl", srch_vl);
			ds_accntInqCndt.setColumn(0, "option", option);
			ds_accntInqCndt.setColumn(0, "ns_yn", "N");
			ds_accntInqCndt.setColumn(0, "popup_yn", "N");
			ds_accntInqCndt.setColumn(0, "orderBy", "mem_nm");
			ds_accntInqCndt.setColumn(0, "orderDirection", "asc");
			return_bool = true;
			break;
		}
	}
	return true;
}
