        break;
        
        // 결과 데이터 조회전
        case "srchReqResultCount":
		//ds_result_list.clearData();
			return_bool = true;
        break;
    }
	
	ds_inqCndt.clearData();
    ds_inqCndt.addRow();
    //ds_inqCndt.setColumn(0, "req_day",cal_req_day.value); // 청구일
    ds_inqCndt.setColumn(0, "wdrw_dt",cal_req_day.value); // 청구일
       
	return return_bool;
}


/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, searchInfo) {

	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
		
        // 1차산출 구분별 조회
		case "srchWdrwbyReqbit": 
        if (!fn_preSearch(sType, "srchWdrwbyReqbit")) return false;
	    szSvcid       = "srchWdrwbyReqbit";
		szController  = "/dlw/wdrw/wdrw-by-reqbit/list";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_list=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        
        // 결과 데이터
		case "srchReqResultCount": 
        if (!fn_preSearch(sType, "srchReqResultCount")) return false;
	    szSvcid       = "srchReqResultCount";
		szController  = "/dlw/pay/srchReqResultCount";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_result_list=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
    }
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}


