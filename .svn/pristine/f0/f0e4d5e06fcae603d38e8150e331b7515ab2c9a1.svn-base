/* 결과등록, 파일변환 버튼 control */



function fn_setComponentsUseYN() {		

	var sumCmsResultCnt = edt_sum_cms_result_cnt.value;
	var sumCardResultCnt = edt_sum_card_result_cnt.value;
	var sumCardResultAcnt = edt_sum_card_result_acnt.value;
	
	if (sumCmsResultCnt == undefined){
		sumCmsResultCnt   = 0;
	}
	if (sumCardResultCnt == undefined){
		sumCardResultCnt   = 0;
	}
	if (sumCardResultAcnt == undefined){
		sumCardResultAcnt   = 0;
	}
	
	trace("sumCardExtCnt : " + sumCardExtCnt);
	trace("sumCardReqCnt : " + sumCardReqCnt);
	trace("sumCardPayCnt : " + sumCardPayCnt);
	trace("sumCmsExtCnt : " + sumCmsExtCnt);
	trace("sumCmsReqCnt : " + sumCmsReqCnt);
	trace("sumCmsPayCnt : " + sumCmsPayCnt);	
	trace("sumCmsResultCnt : " + sumCmsResultCnt);
	trace("sumCardResultCnt : " + sumCardResultCnt);	
				
	if(gWdrwGubun == "카드") {
		// 결과 데이터가 있고,
		// 결과 총건수와 청구 총건수 일치하는 경우
		// (** 파일변환완료시 조건도 추가해야 함)
		
		if (sumCardReqCnt != 0 || sumCardResultCnt != 0){
			btn_wdrwReq.enable = false;  //청구실행 버튼
			btn_card_down.enable = true; //다운로드 버튼
		} 
		else if (sumCardExtCnt != 0){
			btn_wdrwReq.enable = true;
			btn_card_down.enable = false;
		}else{
			btn_wdrwReq.enable = false;
			btn_card_down.enable = false;
		}
		
		trace (sumCardResultCnt + "xxxxxxxx" + sumCardReqCnt);
		
		if ( ( sumCardResultCnt == sumCardReqCnt ) && sumCardResultCnt != 0 ) {
			// 파일변환만 가능
			//btn_upload.enable = false; //cms업로드 테스트 진행을 위한 주석처리
