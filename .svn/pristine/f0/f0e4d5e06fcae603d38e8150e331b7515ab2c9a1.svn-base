function fn_commSave(saveInfo) {

    var szSvcid = "";
    var szController = "";
    var szInDs = "";
    var szOutDs = "";
    var szParam = "";
    var szRetCallback = "";    
    var payMthd = "";
    var auth_Yn = "Y";
    
    //납입방법
    if(gWdrwGubun == "CMS"){
		payMthd  = "04";    
    }else{
		payMthd  = "06";
    } 
    
    // 유승인 무승인 구분
    if(gWdrwGubun == "카드(무)"){
		auth_Yn = "N";
    }
       
    
    //청구일
    var v_req_day = cal_req_day.value; // 청구일
    var sDepositDay = cal_depositDay.value; // 입금일자
    
    switch(saveInfo) {
    
		// 임시건 산출
		case "saveimsiIchaeDt":
			szSvcid       = "saveimsiIchaeDt";
			szController  = "/dlw/wdrw/temp-wdrw";
			szInDs        = "";
			szOutDs       = "";
			if(gWdrwGubun == "카드") 
				szParam  = "wdrw_gubun=Card" + " req_day="+v_req_day;
			else 
				szParam  = "wdrw_gubun=CMS" + " req_day="+v_req_day;
			szRetCallback = "fn_callBack";
		break;
		
		// 특수 산출 - 2018.12.11
		case "saveSpecialIchaeDt":
			szSvcid       = "saveSpecialIchaeDt";
			szController  = "/dlw/wdrw/special-wdrw";
			szInDs        = "";
			szOutDs       = "";
			//szParam       = "req_day=" + v_req_day;
			szParam       = "req_day=" + v_req_day + " pay_mthd=" + cbo_special_payMthd.value;
			szRetCallback = "fn_callBack";
		break;
		
		// 파일변환
		case "convertFile":
	    szSvcid       = "convertFile";
		szController  = "/dlw/pay/convertFile";
		szInDs        = "";
		szOutDs       = "";
		szParam       = "req_day=" + v_req_day + " pay_mthd=" + payMthd + " auth_yn=" + auth_Yn + " deposit_day=" + sDepositDay;
		szRetCallback = "fn_callBack";
		break;
		
		case "savenauthCancel":
			szSvcid       = "savenauthCancel";
			szController  = "/dlw/wdrw/nauth-cancel-wdrw";
			szInDs        = "";
			szOutDs       = "";
			szParam  = "wdrw_gubun=Card" + " req_day="+v_req_day;
			szRetCallback = "fn_callBack";
		break;
		
		case "cmsFileUpload":
			szSvcid       = "data_upload";
			szController  = "/dlw/wdrw/data_upload";
			szInDs        = "";
			szOutDs       = "";
			szParam       = "req_day=" + v_req_day;
			szRetCallback = "fn_callBack";
		
		break;
    }
	
	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

