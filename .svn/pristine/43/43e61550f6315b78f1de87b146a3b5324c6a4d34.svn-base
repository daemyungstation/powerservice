//온로드
function PS220200_onload(obj:Form, e:LoadEventInfo)
{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.26 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////
	
     gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
     gfn_getList("hrchCd","CAM990","fn_callBack",null,"srchHrchCdSet");   //캠페인유형(계층유형코드)
     fn_setAthrPgmBtn();				// 권한에 따른 버튼 설정
     fn_init();
}


//초기화
function fn_init()
{
    var strDate = gfn_getToday();
    var nDay   = strDate.substr(6,2);
    var nMonth = strDate.substr(4,2);
    var nYear = toNumber(strDate.substr(0,4));
    cbo_srch_y.value = nYear;
    cbo_srch_m.value = nMonth;
	cbo_excd.index = 0;
    cal_srch_stt_join_dt.value = null;
    cal_srch_end_join_dt.value = null;
    cbo_srch_pay_mthd.index = 0;

    edt_ichae_dt.value = null;
    edt_stt_month_cnt.value = null;
    edt_end_month_cnt.value = null;
    edt_stt_true_cnt.value = null;
    edt_end_true_cnt.value = null;
    edt_stt_pay_state.value = null;
    edt_end_pay_state.value = null;
    cbo_cmpg_typ_cd.index = 0;
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
    if (errcd != 0) {
        trace(errcd);
        return false;
    } else {
    
        switch (svcid) {
       
            case "srchUnpyMnge"+sService : //납입방법 조회
				sService +="0";
                gfn_appendSearchDs(ds_listUnpy, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
                gfn_setTotalRowCount(ds_listUnpy, sta_totalCnt, total_count); // 페이지건수 표출
                fn_postSearch();
            break;
            
             case "srchHrchCdSet": // 캠페인 유형
                gfn_getHrchCodeLoad("hrch_cd=='2100' ||hrch_cd=='2200' ||hrch_cd=='2300' ||hrch_cd=='2400'||hrch_cd=='3100' ||hrch_cd=='3200'||hrch_cd=='3300' ||hrch_cd=='3400'","ALL",ds_cmpgTypCdList,null,2,"Y");
				ds_cmpgTypCdList.keystring = "S:hrch_cd"; //소팅
            break;

        }
    }
}
/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch(sType)
{
    if (sType == gfn_getCommTypeInit())
    {
        ds_inqCndt.clearData();

        ds_inqCndt.addRow();
        ds_inqCndt.setColumn(0, "data_dt", cbo_srch_y.value +cbo_srch_m.value);
        ds_inqCndt.setColumn(0, "pay_mthd", cbo_srch_pay_mthd.value);
        ds_inqCndt.setColumn(0, "ichae_dt", edt_ichae_dt.value);
        ds_inqCndt.setColumn(0, "stt_month_cnt", edt_stt_month_cnt.value);
        ds_inqCndt.setColumn(0, "end_month_cnt",  edt_end_month_cnt.value);
        ds_inqCndt.setColumn(0, "stt_true_cnt", edt_stt_true_cnt.value);
        ds_inqCndt.setColumn(0, "end_true_cnt",  edt_end_true_cnt.value);
        ds_inqCndt.setColumn(0, "stt_pay_state", edt_stt_pay_state.value);
        ds_inqCndt.setColumn(0, "end_pay_state",  edt_end_pay_state.value);
        ds_inqCndt.setColumn(0, "stt_join_dt", cal_srch_stt_join_dt.value);
        ds_inqCndt.setColumn(0, "end_join_dt",  cal_srch_end_join_dt.value);
        ds_inqCndt.setColumn(0, "mem_nm",  edt_end_pay_state00.value);
        ds_inqCndt.setColumn(0, "camp_type",  cbo_cmpg_typ_cd.value);
        ds_inqCndt.setColumn(0, "chk_excd",  cbo_excd.value);
//         if(chk_excd.value == 'Y'){
// 			ds_inqCndt.setColumn(0, "chk_excd",  chk_excd.value);
//         }
        
        ds_inqCndt.setColumn(0, "orderBy",  'unpy_id');
        ds_inqCndt.setColumn(0, "orderDirection",  'DESC');
    }
    return true;
}

function fn_preSrch(){
    if(!gfn_isNull(cal_srch_stt_join_dt.value)){
        if(gfn_isNull(cal_srch_end_join_dt.value)){
            alert("가입일을 입력해 주세요.");
