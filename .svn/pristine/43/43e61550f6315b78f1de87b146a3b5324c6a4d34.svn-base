//버튼클릭이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
    
    switch(obj){
        case btn_Search: //조회버튼
            if(fn_preSrch()){
                fn_commSearch(gfn_getCommTypeInit()); // 조회
                nScrollNum = 0; //초기화
            }
        break;

        case btn_Init : //초기화버튼
            fn_init();
        break;

         case btn_excelDw : //엑셀다운로드
              if(ds_listUnpy.rowcount >= 1){
				var colDS = gfn_excelPrepareExcelDS("grd_list_extc");
			
				gfn_excelDown("미납실적관리", "XLS_UNPY", "UnpyMngeMap.getUnpyMngeList", "UnpyMngeMap.getUnpyMngeCount", ds_inqCndt, true, colDS);
				//gfn_exportGridToExcel(grd_list_extc,"추출고객 대상리스트(엑셀)_"+gfn_getTodayFull());
             }else{
                 alert('조회된 대상고객이 없습니다.');
                 return;
             }
         break;
    }
}

