/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var gv_user_id = "";	// 상담자
var gv_emple_no = "";	// 담당사원

var tc_hpcl = 0; 		// 전체건수

var xlsYn; 		// 엑셀권한
var gvMenuId;   // 프로그램 로그 추가 2018.02.19
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL420901_onload(obj:Form, e:LoadEventInfo)
{
	//////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2018.02.19 정승철
	gvMenuId = GV_LOGMENUID;
	//////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	fn_setAthrPgmBtn();				// 권한에 따른 버튼 설정
	gfn_getList("dlwCd", "81"); 	// 공통코드조회
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		btn_excl.visible = true;
	} else {
		btn_excl.visible = false;
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("81", "", ds_statCd, gds_DmlCd); // 상담상태
			
			fn_setStatChk();// 상담상태 설정
			fn_init(); 		// 검색부 초기화
			fn_commSearch(gfn_getCommTypeInit()); // 해피콜 조회
			break;
			
			case "srchHpcl":
			gfn_appendSearchDs(ds_list_hpcl, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_hpcl, sta_TotalCnt, tc_hpcl); // 페이지건수 표출
			fn_postSearch();
			break;
			
			case "srchHpclExcelList": // 엑셀다운
			gfn_exportGridToExcel(grd_list_excel,"해피콜조회_"+gfn_getTodayFull());
			break;
			
			case "hpclBndeAckd":
			alert("일괄승인 처리 완료되었습니다.");
			fn_commSearch(gfn_getCommTypeInit()); // 해피콜 조회
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (gfn_isNull(cal_srch_stt_dt.value) || gfn_isNull(cal_srch_end_dt.value)) {
			alert("검색기간을 입력하십시요.");
			return false;
		} else if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
	
		var sStatCds = "";
		for (var i=0; i < ds_statCd.rowcount+1; i++) {
			if (i != ds_statCd.rowcount) {
				var sCd = "chk_" + ds_statCd.getColumn(i, "cd");
				if (eval(eval("'div_stat.'+sCd")).value) {
					if (sStatCds != "") {
						sStatCds += ",";
					}
					sStatCds += "'"+ds_statCd.getColumn(i, "cd")+"'";
				}
			} else {
				var sCd = "chk_";
				if (div_stat.chk_.value) {
					if (sStatCds != "") {
						sStatCds += ",";
					}
					sStatCds += "''";
				}
			}
		}

		// 조회조건 및 default 정렬
 		ds_inqCndt.clearData();
 		ds_list_hpcl.keystring = ""; // 정렬컬럼 초기화
		ds_inqCndt.addRow();
		
		if (gfn_isNull(edt_srch_user_nm.value)) {
			gv_user_id = "";
		}
		if (gfn_isNull(edt_srch_emple_nm.value)) {
			gv_emple_no = "";
		}
		ds_inqCndt.setColumn(0, "user_id", gv_user_id);
		ds_inqCndt.setColumn(0, "emple_no", gv_emple_no);
		ds_inqCndt.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
		ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
		ds_inqCndt.setColumn(0, "date_typ", cbo_srch_dateTyp.value);
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "statcds", sStatCds);
		
		ds_inqCndt.setColumn(0, "orderBy", "assign_dm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

// 엑셀 추출전
function fn_preSrchExcelDownList()
{
	if (gfn_isNull(cal_srch_stt_dt.value) || gfn_isNull(cal_srch_end_dt.value)) {
		alert("검색기간을 입력하십시요.");
		return false;
	} else if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
		alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
		return false;
	}
	
	var sStatCds = "";
	for (var i=0; i < ds_statCd.rowcount+1; i++) {
		if (i != ds_statCd.rowcount) {
			var sCd = "chk_" + ds_statCd.getColumn(i, "cd");
			if (eval(eval("'div_stat.'+sCd")).value) {
				if (sStatCds != "") {
					sStatCds += ",";
				}
				sStatCds += ds_statCd.getColumn(i, "cd");
			}
		} else {
			var sCd = "chk_";
			if (div_stat.chk_.value) {
				if (sStatCds != "") {
					sStatCds += ",";
				}
				sStatCds += "''";
			}
		}
	}
		
	// 조회조건 및 default 정렬
 	ds_inqCndt_excel.clearData();
	ds_inqCndt_excel.addRow();
	
	if (gfn_isNull(edt_srch_user_nm.value)) {
		gv_user_id = "";
	}
	if (gfn_isNull(edt_srch_emple_nm.value)) {
		gv_emple_no = "";
	}
	ds_inqCndt_excel.setColumn(0, "user_id", gv_user_id);
	ds_inqCndt_excel.setColumn(0, "emple_no", gv_emple_no);
	ds_inqCndt_excel.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
	ds_inqCndt_excel.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
	ds_inqCndt_excel.setColumn(0, "date_typ", cbo_srch_dateTyp.value);
	ds_inqCndt_excel.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
	ds_inqCndt_excel.setColumn(0, "end_dt", cal_srch_end_dt.value);
	ds_inqCndt_excel.setColumn(0, "statcds", sStatCds);
	
	ds_inqCndt_excel.setColumn(0, "excel_yn", "Y");
		
	ds_inqCndt_excel.setColumn(0, "orderBy", "assign_dm");
	ds_inqCndt_excel.setColumn(0, "orderDirection", "desc");
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 해피콜 조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_hpcl, sta_TotalCnt, 500, tc_hpcl)) return;
	
	var szSvcid       = "srchHpcl";
    var szController  = "/dlw/cons-prod/hpcl-list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

// 일괄승인
function fn_hpclBndeAckd()
{
	// 일괄배정할 IVR 콜백 설정
	var sSelectcheck = "";
	for(var i=ds_list_hpcl.rowcount-1; i>=0; i--) {
		if(ds_list_hpcl.getColumn(i, "_chk") == 1 && ds_list_hpcl.getColumn(i, "stat") != "01") {
			if (sSelectcheck != "") {
				sSelectcheck += ","
			}
			sSelectcheck += ds_list_hpcl.getColumn(i, "accnt_no");
		}
	}
	
	if (gfn_isNull(sSelectcheck)) {
		alert("승인할 해피콜 건을 선택하세요.");
		return false;
	}
	
	var szSvcid       = "hpclBndeAckd";
    var szController  = "/dlw/cons-prod/hpcl/bnde-ackd";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "selectcheck="+sSelectcheck;
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 엑셀다운로드
function fn_srchExcelDownList()
{	
	if (!fn_preSrchExcelDownList()) return false;
	
	var szSvcid       = "srchHpclExcelList";
    var szController  = "/dlw/cons-prod/hpcl-list";
    var szInDs        = "ds_input=ds_inqCndt_excel";
    var szOutDs       = "ds_list_excel=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 상담상태 설정
function fn_setStatChk()
{
	var nGap = 15;
	var nLeft= 10;
	var nTop = 7;
	var nBottom = 25;
	var objFont = gfn_getObjFont(9,"Dotum");
	objFont.bold = false;
			
	for (var i=0; i < ds_statCd.rowcount+1; i++) {
		if (i != ds_statCd.rowcount) {
			var objCd = ds_statCd.getColumn(i, "cd");
			var objCdNm = ds_statCd.getColumn(i, "cd_nm");
		} else {
			var objCd = "";
			var objCdNm = "미진행";
		}
				
		var objTextSize = gfn_getTextSize(objCdNm, objFont);
		var txtSize = objTextSize.cx+40;
					
		var oChkbox = new CheckBox();
		oChkbox.init("chk_"+objCd, nLeft, nTop, nLeft + txtSize, nBottom);
		div_stat.addChild("chk_"+objCd, oChkbox);
		oChkbox.text = objCdNm;
		if (oChkbox.name == "chk_01" || oChkbox.name == "chk_011") {
			oChkbox.value = false;
		} else {
			oChkbox.value = true;
		}
					
		if (oChkbox.position.right > 950) {
			nLeft = 10;
			nTop = nTop + 20;
			nBottom = nBottom + 20;
			oChkbox.position = "absolute "+nLeft+" "+nTop+" "+nLeft + txtSize+" "+nBottom;
			nLeft = nLeft + txtSize;
		} else {
			nLeft = oChkbox.position.right;
		}
				
		//div_stat.position.height = nBottom+2;
		oChkbox.show();
	}
}

// 초기화
function fn_init()
{
	//  검색기간 설정
	cbo_srch_dateTyp.index = 0;
	var sToday = gfn_getToday();
	cal_srch_stt_dt.value = gfn_addDate(sToday, -30);
	cal_srch_end_dt.value = sToday;
	
	gv_user_id = gds_UserInfo.getColumn(0, "user_id");
	edt_srch_user_nm.value = gds_UserInfo.getColumn(0, "user_nm");
	gv_emple_no = "";
	edt_srch_emple_nm.value = "";
	edt_srch_accnt_no.value = "";
	edt_srch_prod_cd.value = "";
	edt_srch_prod_nm.value = "";
	
	// 상담상태 초기화
	for (var i=0; i < ds_statCd.rowcount+1; i++) {
		if (i != ds_statCd.rowcount) {
			var sCd = "chk_" + ds_statCd.getColumn(i, "cd");
		} else {
			var sCd = "chk_"
		}
		if (sCd == "chk_01" || sCd == "chk_011") {
			eval(eval("'div_stat.'+sCd")).value = false;
		} else {
			eval(eval("'div_stat.'+sCd")).value = true;
		}
	}
}

// 상담사, 담당사원조회 팝업
function fn_srchEmplePop(sType)
{
	var sPrmr = "";
	var sPagNm = "";
	if (sType == "usr") {
		sPrmr = {p_empl_nm:edt_srch_user_nm.value, p_pageTyp:"DL040901", p_typ:"usr"};
		sPagNm = "상담자 조회";
	} else {
		sPrmr = {p_empl_nm:edt_srch_emple_nm.value, p_pageTyp:"DL040901", p_typ:sType};
		sPagNm = "담당사원 조회";
	}
	
	var reVal = gfn_diaLog(sPagNm, -1, -1, -1, -1, "PS990000::PS992002.xfdl", false, sPrmr, false, false, true);
}

// 회원조회 팝업
function fn_srchAccntNoPop()
{
	var sSrchTyp = "mem_nm";
	if (!gfn_isNull(edt_srch_accnt_no.value)) {
		sSrchTyp = "accnt_no";
	}
	
	var sPrmr = {p_srch_typ:sSrchTyp, p_srch_vl:edt_srch_accnt_no.value, p_gubun_mode:"", p_option:""};
	var reVal = gfn_diaLog("회원조회", -1, -1, -1, -1, "PS990000::PS992400.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 회원정보 선택시
function fn_setAccntNo()
{
	edt_srch_accnt_no.value = ds_accnt_info.getColumn(0, "accnt_no");
}

// 상품정보 팝업
function fn_openProdPopup(sType)
{
	var sPrmr;
	if (sType == "btn") {
		sPrmr = {p_prod_cd:"", p_prod_nm:"", p_mode:"set"};
	} else if (sType == "cd") {
		sPrmr = {p_prod_cd:edt_srch_prod_cd.value, p_prod_nm:"", p_mode:"set"};
	} else {
		sPrmr = {p_prod_cd:"", p_prod_nm:edt_srch_prod_nm.value, p_mode:"set"};
	}
	var reVal = gfn_diaLog("상품조회", -1, -1, -1, -1, "PS990000::PS992200.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 상품 선택시
function fn_prodSetInfo()
{
	edt_srch_prod_cd.value = ds_prodList.getColumn(0, "prod_cd");
	edt_srch_prod_nm.value = ds_prodList.getColumn(0, "prod_nm");
}

// 처리결과
function fn_postSearch()
{
	if (ds_list_hpcl.rowcount < 1) {
		alert("조회결과가 없습니다.");
		return;
	}
}

// 권한에 따른 버튼 설정
function fn_setAthrPgmBtn()
{
	gfn_athrPgm(gfn_gdsCd_admin());
	
	if (xlsYn == "Y") {
		btn_excl.visible = true;
	}
	
	for (var i=0; i<ds_indvFnctAthrPgmFilter.rowcount; i++) {
		if (ds_indvFnctAthrPgmFilter.getColumn(i, "cnpn_id") == "bndeAckd") {
			if (ds_indvFnctAthrPgmFilter.getColumn(i, "amnt_yn") == "1") {
				btn_bndeAckd.visible = true;
			}
		}
	}
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// button click
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_srchUser") {			// 상담자 조회팝업
		fn_srchEmplePop("usr");
	} else if (obj.name == "btn_srchEmple") {	// 담당사원 조회팝업
		fn_srchEmplePop("emp");
	} else if (obj.name == "btn_srchProd") {	// 가입상품 조회팝업
		fn_openProdPopup("btn");
	} else if (obj.name == "btn_srchAccntNo") {	// 회원번호 조회팝업
		fn_srchAccntNoPop();
	} else if (obj.name == "btn_search") {	    // 조회버튼
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_init") {		// 초기화버튼
		fn_init();
	} else if (obj.name == "btn_excl") {		// 엑셀다운버튼
		fn_srchExcelDownList();
	} else if (obj.name == "btn_bndeAckd") {	// 일괄승인버튼
		fn_hpclBndeAckd();
	}
}

// onkeydown
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if (obj.name == "edt_srch_user_nm") {
			fn_srchEmplePop("usr");
		} else if (obj.name == "edt_srch_emple_nm") {
			fn_srchEmplePop("emp");
		} else if (obj.name == "edt_srch_prod_cd") {
			fn_openProdPopup("cd");
		} else if (obj.name == "edt_srch_prod_nm") {
			fn_openProdPopup("nm");
		} else if (obj.name == "edt_srch_accnt_no") {
			fn_srchAccntNoPop();
		}
	}
}

// 그리드 헤더 사이즈 초기화
function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_resetColsize(grd_list);
}

// 그리드 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/dlw/cons-prod/hpcl-list"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_hpcl, sta_TotalCnt, 500, tc_hpcl)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_hpcl, sta_TotalCnt, tc_hpcl); // 페이지건수 표출
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}
