		    /*
		    case "getWdrwReqAccntConfirm" :
			    if(gv_wdrw_req_accnt > 0)
				{	
					alert("이 고객은 현재 금액 청구중인 회원입니다. 청구 삭제후 등록 가능합니다.");
					return false;
				}
			break;
			*/
			
			// 회원조회시 산출 또는 청구중인지 체크 
