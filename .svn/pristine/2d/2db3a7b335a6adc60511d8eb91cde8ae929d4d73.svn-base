//조회
function fn_commSearch(sType, searchInfo) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
        //CMS
        case "CmsCsmm": // CMS회원 조회
        if (!fn_preSearch(sType,"srchCmsCsmm")) return false;
	    if(!gfn_pageSearch(sType, ds_cmsCsmm_list, tab_cms.sta_TotalCnt, 500, tc_cmsCsmm)) return;
	    szSvcid       = "srchCmsCsmm";
		szController  = "/dlw/cms/cms-csmm/list";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cmsCsmm_list_tmp=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        case "CmsRgsnHstr": //CMS 등록이력 조회
	    if(!gfn_pageSearch(sType, ds_cmsRgsnHist_list, tab_cms.sta_cmsRgsnHstrTotalCnt, 500, tc_cmsRgsnHstr)) return;
    	szSvcid       = "srchCmsRgsnHstr";
		szController  = "/dlw/cms/cms-rgsn-hstr";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cmsRgsnHist_list_tmp=ds_output";
		szParam       = "accnt_no=" + tab_cms.aplc_accnt_no.value
                      + " orderBy=upd_dm orderDirection=asc";
		szRetCallback = "fn_callBack";
        break;
        case "CmsWdrwHstr": //CMS 출금이체의뢰내역 조회
		if(!gfn_pageSearch(sType, ds_cmsWdrwHist_list, tab_cms.sta_cmsWdrwHstrTotalCnt, 500, tc_cmsWdrwHstr)) return;
		szSvcid       = "srchCmsWdrwHstr";
		szController  = "/dlw/cms/cms-wdrw-hstr";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cmsWdrwHist_list_tmp=ds_output";
		szParam       = "accnt_no=" + tab_cms.aplc_accnt_no.value
					  + " orderBy=inv_dt orderDirection=desc";
		szRetCallback = "fn_callBack";
        break;
        
        case "CmsWdrwChk": //CMS 산출중여부확인
		szSvcid       = "srchCmsWdrwChk";
		szController  = "/dlw/cms/cms-wdrw-chk";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cardWdrwHist_list_tmp=ds_output";
		szParam       = "accnt_no=" + tab_cms.aplc_accnt_no.value;
		szRetCallback = "fn_callBack";
        break;
        
        //Card
        case "CardCsmm": // Card회원 조회
        if (!fn_preSearch(sType,"srchCardCsmm")) return false;
	    if(!gfn_pageSearch(sType, ds_cardCsmm_list, tab_card.sta_cardCsmmTotalCnt, 500, tc_cardCsmm)) return;
	    szSvcid       = "srchCardCsmm";
		szController  = "/dlw/card/card-csmm/list";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cardCsmm_list_tmp=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        case "CardRgsnHstr": //Card 등록이력 조회
	    if(!gfn_pageSearch(sType, ds_cardRgsnHist_list, tab_card.sta_cardRgsnHstrTotalCnt, 500, tc_cardRgsnHstr)) return;
    	szSvcid       = "srchCardRgsnHstr";
		szController  = "/dlw/card/card-rgsn-hstr";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cardRgsnHist_list_tmp=ds_output";
		szParam       = "accnt_no=" + tab_card.edt_cardAccntNo.value
                      + " orderBy=app_dt orderDirection=asc";
		szRetCallback = "fn_callBack";
        break;
        case "CardWdrwHstr": //Card 출금이체의뢰내역 조회
		if(!gfn_pageSearch(sType, ds_cardWdrwHist_list, tab_card.sta_cardWdrwHstrTotalCnt, 500, tc_cardWdrwHstr)) return;
		szSvcid       = "srchCardWdrwHstr";
		szController  = "/dlw/card/card-wdrw-hstr";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cardWdrwHist_list_tmp=ds_output";
		szParam       = "accnt_no=" + tab_card.edt_cardAccntNo.value
					  + " orderBy=inv_dt orderDirection=desc";
		szRetCallback = "fn_callBack";
        break;
        case "CardWdrwChk": //Card 산출중여부확인
		szSvcid       = "srchCardWdrwChk";
		szController  = "/dlw/card/card-wdrw-chk";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_cardWdrwHist_list_tmp=ds_output";
		szParam       = "accnt_no=" + tab_card.edt_cardAccntNo.value;
		szRetCallback = "fn_callBack";
        break;
    }
    
    // 트랜젝션 공통함수 호출
    if( searchInfo == "CmsCsmm" || searchInfo == "CardCsmm" )
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
	else
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//저장
function fn_commSave(saveInfo) {

    var szSvcid = ""
    var szController = ""
    var szInDs = ""
    var szOutDs = ""
    var szParam = ""
    var szRetCallback = ""

    switch(saveInfo) {
        case "CmsRgsnInfo": //CMS 이체정보 수정
		if (!fn_preSave("saveCmsRgsnInfo")) return false;
		
		
		if(confirm("수정하시겠습니까?")) {
		  
		    ds_cmsCsmm_list_1_save.clearData();
		    ds_cmsCsmm_list_1_save.addRow();
		    ds_cmsCsmm_list_1_save.copyRow(0,ds_cmsCsmm_list,ds_cmsCsmm_list.rowposition);
			szSvcid       = "saveCmsRgsnInfo";
			szController  = "/dlw/cms/cms-csmm/save";
			szInDs        = "ds_input=ds_cmsCsmm_list_1_save";
			szOutDs       = "";
			szParam       = "";
			szRetCallback = "fn_callBack";        
		} else {return false;}
        break;
        
        case "CardRgsnInfo": //Card 이체정보 수정
		if (!fn_preSave("saveCardRgsnInfo")) return false;
		
		if(confirm("수정하시겠습니까?")) {
		   ds_cardCsmm_list_1_save.clearData();
		  ds_cardCsmm_list_1_save.addRow();
		   	ds_cardCsmm_list_1_save.copyRow(0,ds_cardCsmm_list,ds_cardCsmm_list.rowposition);
		   		   trace(ds_cardCsmm_list_1_save.saveXML());
			szSvcid       = "saveCardRgsnInfo";
			szController  = "/dlw/card/card-csmm/save";
			szInDs        = "ds_input=ds_cardCsmm_list_1_save";
			szOutDs       = "";
			szParam       = "";
			szRetCallback = "fn_callBack";
		} else {return false;}
        break;
    }

	// 트랜젝션 공통함수 호출
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}
