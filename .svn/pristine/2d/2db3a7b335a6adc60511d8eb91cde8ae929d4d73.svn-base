function fn_preSearch(sType, svcid) {
    var return_bool = true;
    switch(svcid) {
        case "srchCmsCsmm": // Cms 회원정보 조회
		if (gfn_isNull(tab_cms.srch_typ.value)) {
			 alert("검색조건을 선택하세요.");
			 return false;
		}
		if (tab_cms.srch_typ.value == "ichae_dt") {
		    if (gfn_isNull(tab_cms.ichae_stt_dt.value) || gfn_isNull(tab_cms.ichae_end_dt.value)) {
				 alert("이체일자를 입력하세요.");
				 return false;
			}     
		} else {
		    if (gfn_isNull(tab_cms.srch_vl.value)) {
			    alert("검색어를 입력하세요.");
				return false;
			}
		}
		if (sType == gfn_getCommTypeInit())
		{
			// 조회조건 및 default 정렬
			ds_cmsCsmm_list.keystring = ""; // 정렬컬럼 초기화
			
			ds_inqCndt.clearData();
			ds_inqCndt.addRow();
			ds_inqCndt.setColumn(0, "srch_typ", tab_cms.srch_typ.value);
			ds_inqCndt.setColumn(0, "srch_vl", tab_cms.srch_vl.value);
			ds_inqCndt.setColumn(0, "ichae_stt_dt", tab_cms.ichae_stt_dt.value);
			ds_inqCndt.setColumn(0, "ichae_end_dt", tab_cms.ichae_end_dt.value);
			ds_inqCndt.setColumn(0, "orderBy", "cms_join_dt");
			ds_inqCndt.setColumn(0, "orderDirection", "asc");
		}
        break;
        
        case "srchCardCsmm": // Card 회원정보 조회
		if (sType == gfn_getCommTypeInit())
		{
			// 조회조건 및 default 정렬
			ds_cardCsmm_list.keystring = ""; // 정렬컬럼 초기화
			
			ds_inqCndt.clearData();
			ds_inqCndt.addRow();
			ds_inqCndt.setColumn(0, "srch_typ", tab_card.srch_typ.value);
			ds_inqCndt.setColumn(0, "srch_vl", tab_card.srch_vl.value);
			ds_inqCndt.setColumn(0, "srch_prodCd", tab_card.srch_prodCd.value);
			//ds_inqCndt.setColumn(0, "srch_prodNm", tab_card.srch_prodNm.value);
			ds_inqCndt.setColumn(0, "srch_deptCd", tab_card.srch_deptCd.value);
			//ds_inqCndt.setColumn(0, "srch_deptNm", tab_card.srch_deptNm.value);
			ds_inqCndt.setColumn(0, "srch_ackdSttDt", tab_card.srch_ackdSttDt.value);
			ds_inqCndt.setColumn(0, "srch_ackdEndDt", tab_card.srch_ackdEndDt.value);
			ds_inqCndt.setColumn(0, "srch_cardNo", tab_card.srch_cardNo.value);
			ds_inqCndt.setColumn(0, "orderBy", "card_join_dt");
			ds_inqCndt.setColumn(0, "orderDirection", "asc");
		}
        break;
    }
     
	return return_bool;
}

