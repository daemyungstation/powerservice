//리스트데이터 상세부 출력 (CMS)
function fn_cmsDtlView(num)
{
    if(ds_cmsCsmm_list.getColumn(num,"del_fg") == "N") {
		if (pritYn != "N") {
			//tab_cms.btn_cmsTrmt.visible = true;
		}
        if (amntYn != "N") {
			tab_cms.btn_cmsSave.visible = true;
        }
        tab_cms.aplc_birth.visible = true;
        tab_cms.mae_aplc_birth.visible = true;
        tab_cms.stc_aplc_birth.visible = true;
        tab_cms.stc_cms_cancel.visible = false;
    } else {
        tab_cms.btn_cmsTrmt.visible = false;
        tab_cms.btn_cmsSave.visible = false;
        tab_cms.aplc_birth.visible = false;
        tab_cms.mae_aplc_birth.visible = false;
        tab_cms.stc_aplc_birth.visible = false;
        tab_cms.stc_cms_cancel.visible = true;
        tab_cms.stc_cms_cancel.bringToFront();
    }
        
    tab_cms.aplc_prod_amt.value = gfn_commaStr(ds_cmsCsmm_list.getColumn(num,"prod_amt"));
    tab_cms.aplc_manager_nm.value = ds_cmsCsmm_list.getColumn(num,"manager_nm")+"("+ds_cmsCsmm_list.getColumn(num,"dept_nm")+")";
    tab_cms.aplc_bank_cd.value = "(" + ds_cmsCsmm_list.getColumn(num,"bank_cd")+")"+ ds_cmsCsmm_list.getColumn(num,"bank_nm");
    
    tab_cms.rdo_tranDt.value = ds_cmsCsmm_list.getColumn(num,"ichae_dt");
    trace(tab_cms.rdo_tranDt.value);
    trace(tab_cms.rdo_tranDt.index);
  //  alert(tab_cms.rdo_tranDt.index);
    if (tab_cms.rdo_tranDt.index < 0) {
		tab_cms.rdo_tranDt.index = 5;
		tab_cms.edt_tranDt.enable = true;
		tab_cms.edt_tranDt.value = ds_cmsCsmm_list.getColumn(num,"ichae_dt");
    } else {
		tab_cms.edt_tranDt.enable = false;
    }
    
    fn_CmsprePayYnChecked(tab_cms.aplc_pre_pay_yn);
    fn_CmsyenCheYnChecked(tab_cms.aplc_yen_che_pre_pay_yn);
    
    // 2018.12.03 정승철 임의지정 (정상선납, 연체건) 막음
    fn_disableTempYn("CMS");

    fn_commSearch(gfn_getCommTypeInit(),"CmsRgsnHstr");
}

//리스트데이터 상세부 출력 (Card)
function fn_cardDtlView(num)
{   
    if(ds_cardCsmm_list.getColumn(num,"del_fg") == "N") {
		if (amntYn != "N") {
			tab_card.btn_cardSave.visible = true;
        }
        tab_card.stc_card_cancel.visible = false;
    } else {
        tab_card.btn_cardSave.visible = false;
        tab_card.stc_card_cancel.visible = true;
        tab_card.stc_card_cancel.bringToFront();
    }
    
    tab_card.edt_cardProdAmt.value = gfn_commaStr(ds_cardCsmm_list.getColumn(num,"prod_amt"));
    tab_card.edt_cardEmployee.value = ds_cardCsmm_list.getColumn(num,"emple_nm")+"("+ds_cardCsmm_list.getColumn(num,"dept_nm")+")";
    
    //tab_card.edt_cardYear.value = ds_cardCsmm_list.getColumn(num,"expire_date").substring(0,2);
    //tab_card.edt_cardMon.value = ds_cardCsmm_list.getColumn(num,"expire_date").substring(2,4);
    // 유효일 MASKING - 2018.01.12 정승철
    var vYY = ds_cardCsmm_list.getColumn(num,"expire_date").substring(0,2);
    var vMM = ds_cardCsmm_list.getColumn(num,"expire_date").substring(2,4);
    if( !gfn_isNull(vYY) ) tab_card.edt_cardYear.value = '**';
    if(!gfn_isNull(vMM)) tab_card.edt_cardMon.value = '**';
    
    tab_card.rdo_tranDt.value = ds_cardCsmm_list.getColumn(num,"pay_dt");
    if (tab_card.rdo_tranDt.index < 0) {
		tab_card.rdo_tranDt.index = 5;
		tab_card.edt_tranDt.enable = true;
		tab_card.edt_tranDt.value = ds_cardCsmm_list.getColumn(num,"pay_dt");
    } else {
		tab_card.edt_tranDt.enable = false;
    }
    
    fn_CardprePayYnChecked(tab_card.ckb_cardPrePayYn);
    fn_CardyenCheYnChecked(tab_card.ckb_cardYenChePrePayYn);
    
    // 2018.12.03 정승철 임의지정 (정상선납, 연체건) 막음
    fn_disableTempYn("CARD");

    fn_commSearch(gfn_getCommTypeInit(),"CardRgsnHstr");
    
    
}
