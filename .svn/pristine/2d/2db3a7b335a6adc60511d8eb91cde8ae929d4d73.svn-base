		else if(sDelFg == "R")
		{
			alert("현재 청구중인 회원입니다.");
			return false;
		}
		
        if (gfn_isNull(tab_card.edt_cardAccntNo.value)) {
			alert("Card회원을 선택하세요.");
			return false;
		}
		
		var sCardPayDt = "";
		if (tab_card.rdo_tranDt.index == 5) {
			sCardPayDt = tab_card.edt_tranDt.value;
		} else {
			sCardPayDt = tab_card.rdo_tranDt.value;
		}
		
        if (gfn_isNull(sCardPayDt)) {
			alert("결제일을 입력하세요.");
			return false;
		}
		if(gfn_transNullToNumber(sCardPayDt) > 31) {
			alert("결제일은 최대 31일입니다.");
			return false;
		}		
		ds_cardCsmm_list.setColumn(0, "pay_dt", sCardPayDt);
		
        if (gfn_isNull(tab_card.edt_cardYear.value)) {
			alert("유효일(년)을 입력하세요.");
			return false;
		}
        if (gfn_isNull(tab_card.edt_cardMon.value)) {
			alert("유효일(월)을 입력하세요.");
			return false;
		} else {
		    if (gfn_transNullToNumber(tab_card.edt_cardMon.value) > 12) {
		        alert("유효일의 월은 최대 12월입니다.");
		        return false;
		    }
		}
		if (tab_card.ckb_cardPrePayYn.value == "Y") {
			if(gfn_isNull(tab_card.edt_cardPrePayGunsu.value) || gfn_transNullToNumber(tab_card.edt_cardPrePayGunsu.value) == 0) {
				alert("지정출금 건수를 입력하세요.");
				return false;
			}
		}     
		if (tab_card.ckb_cardYenChePrePayYn.value == "Y") {
			if(gfn_isNull(tab_card.edt_cardYenChePrePayGunsu.value)  || gfn_transNullToNumber(tab_card.edt_cardYenChePrePayGunsu.value) == 0) {
				alert("연체 지정출금 건수를 입력하세요.");
				return false;
			}
		}
        
        return_bool =  true;
        break;
    }
    
    return return_bool;

}
