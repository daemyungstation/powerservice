			fn_noGridMasking();
		}
        break;
    }
    return;
}

//출금의뢰내역 그리드 결과값 표시 변환
function fn_convertCmsWdrwHstrStat(stat, ipms_nm) {
    var str = "";
    if (stat == "03") {
        str = "불능("+imps_nm+")";
    } else if (stat == "01") {
        str = "신청전";
    } else if (stat == "02") {
        str = "신청중";
    } else if (stat == "04") {
        str = "완료";
    } else if (stat == "05") {
        str = "취소";
    }    
    return str;
}

