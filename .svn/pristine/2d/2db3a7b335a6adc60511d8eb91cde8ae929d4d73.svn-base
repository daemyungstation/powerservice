			case "srchCardRgsnHstr": // Card 등록이력조회
		    gfn_appendSearchDs(ds_cardRgsnHist_list, ds_cardRgsnHist_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
