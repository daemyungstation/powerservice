			fn_commSearch(gfn_getCommTypeInit(), "CardCsmm"); // 조회
			break;
			case "srchCardWdrwChk": // Card 산출중여부 확인
			//trace("cardWdrwChk : " + cardWdrwChk);
			/*if (" " == cardWdrwChk) {
			    fn_commSave("CardRgsnInfo");
			} else {
			    alert(cardWdrwChk + " 산출중인 건입니다.");
