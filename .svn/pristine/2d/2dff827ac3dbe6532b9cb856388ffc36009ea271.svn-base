//스크롤시 append
function grd_dlvList_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

// 그리드 헤더 클릭 이벤트 - 정렬
function grd_dlvList_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (!fn_preSearch()) return false;
	if (obj.name == "grd_dlvList") {
		ds_inqCndt.setColumn(0, "sController", "/chnl/dlv/list");
		ds_inqCndt.setColumn(0, "dsName", "ds_output");
		
		if (!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_dlvList, sta_TotalCnt, 50, total_count)) return;
		gfn_grdHeadClick(obj, e, ds_inqCndt);
		gfn_setTotalRowCount(ds_dlvList, sta_TotalCnt, total_count); // 페이지건수 표출
	} else {
		ds_inqCndt.setColumn(0, "sController", "/chnl/dlv/dtl/list");
		ds_inqCndt.setColumn(0, "dsName", "ds_output");
		
		if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_dlvDtlList, sta_TotalCnt, 50, total_count)) return;
		gfn_grdHeadClick(obj, e, ds_inqCndt);
		gfn_setTotalRowCount(ds_dlvDtlList, sta_TotalCnt, total_count); // 페이지건수 표출
	}
	
}

// 그리드 데이터 선택 이벤트
function grd_dlvList_onSelectChanged(obj:Grid, e:GridSelectEventInfo)
{
	var objDS = this[obj.binddataset];
	var nRow 	= e.row;
	if (nRow >= 0) {
		alert(objDS.getColumn(nRow, "dlv_stat_cd"));
		switch(objDS.getColumn(nRow, "dlv_stat_cd")) {
		case "10":
		case "30":
			this.btn_update.class = "btn_WF";
			this.btn_update.text = "반송";
			break;
			
		case "20":
			if (objDS.getColumn(nRow, "dsps_mthd_cd") == "10") {
				this.btn_update.class = "btn_WF";
				this.btn_update.text = "재발송";
				
			} else {
				this.btn_update.class = "btn_WF_Modif";
				this.btn_update.text = "";
			}
			
			break;
		default:
			break;
		}
	}
}

//버튼클릭 이벤트
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
	// 조회버튼
	case btn_search: 
		fn_commSearch(gfn_getCommTypeInit()); // 조회
		break;
	
	// 초기화
	case btn_init: 
		fn_init();
		break;
	
	// 신규
	case btn_create: 
		fn_openPop("물류 등록", "DL450000::PS450401.xfdl");
		break;
	
	// 수정버튼
	case btn_update: 
		if(ds_dlvList.rowcount == 0){
			sta_TotalCnt.text = "조회 결과가 없습니다.";
			return;
		}

		var sPrmr = "";
		switch(this.btn_update.text) {
		case "반송":
			sPrmr = {dlv_stat_cd : "20"}
			break;
		case "재발송":
			sPrmr = {dlv_stat_cd : "30"}
			break;
		default:
			sPrmr = {dlv_stat_cd : ""}
			break;
		}
		fn_openPop("물류 수정", "DL450000::PS450402.xfdl", sPrmr);
		break;
		
	case btn_accntNo :
		fn_srchAccntNoPop();
		break;
		
	// 엑셀 업로드 버튼
	case btn_excel_upload: 
		var sPrmr = {p_szController: "/chnl/dlv/excel-upload"}
		fn_openPop("엑셀업로드", "PS990000::PS999995.xfdl", sPrmr);
		break;
	
	// 엑셀 템플릿 다운로드 버튼
	case btn_excel_template:
		gfn_templateDown("dlv_sample.xlsx")
		break;
		
	case btn_excel:
		trace(cbo_dlvMthdCd.value);
		if (cbo_dlvMthdCd.value == '' || cbo_dlvMthdCd.value == null){
			alert("처리방법을 선택하셔야 합니다.");
			return;
		}
		
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();

		ds_inqCndt.setColumn(0, "accnt_no", this.edt_srch_accntNo.value);
		ds_inqCndt.setColumn(0, "dlv_type_cd", this.cbo_dlvTypecd.value);
		ds_inqCndt.setColumn(0, "dlv_stat_cd", this.cbo_dlvStatCd.value);
		ds_inqCndt.setColumn(0, "dsps_mthd_cd", this.cbo_dlvMthdCd.value);
	
		if (Tab_Main.tabindex == 0) {
			// gfn_excelDown(파일명, XLS아이디, 리스트맵, 카운트맵, 파라미터 데이터셋);
			//gfn_excelDown("발송관리", "XLS_DLV", "DlvMap.getDlvList", "DlvMap.getDlvCount", ds_inqCndt);
			
			var colDS = gfn_excelPrepareExcelDS("grd_excel");
			gfn_excelDown("발송관리", "XLS_DLCC", "DlvMap.getDlvList", "DlvMap.getDlvCount", ds_inqCndt, true, colDS, false);

			
		} else {
			// gfn_excelDown(파일명, XLS아이디, 리스트맵, 카운트맵, 파라미터 데이터셋);
			//gfn_excelDown("발송관리 상세", "XLS_DLV", "DlvMap.getDlvDtlList", "DlvMap.getDlvDtlCount", ds_inqCndt);
		}

		break;
	}
}

