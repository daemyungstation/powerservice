//검색조건 초기화
function fn_init()
{
	this.edt_srch_accntNo.value = "";
	this.edt_srch_memNm.value = "";
	this.cbo_dlvMthdCd.index = 0;
	this.cbo_dlvStatCd.index = 0;
	ds_dlvList.clear();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
//콜백
function fn_callBack(svcid, errcd, errmsg) {
	if (errcd < 0) {
		trace("errcd  :   " +errcd +"   \n errmsg   :   "+ errmsg +"    \n svcid   :   "+ svcid)
		return false;
	} else {
		switch(svcid){
		case "srchCdSet" :		// 코드셋
			gfn_getCode("CHL080", "ALL", ds_dlvTypeCd, ds_cdSet);
			gfn_getCode("CHL090", "ALL", ds_dlvStatCd, ds_cdSet);
			gfn_getCode("CHL100", "ALL", ds_dlvMthdCd, ds_cdSet);
			this.cbo_dlvTypecd.index = 0;
			this.cbo_dlvStatCd.index = 0;
			this.cbo_dlvMthdCd.index = 0;
			break;
		
		case "searchDlv" :		// 조회 콜백
			gfn_appendSearchDs(ds_dlvList, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_dlvList, sta_TotalCnt, total_count); // 페이지건수 표출
			fn_postSearch(ds_dlvList);
			btn_update.visible = bAmnt & (ds_dlvList.rowcount > 0)
			break;
		
		case "searchDtlDlv" :	// 조회 콜백(상세)
			gfn_appendSearchDs(ds_dlvDtlList, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_dlvDtlList, sta_TotalCnt, total_count); // 페이지건수 표출
			fn_postSearch(ds_dlvDtlList);
			break;
			
		case "deleteDlv":		// 삭제 콜백
			fn_commSearch(gfn_getCommTypeInit()); // 재조회
			break;
		case "excel" :
			if (ds_list_tmp.rowcount <= 0) {
				alert("엑셀다운로드 데이터가 없습니다.");
				return;
			}
			
			var excelFileNm = "";
			if (Tab_Main.tabindex == 0) {
				excelFileNm = "물류현황_";
			} else {
				excelFileNm = "물류현황 상세_";
			}
			gfn_exportGridToExcel(grd_excel, excelFileNm + gfn_getTodayFull());
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch(sType)
{
	if (Tab_Main.tabindex == 1){
		alert('준비중 입니다.');
		return false;
	}
	if (sType == gfn_getCommTypeInit())
	{	
	
		if ((cbo_dlvMthdCd.value == '' || cbo_dlvMthdCd.value == null) && (cbo_dlvStatCd.value == '' || cbo_dlvStatCd.value == null) && (edt_srch_accntNo.value=="" || edt_srch_accntNo.value==null) ){
			alert("전체 선택 시 회원정보는 필수입니다.");
			return;
		}
		if ((cbo_dlvMthdCd.value == '' || cbo_dlvMthdCd.value == null) && (cbo_dlvStatCd.value == '10') && (edt_srch_accntNo.value=="" || edt_srch_accntNo.value==null) ){
			alert("발송 선택 시 회원정보는 필수입니다.");
			return;
		}
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		
		ds_inqCndt.setColumn(0, "accnt_no", this.edt_srch_accntNo.value);
		ds_inqCndt.setColumn(0, "dlv_type_cd", this.cbo_dlvTypecd.value);
		ds_inqCndt.setColumn(0, "dlv_stat_cd", this.cbo_dlvStatCd.value);
		ds_inqCndt.setColumn(0, "dsps_mthd_cd", this.cbo_dlvMthdCd.value);
	}
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)조회,수정,삭제                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType) {
    if (!fn_preSearch(sType)) return false;
	
	switch(Tab_Main.tabindex) {
	case 0:
		btn_excel_template.visible = true;
		btn_excel_upload.visible = bRgsn & bAmnt & true;
		btn_create.visible = bRgsn & true;
		
		// 호출 건수 설정 및 조회여부 설정
		if(!gfn_pageSearch(sType, ds_dlvList, sta_TotalCnt, 50, total_count)) return; 
		
		var szSvcid       = "searchDlv";
		var szController  = "/chnl/dlv/list";
		var szInDs        = "ds_input=ds_inqCndt";
		var szOutDs       = "ds_list_tmp=ds_output";
		var szParam       = "";
		var szRetCallback = "fn_callBack";

		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
		break;
	
	case 1:
		/*
		btn_excel_template.visible = false;
		btn_excel_upload.visible = false;
		btn_update.visible = false;
		btn_create.visible = false;
		
		// 호출 건수 설정 및 조회여부 설정
		if(!gfn_pageSearch(sType, ds_dlvDtlList, sta_TotalCnt, 50, total_count)) return; 
		
		var szSvcid       = "searchDtlDlv";
		var szController  = "/chnl/dlv/dtl/list";
		var szInDs        = "ds_input=ds_inqCndt";
		var szOutDs       = "ds_list_tmp=ds_output";
		var szParam       = "";
		var szRetCallback = "fn_callBack";

		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
		*/

		break;
	}
	
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
