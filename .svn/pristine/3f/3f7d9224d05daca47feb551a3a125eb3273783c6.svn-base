                       , FN_COM_NM('82',RESON) AS RESON
                       , '0' AS COMP_RESON
                       , '0' AS RESN_MTHD
                       , RS.BANK_ACCNT_NO
                       , RS.BANK_CD
                       , RS.BANK_NM
