                        DEBT_TOT_AMT,
                        DEBT_YEN_CNT,
                        DEBT_REL_AMT,
                        (DEBT_TOT_AMT - DEBT_REL_AMT) AS DEBT_DIFF_AMT,
                        CANCEL_YN,
                        CANCEL_DT,

                        CASE WHEN TBL1.MONTH_CNT >= TOT_REL_CNT  THEN TOT_REL_CNT - TBL1.REL_CNT
                        ELSE TBL1.MONTH_CNT - TBL1.REL_CNT
                        END AS REAL_YEN_CNT,

                        (SELECT SUM(REL_AMT) + SUM(ADD_AMT) FROM PRODUCT_NO_DATA WHERE PROD_CD = TBL1.PROD_CD AND NO <![CDATA[<=]]> TBL1.REL_CNT) AS REAL_REL_AMT,

                        CASE WHEN TBL1.MONTH_CNT >= TOT_REL_CNT  THEN (SELECT SUM(REL_AMT) + SUM(ADD_AMT) FROM PRODUCT_NO_DATA WHERE PROD_CD = TBL1.PROD_CD)
                        ELSE (SELECT SUM(REL_AMT) + SUM(ADD_AMT) FROM PRODUCT_NO_DATA WHERE PROD_CD = TBL1.PROD_CD AND NO <![CDATA[<=]]> TBL1.MONTH_CNT)
                        END AS REAL_TOT_AMT
                    FROM
                    (
                        SELECT
                            DR.MST_SEQ,
                            DR.DEBT_DT,
                            DR.ACCNT_NO,
                            DR.REL_CNT AS DEBT_REL_CNT,
                            (SELECT SUM(REL_AMT) + SUM(ADD_AMT) FROM PRODUCT_NO_DATA WHERE PROD_CD = MBID.PROD_CD AND NO <![CDATA[<=]]> DR.MONTH_COUNT) AS DEBT_TOT_AMT,
                            (SELECT SUM(REL_AMT) + SUM(ADD_AMT) FROM PRODUCT_NO_DATA WHERE PROD_CD = MBID.PROD_CD AND NO <![CDATA[<=]]> DR.REL_CNT ) AS DEBT_REL_AMT,
                            (DR.MONTH_COUNT - DR.REL_CNT) AS DEBT_YEN_CNT,
                            DR.SAFE_KEY,
                            CANCEL_YN,
                            CANCEL_DT,
                            MBID.PROD_CD,
                            (TRUNC(MONTHS_BETWEEN(TO_CHAR(SYSDATE,'YYYYMM')||'01',SUBSTR(MBID.JOIN_DT,0,6) || '01')+1)) AS MONTH_CNT,
                            NVL((SELECT COUNT(*) FROM PAY_MNG_DTL WHERE ACCNT_NO = MBID.ACCNT_NO AND DEL_FG = 'N'),0) AS REL_CNT,
                            (SELECT COUNT(*) FROM PRODUCT_NO_DATA WHERE PROD_CD = MBID.PROD_CD AND REL_AMT > 0) AS TOT_REL_CNT
                        FROM TB_DEBT_REG DR INNER JOIN
                             MEM_PROD_ACCNT MBID ON DR.ACCNT_NO = MBID.ACCNT_NO
                        WHERE 1=1
                            AND DR.MST_SEQ = #{mst_seq}
                    ) TBL1
                ) MAIN
       </select>

     <!-- ================================================================
     * 날짜 : 20221230
     * 이름 : 임동진
     * 추가내용 : 채무불이행등재 입금정보
     * 대상 테이블 : TB_DEBT_REG
     * ================================================================
     * -->
       <select id="DlwItoMngMap.getDebtPayInfo" parameterType="map" resultType="resultMap">
            SELECT
                    /*DlwItoMngMap.getDebtPayInfo */
                     MAIN.NO,
                     TRUE_PAY_AMT,
                     RELAT_PAY_AMT,
                     ADD_PAY_AMT,
                     (TRUE_PAY_AMT + RELAT_PAY_AMT + ADD_PAY_AMT) AS TOT_AMT,
                     TO_CHAR(ADD_MONTHS(JOIN_DT, NO -1),'YYYYMMDD') AS MON_NO,
                     (
                         CASE WHEN STAT = 0 THEN '정상'
                              WHEN STAT = 1 THEN '선납'
                         ELSE '연체'
                         END
                     ) AS STAT_NM,
                     CASE WHEN TRUE_PAY_AMT > 0 THEN PAY_DAY1
                          WHEN TRUE_PAY_AMT = 0 AND RELAT_PAY_AMT > 0 THEN PAY_DAY2
                     ELSE PAY_DAY3
                     END AS PAY_DAY,
                     CASE WHEN TRUE_PAY_AMT > 0 THEN LF_DMUSER.FN_COM_NM('01', MAIN.PAY_MTHD1)
                          WHEN TRUE_PAY_AMT = 0 AND RELAT_PAY_AMT > 0 THEN LF_DMUSER.FN_COM_NM('01', MAIN.PAY_MTHD2)
                     ELSE LF_DMUSER.FN_COM_NM('01', MAIN.PAY_MTHD3)
                     END AS PAY_MTHD_NM
             FROM
             (
                 SELECT
                     ACCNT_NO,
                     NO,
                     MAX(JOIN_DT) AS JOIN_DT,
                     MAX((CASE WHEN PAY_GNB = 'PM' THEN PAY_AMT ELSE 0 END)) AS TRUE_PAY_AMT,
                     MAX((CASE WHEN PAY_GNB = 'PM1' THEN PAY_AMT ELSE 0 END)) AS RELAT_PAY_AMT,
                     MAX((CASE WHEN PAY_GNB = 'PM2' THEN PAY_AMT ELSE 0 END)) AS ADD_PAY_AMT,
                     MIN((CASE WHEN PAY_GNB = 'PM' THEN PAY_DAY ELSE '99999999' END)) AS PAY_DAY1,
                     MIN((CASE WHEN PAY_GNB = 'PM1' THEN PAY_DAY ELSE '99999999' END)) AS PAY_DAY2,
                     MIN((CASE WHEN PAY_GNB = 'PM2' THEN PAY_DAY ELSE '99999999' END)) AS PAY_DAY3,
                     MIN((CASE WHEN PAY_GNB = 'PM' THEN STAT ELSE 9 END)) AS STAT,
                     MIN((CASE WHEN PAY_GNB = 'PM' THEN PAY_MTHD ELSE '99' END)) AS PAY_MTHD1,
                     MIN((CASE WHEN PAY_GNB = 'PM1' THEN PAY_MTHD ELSE '99' END)) AS PAY_MTHD2,
                     MIN((CASE WHEN PAY_GNB = 'PM2' THEN PAY_MTHD ELSE '99' END)) AS PAY_MTHD3,
                     ROW_NUMBER() OVER(ORDER BY NO DESC, PAY_DAY DESC) AS PAGE_INDX
                 FROM
                 (
                     SELECT
                         MPA.ACCNT_NO,
                         MPA.JOIN_DT,
                         'PM' AS PAY_GNB,
                         PM.PAY_MTHD,
                         PM.STAT,
                         PM.NO,
                         PM.PAY_AMT,
                         PM.PAY_DAY
                     FROM LF_DMUSER.MEM_PROD_ACCNT MPA LEFT OUTER JOIN
                         LF_DMUSER.PAY_MNG PM ON MPA.ACCNT_NO = PM.ACCNT_NO AND PM.DEL_FG = 'N'
                     WHERE 1=1
                     AND MPA.ACCNT_NO = #{accnt_no}
                     AND MPA.DEL_FG = 'N'

                     UNION ALL

                     SELECT
                         MPA.ACCNT_NO,
                         MPA.JOIN_DT,
                         'PM1' AS PAY_GNB,
                         PM.PAY_MTHD,
                         PM.STAT,
                         PM.NO,
                         PM.PAY_AMT,
                         PM.PAY_DAY
                     FROM LF_DMUSER.MEM_PROD_ACCNT MPA LEFT OUTER JOIN
                         LF_DMUSER.PAY_MNG_DTL PM ON MPA.ACCNT_NO = PM.ACCNT_NO AND PM.DEL_FG = 'N'
                     WHERE 1=1
                     AND MPA.ACCNT_NO = #{accnt_no}
                     AND MPA.DEL_FG = 'N'

                     UNION ALL

                     SELECT
                         MPA.ACCNT_NO,
                         MPA.JOIN_DT,
                         'PM2' AS PAY_GNB,
                         PM.PAY_MTHD,
                         PM.STAT,
                         PM.NO,
                         PM.PAY_AMT,
                         PM.PAY_DAY
                     FROM LF_DMUSER.MEM_PROD_ACCNT MPA LEFT OUTER JOIN
                         LF_DMUSER.PAY_MNG_DTL1 PM ON MPA.ACCNT_NO = PM.ACCNT_NO AND PM.DEL_FG = 'N'
                     WHERE 1=1
                     AND MPA.ACCNT_NO = #{accnt_no}
                     AND MPA.DEL_FG = 'N'
                 )
                 GROUP BY ACCNT_NO, NO, PAY_DAY
                 HAVING NO > 0
             ) MAIN
             ORDER BY MAIN.NO DESC
       </select>

