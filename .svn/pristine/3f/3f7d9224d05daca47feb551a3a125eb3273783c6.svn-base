     * 대상 테이블 : RESCISSION , MEM_PROD_ACCNT , MEMBER , PRODUCT , TB_MEMBER_BASIC_INFO_DAY
     * ================================================================
     * -->
       <select id="DlwItoMngMap.getResnDayCloseList2" parameterType="map" resultType="resultMap">
          <![CDATA[
          WITH TBLMAIN
          AS
          ( SELECT /* DlwItoMngMap.getResnDayCloseList2 */
                     ACCNT_NO
                 , MEM_NM
                 , PROD_CD
                 , PROD_NM
                 , JOIN_DT
                 , DAMDANG
                 , DEPT_NM
                 , RESN_ACPT_DAY
                 , RESN_PROC_DAY
                 , RESN_SEND_DAY
                 , RESON
                 , COMP_RESON
                 , RESN_MTHD
                 , BANK_ACCNT_NO
                 , BANK_CD
                 , BANK_NM
                 , DEPR
