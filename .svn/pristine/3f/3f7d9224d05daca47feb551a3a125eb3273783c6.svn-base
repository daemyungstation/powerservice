                   WHERE 1=1
                     AND RS.DEL_FG = 'N'
                     AND RESN_PROC_DAY = #{resn_proc_day}
                     AND RESN_CL = '03'
                     AND RESN_PROC_YN = 'Y'

                   UNION ALL

                  SELECT RS.ACCNT_NO
