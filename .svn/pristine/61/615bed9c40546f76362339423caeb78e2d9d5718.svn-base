	var reVal = fn_diaLog(pop_id, -1, -1, 1450, 920, "DL430000::DL430102.xfdl", false, sPrmr, true, false, true, null, true, true);
}

function cbo_event_yn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
