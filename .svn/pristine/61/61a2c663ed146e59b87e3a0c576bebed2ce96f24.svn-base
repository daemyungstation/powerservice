function fn_commSearch(sType, searchInfo)
{	
    if (!fn_preSearch()) return false;	
        
	switch(searchInfo) {
		case "selectNonPayPopList": 
			ds_listNonPay.clearData();	
		
			var szSvcid       = "selectNonPayPopList";
			var szController  = "/dlw/nonPay/selectNonPayPopList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listNonPayTemp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
		
		case "selectNonPayPopDetailList": 
			if(!gfn_pageSearch(sType, ds_listNonPayDetail, sta_TotalCnt, 500, nTotalListCnt)) return;		
			
			var szSvcid       = "selectNonPayPopDetailList";
			var szController  = "/dlw/nonPay/selectNonPayPopDetailList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listNonPayDetailTemp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
	}
	
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

