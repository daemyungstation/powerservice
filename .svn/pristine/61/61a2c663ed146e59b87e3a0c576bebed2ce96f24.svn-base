function btn_createNonPay_onclick(obj:Button,  e:ClickEventInfo)
{

	if(ds_listNonPay.rowcount == 0){
		alert("미납산출 대상이 없습니다.");
		return false;
	}	
	
	if ( gfn_isEmpty(edt_diff_cnt_stt01.value) ) {
		alert("산출대상수를 입력해주세요.");
		return false;
	}
		
	var totCnt = ds_listNonPay.getColumn(0,"tot_cnt") * 1;
	var retCnt = ds_listNonPay.getColumn(0,"ret_cnt")* 1;
	var resnCnt = ds_listNonPay.getColumn(0,"resn_cnt")* 1;
	var evtCnt = ds_listNonPay.getColumn(0,"event_cnt")* 1;
	var shareCnt = ds_listNonPay.getColumn(0,"share_cnt")* 1;
	
	var diffCnt = totCnt - (retCnt + resnCnt + evtCnt + shareCnt)
	var MakeCnt = edt_diff_cnt_stt01.value  * 1;
	
	trace(diffCnt);
		
	if(MakeCnt > diffCnt){
		alert("산출대상수가 분배대상수보다 많습니다.");
		return false;
	}
	
	if(confirm("미납대상에 대한 내용을 산출 하시겠습니까?") == false) 
	{
		return false;
	}
	
	ds_inqCndt.clearData();
	var nRow = ds_inqCndt.addRow();
	
	ds_inqCndt.setColumn(nRow, "ext_dt"    , div_monthCal.getDate());
	ds_inqCndt.setColumn(nRow, "mst_seq"    , xTit);
	ds_inqCndt.setColumn(nRow, "diff_cnt_stt"    , edt_diff_cnt_stt.value);
	ds_inqCndt.setColumn(nRow, "diff_cnt_end"    , edt_diff_cnt_end.value);
	ds_inqCndt.setColumn(nRow, "section_t_np"    , cbo_section_t_np.value);
	ds_inqCndt.setColumn(nRow, "dCnt"    , edt_diff_cnt_stt01.value);

	ds_inqCndt.setColumn(nRow, "section_thr", cbo_sectionthr.value);	
	ds_inqCndt.setColumn(nRow, "accnt_no", edt_accnt_no.value);
	ds_inqCndt.setColumn(nRow, "diff_n", chk_diffN.value);
	ds_inqCndt.setColumn(nRow, "accnt_no_n", chk_accntNoN.value);
	ds_inqCndt.setColumn(nRow,"col_no", colNo);
	ds_inqCndt.setColumn(nRow,"np_gbn", cbo_nonPayGubun.value);
	
	var szSvcid       = "insertNonPayMentDtl";
	var szController  = "/dlw/nonPay/insertNonPayMentDtl";
	var	szInDs        = "ds_input=ds_inqCndt";		
	var	szOutDs       = "";
	var	szParam       = "";
	var	szRetCallback = "fn_callBack";
		
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
	
}

