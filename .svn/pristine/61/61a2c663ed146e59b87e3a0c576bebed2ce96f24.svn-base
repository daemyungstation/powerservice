    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

//var xMasterNo;

var nTotalListCnt = 0;
var xToday;
var xTit;
var xGubun;
var colNo;
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL562001P01_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정;
	
	fn_init();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/

function fn_callBack(svcid, errcd, errmsg) 
{
    if (errcd < 0) 
    {
		alert("조회를 실패하였습니다.\n" + errmsg);
    } 
    else 
    {
		switch (svcid) {	
			
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
				gfn_getCode("0189", "SELECT", ds_section3, gds_DmlCd); // 섹션3
				cbo_sectionthr.index = 0;
			break;
			
			case "selectNonPayPopList":				
				gfn_appendSearchDs(ds_listNonPay, ds_listNonPayTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				//gfn_setTotalRowCount(ds_listNonPay, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출											
				fn_postSearch();	
				ds_listNonPayDetail.clearData();
				sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
			break;			
			
			case "selectNonPayPopDetailList":				
				gfn_appendSearchDs(ds_listNonPayDetail, ds_listNonPayDetailTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_listNonPayDetail, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출							
			break;
										
			case "insertNonPayMentDtl": 
				alert("생성완료되었습니다.");
				opener.fn_commSearch(gfn_getCommTypeInit(), "selectNonPayList");
				close();
			break;
			
		}
    }
}

function fn_postSearch()
{
	if(ds_listNonPay.getRowCount() < 1) 
	{
		alert("조회 결과가 없습니다.");
	} else {

	}
	return;
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch()
{
	ds_inqCndt.clearData();
	var nRow = ds_inqCndt.addRow();
	
	if ( gfn_isEmpty(div_monthCal.getDate()) ) {
		alert("미납생성월은 필수 입력입니다.");
		return false;
	}
	
	if (!gfn_isEmpty(edt_diff_cnt_end.value) ) {
		if (gfn_isEmpty(edt_diff_cnt_stt.value) ) {
			alert("시작 연체회차를 입력해주세요.");
			return false;
		}	
	}
	
	if (!gfn_isEmpty(edt_diff_cnt_stt.value) ) {
		if (gfn_isEmpty(edt_diff_cnt_end.value) ) {
			alert("종료 연체회차를 입력해주세요.");
			return false;
		}	
	}
	
	var sttNo = edt_diff_cnt_stt.value  * 1;
	var endNo = edt_diff_cnt_end.value  * 1;
	
	if(sttNo > endNo){
		alert("시작 연체회차가 종료 연체회차보다 큽니다.");
		return false;
	}
	
	ds_inqCndt.setColumn(nRow, "ext_dt", div_monthCal.getDate());
	ds_inqCndt.setColumn(nRow, "section_t_np", cbo_section_t_np.value);	
	ds_inqCndt.setColumn(nRow, "diff_cnt_stt", edt_diff_cnt_stt.value);
	ds_inqCndt.setColumn(nRow, "diff_cnt_end", edt_diff_cnt_end.value);
	ds_inqCndt.setColumn(nRow, "section_thr", cbo_sectionthr.value);	
	ds_inqCndt.setColumn(nRow, "accnt_no", edt_accnt_no.value);
	ds_inqCndt.setColumn(nRow, "diff_n", chk_diffN.value);
	ds_inqCndt.setColumn(nRow, "special_yn", chk_specialYn.value);
	ds_inqCndt.setColumn(nRow, "accnt_no_n", chk_accntNoN.value);
	ds_inqCndt.setColumn(nRow,"col_no",colNo);

	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
