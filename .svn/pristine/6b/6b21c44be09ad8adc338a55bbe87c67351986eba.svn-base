		ds = ds_t2_list;
		grid = tab_main.tabpage2.grd_list;
		title = "행사자별";
	} else if (tab_main.tabindex == 3) {
		ds = ds_t3_list;
		grid = tab_main.tabpage3.grd_list;
		title = "상품분류별";
