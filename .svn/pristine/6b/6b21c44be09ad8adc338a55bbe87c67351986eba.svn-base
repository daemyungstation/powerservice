	var ds, grid, title;
	if (tab_main.tabindex == 0) {
		ds = ds_t0_list;
		grid = tab_main.tabpage0.grd_list;
		title = "지부별평균";
	} else if (tab_main.tabindex == 1) {
		ds = ds_t1_list;
		grid = tab_main.tabpage1.grd_list;
