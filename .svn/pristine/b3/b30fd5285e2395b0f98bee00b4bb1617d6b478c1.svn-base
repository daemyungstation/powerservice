// Search Validate Check
function fn_preSearch() {
	// 검색조건 셋팅
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	
	return true;
}

// View Search Validate Check
function fn_preViewSearch() {
	// 검색조건 셋팅
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	if (ds_list_tree.rowposition > -1) {
		var sEvltTypId = ds_list_tree.getColumn(ds_list_tree.rowposition, "evlt_typ_id");
		ds_inqCndt.setColumn(0, "evlt_typ_id", sEvltTypId);
	}
	
	return true;
}
// Save Validate Check
function fn_preSaveData() {
	if(gfn_isNull(ds_list_view.getColumn(0, "evlt_typ_nm"))){
		alert("유형명을 입력하세요.");
		return false;
	}
	if(gfn_isNull(ds_list_view.getColumn(0, "expe_sqnc"))){
		alert("순서를 선택하세요.");
		return false;
	}
	
	if(gfn_getLengthB(ds_list_view.getColumn(0, "evlt_bas_cntn")) > 500){
		alert("평가기준내용 최대 500Byte를 넘을수 없습니다.");
		return false;
	}	

	var sEvltTypId = ds_list_view.getColumn(0, "evlt_typ_id");
	var sHgrnEvltTypId = ds_list_view.getColumn(0, "hgrn_evlt_typ_id");
	var sExpeSqnc = ds_list_view.getColumn(0, "expe_sqnc");
	var nRow = ds_list_tree.findRowExpr("expe_sqnc=='"+sExpeSqnc+"' && hgrn_evlt_typ_id=='"+sHgrnEvltTypId+"'");
	
	if (nRow > -1 && ds_list_tree.rowposition != nRow) {
		if (!confirm("중복되는 순서가 있습니다. 순서를 자동증가하고 저장하시겠습니까?")) {
			return false;
		}
		
		ds_list_view.setColumn(0, "dup_yn", "Y");
		if (!gfn_isNull(sEvltTypId)) {
			ds_list_view.setColumn(0, "old_expe_sqnc", ds_list_tree.getColumn(ds_list_tree.findRow("evlt_typ_id", sEvltTypId), "expe_sqnc"));
		} else {
			ds_list_view.setColumn(0, "old_expe_sqnc", ds_expeSqnc.getColumn(ds_expeSqnc.rowcount, "cd"));
		}
		
	} else {
		if(!confirm("저장하시겠습니까?")) {
			return false;
		}
	}
	
	return true;
}

