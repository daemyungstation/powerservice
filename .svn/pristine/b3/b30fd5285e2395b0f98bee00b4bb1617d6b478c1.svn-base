/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
		case "getEvltTypTreeView":
			ds_list_tree.clearData();
			gfn_appendSearchDs(ds_list_tree, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			ds_list_tree.rowposition = -1; 
			fn_viewSearch();
			break;	
		
		case "getView":
			ds_list_view.clearData();
			gfn_appendSearchDs(ds_list_view, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			setMode("R");
			fn_commSearch_right();
			break;
		
		case "getTypSave":
			fn_init();
			break; 	
			
		case "getTypdelete":
			fn_init();
			break; 
		}
 	}		
}

