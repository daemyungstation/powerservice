// Save Validate Check
function fn_preSaveData_right() {
	var strLength = 0; // 길이체크
	
	if (gfn_isNull(ds_evltTyp_tmp.getColumn(0, "evlt_item_nm"))){
		alert("항목명을 입력하세요.");
		return false;
	}
	if (gfn_isNull(ds_evltTyp_tmp.getColumn(0, "evlt_item_scr"))){
		alert("점수를 입력하세요.");
		return false;
	}
	if (gfn_isNull(ds_evltTyp_tmp.getColumn(0, "expe_sqnc"))){
		alert("순서를 선택하세요.");
		return false;
	}
	
	if (gfn_getLengthB(ds_evltTyp_tmp.getColumn(0, "evlt_item_nm")) > 250) {
		alert("항목명은 최대 250Byte를 넘을수 없습니다.");
		return false;
	}	
	
	if (gfn_getLengthB(ds_evltTyp_tmp.getColumn(0, "evlt_item_expi")) > 500) {
		alert("설명은 최대 500Byte를 넘을수 없습니다.");
		return false;
	}
	
	var sExpeSqnc = ds_evltTyp_tmp.getColumn(0, "expe_sqnc");
	var sEvltTypId = ds_evltTyp_tmp.getColumn(0, "evlt_typ_id");
	var nRow = ds_list_evltTyp.findRowExpr("expe_sqnc=='"+sExpeSqnc+"' && evlt_typ_id=='"+sEvltTypId+"'");
	if (nRow>-1 && (gfn_isNull(sEvltTypId) || nRow != ds_list_evltTyp.rowposition)) {
		if (!confirm("중복되는 순서가 있습니다. 순서를 자동증가하고 저장하시겠습니까?")) {
			return false;
		}
		
		ds_evltTyp_tmp.setColumn(0, "dup_yn", "Y");
		if (!gfn_isNull(sEvltTypId)) {
			ds_evltTyp_tmp.setColumn(0, "old_expe_sqnc", ds_list_evltTyp.getColumn(ds_list_evltTyp.rowposition, "expe_sqnc"));
		} else {
			ds_evltTyp_tmp.setColumn(0, "old_expe_sqnc", ds_expeSqnc_right.getColumn(ds_expeSqnc_right.rowcount, "cd"));
		}
		
	} else {
		if(!confirm("저장하시겠습니까?")) {
			return false;
		}
	}
	
	return true;
}

