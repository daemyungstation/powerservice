function btn_onclick(obj:Button,  e:ClickEventInfo) {
	switch(obj.name) {
	case "btn_new" :
		setMode("C");
		break;
		
	case "btn_cancel" :
		fn_viewSearch();
		break;
		
	case "btn_save" :
		fn_save();
		break;
		
	case "btn_update" :
		setMode("U");
		break;
		
	case "btn_delete" :
		if(confirm("평가유형을 삭제하시겠습니까?")) {
			fn_delete();
		}
		break;
		
	}
}

// 좌측 모드 컨트롤
function setMode(mode:String) {
	switch(mode) {
	case "C" :
		var sEvltTypId = ds_list_view.getColumn(0, "evlt_typ_id");
		var sEvltTypNm = ds_list_view.getColumn(0, "evlt_typ_nm");
		var sEvltTypLvlCnt = ds_list_view.getColumn(0, "evlt_typ_lvl_cnt");
		if(sEvltTypLvlCnt >= "3"){
			alert("소분류까지만 등록할 수 있습니다.");
			return false;
		}
		
		// 정렬순서 Setting
		var nChildCnt = 1;
		if (ds_list_tree.rowposition > -1) {
			nChildCnt = ds_list_tree.getCaseCount("hgrn_evlt_typ_id=='"+sEvltTypId+"'") + 1;
		} else {
			nChildCnt = ds_list_tree.getCaseCount("hgrn_evlt_typ_id==null") + 1;
		}
		ds_expeSqnc.clearData();
		for (var i=0; i<nChildCnt; i++) {
			ds_expeSqnc.addRow();
			ds_expeSqnc.setColumn(i, "cd", i+1);
			ds_expeSqnc.setColumn(i, "cd_nm", i+1);
		}
		
		ds_list_view.clearData();
		ds_list_view.addRow();
		ds_list_view.setColumn(0, "hgrn_evlt_typ_id", sEvltTypId);
		ds_list_view.setColumn(0, "hgrn_evlt_typ_nm", sEvltTypNm);
		ds_list_view.setColumn(0, "evlt_typ_lvl_cnt", toNumber(sEvltTypLvlCnt)+1);
		ds_list_view.setColumn(0, "expe_sqnc", nChildCnt);
		
		btn_save.visible = true;
		btn_cancel.visible = true;
		btn_new.visible = false;
		btn_update.visible = false;
		btn_delete.visible = false;
		
		edt_evlt_typ_nm.readonly = false;			// 유형명
		cbo_expeSqnc.readonly = false;				// 순서
		TextArea_evlt_bas_cntn.readonly = false;	// 평가기준내용
		break;
		
	case "U" :
		var sHgrnEvltTypId = ds_list_view.getColumn(0, "hgrn_evlt_typ_id");
		var sEvltTypNm = ds_list_view.getColumn(0, "evlt_typ_nm");
		
		// 정렬순서 Setting
		var nChildCnt = 1;
		if (ds_list_tree.rowposition > -1) {
			nChildCnt = ds_list_tree.getCaseCount("hgrn_evlt_typ_id=='"+sHgrnEvltTypId+"'");
		}
		ds_expeSqnc.clearData();
		for (var i=0; i<nChildCnt; i++) {
			ds_expeSqnc.addRow();
			ds_expeSqnc.setColumn(i, "cd", i+1);
			ds_expeSqnc.setColumn(i, "cd_nm", i+1);
		}
		
		btn_save.visible = true;
		btn_cancel.visible = true;
		btn_new.visible = false;
		btn_update.visible = false;
		btn_delete.visible = false;
		
		edt_evlt_typ_nm.readonly = false;			// 유형명
		cbo_expeSqnc.readonly = false;				// 순서
		TextArea_evlt_bas_cntn.readonly = false;	// 평가기준내용
		break;
		
	case "R" :
		//콤보박스 자동증가
		ds_expeSqnc.clearData();
		ds_expeSqnc.addRow();
		ds_expeSqnc.setColumn(0, "cd", ds_list_view.getColumn(ds_list_view.rowposition, "expe_sqnc"));
		ds_expeSqnc.setColumn(0, "cd_nm", ds_list_view.getColumn(ds_list_view.rowposition, "expe_sqnc"));
				
		btn_save.visible = false;
		btn_cancel.visible = false;
		btn_new.visible = true;
		
		if (ds_list_tree.rowposition > -1 && ds_list_tree.getColumn(ds_list_tree.rowposition, "evlt_typ_id") != null) {
			btn_update.visible = true;
			btn_delete.visible = true;
			
			btn_update.position2.left = 188;
			btn_new.position2.left = 114;
		} else {
			btn_update.visible = false;
			btn_delete.visible = false;
			
			btn_new.position2.left = 262;
		}
		
		edt_evlt_typ_nm.readonly = true;				// 유형명
		cbo_expeSqnc.readonly = true					// 순서
		TextArea_evlt_bas_cntn.readonly = true			// 평가기준내용
		
		break;
	}
	
	if ("N" == ds_athrPgmFilter.getColumn(0, 'rgsn_yn')) btn_new.visible = false;
	if ("N" == ds_athrPgmFilter.getColumn(0, 'amnt_yn')) btn_update.visible = false;
	if ("N" == ds_athrPgmFilter.getColumn(0, 'dltn_yn')) btn_delete.visible = false;
}


