	case "C" :
		var sEvltTypLvlCnt = ds_list_tree.getColumn(ds_list_tree.rowposition, "evlt_typ_lvl_cnt");
		if(sEvltTypLvlCnt != "3"){
			alert("평가유형 소분류만 평가유형이 추가 가능 합니다.");
			return false;
		}
		
		// 정렬순서 Setting
		var sEvltTypId = ds_list_tree.getColumn(ds_list_tree.rowposition, "evlt_typ_id");
		var nChildCnt = ds_list_evltTyp.getCaseCount("evlt_typ_id=='"+sEvltTypId+"'") + 1;
		ds_expeSqnc_right.clearData();
		for (var i=0; i<nChildCnt; i++) {
			ds_expeSqnc_right.addRow();
			ds_expeSqnc_right.setColumn(i, "cd", i+1);
			ds_expeSqnc_right.setColumn(i, "cd_nm", i+1);
		}
		
		
		ds_evltTyp_tmp.clearData();
		ds_evltTyp_tmp.addRow();
		ds_evltTyp_tmp.setColumn(0, "evlt_typ_id", sEvltTypId);
		ds_evltTyp_tmp.setColumn(0, "expe_sqnc", nChildCnt);
		
		btn_save_right.visible = true;
		btn_cancel_right.visible = true;
		btn_new_right.visible = false;
		btn_update_right.visible = false;
		
		edt_evlt_item_nm.readonly = false;
		edt_evlt_item_scr.readonly = false;
		cbo_expeSqnc2.readonly = false;
		TextArea_evlt_item_expi.readonly = false;
		break;
		
	case "U" :
		// 정렬순서 Setting
		var sEvltTypId = ds_evltTyp_tmp.getColumn(ds_evltTyp_tmp.rowposition, "evlt_typ_id");
		var nChildCnt = ds_list_evltTyp.getCaseCount("evlt_typ_id=='"+sEvltTypId+"'");
		ds_expeSqnc_right.clearData();
		for (var i=0; i<nChildCnt; i++) {
			ds_expeSqnc_right.addRow();
			ds_expeSqnc_right.setColumn(i, "cd", i+1);
			ds_expeSqnc_right.setColumn(i, "cd_nm", i+1);
		}
		
		btn_save_right.visible = true;
		btn_cancel_right.visible = true;
		btn_new_right.visible = false;
		btn_update_right.visible = false;
		
		edt_evlt_item_nm.readonly = false;
		edt_evlt_item_scr.readonly = false;
		cbo_expeSqnc2.readonly = false;
		TextArea_evlt_item_expi.readonly = false;
		break;
		
	case "R" :
		ds_evltTyp_tmp.clearData();
		ds_evltTyp_tmp.addRow();
		
		if (ds_list_evltTyp.rowposition > -1) {
			ds_evltTyp_tmp.copyRow(0, ds_list_evltTyp, ds_list_evltTyp.rowposition);
			
			//콤보박스 자동증가
			ds_expeSqnc_right.clearData();
			ds_expeSqnc_right.addRow();
			ds_expeSqnc_right.setColumn(0, "cd", ds_evltTyp_tmp.getColumn(ds_evltTyp_tmp.rowposition, "expe_sqnc"));
			ds_expeSqnc_right.setColumn(0, "cd_nm", ds_evltTyp_tmp.getColumn(ds_evltTyp_tmp.rowposition, "expe_sqnc"));
		}
				
		btn_save_right.visible = false;
		btn_cancel_right.visible = false;
		btn_new_right.visible = true;
		
		if (ds_evltTyp_tmp.getColumn(ds_evltTyp_tmp.rowposition, "evlt_typ_id") != null) {
			btn_update_right.visible = true;
			
			btn_update_right.position2.right = 10;
			btn_new_right.position2.right = 90;
		} else {
			btn_update_right.visible = false;
			
			btn_new_right.position2.right = 10;
		}
		
		edt_evlt_item_nm.readonly = true;
		edt_evlt_item_scr.readonly = true;
		cbo_expeSqnc2.readonly = true;
		TextArea_evlt_item_expi.readonly = true;
		break;
	}
	
	if ("N" == ds_athrPgmFilter.getColumn(0, 'rgsn_yn')) btn_new_right.visible = false;
	if ("N" == ds_athrPgmFilter.getColumn(0, 'amnt_yn')) btn_update_right.visible = false;
	if ("N" == ds_athrPgmFilter.getColumn(0, 'dltn_yn')) btn_delete_right.visible = false;
}
]]></Script>
  </Form>
</FDL>
