// 우측 버튼 이벤트
function btn_right_onclick(obj:Button,  e:ClickEventInfo) {
	switch(obj.name) {
	case "btn_new_right" :
		setMode_right("C");
		break;
		
	case "btn_cancel_right" :
		setMode_right("R");
		break;
		
	case "btn_save_right" :
		fn_save_right();
		break;
		
	case "btn_update_right" :
		setMode_right("U");
		break;
		
	case "btn_delete_right" :
		var nDeleteCnt = ds_list_evltTyp.getCaseCount("_chk=='1'");            
		if (nDeleteCnt < 1) {
			alert("삭제할 평가항목을 선택하세요.");
			return;
		}	
		
		if(!confirm("평가항목을 삭제하시겠습니까?")) {
			return false;
		}
		fn_delete_right();
		break;
	
	case "btn_copy_right" :
		var nDeleteCnt = ds_list_evltTyp.getCaseCount("_chk=='1'");            
		if (nDeleteCnt < 1) {
			alert("복사할 평가항목을 선택하세요.");
			return;
		}	
		
		if(!confirm("복사하시겠습니까?")) {
			return false;
		}
		fn_copy_right();
		break;
	
	}
}

