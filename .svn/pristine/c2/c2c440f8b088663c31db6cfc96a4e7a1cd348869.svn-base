/********************************************************************************************/
/* 1. Include Library include                                                               */
/********************************************************************************************/
include "LIB::lib_All.xjs";




/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트,전역변수,초기화) 처리                         	*/
/********************************************************************************************/
//온로드
function PS090404_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj);
	fn_commSearch(gfn_getCommTypeInit(),athr_cd,gds_cd,pgm_cd); // 조회
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
	if (errcd < 0) {
		trace(errcd);
		alert("errcd :  "+errcd + "  errmsg :  "+errmsg);
	}else{
		switch(svcid){
			case "srchPgmIndvFnctAthr"://조회콜백
				gfn_appendSearchDs(ds_list_pgmIndvFnctAthr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				fn_postSearch();
			break;
			
			case "insertPgmmIndvFnctAthr" ://권한선택
				if(lvl_cnt == "1"){
					opener.fn_commSearch_majorPgmAthr(gfn_getCommTypeInit());
				}else if(lvl_cnt == "2"){
					opener.fn_commSearch_middlePgmAthr(gfn_getCommTypeInit());
				}else if(lvl_cnt == "3"){
					opener.fn_commSearch_minorPgmAthr(gfn_getCommTypeInit());
				}
				
				close();
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch(sType,athr_cd,gds_cd,pgm_cd)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		//ds_faqDmndList.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "athr_cd", athr_cd);
		ds_inqCndt.setColumn(0, "pgm_cd", pgm_cd);
		ds_inqCndt.setColumn(0, "gds_cd", gds_cd);
		ds_inqCndt.setColumn(0, "orderBy", "pgm_indv_fnct_nm");
		ds_inqCndt.setColumn(0, "orderDirection", "asc");
	}
	return true;
}

//선택전 insert 처리
function fn_preInsert()
{
	for(var i=ds_list_pgmIndvFnctAthr.rowcount-1;i>=0;i--) {
		if(ds_list_pgmIndvFnctAthr.getColumn(i, "_chk") == 1) {
			ds_list_pgmIndvFnctAthr.setRowType(i,4);
		}else{
			ds_list_pgmIndvFnctAthr.setRowType(i,1);
		}
	}
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType,athr_cd,gds_cd,pgm_cd)
{	
    if (!fn_preSearch(sType,athr_cd,gds_cd,pgm_cd)) return false;
	
	var szSvcid       = "srchPgmIndvFnctAthr";
    var szController  = "/syst/pgm-indv-fnct-athr/add-list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//프로그램 개별기능  권한 등록
function fn_insertData()
{	
	if (!fn_preInsert()) return false;

	var szSvcid       = "insertPgmmIndvFnctAthr";
    var szController  = "/syst/pgm-indv-fnct-athr/insert";
    var szInDs        = "ds_input=ds_list_pgmIndvFnctAthr::U";
    var szOutDs       = "";
    var szParam       = "athr_cd="+athr_cd;
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}



/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//처리결과
function fn_postSearch()
{
	if(ds_list_pgmIndvFnctAthr.rowcount < 1) {
		btn_select.enable = false;
		alert("조회 결과가 없습니다.");
	} else {
		btn_select.enable = true;
	}
	return;
}


/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){
		case btn_close : //닫기버튼
			close();
		break;
		
		case btn_select: //선택 버튼
		if(!gfn_chkSelectValidation(ds_list_pgmIndvFnctAthr)){
			alert("선택한 개별기능이 없습니다.");
			return false;
		}else if (confirm("선택한 개별기능 권한을 추가하시겠습니까?")) {
			fn_insertData();
		}
		break;
	}
}
	
		
//해더클릭이벤트
function grd_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (!fn_preSearch()) return false;
	
	ds_inqCndt.setColumn(0, "sController", "/syst/pgm-indv-fnct-athr/add-list");
	ds_inqCndt.setColumn(0, "dsName", "ds_output");
	gfn_grdHeadClick(obj, e, ds_inqCndt);
}
]]></Script>
