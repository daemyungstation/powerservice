// 초기화
function fn_init()
{
	var sTabPageId = tab_eventMng.tabpages[tab_eventMng.tabindex].name;
	
	if(sTabPageId == "tab_main")
	{
		tab_eventMng.tab_main.Combo_gubun.index = 0;
		tab_eventMng.tab_main.branch_nm.value = "";
		ds_list.clearData();
		ds_listDetail.clearData();
	}
	else if(sTabPageId == "tab_EvntMan")
	{
		tab_eventMng.tab_EvntMan.edt_evt_mngr_nm.value = "";
		tab_eventMng.tab_EvntMan.cbo_job_duty.value = "";
		tab_eventMng.tab_EvntMan.cbo_branch.value = "";
		tab_eventMng.tab_EvntMan.cbo_use_yn.value = "";
		ds_listEvntMan.clearData();
	}
	else if(sTabPageId == "tab_Branch")
	{
		tab_eventMng.tab_Branch.Combo_gubun.value = "";
		tab_eventMng.tab_Branch.branch_nm.value = "";
		ds_listBranch.clearData();
	}
	else if(sTabPageId == "tab_Warehouse")
	{
		tab_eventMng.tab_Warehouse.edt_wh_nm.value = "";
		ds_listWarehouse.clearData();
	}
}

// 조회결과
function fn_postSearch(sSvcId)
{
    switch (sSvcId)
	{
		case "getMainList" :
			if(ds_list.getRowCount() <= 0)
			{
				alert("조회결과가 없습니다.");
				return false;
			} else {
				ds_inqCndt.setColumn(0, "branch", ds_list.getColumn(ds_list.rowposition, "branch_cd"));
				fn_commSearch(gfn_getCommTypeInit(), "getDetailList");
			}
		break;

		case "getEventManList" :
			if(ds_listEvntMan.getRowCount() <= 0)
			{
				alert("조회결과가 없습니다.");
				return false;
			}
		break;
		
		case "getEvntBranchList" :
			if(ds_listBranch.getRowCount() <= 0)
			{
				alert("조회결과가 없습니다.");
				return false;
			}
		break;
		
		case "getEvntWHList" : 
			if(ds_listWarehouse.getRowCount() <= 0)
			{
				alert("조회결과가 없습니다.");
				return false;
			}
		break;
	}
}

