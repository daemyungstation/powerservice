/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0)
	{
		return false;
	}

	switch (svcid)
	{
		//  공통코드
		case "selectDlwCode": // 대명코드 조회
			gfn_getCode("0138", "ALL", ds_evt_jik, gds_DmlCd); // 직책
			
			fn_init();
		break;

		case "getMainList" :
			fn_postSearch("getMainList");
		break;

		case "getEventManList" :
			fn_postSearch("getEventManList");
		break;

		case "getEvntBranchList" :
			fn_postSearch("getEvntBranchList");
		break;

		case "getEvntWHList" : 
			fn_postSearch("getEvntWHList");
		break;
		
		case "srchComboJibu":
			ds_jibu.insertRow(0);
			ds_jibu.setColumn(0, "branch_nm", "전체");
		break;
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType, sSvcId)
{
	if (gfn_isNull(sSvcId) == true)
	{
	    alert("서비스명이 올바르지 않습니다.");
	}

	switch(sSvcId)
	{

		case "getMainList" :
			ds_inqCndt.clearData();
			var nRow = ds_inqCndt.addRow();
			
			ds_inqCndt.setColumn(nRow, "dir_out_gubun", tab_eventMng.tab_main.Combo_gubun.value);
			ds_inqCndt.setColumn(nRow, "branch_nm", tab_eventMng.tab_main.branch_nm.value);
		break;

		case "getEventManList" :
			ds_inqCndtEvntMan.clearData();
			var nRow = ds_inqCndtEvntMan.addRow();
			
			ds_inqCndtEvntMan.setColumn(nRow, "evt_mngr_nm", tab_eventMng.tab_EvntMan.edt_evt_mngr_nm.value);
			ds_inqCndtEvntMan.setColumn(nRow, "job_duty", tab_eventMng.tab_EvntMan.cbo_job_duty.value);
			ds_inqCndtEvntMan.setColumn(nRow, "branch", tab_eventMng.tab_EvntMan.cbo_branch.value);
			ds_inqCndtEvntMan.setColumn(nRow, "use_yn", tab_eventMng.tab_EvntMan.cbo_use_yn.value);
		break;

		case "getEvntBranchList" :
			ds_inqCndtBranch.clearData();
			var nRow = ds_inqCndtBranch.addRow();
			
			ds_inqCndtBranch.setColumn(nRow, "dir_out_gubun", tab_eventMng.tab_Branch.Combo_gubun.value);
			ds_inqCndtBranch.setColumn(nRow, "branch_nm", tab_eventMng.tab_Branch.branch_nm.value);
		break;
		
		case "getEvntWHList" : 
			ds_inqCndtWarehouse.clearData();
			var nRow = ds_inqCndtWarehouse.addRow();
			
			ds_inqCndtWarehouse.setColumn(nRow, "wh_nm", tab_eventMng.tab_Warehouse.edt_wh_nm.value);
		break;
	}

	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 문자발송이력 조회
function fn_commSearch(sType, sSvcId)
{
	if (fn_preSearch(sType, sSvcId) == false) {
		return false;
	}

	var szParam       = "";
	var szRetCallback = "fn_callBack";

	switch (sSvcId) {
		case "getMainList" :
			var szSvcid       = "getMainList";
			var szController  = "/dlw/evnt/mng/getEvntBranchList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list=ds_output";
		break;
		
		case "getDetailList" :
			var szSvcid       = "getDetailList";
			var szController  = "/dlw/evnt/mng/getEventManList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listDetail=ds_output";
		break;

		case "getEventManList" :
			var szSvcid       = "getEventManList";
			var szController  = "/dlw/evnt/mng/getEventManList";
			var szInDs        = "ds_input=ds_inqCndtEvntMan";
			var szOutDs       = "ds_listEvntMan=ds_output";
		break;

		case "getEvntBranchList" :
			var szSvcid       = "getEvntBranchList";
			var szController  = "/dlw/evnt/mng/getEvntBranchList";
			var szInDs        = "ds_input=ds_inqCndtBranch";
			var szOutDs       = "ds_listBranch=ds_output";
		break;
		
		case "getEvntWHList" :
			var szSvcid       = "getEvntWHList";
			var szController  = "/dlw/evnt/mng/getEvntWHList";
			var szInDs        = "ds_input=ds_inqCndtWarehouse";
			var szOutDs       = "ds_listWarehouse=ds_output";
		break; 
	}

	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 지부 콤보박스 리스트
function fn_getJibu(sType)
{
	if (!fn_preSearch(sType, "getEventManList")) { return false; }
	var szSvcid       = "srchComboJibu";
    var szController  = "/common/getBranchList";
    var szInDs        = "ds_input=ds_inqCndtEvntMan";
    var szOutDs       = "ds_jibu=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
} 

