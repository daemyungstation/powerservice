function DL471200_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	//gfn_createPagingDataset(obj);
	
	
	gfn_getList("dlwCd","0138","fn_callBack",false);   
    fn_getJibu(gfn_getCommTypeInit()); // 콤보박스 지부데이터 조회
	//gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	//fn_commSearch(gfn_getCommTypeInit(), "getEventManList");
