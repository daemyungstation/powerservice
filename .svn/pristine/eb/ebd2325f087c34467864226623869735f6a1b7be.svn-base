    <Script type="xscript4.0"><![CDATA[
include "LIB::lib_All.xjs";

function Comm_ReceiveMessage_onload(obj:Form, e:LoadEventInfo)
{
	gfn_commPopInit(obj);

	fn_Init();
}

function fn_Init()
{
	fn_Search();
}

function fn_Search()
{
	// Input 정보 설정
	ds_Input.setColumn(0, "recpUserId", gds_UserInfo.getColumn(0, "userId"));
	//ds_Input.setColumn(0, "recpUserId", "6000");

	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_nameSpace("com.kyowon.krp.sys.dc.generaladmnistrationdc.DCGnrlAdmnSqlMap");
	gfn_setKwData_trscCode("SYS00001R10");
	gfn_setKwData_sqlId("getRecpMsgList");

	gfn_tranDIS("APPEND"
				, "ds_Input=ds_Input"
				, "ds_MessageList=datas"
				, "fn_callBack");
}

function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	}

	fn_setMessageConfirmInfo();

	//trace(ds_MessageList.saveXML());

	if(this.ds_MessageList.rowcount > 0) {
		fn_SearchMsgInfo(grd_MsgList, 0);
	}
}

// 미확인 건수정보 설정
function fn_setMessageConfirmInfo()
{
	var nTotCnt		= ds_MessageList.rowcount;
	var nNViewCnt 	= gfn_transNullToNumber(ds_MessageList.getCaseCount("recpCnftYn!='Y'"));

	var sConfirmInfo = "미확인   <fs v='9'><fc v='#f0850c'><b v='true'>"+nNViewCnt+"</b></fc></fs> <fc v='#a1a1a1'>통/전체 "+nTotCnt+"통</fc>";

	stc_confirmCnt.text = sConfirmInfo;

	ap_BottomFrame.form.fn_setMsgCnt(nNViewCnt);
}

function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

function btn_Close07_onclick(obj:Button,  e:ClickEventInfo)
{

}

function Grid02_oncellclick(obj:Grid, e:GridClickEventInfo)
{

}

function grd_MsgList_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	fn_SearchMsgInfo(obj, e.row);
}


// 메세지정보 조회 및 수신확인 처리
function fn_SearchMsgInfo(obj:Grid, nRow)
{
	var objDs = objects[obj.binddataset];

	if(objDs.getColumn(nRow, "recpCnftYn") == "Y") {
		ds_MsgInfo.clearData();
		var nARow = ds_MsgInfo.addRow();
		ds_MsgInfo.setColumn(nARow, "msgNm", 		objDs.getColumn(nRow, "msgNm"));
		ds_MsgInfo.setColumn(nARow, "dpmsUserNm", 	objDs.getColumn(nRow, "dpmsUserNm"));
		ds_MsgInfo.setColumn(nARow, "recpCnftDttm", objDs.getColumn(nRow, "recpCnftDttm"));
		ds_MsgInfo.setColumn(nARow, "msgCntn", 		objDs.getColumn(nRow, "msgCntn"));
		return;
	}

	var dpmsNo = objDs.getColumn(nRow, "dpmsNo");
	ds_Input.setColumn(0, "dpmsNo", dpmsNo);

	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_trscCode("SYS00002R18");

	gfn_tranNMCI("PushMessage"
				, "ds_Input=ds_Input"
				, "ds_MsgInfo=ds_Output"
				, "fn_fn_SearchMsgInfo_callBack");
}

