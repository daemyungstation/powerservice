    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_list = 0;	// 전체건수

var xAccntNo;

var xID;
var xMemNo;
var xConsTxt;
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL520008_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin()); // 권한
	//gfn_getList("dlwCd","0114","fn_callBack",false);
	
	fn_init(); // 초기화
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {		
			case "getOnlnMemberList": // 홈페이지 회원조회
				gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_list); // 페이지건수 표출

				// 조회결과 존재시 하단 정보창 DS COPY
				if(ds_list.rowcount > 0){
					xID = ds_list.getColumn(ds_list.rowposition,"id");					
					fn_commSearch(gfn_getCommTypeAppend(), "getOnlnMemberDtlList");
				} else {
					alert("조회 내역이 없습니다.");
					ds_list_dtl.clearData();					
				}				
			break;
			
			case "getOnlnMemberDtlList": // 홈페이지 회원 상세
				SetComponent(); //버트 및 컴퍼넌트 설정
			break;
								
			case "updateMemberInfo": // 저장처리
				alert("저장 완료");
				fn_commSearch(gfn_getCommTypeAppend(), "getOnlnMemberDtlList");
			break;
			
			case "deleteExtSpecial": // 삭제처리
				alert("삭제 완료");
				fn_commSearch(gfn_getCommTypeInit(), "getOnlnMemberList"); // 조회
			break;
			
			case "getOnlnMemberListExcel": // 엑셀
				gfn_exportGridToExcel(grd_list_excel, "특수고객청구관리_"+gfn_getTodayFull()+"");
			break;				
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "stt_dt", cal_srchSttDt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srchEndDt.value);
		ds_inqCndt.setColumn(0, "search_gbn", cbo_search_gbn.value);				
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회
function fn_commSearch(sType, searchInfo)
{
    switch(searchInfo) {
    
		// 조회
        case "getOnlnMemberList":
			if (!fn_preSearch(sType)) return false;
			
			// 호출 건수 설정 및 조회여부 설정
			if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 2000, tc_list)) return;
		
			var szSvcid       = "getOnlnMemberList";
			var szController  = "/dlw/onlnMng/getOnlnMemberList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
		
		// 조회
        case "getOnlnMemberDtlList":					
        if (!fn_preSearch(sType)) return false;
        
			var szSvcid       = "getOnlnMemberDtlList";
			var szController  = "/dlw/onlnMng/getOnlnMemberDtlList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_dtl=ds_output ds_pay_list=ds_output2";
			var szParam       = "sID=" + xID;
			var szRetCallback = "fn_callBack";
		break;		
		
		// 엑셀 조회
        case "getOnlnMemberListExcel":
			var szSvcid       = "getOnlnMemberListExcel";
			var szController  = "/dlw/onlnMng/getOnlnMemberList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_excel=ds_output";
			var szParam       = "excel_yn=Y";
			var szRetCallback = "fn_callBack";
		break;
	}

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{	
	var sToday = gfn_getToday();
	var sStartDate = gfn_addDate(sToday, -7);
	
	cal_srchSttDt.value = sToday;
	cal_srchEndDt.value = sToday;
		
	cbo_search_gbn.value = "01";
}

function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{	
	fn_commSearch(gfn_getCommTypeInit(), "getOnlnMemberList"); // 조회
}

