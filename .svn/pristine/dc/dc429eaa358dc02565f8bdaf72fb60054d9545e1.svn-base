		txt_dtlDesc.value = xMemo;
	}
	
	fn_commSearch();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
	trace('------------------에러 콜백');
		trace(errmsg);
		
		return false;
	} else {
		switch (svcid) {
			case "getUplusPopList":
				if(ds_list.rowcount > 1) {
					alert('해당 중복된 가입번호가 존재합니다.');
					close();
				}
			break;
			
		}
	}
}

