    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";
include "LIB::lib_ConsTyp.xjs";

var tc_ctiHstr = 0; // 전체 건수

var initCnt = 0;

var gv_cmpg_id = "";// 캠페인ID

// 권한 설정
var athrPpupYn; // 팝업오픈권한
var xlsYn;      // 엑셀권한

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function frm_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_createTeamUserInqDs(obj); 	// 팀에 따른 사용자 조회를 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin());
	gfn_getList("consTyp", "", null, null, "consTyp1");   			// 상담유형대분류코드조회
	gfn_getList("teamCd", null, null, null, null, "ds_allTeamCd"); 	// 팀코드 조회
	gfn_getList("hrchCd", "CAM990", null, null, "srchHrchCdSet");	// 캠페인유형(계층유형코드)
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "consTyp1":		// 상담유형 대분류
			gfn_getConsTypLoad("", "ALL", ds_cons_typ1);
			++initCnt;
			fn_init(initCnt);
			break;
			
			case "consTyp2":		// 상담유형 중분류
			gfn_getConsTypLoad(cbo_srch_cons_typ1_cd.value, "ALL", ds_cons_typ2);
			break;
			
			case "consTyp3":		// 상담유형 소분류
			gfn_getConsTypLoad(cbo_srch_cons_typ2_cd.value, "ALL", ds_cons_typ3);
			break;
		
			case "srchHrchCdSet":	// 캠페인유형 계층코드
			gfn_getHrchCodeLoad("", "ALL", ds_cmpgTyp1Cd, null, 1);
			++initCnt;
			fn_init(initCnt);
		    break;
			
			case "selectTeamCode":	// 상담그룹 설정
			gfn_getTeamCodeLoad("", "ALL", ds_srch_hgrnTeamCd, ds_allTeamCd, "1");
			++initCnt;
			fn_init(initCnt);
			break;
			
			case "selectUserCode":	// 상담사 설정
			gfn_getUserCdLoad("", "" ,"ALL", ds_srch_userId);
			
			cbo_srch_user_id.index = 0;
			if (ds_srch_userId.rowcount > 1) {
				cbo_srch_user_id.enable = true;
			} else {
				cbo_srch_user_id.enable = false;
			}
			break;
			
			case "srchRecHstr":
			gfn_appendSearchDs(ds_list_recHstr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_recHstr, sta_TotalCnt, tc_ctiHstr); // 페이지건수 표출
			
			if (ds_list_recHstr.rowcount > 0) {
				var sRecConsYn = ds_list_recHstr.getColumn(0, "rec_cons_yn");
				var sRecProdYn = ds_list_recHstr.getColumn(0, "rec_prod_yn");
				var sRecObYn = ds_list_recHstr.getColumn(0, "rec_ob_yn");
				
				if (sRecConsYn == "Y") {
					if (sRecProdYn == "Y") {
						if (sRecObYn == "Y") {
							grd_list.formatid = "all";
						} else {
							grd_list.formatid = "cons_prod";
						}
					} else {
						if (sRecObYn == "Y") {
							grd_list.formatid = "cons_ob";
						} else {
							grd_list.formatid = "cons";
						}
					}
				} else {
					if (sRecProdYn == "Y") {
						if (sRecObYn == "Y") {
							grd_list.formatid = "prod_ob";
						} else {
							grd_list.formatid = "prod";
						}
					} else {
						if (sRecObYn == "Y") {
							grd_list.formatid = "ob";
						} else {
							grd_list.formatid = "rec";
						}
					}
				}
			} else {
				grd_list.formatid = "rec";
			}
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (gfn_isNull(cal_srch_stt_dt.value) || gfn_isNull(cal_srch_end_dt.value)) {
			alert("검색기간을 입력하세요.");
			return false;
		}
	
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_recHstr.keystring = ""; // 정렬컬럼 초기화
		ds_inqCndt.addRow();
		
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "call_typ_cd", cbo_srch_call_typ.value);
		ds_inqCndt.setColumn(0, "call_typ_dtl_cd", cbo_srch_call_typ_dtl.value);
		ds_inqCndt.setColumn(0, "anino", edt_srch_anino.value);
		ds_inqCndt.setColumn(0, "team1_cd", cbo_srch_hgrn_team_cd.value);
		ds_inqCndt.setColumn(0, "team2_cd", cbo_srch_team_cd.value);
		ds_inqCndt.setColumn(0, "user_id", cbo_srch_user_id.value);
		
		ds_inqCndt.setColumn(0, "bzpt_div_yn", "Y");
		
		ds_inqCndt.setColumn(0, "rec_cons_yn", "N");
		ds_inqCndt.setColumn(0, "cons_mem_nm", edt_srch_cons_mem_nm.value);
		ds_inqCndt.setColumn(0, "cons_typ1_cd", cbo_srch_cons_typ1_cd.value);
		ds_inqCndt.setColumn(0, "cons_typ2_cd", cbo_srch_cons_typ2_cd.value);
		ds_inqCndt.setColumn(0, "cons_typ3_cd", cbo_srch_cons_typ3_cd.value);
		ds_inqCndt.setColumn(0, "cons_conc_yn", cbo_cons_conc_yn.value);
		if (!gfn_isNull(edt_srch_cons_mem_nm.value) || !gfn_isNull(cbo_srch_cons_typ1_cd.value) || !gfn_isNull(cbo_srch_cons_typ2_cd.value)
		|| !gfn_isNull(cbo_srch_cons_typ3_cd.value) || !gfn_isNull(cbo_cons_conc_yn.value)) {
			ds_inqCndt.setColumn(0, "rec_cons_yn", "Y");
		}
		
		ds_inqCndt.setColumn(0, "rec_prod_yn", "N");
		ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
		ds_inqCndt.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
		if (!gfn_isNull(edt_srch_accnt_no.value) || !gfn_isNull(edt_srch_prod_cd.value)) {
			ds_inqCndt.setColumn(0, "rec_prod_yn", "Y");
		}
		
		ds_inqCndt.setColumn(0, "rec_ob_yn", "N");
		ds_inqCndt.setColumn(0, "cmpg_id", gv_cmpg_id);
		ds_inqCndt.setColumn(0, "cmpg_typ1_cd", cbo_srch_cmpg_typ1_cd.value);
		ds_inqCndt.setColumn(0, "cmpg_typ2_cd", cbo_srch_cmpg_typ2_cd.value);
		ds_inqCndt.setColumn(0, "ob_conc_yn", cbo_ob_conc_yn.value);
		if (!gfn_isNull(gv_cmpg_id) || !gfn_isNull(cbo_srch_cmpg_typ1_cd.value) || !gfn_isNull(cbo_srch_cmpg_typ2_cd.value)
		|| !gfn_isNull(cbo_ob_conc_yn.value)) {
			ds_inqCndt.setColumn(0, "rec_ob_yn", "Y");
		}
		
		ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 녹취 조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_recHstr, sta_TotalCnt, 100, tc_ctiHstr)) return;
	
	var szSvcid       = "srchRecHstr";
    var szController  = "/cti-hstr/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init(sType)
{
	if (sType != 3) return false;
	initCnt = 0;
	
	var sToday = gfn_getToday();
	cal_srch_stt_dt.value = sToday;		// 녹취일자
	cal_srch_end_dt.value = sToday;
	cbo_srch_call_typ.index = 0;		// 콜유형
	cbo_srch_call_typ_dtl.index = 0;
	edt_srch_anino.value = "";			// 인입번호
	
	edt_srch_cons_mem_nm.value = "";	// 회원명
	
	var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
	if(sUserAthrCd.substr(0,2)=='TM'){
		cbo_srch_hgrn_team_cd.value = 'T20000';
		cbo_srch_hgrn_team_cd.enable = false;
		gfn_getTeamCodeLoad(cbo_srch_hgrn_team_cd.value, "ALL", ds_srch_teamCd, ds_allTeamCd);
		cbo_srch_team_cd.value = gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
		cbo_srch_team_cd.enable = false;
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL");
		cbo_srch_user_id.index = 0;
	}else{
		gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id);		// 상담그룹 및 상담사 초기화
		ConsTypInit(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd); 	// 상담유형 초기화	
	}
	
	//gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id);		// 상담그룹 및 상담사 초기화
	//ConsTypInit(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd); 	// 상담유형 초기화	
	cbo_cons_conc_yn.index = 0;			// 상담 계약여부
	edt_srch_accnt_no.value = "";		// 회원번호
	edt_srch_prod_cd.value = "";		// 상품코드
	edt_srch_prod_nm.value = "";
	gv_cmpg_id = "";					//	캠페인ID
	edt_srch_cmpg_nm.value = "";
	cbo_srch_cmpg_typ1_cd.index = 0;	// 캠페인유형1
	cbo_srch_cmpg_typ2_cd.index = 0;	// 캠페인유형2
	cbo_srch_cmpg_typ2_cd.enable = false;
	cbo_ob_conc_yn.index = 0;			// 캠페인 계약여부
	
	if ("Y" == xlsYn) {
		btn_excel.visible = true;
	}
}

// 회원조회 팝업
function fn_srchAccntNoPop()
{
	var sSrchTyp = "mem_nm";
	if (!gfn_isNull(edt_srch_accnt_no.value)) {
		sSrchTyp = "accnt_no";
	}
	
	var sPrmr = {p_srch_typ:sSrchTyp, p_srch_vl:edt_srch_accnt_no.value, p_gubun_mode:"", p_option:""};
	var reVal = gfn_diaLog("회원조회", -1, -1, -1, -1, "PS990000::PS992400.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 회원정보 선택시
function fn_setAccntNo()
{
	edt_srch_accnt_no.value = ds_accnt_info.getColumn(0, "accnt_no");
	fn_commSearch(gfn_getCommTypeInit());
}

// 상품정보 팝업
function fn_openProdPopup(sType)
{
	var sPrmr;
	if (sType == "btn") {
		sPrmr = {p_prod_cd:"", p_prod_nm:"", p_mode:"set"};
	} else if (sType == "cd") {
		sPrmr = {p_prod_cd:edt_srch_prod_cd.value, p_prod_nm:"", p_mode:"set"};
	} else {
		sPrmr = {p_prod_cd:"", p_prod_nm:edt_srch_prod_nm.value, p_mode:"set"};
	}
	var reVal = gfn_diaLog("상품조회", -1, -1, -1, -1, "PS990000::PS992200.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 상품 선택시
function fn_prodSetInfo()
{
	edt_srch_prod_cd.value = ds_prodList.getColumn(0, "prod_cd");
	edt_srch_prod_nm.value = ds_prodList.getColumn(0, "prod_nm");
	fn_commSearch(gfn_getCommTypeInit());
}

// 캠페인 조회 팝업
function fn_srchCmpgIdPop(sType)
{
	var sPrmr;
	if (sType == "btn") {
		sPrmr = {p_cmpg_nm:""};
	} else {
		sPrmr = {p_cmpg_nm:edt_srch_cmpg_nm.value};
	}
	var reVal = gfn_diaLog("캠페인조회", -1, -1, -1, -1, "PS990000::PS994400.xfdl", false, sPrmr, true, false, true, null, null, true);
}

// 캠페인 선택시
function fn_cmpgSetInfo()
{
	gv_cmpg_id = ds_cmpgList.getColumn(0, "cmpg_id");
	edt_srch_cmpg_nm.value = ds_cmpgList.getColumn(0, "cmpg_nm");
	fn_commSearch(gfn_getCommTypeInit());
}

// 녹취 청취
function fn_recPlay(rec_id)
{
	if (gfn_isNull(rec_id)) {
		alert("녹취 아이디가 없습니다.");
		return;
	}
	
	ap_TopFrame.form.gfn_play(rec_id);
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 그리드 헤더 사이즈 초기화
function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_resetColsize(grd_list);
}

// 그리드 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/cti-hstr/list"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_recHstr, sta_TotalCnt, 100, tc_ctiHstr)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_recHstr, sta_TotalCnt, tc_ctiHstr); // 페이지건수 표출
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

// combo onitemchanged
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if (obj.name == "cbo_srch_hgrn_team_cd") {			// 상담그룹1
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "1", ds_allTeamCd, ds_srch_teamCd, "ALL");
	} else if (obj.name == "cbo_srch_team_cd") {		// 상담그룹2
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_user_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL");
	} else if (obj.name == "cbo_srch_cons_typ1_cd") {	// 상담유형 대분류
		ConsTypSelect(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd, "1");
	} else if (obj.name == "cbo_srch_cons_typ2_cd") {	// 상담유형 중분류
		ConsTypSelect(cbo_srch_cons_typ1_cd, cbo_srch_cons_typ2_cd, cbo_srch_cons_typ3_cd, "2");
	} else if (obj.name == "cbo_srch_cmpg_typ1_cd") {	// 캠페인유형1
		gfn_getHrchCodeLoad(cbo_srch_cmpg_typ1_cd.value, "SELECT", ds_cmpgTyp2Cd, null);
		cbo_srch_cmpg_typ2_cd.index = 0;
		if (obj.value != "" && obj.value != null) {
			cbo_srch_cmpg_typ2_cd.enable = true;
		} else {
			cbo_srch_cmpg_typ2_cd.enable = false;
			cbo_srch_cmpg_typ2_cd.value = "";
		}
	}
}

// button onclick
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_init") {				// 초기화 버튼
		fn_init(3);
	} else if (obj.name == "btn_search") {		// 검색 버튼
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_srchAcntNo") {	// 회원번호조회 버튼
		fn_srchAccntNoPop();
	} else if (obj.name == "btn_srchProdCd") {	// 상품조회 버튼
		fn_openProdPopup("btn");
	} else if (obj.name == "btn_srchCmpgId") {	// 캠페인조회 버튼
		fn_srchCmpgIdPop("btn");
	}
}

// onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if (obj.name == "edt_srch_accnt_no") {			// 회원번호
			fn_srchAccntNoPop();
		} else if (obj.name == "edt_srch_prod_cd") {	// 상품코드
			fn_openProdPopup("cd");
		} else if (obj.name == "edt_srch_prod_nm") {	// 상품명
			fn_openProdPopup("nm");
		} else if (obj.name == "edt_srch_cmpg_nm") {	// 캠페인
			fn_srchCmpgIdPop("nm");
		} else {										// 기타 - 조회
			fn_commSearch(gfn_getCommTypeInit());
		}
	}
}

// grid cell click
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.cell == "7"){
		var sRecId = ds_list_recHstr.getColumn(e.row, "rec_id");
		if (gfn_isNull(sRecId)) return;
		fn_recPlay(sRecId);
	}
}

//버튼클릭 이벤트
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj){		
		case btn_excel:
		var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
		var sUserTeamCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd');
		if (sUserAthrCd.substring(0,2) == "TM") {
			ds_inqCndt.setColumn(0, "bzpt_divs", "'999999','"+sUserTeamCd+"'");
		}
		ds_inqCndt.setColumn(0, "cntr_cd" , "CCA")
		var colDS = gfn_excelPrepareExcelDS("grd_list");
		gfn_excelDown("녹취관리", "XLS_REC", "CtiCrncHstrMap.getCtiCrncHstrList", "CtiCrncHstrMap.getCtiCrncHstrCount", ds_inqCndt, true, colDS);
		break;
	}
}


