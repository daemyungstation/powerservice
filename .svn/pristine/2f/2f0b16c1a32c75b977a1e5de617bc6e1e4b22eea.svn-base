			// gfn_getCode("02", "ALL", ds_evt_typ, gds_DmlCd); // 코드조회결과 필터
			// cbo_event.index = 0;			
			// fn_init(); // 검색부 초기화
		break;

	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch()
{
	ds_cond.clearData();
	ds_cond.addRow();	

	if (gfn_isEmpty(cal_stt_dt.value) ) {
		alert("정산년월을 입력해주십시오.");
		return false;
	}		

	var stt_dt = gfn_getCalVal(cal_stt_dt);
	stt_dt = stt_dt.substring(0,6);

	ds_cond.setColumn(0, "stt_dt", stt_dt);
	
	return true;
}

//조회
function fn_search(obj, e)
{	
	// 호출 건수 설정 및 조회여부 설정
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "searchGongjeMg";    
    var szController  = "/gongje/new/magam_data";
    var szInDs        = "ds_input=ds_cond";
	var	szOutDs       = "ds_pay=ds_output ds_mem=ds_output2";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//초기화
function fn_init()
{
	var sYmd 			= gfn_getToday();	
 	
 	cal_stt_dt.value 	= sYmd.substr(0,6); 		
}
/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
