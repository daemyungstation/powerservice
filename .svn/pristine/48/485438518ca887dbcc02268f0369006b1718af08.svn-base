/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "dlwCd": // 공통코드 조회 callBack Service ID
		break;
		
		// (탭 1) 매출입현황 조회
		case "searchBuyList":
			ds_pur_list.deleteAll();
			gfn_appendSearchDs(ds_pur_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			if(ds_pur_list.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_pur_list.clearData();
			}
		break;
		
		// (탭 2) 회원별매출입 조회
		case "searchMemList": 
		trace("@@@ : " +  ds_mem_tmp.rowcount);
		
			//ds_mem_list.deleteAll();
			gfn_appendSearchDs(ds_mem_list, ds_mem_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_mem_list, sta_TotalCnt, tc_log); // 페이지건수 표출
			trace("tc_log : " + tc_log);

			/*if(ds_mem_list.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_mem_list.clearData();
			}*/
			
			// (해당 기준월) 최종마감처리가 되면 다른 작업 못하도록 버튼 숨김
			/*
			if(vFinalMgCnt > 0) {
				btn_mg.visible = false;
				btn_save.visible = false;
				btn_delete.visible = false;
				btn_insert.visible = false;
			} else if(vGubun == "basic_data"){ // 기본 데이터 조회시 저장작업 가능
				btn_mg.visible = false;
				btn_save.visible = true;
				btn_delete.visible = false;
				btn_insert.visible = false;
				btn_save.text = "저장";
			} else if(vGubun == "middle_data"){ // 1차 저장데이터 조회시 수정(저장) 및 삭제, 최종마감처리 가능
				btn_mg.visible = true;
				btn_save.visible = true;
				btn_delete.visible = true;
				btn_insert.visible = true;
				btn_save.text = "수정";
			}
			*/
			
		break;
		
		case "saveAccntPurSale": // 1차 저장
			alert("저장되었습니다.");
			
			// 처음 저장시에만 재조회
			if(saveGb == "INSERT") fn_commSearch(gfn_getCommTypeInit(), 'searchMemList'); // 재조회
		break;
		
		case "saveAccntPurSaleMg": // 마감
			alert("최종마감 되었습니다.");
			fn_commSearch(gfn_getCommTypeInit(), 'searchMemList'); // 재조회
		break;
		
		case "srchCntAccntPurSaleMg": // 마감카운트 체크
			//alert("mListCnt : " + mListCnt);
		break;
		
		case "updateAccntPurSale": // 최종마감 전 매입 또는 매출금 수정
			alert("수정되었습니다.");
		break;
		
		case "deleteAccntPurSale": // 삭제처리
			alert("삭제되었습니다.");
			fn_commSearch(gfn_getCommTypeInit(), 'searchMemList'); // 재조회
		break;
		
		case "searchMemListExcel": // 엑셀
			gfn_exportGridToExcel(grd_mem_excel, "회원별 매입매출현황_"+gfn_getTodayFull()+"");
		break;
		

	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch(sType, svcid)
{
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	
	
	var vMonth = cal_stt_dt.getDate();  // 마감월(시작)
	ds_inqCndt.setColumn(0, "pur_dt", vMonth);
	
	var vMonth2 = cal_end_dt.getDate(); // 마감월(종료)
	var vMonth3 = gfn_addMonth(vMonth2, 0); 
	ds_inqCndt.setColumn(0, "end_pur_dt", gfn_getLastDay(vMonth3.substr(0,6)));
	
	/*
	var vMonth = cal_stt_dt.value; // 기준월(시작)
	ds_inqCndt.setColumn(0, "pur_dt", gfn_addMonth(vMonth, -1));
	var vMonth2 = cal_end_dt.value; // 기준월(종료)
	ds_inqCndt.setColumn(0, "end_pur_dt", gfn_addMonth(vMonth, -1));
	*/
	
	// 조회조건 체크
	if(vMonth =="" || vMonth == null){
		alert("시작기준월을 입력하십시요");
		cal_stt_dt.setFocus();
		return;
	}
	if(vMonth2 =="" || vMonth2 == null){
		alert("종료기준월을 입력하십시요");
		cal_end_dt.setFocus();
		return;
	}
	
	if(vMonth > vMonth2) {
		alert("마감월(시작)이 마감월(종료)보다 더 큽니다.");
		cal_stt_dt.setFocus();
		return;
	}
	
	if(svcid == "searchMemList" && gfn_getMonthDiff(vMonth, vMonth2) > 1) {
		alert('조회기간을 한달이내로 조정해주시기 바랍니다.');
		return;
	}
	
	// 전월조건 구하기
	/*
	var vMonth2 = gfn_addMonth(vMonth, -1);  // 기준월의 전월 첫 날짜
	var vMonth3 = gfn_getLastDay(vMonth2.substr(0,6)); // 기준월의 전월 마지막 날짜
	ds_inqCndt.setColumn(0, "bef_pur_dt_start", vMonth2);
	ds_inqCndt.setColumn(0, "bef_pur_dt_end", vMonth3);
	*/
	
	//trace("gv_pur_comp_cd : " + gv_pur_comp_cd);
	// 매입업체 조건
	ds_inqCndt.setColumn(0, "pur_comp", gv_pur_comp_cd);
	// 회원번호 조건
	ds_inqCndt.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
	
	// 입금일 조건
	ds_inqCndt.setColumn(0, "stt_pay_dt", cal_stt_pay.value);
	ds_inqCndt.setColumn(0, "end_pay_dt", cal_end_pay.value);
	
	
	if (tab_main.tabindex == 0) ds_pur_list.clearData();
	//else if (tab_main.tabindex == 1) ds_mem_list.clearData();
	
	return true;
}

//조회
function fn_commSearch(sType, searchInfo)
{

trace("fn_commSearch [ searchInfo ] : " + searchInfo);

	// 호출 건수 설정 및 조회여부 설정
	if ( !fn_preSearch() ) {
		return;
	}
		
	//if (tab_main.tabindex == 0) {
	if (searchInfo == "searchBuyList") {
		var szSvcid       = "searchBuyList";
		var szController  = "/dlw/resn/accpur-mg-list"; // 매출입현황(최종마감)
		var szInDs        = "ds_input=ds_inqCndt";
		var szOutDs       = "ds_list_tmp=ds_output";
		var szParam       = "";
		var szRetCallback = "fn_callBack";
	} else if (searchInfo == "searchMemList") {
	    if (!fn_preSearch(sType,"searchMemList")) return false;
	    if(!gfn_pageSearch(sType, ds_mem_list, sta_TotalCnt, 2000, tc_log)) return;
	    
		var szSvcid       = "searchMemList";    
		var szController  = "/dlw/resn/mem-mg-list"; // 회원별매출입 조회(최종마감)
		var szInDs        = "ds_input=ds_inqCndt";
		var szOutDs       = "ds_mem_tmp=ds_output";
		var szParam       = "";
		var szRetCallback = "fn_callBack";
		
	} else if (searchInfo == "searchMemListExcel") { // 
		var szSvcid       = "searchMemListExcel";    
		var szController  = "/dlw/resn/mem-mg-list"; // 회원별매출입 조회(최종마감)  -  엑셀 전체조회
		var szInDs        = "ds_input=ds_inqCndt";
		var szOutDs       = "ds_mem_tmp_excel=ds_output";
		var szParam       = "excel_fg=Y";
		var szRetCallback = "fn_callBack";
	}
		
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
}

