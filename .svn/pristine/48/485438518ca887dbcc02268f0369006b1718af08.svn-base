		btn_accntNo.visible = false;
		btn_mg.visible = false;
		btn_save.visible = false;
		btn_delete.visible = false;
		btn_insert.visible = false;
	}
	
	// (탭 2) 회원별매출입
	else if(obj.tabindex == "1"){
	
