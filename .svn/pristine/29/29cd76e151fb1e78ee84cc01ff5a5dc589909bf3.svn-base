    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt = 0;   //전체 건수
var excelGb = ""; // 액셀다운로드할때 조회된 쿼리구분
var gvMenuId;  // 프로그램 로그 추가 2017.12.04

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL564000_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정;
	fn_init();
	
	fn_commSearch(gfn_getCommTypeInit(), "selectNonPayTitleList");
	
	gvMenuId = GV_LOGMENUID; // 프로그램 로그
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) 
	{
		switch (svcid) 
		{
			case "selectNonPayTitleList":
			break;		
			case "selectCrdtCllctnList" :			    
			break;
		}
		return false;
	} 
	else 
	{
		switch (svcid) 
		{
			case "selectNonPayTitleList": 
			ds_listNonPayTit.insertRow(0);
	      //alert(this.ds_listNonPayTit.saveXML());
		    ds_listNonPayTit.setColumn(0, "np_title", "전체");
			cbo_nonPayTit.index = 0;
			break;
			
			case "selectCrdtCllctnList" :
			    ds_list.filter("");
				gfn_appendSearchDs(ds_list, ds_listTemp);
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, nTotalListCnt, null, true, null); 			
			  //gfn_setTotCnt(ds_list, sta_TotalCnt); // 건수 표시
				if(ds_list.rowcount < 1) 
				{
					alert("조회된 자료가 없습니다.");
				    ds_list.clearData();
				}
			break;
				
		}
	}
}
/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/


function fn_preSearch(searchInfo)
{
		ds_cond.clearData();	
		ds_cond.addRow();
		var sToMonth = div_monthCal.getDate();
						
		if(gfn_IsNull(sToMonth) == true)
		{
			alert("산출월은 필수 입력입니다.");
			return false;
		}
		ds_cond.setColumn(0,"ext_dt", div_monthCal.getDate());
		ds_cond.setColumn(0,"np_gbn", cbo_nonPayGubun.value);
					
		
		if(searchInfo != 'selectNonPayTitleList') 
		{
			ds_cond.setColumn(0, "ext_dt"       , div_monthCal.getDate());
			ds_cond.setColumn(0, "mst_seq"		, cbo_nonPayTit.value);
			ds_cond.setColumn(0, "np_gbn"		, cbo_nonPayGubun.value);
			
			ds_cond.setColumn(0, "accnt_no"		, edt_accntNo.value);
			ds_cond.setColumn(0, "section_t_np"	, cbo_BondGubun.value);
			ds_cond.setColumn(0, "diff_yn"		, cbo_Redemption.value);
		}
	return true;
}
/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 채권추심회원현황  테이블에서 조회
function fn_commSearch(sType, searchInfo) 
{
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
	if(!fn_preSearch(searchInfo)) 
	{
		return false;
	}
	
	switch(searchInfo) 
	{
        //미납생성월 기준 미납TITLE 조회
        case "selectNonPayTitleList": 
			szSvcid       = "selectNonPayTitleList";
			szController  = "dlw/itomng/titList";
			szInDs        = "ds_input=ds_cond";
			szOutDs       = "ds_listNonPayTit=ds_output";
			szParam       = "";
			szRetCallback = "fn_callBack";
        break;

        case "selectCrdtCllctnList":
			if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 500, nTotalListCnt))
			{
				return;
			}    				
			szSvcid       = "selectCrdtCllctnList";
			szController  = "dlw/itomng/selectCrdtCllctnList";
			szInDs        = "ds_input=ds_cond";
			szOutDs       = "ds_listTemp=ds_output";
			szParam       = "";
			szRetCallback = "fn_callBack";
	    break;
	}
	
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
//초기화
function fn_init()
{		
    //  검색기간 설정
	var sToday = gfn_getToday();
	div_monthCal.setDate(sToday);
	cbo_nonPayGubun.index = 0;
	edt_accntNo.value = "";
	cbo_BondGubun.index = 0;
	cbo_Redemption.index = 0;
	
	sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
	//sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
	ds_list.clearData();	
	ds_listTemp.clearData();
	
	div_monthCal.setDateCallback("fn_divMonthCalValueChanged");


}

//조회 결과
function fn_postSearch(svcid)
{
	switch (svcid) 
	{
		case "selectCrdtCllctnList" :
	        if(ds_list.getRowCount() <= 0)
	        {
		        alert("조회된 데이터가 없습니다.");
		        return false;
	        }
		break;
	}
}

/********************************************************************************************/
