		gfn_setTotalRowCount(ds_list_resn2, tab_resn.tp_resn2.sta_TotalCnt, tc_resn2); // 페이지건수 표출
	}
	else if(sTabPageId == "tp_resn3")
	{
		ds_inqCndt.setColumn(0, "dsName", "ds_output3");	// 결과를 받을 데이터셋명
		
		if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_resn3, tab_resn.tp_resn3.sta_TotalCnt, 500, tc_resn)) return;
		gfn_grdHeadClick(obj, e, ds_inqCndt);
		gfn_setTotalRowCount(ds_list_resn3, tab_resn.tp_resn3.sta_TotalCnt, tc_resn3); // 페이지건수 표출
	}	
	
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

// onkeydown
function srch_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		if (obj.name == "edt_srch_prod_cd") {
			fn_openProdPopup("cd");
		} else if (obj.name == "edt_srch_prod_nm") {
			fn_openProdPopup("nm");
		} else if (obj.name == "edt_srch_accnt_no") {
			fn_srchAccntNoPop();
		}
	}
}

// button click
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_init") {				// 초기화 버튼
		fn_init();
	} else if (obj.name == "btn_accntNo") {		// 회원번호조회 버튼
		fn_srchAccntNoPop();
	} else if (obj.name == "btn_srchProd") {	// 상품조회 버튼
		fn_openProdPopup("btn");
	} else if (obj.name == "btn_search") {		// 검색 버튼
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_excel") {		// 엑셀다운로드 버튼
		fn_excel();
	} else if (obj.name == "btn_send") {		// 송금일 버튼
		fn_SendPayment();
	} else if (obj.name == "btn_reg") {			// 등록팝업
		fn_resnRegPop();
	}
}

// grid oncelldblclick
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	trace(e.col);
	if(e.col == 1) fn_resnTestPop(e.row);	
	if(e.col > 2) fn_resnUpdatePop(e.row);	
}

// 권한에 따른 버튼 설정
function fn_setAthrPgmBtn()
{
    gfn_athrPgm(gfn_gdsCd_admin());
    
    if (xlsYn == "N") {
        btn_excel.visible = false;
        
        // 해당 라인 컴포넌트 위치조정
        btn_gdInit.position2 = "absolute r:154 w:16 t:91 h:20";
        btn_send.position2 = "absolute r:75 w:72 t:87 h:20";
        btn_reg.position2 = "absolute r:0 w:72 t:87 h:20";
    }
    
	stc_10.visible = false;
	cmb_signComp.visible = false;
	stc_03.visible = false;
	cbo_work_yn.visible = false;
	cbo_srch_dateTyp.innerdataset.filter("codecolumn=='01' || codecolumn=='02'");
	cbo_srch_dateTyp.value = "01"
}

function tab_resn_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{	
	// 호출 건수 설정 및 조회여부 설정
	if(e.postindex == 3){
		stc_10.visible = true;
		cmb_signComp.visible = true;
		stc_03.visible = true;
		cbo_work_yn.visible = true;
		cbo_srch_dateTyp.innerdataset.filter("codecolumn=='03'");
		cbo_srch_dateTyp.value = "03"
	} else {
		stc_10.visible = false;
		cmb_signComp.visible = false;
		stc_03.visible = false;
		cbo_work_yn.visible = false;
		cbo_srch_dateTyp.innerdataset.filter("codecolumn=='01' || codecolumn=='02'");
		cbo_srch_dateTyp.value = "01"
	}
}
