    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_resn0 = 0;	// 전체건수
var tc_resn = 0;	// 전체건수
var tc_resn2 = 0;	// 전체건수
var tc_resn3 = 0;	// 전체건수

var tc_dataAthrQury;

var gvMenuId;   // 프로그램 로그 추가 2018.03.20
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL440101_onload(obj:Form, e:LoadEventInfo)
{
	//////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2018.03.20 정승철
	gvMenuId = GV_LOGMENUID;
	//////////////////////////////////////////////////////////////////////////////////////////
	
	fn_setAthrPgmBtn(); // 권한에 따른 버튼 설정
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	gfn_getList("dlwCd", "08,64,95"); // 공통코드조회
	
	if(gds_UserInfo.getColumn(gds_UserInfo.rowposition,'excel_yn') == "Y") {
		btn_excel.visible = true;
	} else {
		btn_excel.visible = false;
	}
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("08", "ALL", ds_acpgMthdCd, gds_DmlCd); 	// 접수방법
			gfn_getCode("64", "ALL", ds_payMthdCd, gds_DmlCd); 		// 해약방법
			//gfn_getCode("95", "ALL", ds_divCd, gds_DmlCd); 			// 구분
			
			fn_init(); // 검색부 초기화
			//fn_commSearch(gfn_getCommTypeInit()); // 고객 조회
			
			break;
			
			case "srchResnList0": // 해약 조회
			
			gfn_appendSearchDs(ds_list_resn0, ds_list_tmp0); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_resn0, tab_resn.tp_resn0.sta_TotalCnt, tc_resn0); // 페이지건수 표출
			if(ds_list_resn0.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_list_resn0.clearData();
			}
			break;	
			
			case "srchResnList1": // 해약 조회
			gfn_appendSearchDs(ds_list_resn, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_resn, tab_resn.tp_resn1.sta_TotalCnt, tc_resn); // 페이지건수 표출
			if(ds_list_resn.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_list_resn.clearData();
			}
			break;
			
			case "srchResnList2": // 해약 조회
			
			gfn_appendSearchDs(ds_list_resn2, ds_list_tmp2); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_resn2, tab_resn.tp_resn2.sta_TotalCnt, tc_resn2); // 페이지건수 표출
			if(ds_list_resn2.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_list_resn2.clearData();
			}
			break;
			
			case "srchResnList3": // 해약 조회
			
			gfn_appendSearchDs(ds_list_resn3, ds_list_tmp3); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_resn3, tab_resn.tp_resn3.sta_TotalCnt, tc_resn3); // 페이지건수 표출
			if(ds_list_resn3.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_list_resn3.clearData();
			}
			break;			
			
			case "srchResnExcelList": // 엑셀다운
			//	Static00.text=tc_dataAthrQury;
			//	ds_inqCndt.setColumn(0, "cntr_cd", ds_list_excel.getColumn(0, "cntr_cd"));
			//	ds_inqCndt.setColumn(0, "path_typ", ds_list_excel.getColumn(0, "path_typ"));
				ds_inqCndt.setColumn(0, "dataAthrQury", tc_dataAthrQury);
				grd_list.formatid = "excel";
				var sTabPageId = tab_resn.tabpages[tab_resn.tabindex].name;
				
				grd_list.formatid = "default";
				
				//gfn_excelDown("해약목록", "DL440101", "DlwResnMap.getResnList", "DlwResnMap.getResnCount", ds_inqCndt, true, colDS, true);
				
				if(sTabPageId == "tp_resn1")
				{
					var colDS = gfn_excelPrepareExcelDS("grd_list");
				
					gfn_excelDown("해약목록", "DL440101", "DlwResnMap.getResnNewList", "DlwResnMap.getResnNewCount", ds_inqCndt, true, colDS, true);
				}
				else if(sTabPageId == "tp_resn2")
