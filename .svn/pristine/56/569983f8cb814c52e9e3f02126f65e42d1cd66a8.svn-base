// 해약현황 조회
function fn_commSearch(sType)
{		
	var szSvcid       = "";	
	var sTabPageId = tab_resn.tabpages[tab_resn.tabindex].name;		
		
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(sTabPageId == "tp_resn1"){
		if(!gfn_pageSearch(sType, ds_list_resn, tab_resn.tp_resn1.sta_TotalCnt, 50, tc_resn)) return;		
		szSvcid       = "srchResnList1";
	} else if(sTabPageId == "tp_resn2"){
		if(!gfn_pageSearch(sType, ds_list_resn2, tab_resn.tp_resn2.sta_TotalCnt, 50, tc_resn2)) return;		
		szSvcid       = "srchResnList2";
	} else if(sTabPageId == "tp_resn3"){
		if(!gfn_pageSearch(sType, ds_list_resn3, tab_resn.tp_resn3.sta_TotalCnt, 500, tc_resn3)) return;		
		szSvcid       = "srchResnList3";
	} else if(sTabPageId == "tp_resn0"){
		if(!gfn_pageSearch(sType, ds_list_resn0, tab_resn.tp_resn0.sta_TotalCnt, 500, tc_resn0)) return;		
		szSvcid       = "srchResnList0";
	}
	
    var szController  = "/dlw/resn/newList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output ds_list_tmp2=ds_output2 ds_list_tmp3=ds_output3 ds_list_tmp0=ds_output0";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

