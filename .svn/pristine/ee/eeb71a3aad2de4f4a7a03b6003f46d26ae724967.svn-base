/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch()
{
	ds_cond.clearData();
	ds_cond.addRow();	
	
	var condCnt			= 0;
	var sFrDt 			= "";
	var sToDt 			= "";
	var sCpNm 			= edt_cp_nm.value;
	var sGubun          = cbo_gubun.value;
	var sBranchNm		= cbo_branch.value;
	var sProdCd			= edt_prod_cd.value;
	
	if ( !gfn_isEmpty(cal_start_dt.value) ) {
		sFrDt = cal_start_dt.value;
	}	
	if ( !gfn_isEmpty(cal_end_dt.value) ) {
		sToDt = cal_end_dt.value;
	}
	
	if ( gfn_isEmpty(sFrDt) ) {
		alert("행사종료 시작일자는 필수 입력입니다.");
		cal_start_dt.setFocus();
		return false;
	}
	if ( gfn_isEmpty(sToDt) ) {
		alert("행사종료 종료일자는 필수 입력입니다.");
		cal_end_dt.setFocus();
		return false;
	}
	
	if ( !gfn_isEmpty(sCpNm) && sCpNm.length < 2 ) {
		alert("CP명은 최소 2자리 이상 입력해야 합니다.");
		edt_cp_nm.setFocus();
		return false;
	}
	
	ds_cond.setColumn(0, "start_dt"		, sFrDt		);
	ds_cond.setColumn(0, "end_dt"		, sToDt		);	
	ds_cond.setColumn(0, "cp_nm"		, sCpNm		);
	ds_cond.setColumn(0, "gubun"        , sGubun    );
	ds_cond.setColumn(0, "branch"		, sBranchNm	);
	ds_cond.setColumn(0, "prod_cd"		, sProdCd	);
	ds_cond.setColumn(0, "tab_gubun"	, tab_main.tabindex	);
	
	return true;
}

//조회
function fn_search(obj, e)
{	
	// 호출 건수 설정 및 조회여부 설정
	if ( !fn_preSearch() ) {
		return;
	}
	
	var szSvcid       = "searchaddsales";    
    var szController  = "/dlw/evnt/addsalesin-list"; // 직영
    
    if (tab_main.tabindex == 1) {
		szController  = "/dlw/evnt/addsalesout-list"; // 외주
    }
    else if (tab_main.tabindex == 2)
    {
		szController  = "/dlw/evnt/funeral-list"; // 장지
    }
    
    var szInDs        = "ds_input=ds_cond";
    
    var szOutDs       = "";
    
    if (tab_main.tabindex == 0) {
		szOutDs = "ds_t0_list=ds_output";
		fv_searchCallback = fn_postSearch0;
    } else if (tab_main.tabindex == 1) {
		szOutDs = "ds_t1_list=ds_output";
		fv_searchCallback = fn_postSearch1;
    } else if (tab_main.tabindex == 2) {
        szOutDs = "ds_t2_list=ds_output";
		fv_searchCallback = fn_postSearch2;
    } else {
		return;
    }
    
    btn_Search.enable = false;
    
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

