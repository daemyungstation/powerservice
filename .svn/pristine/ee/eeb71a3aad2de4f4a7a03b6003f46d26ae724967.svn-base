// 서머리 행의 백분율 표시
function fn_getBranchPercent(sCol) {
	var nVal	= ds_t1_list.getSum(sCol);
	var nTotal 	= ds_t1_list.getSum("tot_cnt");
	var rst = Math.round(nVal / nTotal * 100);
	if (isNaN(rst)) {
		return "";
	}
	return rst + "%";	
}

function fn_getBranchPercent3(sCol) {
