    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS030201_onload(obj:Form, e:LoadEventInfo)
{
	gfn_getList("dlwCd", "0123,0153"); // 공통코드조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("0123", "SELECT", ds_sex, gds_DmlCd); 			// 코드조회결과 필터
			gfn_getCode("0153", "SELECT", ds_credit_rating, gds_DmlCd); // 코드조회결과 필터
			
			fn_commSearch();
			break;
			
			case "srchCustBasi":
			fn_postSearch();
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회
function fn_preSearch()
{
	if (gfn_isNull(p_mem_no)) {
		alert("회원 아이디가 없습니다.");
		colse();
		return false;
	}
	return true;
}

// 개인정보 동의여부 조회전
function fn_preSrchPers()
{
	ds_pers_info.clearData();
	if (gfn_isNull(ds_member.getColumn(0, "mem_no"))) {
		return false;
	}
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원 조회
function fn_commSearch()
{
	if (!fn_preSearch()) return false;
	
	var szSvcid       = "srchCustBasi";
    var szController  = "/cons/cust-basi/view";
    var szInDs        = "";
    var szOutDs       = "ds_member=ds_output ds_count_prod=ds_countput";
    var szParam       = "mem_no=" + p_mem_no;
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 개인정보 동의여부 조회
function fn_srch_pers_info()
{
	if (!fn_preSrchPers()) return false;
	
	var szSvcid       = "srchPersInfo";
    var szController  = "/cons/pers-info-pcus-cnsn/view";
    var szInDs        = "";
    var szOutDs       = "ds_pers_info=ds_output";
    var szParam       = "mem_no=" + ds_member.getColumn(0, "mem_no");
    var szRetCallback = "";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 개인정보활용동의 값 변경
function combo_onchanged(obj:CheckBox, e:CheckBoxChangeEventInfo)
{
	if (obj.name == "prif_gthr_cnsn_yn") {
		if (obj.value == "Y") prif_gthr_cnsn_dt.visible = true;
		else prif_gthr_cnsn_dt.visible = false;
	} else if (obj.name == "prif_thpr_offr_cnsn_yn") {
		if (obj.value == "Y") prif_thpr_offr_cnsn_dt.visible = true;
		else prif_thpr_offr_cnsn_dt.visible = false;
	} else if (obj.name == "ents_offr_cnsn_yn") {
		if (obj.value == "Y") ents_offr_cnsn_dt.visible = true;
		else ents_offr_cnsn_dt.visible = false;
	} else if (obj.name == "prif_mrkn_pcus_cnsn_yn") {
		if (obj.value == "Y") {
			prif_mrkn_pcus_cnsn_dt.visible = true;
			sta_Dash.visible = true;
			prif_mrkn_pcus_vld_dt.visible = true;
		} else {
			prif_mrkn_pcus_cnsn_dt.visible = false;
			sta_Dash.visible = false;
			prif_mrkn_pcus_vld_dt.visible = false;
		}
	}
}

// 조회 결과
function fn_postSearch()
{
	fn_srch_pers_info();
	
	fn_businessYn(ds_member.getColumn(0, "business_yn"));
	
		
	if (!gfn_isNull(ds_member.getColumn(0, "brth_mon_day"))) {
		brth_mon_day.mask = "####-##-##";
	} else {
		brth_mon_day.mask = "";
	}
	
	gfn_SetHomeZipMask(home_zip);
	gfn_phoneNumObj(cell);
	gfn_phoneNumObj(home_tel);
	gfn_phoneNumObj(wrpl_tel);
	
	if (sex.value == undefined) {
		sex.value = "";
	}
	if (credit_rating.value == undefined) {
		credit_rating.value = "";
	}
}

// 사업자 여부 설정
function fn_businessYn(sType)
{
	if (sType == "Y") {
		ci_txt.text = "사업자번호";
		ci_val.value = "";
		ci_val.visible = false;
		
		mem_nm_txt.text = "업체명";
		idn_no.move(575, 96);
		idn_no.visible = true;
	} else {
		ci_txt.text = "CI/DI";
		ci_val.visible = true;
		
		mem_nm_txt.text = "회원명";
		idn_no.visible = false;
		idn_no.value = "";
	}
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 닫기
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}
]]></Script>
