    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt = 0;

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL562001_onload(obj:Form, e:LoadEventInfo)
{
	//////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2018.01.11 정승철
	gvMenuId = GV_LOGMENUID;
	//////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정;
	
	//fn_setAthrPgmBtn();
	
	fn_init();
	
	fn_commSearch(gfn_getCommTypeInit(), "selectNonPayTitleList");
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
			case "selectNonPayTitleList":
			break;
			case "selectNonPayList":
			break;
	    }
		return false;
	} else {
		switch (svcid) {			
			case "selectNonPayTitleList": 
			ds_listNonPayTit.insertRow(0);
			ds_listNonPayTit.setColumn(0, "np_title", "전체");
			cbo_nonPayTit.index = 0;
			break;
			
			case "selectNonPayList": 
			
			gfn_appendSearchDs(ds_listNonPay, ds_listNonPayTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_listNonPay, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출
			if(ds_listNonPay.rowcount < 1) {
				alert("조회된 자료가 없습니다.");
				ds_listNonPay.clearData();
			}
			
			break;

			
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(searchInfo)
{
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	
//	if ( gfn_isEmpty(empleNo) && gfn_isEmpty(empleNm) && gfn_isEmpty(idnNo) && gfn_isEmpty(deptCd) ) {
		if ( gfn_isEmpty(div_monthCal.getDate()) ) {
			alert("미납생성월은 필수 입력입니다.");
			return false;
		}
//	}	
	
	ds_inqCndt.setColumn(0, "ext_dt"		, div_monthCal.getDate());
	ds_inqCndt.setColumn(0, "np_gbn"		, cbo_nonPayGubun.value);
	
	
	if(searchInfo != 'selectNonPayTitleList') {
		ds_inqCndt.setColumn(0, "mst_seq"		, cbo_nonPayTit.value);
		ds_inqCndt.setColumn(0, "accnt_no"		, edt_accnt_no.value);
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, searchInfo) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    if ( !fn_preSearch(searchInfo) ) {
		return;
	}
    
    switch(searchInfo) {
        //미납생성월 기준 미납TITLE 조회
        case "selectNonPayTitleList": 
	    szSvcid       = "selectNonPayTitleList";
		szController  = "/dlw/nonPay/titList";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_listNonPayTit=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        
        case "selectNonPayList": 
        
        if(!gfn_pageSearch(sType, ds_listNonPay, sta_TotalCnt, 500, nTotalListCnt)) return;		
        
	    szSvcid       = "selectNonPayList";
		szController  = "/dlw/nonPay/list";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_listNonPayTemp=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
    }
    
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{
	ds_inqCndt.clearData();
	ds_listNonPayTit.clearData();
	ds_listNonPay.clearData();
	ds_listNonPayTemp.clearData();
	
	//  검색기간 설정
	var sToday = gfn_getToday();
	div_monthCal.setDate(sToday);
	
	cbo_nonPayGubun.index = 0;
	edt_accnt_no.value = "";
	
	div_monthCal.setDateCallback("fn_divMonthCalValueChanged");
}

function btn_srch_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit(), "selectNonPayList");
}

function tab_deductionMgmt_grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend(), "selectNonPayList");
}

function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init();
}

function btn_nonPayCalcuration_onclick(obj:Button,  e:ClickEventInfo)
{
	if(gfn_isNull(cbo_nonPayTit.value)) {
		alert("미납TITLE를 선택하세요.");
		return false;
	}
	
	var sPrmr = {pToday:div_monthCal.getDate(), pGubun:cbo_nonPayGubun.value , pTit:cbo_nonPayTit.text , pTitValue:cbo_nonPayTit.value};
	var reVal = gfn_diaLog("회원조회", -1, -1, -1, -1, "DL560000::DL562001P01.xfdl", false, sPrmr, true, false, true, null, null, true);
}

function cbo_nonPayGubun_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	stc_02.text = "";
	fn_commSearch(gfn_getCommTypeInit(), "selectNonPayTitleList");
}

function cbo_nonPayTit_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(gfn_isNull(cbo_nonPayTit.value)) {
		stc_02.text = "";
	} else {
		var nRow = ds_listNonPayTit.findRow("mst_seq", cbo_nonPayTit.value);
		stc_02.text = ds_listNonPayTit.getColumn(nRow, "np_etc");
	}
	
}

function btn_nonPayTitle_onclick(obj:Button,  e:ClickEventInfo)
{ 
	var sPrmr = {pToday:div_monthCal.getDate()};
	var reVal = gfn_diaLog("회원조회", -1, -1, -1, -1, "DL560000::DL562001P02.xfdl", false, sPrmr, true, false, true, null, null, true);
}



function fn_divMonthCalValueChanged()
{
	var sMonthCal = div_monthCal.getDate();
	
	if(!gfn_isNull(sMonthCal) && sMonthCal.length == 6) {
		stc_02.text = "";
		fn_commSearch(gfn_getCommTypeInit(), "selectNonPayTitleList");
	}	
}]]></Script>
