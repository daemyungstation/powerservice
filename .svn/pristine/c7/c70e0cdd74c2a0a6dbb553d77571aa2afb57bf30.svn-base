{
	obj.updateToDataset();
	var bzdpDivCdIdx = obj.getBindCellIndex("body", "bzdpDivCd");
	var scrnIdIdx = obj.getBindCellIndex("body", "scrnId");
	var scrnNoIdx = obj.getBindCellIndex("body", "scrnNo");
	var strParam = {pv_scrnNo:obj.getCellText(e.row, scrnNoIdx),pv_scrnId:obj.getCellText(e.row, scrnIdIdx),pv_bzdpDivCd:obj.getCellValue(e.row, bzdpDivCdIdx)};
	var rtn = gfn_diaLog("SYS0000201003", -1, -1, 447, 429, "SYS::SYS0000201003.xfdl", false, strParam, false, true, true);
	
	if(rtn instanceof Object) {
 		trace(rtn["scrnCnt"]);

 		// 팝업에서 선택된 데이타를 그리드에 보여준다.
 		this.ds_list.setColumn(e.row, "rtntScrnCnt", rtn["scrnCnt"]);
		//ds_list.setColumn(nRow, 1, "");
 		//obj.moveToNextCell();
	}
	
}

