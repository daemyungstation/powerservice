/***************************************************************************
 * 기      능		: (공통) 그리드 팝업
 ***************************************************************************/
function div_body_grd_list_onexpanddown(obj:Grid, e:GridMouseEventInfo)
{
	var sRowType = ds_list.getColumn(obj.getDatasetRow(e.row), gfn_getColNmStatus());
	//trace(sRowType);
	if(sRowType == gfn_getRowTypeInsert() || sRowType == gfn_getRowTypeDelete()){ /*|| sRowType == gfn_getRowTypeUpdate() */
		alert("수정 또는 입력된 내용을 저장 후 입력가능 합니다.");
	}else{
		var sColNm = obj.getCellProperty("body", e.cell, "text").split(":")[1];
		
		//trace("test : " + sColNm);
		//alert(e.cell + "," + obj.getBindCellIndex("body", "rtntScrnCnt") + "," + obj.getCellProperty("body", e.cell, "text"));
		if ( sColNm == "scrnNm" || sColNm == "scrnUrlAddr") {
		//if ( e.cell == obj.getBindCellIndex("body", "scrnNm") || e.cell == obj.getBindCellIndex("body", "scrnUrlAddr")) {
			//trace("scrnTypCd::"+ds_list.getColumn(e.row, "scrnTypCd"));
		
			// 00 메인화면
			// 01 메인부속화면
			// 10 팝업화면
			// 11 팝업부속화면
		
			//if(ds_list.getColumn(e.row, "scrnTypCd") == "11") {			// 팝업부속화면

				var sSubPath = ds_list.getColumn(e.row, "scrnUrlPath");
				var sFileName = ds_list.getColumn(e.row, "scrnUrlAddr");
				var sScrnTypCd = ds_list.getColumn(e.row, "scrnTypCd");
				var sScrnNm = ds_list.getColumn(e.row, "scrnNm");
				
				if (sColNm == "scrnNm") { // 화면명에서 버튼을 누를 경우 항상 현장리뷰쪽을 바라보도록 한다.
					sSubPath = "WSO_CNFT";
				}

				if(gfn_getConnectDiv() == "LOCL") {
					var sPath 	= "C:\\krpdev\\workspace\\KRP_XUI\\WebContent\\xui\\" + sSubPath + "\\" + sFileName;
					var bExistFile 		= VirtualFile.isExist(sPath);	
					if(bExistFile) {
						var strParam = "";
						if (sScrnTypCd == "00" || sScrnTypCd == "01") {
							gfn_openWorkAreaNotExistMenu(sScrnNm, sSubPath + "::" + sFileName);
						} else {
							var rtn = gfn_diaLog("PgmPop", -1, -1, -1, -1, sSubPath + "::" + sFileName , true, strParam, false, false, true);
						}
					} else {
						alert("파일이 존재하지 않습니다.");
					}
				} else {
				
					var objDoc 	= new DomDocument;
					
					objDoc.url = sSubPath + "::" + sFileName;
					var rtn = objDoc.load(false);
					
					//trace("rtn::"+rtn+" Url::"+objDs.getColumn(i, "scrnUrlAddr"));
					
					if(rtn) {
						var strParam = "";
						if (sScrnTypCd == "00" || sScrnTypCd == "01") {
							gfn_openWorkAreaNotExistMenu(sScrnNm, sSubPath + "::" + sFileName);
						} else {
							var rtn = gfn_diaLog("PgmPop", -1, -1, -1, -1, sSubPath + "::" + sFileName, true, strParam, false, false, true);
						}
					} else {
						alert("파일이 존재하지 않습니다.");
					}
				}
				
// 			} else if(ds_list.getColumn(e.row, "scrnTypCd") == "00") {		// 메인화면
// 				//gfn_openNewPage(edt_ScrnNo.value);
// 				//gfn_openNewPageByScrnID(ds_list.getColumn(e.row, "scrnId"));
// 			}
		
		} else if(sColNm == "rtntEvenCnt") {
			fn_OpenPopup_EventSelect(obj, e);
		} else if(sColNm == "rtntScrnCnt"){
			fn_OpenPopup_PgmSelect(obj, e);
		}
	}
	
}

/***************************************************************************
 * 기      능		: 이벤트 선택 팝업 오픈
 ***************************************************************************/
function fn_OpenPopup_EventSelect(obj:Grid, e)
{
	obj.updateToDataset();
	var scrnNoIdx = obj.getBindCellIndex("body", "scrnNo");
	var bzdpDivCdIdx = obj.getBindCellIndex("body", "bzdpDivCd");
	var strParam = {pv_scrnNo:obj.getCellText(e.row, scrnNoIdx),pv_bzdpDivCd:obj.getCellValue(e.row, bzdpDivCdIdx)};
	var rtn = gfn_diaLog("SYS0000201002", -1, -1, 447, 429, "SYS::SYS0000201002.xfdl", false, strParam, false, true, true);

	if(rtn instanceof Object) {
 		trace(rtn["btnCnt"]);
 		trace(rtn["evenCnt"]);

 		// 팝업에서 선택된 데이타를 그리드에 보여준다.
 		this.ds_list.setColumn(e.row, "rtntBtnCnt", rtn["btnCnt"]);
 		this.ds_list.setColumn(e.row, "rtntEvenCnt", rtn["evenCnt"]);
		//ds_list.setColumn(nRow, 1, "");
 		//obj.moveToNextCell();
	}

}


