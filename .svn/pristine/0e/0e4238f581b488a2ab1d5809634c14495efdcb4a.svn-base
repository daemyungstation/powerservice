function fn_createFile() {
	if(ds_list.rowcount>0) {
		if(!(gfn_getToday() > cal_inv_dt.value)) {
			if(confirm("파일작성 하시겠습니까?")){
				fn_commSave("createFileEB21");
			}
		} else {
			alert("출금일자를 금일 이후로 설정하세요.");
			return false;
		}
	} else {
		alert("파일로 작성 할 신청정보가 없습니다.");
		return false;
	}
}

