    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var tc_recrnc = 0; // 전체 건수

var initCnt = 0;

// 권한 설정
var amntYn; 	// 수정권한
var closeYn;	// 임의종결

var gvMenuId;  // 프로그램 로그 추가 2017.12.14
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS040500_onload(obj:Form, e:LoadEventInfo)
{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2017.12.12 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_createTeamUserInqDs(obj); 	// 팀에 따른 사용자 조회를 위한 데이터셋 설정
	fn_setAthrPgmBtn();				// 권한에 따른 버튼 설정
	gfn_getList("cd", "COM020");  	// 공통코드조회
	gfn_getList("teamCd","", null, null, null, "ds_allTeamCd"); // 팀코드 조회
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectCode":
 			gfn_getCode("COM020", "ALL", ds_leftTmCd); // 잔여시간
 			
 			++initCnt;
			fn_init(initCnt);
			break;
			
			case "selectTeamCode":
			gfn_getTeamCodeLoad("", "ALL", ds_srch_hgrnTeamCd, ds_allTeamCd, "1");
			gfn_getTeamCodeLoad("", "SELECT", ds_hgrnTeamCd, ds_allTeamCd, "1");

			++initCnt;
			fn_init(initCnt);			
			break;
			
			case "selectUserCode":
			gfn_getUserCdLoad("", "" ,"ALL", ds_srch_cnsrId);
			
			cbo_srch_cnsr_id.index = 0;
			if (ds_srch_cnsrId.rowcount > 1) {
				cbo_srch_cnsr_id.enable = true;
			} else {
				cbo_srch_cnsr_id.enable = false;
			}
			break;
			
			case "selectCnsrId":
			gfn_getUserCdLoad("", "" ,"SELECT", ds_cnsrId);
			
			cbo_cnsr_id.index = 0;
			if (ds_cnsrId.rowcount > 1) {
				cbo_cnsr_id.enable = true;
			} else {
				cbo_cnsr_id.enable = false;
			}
			break;
			
			case "srchRecrncResr":
			gfn_appendSearchDs(ds_list_recrncResr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			gfn_setTotalRowCount(ds_list_recrncResr, sta_TotalCnt, tc_recrnc); // 페이지건수 표출
			fn_postSearch();
			break;
			
			case "updateRecrncResr":
			fn_commSearch(gfn_getCommTypeInit());
			break;
			
			case "closeRecrncResr":
			fn_commSearch(gfn_getCommTypeInit());
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회전
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())	{
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
		
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list_recrncResr.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "recrnc_stat_cd_list", cbo_srch_recrnc_stat_cd_list.value);
		ds_inqCndt.setColumn(0, "cnsr_id", cbo_srch_cnsr_id.value);
		ds_inqCndt.setColumn(0, "left_tm", cbo_srch_left_tm_cd.value);
		
		// 권한이 TM 일 경우 상담그룹 조회 조건 설정
		var sAthrCd = gds_UserInfo.getColumn(0, "athr_cd");
		if (sAthrCd.indexOf("TM") >= 0) {
			ds_inqCndt.setColumn(0, "team1_cd", "");
			ds_inqCndt.setColumn(0, "team2_cd", "");
			ds_inqCndt.setColumn(0, "bzpt_div_yn", "Y");
		} else {
			ds_inqCndt.setColumn(0, "team1_cd", cbo_srch_hgrn_team_cd.value);
			ds_inqCndt.setColumn(0, "team2_cd", cbo_srch_team_cd.value);
			ds_inqCndt.setColumn(0, "bzpt_div_yn", "N");
		}
		
		//ds_inqCndt.setColumn(0, "orderBy", "amnt_dttm");
		//ds_inqCndt.setColumn(0, "orderDirection", "desc");
		ds_inqCndt.setColumn(0, "orderBy", "recrnc_dttm");
		ds_inqCndt.setColumn(0, "orderDirection", "asc");
	}
	
	return true;
}

// 저장 전
function fn_preSave()
{
	if (gfn_isNull(cbo_hgrn_team_cd.value)) {
		alert("상담그룹1을 선택하세요.");
		cbo_hgrn_team_cd.setFocus();
		return false;
	} else if (gfn_isNull(cbo_team_cd.value)) {
		alert("상담그룹2을 선택하세요.");
		cbo_team_cd.setFocus();
		return false;
	} else if (gfn_isNull(cbo_cnsr_id.value)) {
		alert("담당자를 선택하세요.");
		cbo_cnsr_id.setFocus();
		return false;
	}
	
	ds_recrncResr.setColumn(0, "cnsr_id", cbo_cnsr_id.value);
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 예약콜 조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_recrncResr, sta_TotalCnt, 25, tc_recrnc)) return;
	
	var szSvcid       = "srchRecrncResr";
    var szController  = "/cons/recrnc-resr-dtl/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

//  저장 버튼
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	if (!fn_preSave()) return false;
	
	var szSvcid       = "updateRecrncResr";
    var szController  = "/cons/recrnc-resr-dtl/save/update";
    var szInDs        = "ds_input=ds_recrncResr";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
}

// 임의종결 버튼
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	if (confirm("임의종결하시겠습니까?")) {	
		var szSvcid       = "closeRecrncResr";
		var szController  = "/cons/recrnc-resr-dtl/save/close";
		var szInDs        = "ds_input=ds_recrncResr";
		var szOutDs       = "";
		var szParam       = "";
		var szRetCallback = "fn_callBack";

		// 트랜젝션 공통함수 호출
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","I",gvMenuId);
    }
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init(sType)
{
	if (sType != 2) return false;
	initCnt = 0;
	
	gfn_teamUserInit(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_cnsr_id);
	cbo_srch_recrnc_stat_cd_list.index = 4; // 처리상태 - 접수/처리중
	cbo_srch_left_tm_cd.index = 0;
	cbo_srch_dateTyp.index = 3;
	
	fn_changeDateType();
	
	// 권한이 TM 일 경우 상담그룹 조회 조건 설정
	var sAthrCd = gds_UserInfo.getColumn(0, "athr_cd");
	if (sAthrCd.indexOf("TM") >= 0) {
		var sHgrnTeamCd = gds_UserInfo.getColumn(0, "hgrn_team_cd");
		var sTeamCd = gds_UserInfo.getColumn(0, "team_cd");
			
		cbo_srch_hgrn_team_cd.value = sHgrnTeamCd;
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_cnsr_id, "1", ds_allTeamCd, ds_srch_teamCd, "ALL");
		cbo_srch_team_cd.value = sTeamCd;
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_cnsr_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL");
		
		cbo_srch_hgrn_team_cd.enable = false;
		cbo_srch_team_cd.enable = false;
	}
}

// 날짜 선택
function fn_changeDateType()
{
	gfn_dateSrchDefaultChangeEvnt(cbo_srch_dateTyp, cal_srch_stt_dt, cal_srch_end_dt);
}

// 검색 결과
function fn_postSearch()
{
	if (ds_list_recrncResr.rowcount < 1) {
		ds_recrncResr.clearData();
		fn_setCnpnCorn("NULL");
	} else {
		fn_setRecrncResr();
	}
}

// 상세 설정
function fn_setRecrncResr()
{
	ds_recrncResr.clearData();
	ds_recrncResr.addRow();
	ds_recrncResr.copyRow(0, ds_list_recrncResr, grd_list.selectendrow);
	
	fn_setCnpnCorn("VIEW");
}

// 컴포넌트 제어
function fn_setCnpnCorn(sType)
{
	sta_txt_recrnc.text = "예약콜 상세정보 보기";
	gfn_phoneNumObj(edt_recrnc_cntc_tlno); // 예약번호
	
	gfn_teamUserSelect(cbo_hgrn_team_cd, cbo_team_cd, cbo_cnsr_id, "1", ds_allTeamCd, ds_teamCd, "SELECT", "selectCnsrId");
	cbo_team_cd.value = ds_recrncResr.getColumn(0, "team2_cd");
	gfn_teamUserSelect(cbo_hgrn_team_cd, cbo_team_cd, cbo_cnsr_id, "2", ds_allTeamCd, ds_teamCd, "SELECT", "selectCnsrId");
	cbo_cnsr_id.value = ds_recrncResr.getColumn(0, "cnsr_id");
	
	var sLeftTm = ds_recrncResr.getColumn(0, "left_tm");
	var sRecrncStatCd = ds_recrncResr.getColumn(0, "recrnc_stat_cd");
	if (sRecrncStatCd == "30" || gfn_isNull(sLeftTm)) {
		edt_term.value = "";
	} else if (sLeftTm < 0) {
		edt_term.value = "+" + ds_recrncResr.getColumn(0, "term"); // red
		edt_term.style = "color:red;align:left middle;";
	} else if (sLeftTm >= 0 && sLeftTm < 30) {
		edt_term.style = "color:blue;align:left middle;";
	} else {
		edt_term.style = "color:#333333ff;align:left middle;";
	}
	
	if (sType == "NULL") {
		cbo_hgrn_team_cd.visible = false;
		cbo_team_cd.visible = false;
		cbo_cnsr_id.visible = false;
		
		btn_save.visible = false;
		btn_cancel.visible = false;
		btn_close.visible = false;
		btn_update.visible = false;
		
		mae_recrnc_dttm.mask = "";
		mae_rgsn_dttm_1.mask = "";
		mae_amnt_dttm_1.mask = "";
		mae_rgsn_dttm.mask = "";
		mae_amnt_dttm.mask = "";
	} else {
		cbo_hgrn_team_cd.visible = true;
		cbo_team_cd.visible = true;
		cbo_cnsr_id.visible = true;
		
		mae_recrnc_dttm.mask = "####-##-## ##:##";
		mae_rgsn_dttm_1.mask = "####-##-## ##:##";
		mae_amnt_dttm_1.mask = "####-##-## ##:##";
		mae_rgsn_dttm.mask = "####-##-## ##:##";
		mae_amnt_dttm.mask = "####-##-## ##:##";
		
		if(sType == "VIEW") {
			cbo_hgrn_team_cd.enable = false;
			cbo_team_cd.enable = false;
			cbo_cnsr_id.enable = false;
			
			btn_save.visible = false;
			btn_cancel.visible = false;
			if (ds_recrncResr.getColumn(0, "recrnc_stat_cd") == "30") { // 처리완료
				btn_close.visible = false;
				btn_update.visible = false;
			} else {
				if (closeYn == "Y") {
					btn_close.visible = true;
				}
				if (amntYn == "Y") {
					btn_update.visible = true;
				}
			}
		} else if (sType == "UPDATE") {
			cbo_hgrn_team_cd.enable = true;
			cbo_team_cd.enable = true;
			cbo_cnsr_id.enable = true;
			
			btn_save.visible = true;
			btn_cancel.visible = true;
			btn_close.visible = false;
			btn_update.visible = false;
			
			btn_save.position2 = "absolute r:74 w:69 b:90 h:20";
			btn_cancel.position2 = "absolute r:0 w:72 b:90 h:20";
			
			sta_txt_recrnc.text = "예약콜 상세정보 수정";
		}
	} 
}

// 권한에 따른 버튼 설정
function fn_setAthrPgmBtn()
{
	gfn_athrPgm(gfn_gdsCd_admin());
	
	if (ds_indvFnctAthrPgmFilter.rowcount > 0) {
		closeYn = "Y";
	} else {
		closeYn = "N";
	}
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 초기화
function btn_init_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_init(2);
}

// 콤보 onitemchanged
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if (obj.name == "cbo_srch_hgrn_team_cd") {
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_cnsr_id, "1", ds_allTeamCd, ds_srch_teamCd, "ALL");
	} else if (obj.name == "cbo_srch_team_cd") {
		gfn_teamUserSelect(cbo_srch_hgrn_team_cd, cbo_srch_team_cd, cbo_srch_cnsr_id, "2", ds_allTeamCd, ds_srch_teamCd, "ALL");
	} else if (obj.name == "cbo_srch_dateTyp") {
		fn_changeDateType();
	} else if (obj.name == "cbo_hgrn_team_cd") {
		gfn_teamUserSelect(cbo_hgrn_team_cd, cbo_team_cd, cbo_cnsr_id, "1", ds_allTeamCd, ds_teamCd, "SELECT", "selectCnsrId");
	} else if (obj.name == "cbo_team_cd") {
		gfn_teamUserSelect(cbo_hgrn_team_cd, cbo_team_cd, cbo_cnsr_id, "2", ds_allTeamCd, ds_teamCd, "SELECT", "selectCnsrId");
	}
}

// 조회 버튼
function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit()); // 조회
}

// 그리드 헤더 사이즈 초기화
function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_resetColsize(grd_list);
}

// 그리드 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.cell == 2) return false;

	ds_inqCndt.setColumn(0, "sController", "/cons/recrnc-resr-dtl/list"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명
	
	if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list_recrncResr, sta_TotalCnt, 25, tc_recrnc)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	gfn_setTotalRowCount(ds_list_recrncResr, sta_TotalCnt, tc_recrnc); // 페이지건수 표출
	
	fn_setRecrncResr();
