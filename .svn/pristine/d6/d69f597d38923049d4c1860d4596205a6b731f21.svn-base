	}// 거래처 주소 조회
	else if (sUrl == "MN020000::MN020102.xfdl") {
	trace('enter 거리처 주소 조회');
		opener.edt_cusrmr_zip_cd.value = sZipCd;
		gfn_SetHomeZipMask(opener.edt_cusrmr_zip_cd);
		opener.edt_cusrmr_addr.value = sAddr;
		opener.edt_cusrmr_add2.value = "";
		opener.edt_cusrmr_addr2.setFocus();
		close();
	}
	else if (gfn_isNull(sUrl)) {
		var sName = opener.name;
		if (sName == "PS020408") {							// (신규)고객
			opener.home_zip.value = sZipCd;
			gfn_SetHomeZipMask(opener.home_zip);
			opener.home_addr.value = sAddr;
			opener.home_addr2.value = "";
			opener.home_addr2.setFocus();
			close();
		} else if (sName == "PS992300") {					// 사용자 상세정보 팝업
			opener.ds_user.setColumn(0,"zpcd", sZipCd);
			opener.ds_user.setColumn(0,"home_addr", sAddr);
			opener.ds_user.setColumn(0,"home_dtpt_addr","");
			close();
		} else if (sName == "PS993900") {					// 회원주소관리
			opener.mae_home_zip.value = sZipCd;
			gfn_SetHomeZipMask(opener.mae_home_zip);
			opener.edt_home_addr.value = sAddr;
			opener.edt_home_addr2.value = "";
			opener.edt_home_addr2.setFocus();
			close();
		}else if(sName == "MN020104_1"){											//창고관리
			opener.ds_wh_mng.setColumn(0,"wh_zip", sZipCd);
			opener.ds_wh_mng.setColumn(0,"wh_addr", sAddr);
			opener.ds_wh_mng.setColumn(0,"wh_addr2","");
			close();
		}else if (sName == "MN030101") {					// 회사정보 조회
			opener.ds_list.setColumn(0,"zip", sZipCd);
			opener.ds_list.setColumn(0,"addr", sAddr);
			opener.ds_list.setColumn(0,"addr2","");
			trace('======'+sZipCd+'/'+sAddr);
			close();
		}
	}
}

// edit onkeydown
function edt_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) { // 엔터
		fn_commSearch(gfn_getCommTypeInit());
	}
}
