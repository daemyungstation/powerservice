    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
var pageNo = 0;   // 현재페이지
var totPgNum = 0;   // 전체페이지건수

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트,전역변수,초기화) 처리                         	*/
/********************************************************************************************/
function PS994100_onload(obj:Form, e:LoadEventInfo)
{
	ds_guGun.copyData(ds_guGunAll);
	ds_guGun.filter("cd == '서울특별시'");
	tab.tabpage1.cbo_guGun.index = 0;
	
	ds_guGun2.copyData(ds_guGunAll);
	ds_guGun2.filter("cd == '서울특별시'");
	tab.tabpage2.cbo_guGun.index = 0;
	
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "getBasVlList":
				gfn_appendSearchDs(ds_list_addr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				
				if(!gfn_isNull(totPgNum)){
					tab.tabpage1.sta_pageNo.text = "<fc v='#f0850c'><b v='true'>" + gfn_appendComma(pageNo, 0) + "</b></fc>";
					tab.tabpage1.sta_totPgNum.text = "<fc v='#a1a1a1'><b v='true'>" + gfn_appendComma(totPgNum, 0) + "</b></fc>";
				}	
			break;	
		}	
 	}		
}
/********************************************************************************************/
/* 5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType)
{
	if (gfn_isNull(tab.tabpage1.edt_srch.value)) {
		alert("도로명,지번,건물명을 입력하세요.");
		return false;
	}
	
	if (sType == gfn_getCommTypeInit()) {
		pageNo = 0;
		totPgNum = 0;
		ds_list_addr.clearData();
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "sido", tab.tabpage1.cbo_si.value);
		ds_inqCndt.setColumn(0, "gugun", tab.tabpage1.cbo_guGun.text);
		ds_inqCndt.setColumn(0, "srch", tab.tabpage1.edt_srch.value);
		ds_inqCndt.setColumn(0, "pageNo", pageNo + 1);
	} else {
	    if (toNumber(pageNo) >= toNumber(totPgNum)) {
	        return;
	    }
		ds_inqCndt.clearData();
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "sido", tab.tabpage1.cbo_si.value);
		ds_inqCndt.setColumn(0, "gugun", tab.tabpage1.cbo_guGun.text);
		ds_inqCndt.setColumn(0, "srch", tab.tabpage1.edt_srch.value);
		ds_inqCndt.setColumn(0, "pageNo", toNumber(pageNo) + 1);
	}
	
	var szSvcid       = "getBasVlList";
	var szController  = "/syst/zip-cd/list";
	var szInDs        = "ds_input=ds_inqCndt";
	var szOutDs       = "ds_list_tmp=ds_output";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}
