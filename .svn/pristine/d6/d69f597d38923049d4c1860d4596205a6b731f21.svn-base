//그리드 더블클릭
function tab_tabpage1_grd_consTyp_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var sUrl = opener.url;
	var sZipCd = ds_list_addr.getColumn(e.row, "zip");
	var sAddr = ds_list_addr.getColumn(e.row, "address");

	//마이페이지 > 사용자 상세정보에서 호출시
	//사용자관리 > 사용자관리에서 호출시
	if(sUrl == "PS050000::PS050100.xfdl" || sUrl == "PS080000::PS080300.xfdl"){
		opener.ds_user.setColumn(0,"zpcd", sZipCd);
		opener.ds_user.setColumn(0,"home_addr", sAddr);
		opener.ds_user.setColumn(0,"home_dtpt_addr","");
		close();
	}
	// 고객주소관리
	else if (sUrl == "PS030000::PS030500.xfdl") {
		opener.mae_home_zip.value = sZipCd;
		gfn_SetHomeZipMask(opener.mae_home_zip);
		opener.edt_home_addr.value = sAddr;
		opener.edt_home_addr2.value = "";
		opener.edt_home_addr2.setFocus();
		close();
	}
	//	메인화면 회원기준정보, 고객정보관리, 상담이력 상세
	else if (sUrl == "PS020000::PS02000101.xfdl" || sUrl == "PS030000::PS030100.xfdl" || sUrl == "PS040000::PS040901.xfdl") {
		opener.home_zip.value = sZipCd;
		gfn_SetHomeZipMask(opener.home_zip);
		opener.home_addr.value = sAddr;
		opener.home_addr2.value = "";
		opener.home_addr2.setFocus();
		close();
	}
	// 상품 상세정보, 상담이력 상세_상품
	else if (sUrl == "PS020000::PS02000104.xfdl" || sUrl == "PS040000::PS040904.xfdl") {
		opener.mae_inspl_zip.value = sZipCd;
		gfn_SetHomeZipMask(opener.mae_inspl_zip);
		opener.edt_inspl_addr.value = sAddr;
		opener.edt_inspl_addr2.value = "";
		opener.edt_inspl_addr2.setFocus();
		close();
	}
	// 행사등록
	else if (sUrl == "DL430000::DL430103.xfdl") {
		opener.ds_evnt_list.setColumn(0,"zip", sZipCd);
		opener.ds_evnt_list.setColumn(0,"addr1", sAddr);
		opener.edt_addr2.value = "";
		opener.edt_addr2.setFocus();
		close();
	}else if (sUrl == "MN030000::MN030101.xfdl") {					// 회사정보 조회
			trace('Enter');
			opener.ds_list.setColumn(0,"zip", sZipCd);
			opener.ds_list.setColumn(0,"addr", sAddr);
			opener.ds_list.setColumn(0,"addr2","");
			trace('======'+sZipCd+'/'+sAddr);
			close();
		}else if (sUrl == "DL450000::DL450501.xfdl") {					// 양도양수 신청
			trace('주소찾기='+sZipCd);
			opener.ds_transfer_acq.setColumn(0,"home_zip", sZipCd);
			opener.ds_transfer_acq.setColumn(0,"home_addr", sAddr);
			opener.ds_transfer_acq.setColumn(0,"home_addr2","");
			close();
		}
	else if (gfn_isNull(sUrl)) {
		var sName = opener.name;
		if (sName == "PS020408") {							// (신규)고객
			opener.home_zip.value = sZipCd;
			gfn_SetHomeZipMask(opener.home_zip);
			opener.home_addr.value = sAddr;
			opener.home_addr2.value = "";
			opener.home_addr2.setFocus();
			close();
		} else if (sName == "PS992300") {					// 사용자 상세정보 팝업
			opener.ds_user.setColumn(0,"zpcd", sZipCd);
			opener.ds_user.setColumn(0,"home_addr", sAddr);
			opener.ds_user.setColumn(0,"home_dtpt_addr","");
			close();
		} else if (sName == "PS993900") {					// 회원주소관리
			opener.mae_home_zip.value = sZipCd;
			gfn_SetHomeZipMask(opener.mae_home_zip);
			opener.edt_home_addr.value = sAddr;
			opener.edt_home_addr2.value = "";
			opener.edt_home_addr2.setFocus();
			close();
		} else if (sName == "MN020102_1") {					// 거래처  관리 
			opener.edt_custmr_zip_cd.value = sZipCd;
			opener.edt_custmr_addr.value = sAddr;
			opener.edt_custmr_addr2.value = "";
			close();
			
		}else if (sName == "MN020104_1") {					// 창고 관리
			opener.ds_list.setColumn(0,"wh_zip", sZipCd);
			opener.ds_list.setColumn(0,"wh_addr", sAddr);
			opener.ds_list.setColumn(0,"wh_addr2","");
			close();
		}else if (sName == "MN020105_1") {					// 지부  관리
			opener.branch_zipcode.value = sZipCd;
			opener.branch_addr1.value = sAddr;
			opener.branch_addr2.value = "";
			close();			
		}else if (sName == "MN020107_1") {					// 행사자  관리
			opener.ds_evt_mana_mng.setColumn(0,"zip", sZipCd);
			opener.ds_evt_mana_mng.setColumn(0,"addr1", sAddr);
			opener.ds_evt_mana_mng.setColumn(0,"addr2","");
			close();
		} else if (sName == "MN020103_1") {					// 거래처  관리 
			opener.funrl_zip_cd.value = sZipCd;
			opener.funrl_addr.value = sAddr;
			opener.funrl_addr2.value = "";
			close();
		}
	}
	
	close(sZipCd + "_" + sAddr);
}

