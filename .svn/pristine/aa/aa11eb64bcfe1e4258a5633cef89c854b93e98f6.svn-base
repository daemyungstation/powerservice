StatusbarForm
{
	background	: #aaaaaa;
	border		: 2 double #aaaaaa #dddddd, 0 none #808080, 0 none #808080, 0 none #808080;
	bordertype	: normal 0 0;
	font		: Dotum, 8;
	color		: #000000;

	gripimage		: IMG::gripimage.png;
	progressbargab	: 5;
	progressbarheight	: 17;
	progressbarwidth	: 180;
	zoomcombogab	: 5;
	zoomcomboheight	: 17;
	zoomcombowidth	: 50;
}
StatusbarForm>#progressbar
{
	background	: #bfbfbf;
	border		: 1 solid #888888;
	bordertype	: normal 0 0;
	focusborder	: 0 none #808080;
	font		: Dotum,9;
	color		: #ffffff;
	align		: center middle;
	padding		: 0 0 1 0;
	direction	: forward;
	bartype		: normal;
	barcolor	: @gradation;
	bargradation	: linear 0,0 #888888 100,0 #5d5d5d;
	smooth		: true;
}
StatusbarForm>#zoomcombo
{
	border		: 1 solid #808080;
	bordertype	: normal 0 0;
}
StatusbarForm>#zoomcombo>#comboedit
{
}
StatusbarForm>#zoomcombo>#dropbutton
{
}
StatusbarForm>#zoomcombo>#combolist
{
}
ApplicationMenu, ApplicationMenu:disabled, ApplicationMenu:focused,
ApplicationMenu:mouseover, ApplicationMenu:selected
{
	background	: @gradation;
	gradation	: linear 0,0 #fefefe 0,100 #cfcfcf;
	border		: 0 none #808080, 0 none #808080, 1 solid #888888, 0 none #808080;
	bordertype	: normal;
	font		: Dotum,9;
	color		: #333333;
	padding		: 1 6 1 6;
	itemalign		: center middle;
	itembackground	: @gradation;
	itemgradation	: linear 0,0 #d0d0d000 0,100 #bcbcbc00;
	itemborder		: 2 double #33333300 #bfbfbf00;
	itembordertype	: normal;
	itempadding		: 5 8 5 8;
	popupbackground	: #f2f2f2;
	popupborder		: 1 solid #333333;
	popupbordertype	: normal;
	popupfont		: Dotum,9;
	popupcolor		: #333333;
	popuppadding	: 1 1 1 1;
	popupitembackground	: #f2f2f2;
	popupitemborder		: 0 none #808080;
	popupitembordertype	: normal;
	popupitemheight		: 22;
	popupitempadding	: 0 5 0 7;
	checkboximage		: IMG::ico_Check.png;
	expandimage			: IMG::ico_expand.png;
}
ApplicationMenu:mouseover
{
	itembackground	: @gradation;
	itemgradation	: linear 0,0 #d0d0d0ff 0,100 #bcbcbcff;
	itemborder		: 2 double #333333ff #bfbfbfff;
	popupitembackground	: #117ceb;
	popupcolor		: #ffffff;
}
ApplicationMenu:focused
{
	itembackground	: @gradation;
	itemgradation	: linear 0,0 #d0d0d0ff 0,100 #bcbcbcff;
	itemborder		: 2 double #333333ff #bfbfbfff;
	popupitembackground	: #117ceb;
	popupcolor		: #ffffff;
}
ApplicationMenu:selected
{
	itembackground	: @gradation;
	itemgradation	: linear 0,0 #d0d0d0ff 0,100 #bcbcbcff;
	itemborder		: 2 double #333333ff #bfbfbfff;
	popupitembackground	: #117ceb;
	popupcolor		: #ffffff;
}
ApplicationMenu>#popupmenu
{
	background	: #f2f2f2;
	border		: 1 solid #333333;
	bordertype	: normal;
	font		: Dotum,9;
	color		: #333333;
	align		: left middle;
	padding		: 1 1 1 1;
	itembackground	: #f2f2f2;
	itemborder		: 0 none #808080;
	itembordertype	: normal;
	itemheight		: 22;
	itempadding		: 0 5 0 7;
}
*>#contextmenu, *>#contextmenu:disabled, *>#contextmenu:focused,
*>#contextmenu:mouseover, *>#contextmenu:pushed, *>#contextmenu:selected
{
	background	: #ffffff;
	border		: 1 solid #231916;
	bordertype	: normal;
	font		: Dotum, 9;
	color		: #333333;
	align		: left middle;
	padding		: 0;
	itembackground	: #ffffff;
	itemborder		: 0 none #808080;
	itembordertype	: normal;
	itemheight		: 22;
	itempadding		: 0 5 0 5;
	expandimage		: URL('IMG::img_expend.png');
}
*>#contextmenu:mouseover
{
	color		: #000000;
	itembackground	: #fae5cd;
}
*>#contextmenu:disabled
{
	color		: #999999;
}
