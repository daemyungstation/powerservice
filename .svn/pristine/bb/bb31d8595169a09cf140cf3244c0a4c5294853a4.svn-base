// 장례용품 선택 삭제
function fn_rowdel_Tab1()
{
	var cf = confirm("정말 삭제하시겠습니까?");
	
	if (cf == false) return;
	
	// 체크 여부확인
	var chkfg = false;
	for(var i=0;i<ds_tab1_list.rowcount;i++)
	{
		if(ds_tab1_list.getColumn(i, "chk") == 1) { // 체크 여부 확인
			chkfg = true;
			break;
		}
	}	

	if (chkfg == false){
		alert("삭제할 대상을 선택하세요.");
		return;
	}	
	
	for(var i=ds_tab1_list.rowcount-1; i>=0 ;i--){
		if(ds_tab1_list.getColumn(i, "chk") == 1) { // 체크 여부 확인
			ds_tab1_list.deleteRow(i);
		}
	}	
		
}

