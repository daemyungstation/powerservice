// 수량과 단가 변경에 따른 금액 자동 계산처리(장례용품)
function ds_tab1_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.col != 8){
		var row = ds_tab1_list.rowposition ;
		
		var qty = ds_tab1_list.getColumn(row, "qty");
		var unit_cost = ds_tab1_list.getColumn(row, "unit_cost");
		
		var amt = qty * unit_cost ;

		amt = Number(amt);
		
		obj.enableevent = false;
			ds_tab1_list.setColumn(row, "amt", amt);
		obj.enableevent = true;
	}
	
	fn_cust_init(obj);  // 매입처&품목 초기화
	getsum(); //        하단 경비 총액 합계 실시간 계산처리
	get_gongjesum(); // 하단 공제금액 합계 실시간 계산처리
	fn_jan_amt();     // 잔여금 계산처리	
	fn_tot_recvamt(); // 총 수금액 계산처리 			
	fn_cash_payamt(); // 실 입금액 계산처리	
}

