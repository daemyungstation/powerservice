// 카드선택에 따른 수수료 계산 처리
function ds_tab9_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var row = ds_tab9_list.rowposition ;
	var pay_opt = ds_tab9_list.getColumn(row, "pay_opt");
	
	var Today = new Date();
	var yy = Today.getFullYear();
	var mm = Today.getMonth() + 1; if (mm.length == 1) { mm = "0" + mm; }
	var dd = Today.getDate(); if (dd.length == 1) { dd = "0" + dd; }

	// 현재날짜
	var ymd = yy + "" + mm + "" + dd;	
	
	if(pay_opt == "0002"){ // 카드	
	
		var card_cd = ds_tab9_list.getColumn(row, "card_cd");
		var amt = ds_tab9_list.getColumn(row, "amt");
		amt = parseInt(amt);
		var i;
		
		for(i=0 ; i < ds_card_fee.rowcount ; i++){
			var fee = ds_card_fee.getColumn(i, "card_cd");
			
			if(card_cd == fee){
				var stt_dt = ds_card_fee.getColumn(i, "start_dt");
				var end_dt = ds_card_fee.getColumn(i, "end_dt");
				var fee_rt = ds_card_fee.getColumn(i, "fee_rt");
				
				fee_rt = parseInt(fee_rt);
			}
		} // end for
		
		if(stt_dt <= ymd && end_dt >= ymd){
			var card_fee = amt * fee_rt * 0.01;
			card_fee = parseInt(card_fee);
		}
		
		obj.enableevent = false;
			ds_tab9_list.setColumn(row, "card_fee", card_fee);
			ds_tab9_list.setColumn(row, "pay_amt_cash", 0);
		obj.enableevent = true;			
		
	}else if (pay_opt == "0003"){ // 현금

		var pay_amt_cash = ds_paydata.getColumn(0, "gp_amt");
		//var pay_amt_cash = parseInt(edt_amt_1.value) + parseInt(edt_amt_2.value) + parseInt(edt_amt_3.value);
		var appr_no = ds_tab9_list.getColumn(row, "appr_no");
		
		if (e.col != 10){
			if(!gfn_isNull(appr_no)){
				obj.enableevent = false;
					ds_tab9_list.setColumn(row, "pay_amt_cash", pay_amt_cash);
				obj.enableevent = true;			
			}
		}
	
	}else{
		obj.enableevent = false;
			ds_tab9_list.setColumn(row, "pay_amt_cash", 0);
		obj.enableevent = true;			
	} // end if
	
	
	
	get_cardsum(); // 하단 카드금액 실시간 처리
	fn_jan_amt();     // 잔여금 계산처리	
	fn_tot_recvamt(); // 총 수금액 계산처리 			
	fn_cash_payamt(); // 실 입금액 계산처리	
	
}

