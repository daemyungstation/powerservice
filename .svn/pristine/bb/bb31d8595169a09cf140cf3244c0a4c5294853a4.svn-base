// 마감저장
function btn_Save_onclick(obj:Button,  e:ClickEventInfo)
{
	var bdate = cal_bdate.value;
	
	if(ds_tab2_list.rowcount > 0){
		if(bdate == "" || bdate == null){
			alert("발주일자를 입력하세요.");
			return;
		}
	}
	
	var cf = confirm("마감저장 후 사업자 창고이동을 확인하세요.\n마감저장 하시겠습니까?");
	if (cf == false) return;

	if(g_gubun == "new"){ // 바로 마감저장 시
		mbit = "F";
		fn_funrl_rpt_save(); // 행사보고서 저장	
	}		
	
	fn_funrl_rpt_magam(); // 행사보고서 저장
}

// 사업장내 창고이동
function btn_whmove_onclick(obj:Button,  e:ClickEventInfo)
{
	var gubun = ds_rpt.getColumn(0, "wh_gubun")
	
	g_wh_gubun = gubun;
	
	if(gubun == "N"){
		fn_whMovePopup(); // 창고이동 팝업
	}else{
		var cf = confirm("이미 창고이동 하셨습니다.\n수정하시겠습니까?");
		if (cf == false) return;
		fn_whMovePopup(); // 창고이동 팝업
	}
