/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";
include "LIB::lib_TeamUser.xjs";

var nTotalListCnt = 0; // 전체건수
var gvMenuId;  // 프로그램 로그 추가
var tc_certf = 0;
var form_count = 0;

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS032000_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); 		// 페이징처리 위한 데이터셋 설정
	gvMenuId = GV_LOGMENUID;
	
	fn_init();		
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd)
		return false;
	} 
	else 
	{
		switch (svcid) 
		{						
			case "getPaperJoinList":
				gfn_appendSearchDs(ds_list, ds_listTmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_certf); // 페이지건수 표출
				fn_postSearch();
			break;
			
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원 조회
function fn_commSearch(sType, searchInfo)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 500, tc_certf)) return;
	
	switch(searchInfo) 
	{
        case "getPaperJoinList": 
			
			var szSvcid       = "getPaperJoinList";
			var szController  = "/dlw/rcrtCntr/getJoinList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_listTmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
        break;
    }

	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// pdf파일열기
function fn_getPdfFile()
{			
	var szSvcid       = "getPaperJoinList";
	var szController  = "/dlw/rcrtCntr/getPdfFile";
	var szInDs        = "ds_input=ds_inqCndt";
	var szOutDs       = "";
	var szParam       = "";
	var szRetCallback = "fn_callBack";
    
	gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
