// 초기화
function fn_init()
{
	var sToday = gfn_getToday();
	
	//cal_srch_stt_dt.value = gfn_addDate(sToday, -7);
	//cal_srch_end_dt.value = sToday;
	cal_srch_stt_dt.value = "20220630"; //테스트
	cal_srch_end_dt.value = "20220630"; //테스트
	srch_accnt_no.value   = "";
	cbo_cntrYn.index = 0;
	
	ds_listTmp.clearData();
	ds_list.clearData();
	sta_TotalCnt.text = "검색결과   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc> / 전체   <fs v='9'><fc v='#f0850c'><b v='true'>0</b></fc></fs> <fc v='#a1a1a1'>건</fc>";
}
// 조회 validation check.
function fn_preSearch(sType)
{
	ds_inqCndt.clearData();
	var nRow = ds_inqCndt.addRow();
	ds_inqCndt.setColumn(nRow, "accnt_no", srch_accnt_no.value);
	ds_inqCndt.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
	ds_inqCndt.setColumn(0, "end_dt", cal_srch_end_dt.value);
	ds_inqCndt.setColumn(0, "join_cl", "01");
	ds_inqCndt.setColumn(0, "cntr_yn", cbo_cntrYn.value);
	
	//ds_list.clearData();

	return true;
}

// 조회 결과
function fn_postSearch()
{
	if (ds_list.getRowCount() < 1) 
	{
		ds_list.clearData();
		alert("조회된 결과가 없습니다.");
	} 
}

