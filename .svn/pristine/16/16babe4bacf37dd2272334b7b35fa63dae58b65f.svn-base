    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var gv_accnt_no = "";
var gv_typ = "";

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			// 납입정보 부가서비스 조회결과
			case "srchBugaSrvcMemInfo":
				fn_postSearchBugaSrvc();
				
				fn_setBugaInfo();
			break;
			
			case "srchOdrgInfo":
				if (ds_dlvList.rowcount > 0) {
					ds_dlvList.insertRow(0);
					ds_dlvList.setColumn(0, "dlv_no", "선택");
					ds_dlvList.setColumn(0, "dlv_type_nm", "");
					
					cbo_dlv_no.index = 0;
					cbo_dlv_no.enable = true;
				} else {
					cbo_dlv_no.enable = false;
				}
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// CMS 회원관리 - 개별목록 조회 전
function fn_preSrchBugaSrvcInfo(sType, sAccntNo)
{
	if (gfn_isNull(sType) || gfn_isNull(sAccntNo)) {
		return false;
	}
	
	gv_accnt_no = sAccntNo;
	gv_typ = sType;
	
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "srch_typ", "accnt_no");
	ds_inqCndt.setColumn(0, "srch_vl", gv_accnt_no);
	
	return true;
}

// 발주정보 조회전
function fn_preSrchOdrgInfo(sAccntNo)
{
	ds_odrgInfo.clearData();
	ds_dlvList.clearData();
	
	if (gfn_isNull(sAccntNo)) {
		return false;
	}
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// CMS 회원관리 - 개별목록
function fn_srchBugaSrvcMemInfo(sType, sAccntNo)
{	
	if (!fn_preSrchBugaSrvcInfo(sType, sAccntNo)) return false;
	
	var szSvcid       = "srchBugaSrvcMemInfo";
	var szController  = "";
	if (sType == "card") { 			// card
		szController  = "/dlw/card/card-csmm/list";
    } else {						// cms
		szController  = "/dlw/cms/cms-csmm/list";
    }
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_bugaSrvcMmeInfo=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 발주정보 조회
function fn_srchOdrgInfo(sAccntNo)
{	
	if (!fn_preSrchOdrgInfo(sAccntNo)) return false;
	
	var szSvcid       = "srchOdrgInfo";
	var szController  = "/dlw/cons-prod/odrg-info";
    var szInDs        = "";
    var szOutDs       = "ds_odrgInfo=ds_output ds_dlvList=ds_output_list";
    var szParam       = "accnt_no="+sAccntNo;
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
// 초기화
function fn_init()
{
	ds_bugaSrvcMmeInfo.clearData();

	rdo_pay_mthd_typ.value = "cms"; 	// CMS/Card구분
	stc_txt_brdt.text = "생년월일";	// 생년월일/사업자번호
	
	stc_txt_bank.text = "은행코드"; 	// 은행코드/카드사
	edt_bank_cd.visible = true;
	edt_bank_nm.visible = true;
	edt_card_nm.visible = false;		// 카드사
	edt_card_nm.value = "";
	
	stc_txt_accntno.text = "계좌번호"; // 계좌번호/카드번호
	edt_bankaccnt_no.visible = true;
	edt_card_no.visible = false;
	edt_card_no.value = "";
	
	stc_txt_tran.text = "이체일자";	// 이체일자/결제일
	edt_tran_dt.visible = true;
	edt_pymn_dt.visible = false;
	edt_pymn_dt.value = "";
	
	stc_depr.text = "예금주";			// 예금주/유효일
	edt_depr.visible = true;
	edt_exp_year.visible = false;
	stc_txt_yy.visible = false;
	edt_exp_mon.visible = false;
	stc_txt_mm.visible = false;
	edt_exp_year.value = "";
	edt_exp_mon.value = "";
	
 	stc_txt_rltn.text = "관계";			// 관계/이메일
 	edt_reltn_nm.visible = true;
 	edt_email.visible = false;
 	edt_email.value = "";
	
	stc_txt_cell.value = "예금주휴대폰"; // 예금주휴대폰/ 전화
	stc_txt_cell.visible = false;
	edt_depr_cell.visible = false;
	edt_depr_cell.value = "";
	
	fn_bugaInfoInit();
}

// 발주정보 초기화
function fn_initOdrg()
{
	ds_odrgInfo.clearData();
	ds_dlvList.clearData();
	cbo_dlv_no.text = "선택";
	cbo_dlv_no.enable = false;
	btn_dlv_track.visible = false;
	btn_dlv_detail.visible = false;
}

// 납입정보 부가서비스 조회결과
function fn_postSearchBugaSrvc()
{
	if (ds_bugaSrvcMmeInfo.rowcount > 0) {
		var sDelFg = ds_bugaSrvcMmeInfo.getColumn(0, "del_fg");
		
		if (sDelFg == "N") {
			var sBusinessYn = ds_bugaSrvcMmeInfo.getColumn(0, "business_yn");
			var sIdnNo = ds_bugaSrvcMmeInfo.getColumn(0, "idn_no");
			if (sBusinessYn == "Y") {	// 사업자
				stc_txt_brdt.text = "사업자 번호";
				if (!gfn_isNull(sIdnNo && String(sIdnNo).length == 10)) {
					edt_brdt.value = sIdnNo.substring(0, 3)+"-"+sIdnNo.substring(3, 5)+"-"+sIdnNo.substring(5, 10);
				}
			} else {
				stc_txt_brdt.text = "생년월일";
			}
		} else {
			ds_bugaSrvcMmeInfo.clearData();
		}
	} else {
		ds_bugaSrvcMmeInfo.clearData();
		ds_bugaSrvcMmeInfo.addRow();
		ds_bugaSrvcMmeInfo.addColumn("buga_srvc_typ", "string");
		ds_bugaSrvcMmeInfo.addColumn("expire_date", "string");
		ds_bugaSrvcMmeInfo.setColumn(0, "buga_srvc_typ", gv_typ);
		ds_bugaSrvcMmeInfo.setColumn(0, "expire_date", "");
	}
				
	if (gv_typ == "cms") {					
		stc_txt_cell.value = "예금주휴대폰";
		if (edt_reltn_nm.value == "본인") {
			stc_txt_cell.visible = false;
			edt_depr_cell.visible = false;
			edt_depr_cell.value = "";
		} else {
			stc_txt_cell.visible = true;
			edt_depr_cell.visible = true;
		}
					
		stc_txt_bank.text = "은행코드"; 	// 은행코드/카드사
		edt_bank_cd.visible = true;
		edt_bank_nm.visible = true;
		edt_card_nm.visible = false;		// 카드사
		edt_card_nm.value = "";
					
		stc_txt_accntno.text = "계좌번호"; // 계좌번호/카드번호
		edt_bankaccnt_no.visible = true;
		edt_card_no.visible = false;
		edt_card_no.value = "";
					
		stc_txt_tran.text = "이체일자";	// 이체일자/결제일
		edt_tran_dt.visible = true;
		edt_pymn_dt.visible = false;
		edt_pymn_dt.value = "";
					
		stc_depr.text = "예금주";			// 예금주/유효일
		edt_depr.visible = true;
		edt_exp_year.visible = false;
		stc_txt_yy.visible = false;
		edt_exp_mon.visible = false;
		stc_txt_mm.visible = false;
		edt_exp_year.value = "";
		edt_exp_mon.value = "";
					
		stc_txt_rltn.text = "관계";			// 관계/이메일
		edt_reltn_nm.visible = true;
		edt_email.visible = false;
		edt_email.value = "";
	} else {
		stc_txt_cell.visible = false;
		edt_depr_cell.visible = false;
		edt_depr_cell.value = "";
							
		stc_txt_bank.text = "카드사";      // 은행코드/카드사
		edt_bank_cd.visible = false;
		edt_bank_nm.visible = false;
		edt_card_nm.visible = true;
		edt_card_nm.position2 = "absolute l:9.77% w:24.5% t:118 h:20";
					
		stc_txt_accntno.text = "카드번호"; // 계좌번호/카드번호
		edt_bankaccnt_no.visible = false;
		edt_card_no.visible = true;
		edt_card_no.position2 = "absolute l:45.25% w:17% t:118 h:20";
					
		stc_txt_tran.text = "결제일";	    // 이체일자/결제일
		edt_tran_dt.visible = false;
		edt_pymn_dt.visible = true;
		edt_pymn_dt.position2 = "absolute l:71.75% w:5.22% t:118 h:20";
					
		stc_depr.text = "유효일";			// 예금주/유효일
		edt_depr.visible = false;
		edt_exp_year.visible = true;
		stc_txt_yy.visible = true;
		edt_exp_mon.visible = true;
		stc_txt_mm.visible = true;
		edt_exp_year.position2 = "absolute l:85.27% w:4.5% t:118 h:20";
		stc_txt_yy.position2 = "absolute l:90% w:2.68% t:118 h:20";
		edt_exp_mon.position2 = "absolute l:93.27% w:4.5% t:118 h:20";
		stc_txt_mm.position2 = "absolute l:98% w:2.68% t:118 h:20";
					
		stc_txt_rltn.text = "이메일";	    // 관계/이메일
		edt_reltn_nm.visible = false;
		edt_email.visible = true;
		edt_email.position2 = "absolute l:9.77% w:24.5% t:143 h:20";
		
		if (ds_bugaSrvcMmeInfo.rowcount > 0) {
			var sExpireDate = ds_bugaSrvcMmeInfo.getColumn(0, "expire_date");
			if(!gfn_isNull(sExpireDate)) {
				edt_exp_year.value = sExpireDate.substring(0, 2);
				edt_exp_mon.value = sExpireDate.substring(2, 4);
			}
		}
	}
}

// 부가정보 설정
function fn_setBugaInfo()
{
	fn_bugaInfoInit();
	ds_bugaInfo.addRow();
	if (parent.div_product.ds_bugaInfo.rowcount > 0) {
		ds_bugaInfo.copyRow(0, parent.div_product.ds_bugaInfo, 0);
		
		edt_month_pay_amt.value = gfn_setComma(ds_bugaInfo.getColumn(0, "month_pay_amt"));
		edt_allt_amt.value = gfn_setComma(ds_bugaInfo.getColumn(0, "allt_amt"));
		edt_add_amt.value = gfn_setComma(ds_bugaInfo.getColumn(0, "add_amt"));
	}
}

// 부가정보 초기화
function fn_bugaInfoInit()
{
	ds_bugaInfo.clearData();	
	edt_month_pay_amt.value = "";
	edt_allt_amt.value = "";
	edt_add_amt.value = "";
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 체크박스 변경
function chk_onitemchanged(obj:Radio, e:ItemChangeEventInfo)
{
	fn_srchBugaSrvcMemInfo(obj.value, gv_accnt_no);
}

function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
		ds_dlvList.rowposition = e.postindex;
	if (e.postindex>0) {
		btn_dlv_detail.visible = true;
		var sDlvNo = ds_dlvList.getColumn(ds_dlvList.rowposition, "dlv_no");
		var sDlvStatCd = ds_dlvList.getColumn(e.postindex, "dlv_stat_cd");
		switch(sDlvStatCd) {
		case "10":
		case "30":
			btn_dlv_track.visible = !gfn_isNull(sDlvNo) && true;
			break;
		case "20":
			btn_dlv_track.visible = false;
			break;
		}
	} else {
		btn_dlv_track.visible = false;
		btn_dlv_detail.visible = false;
	}
}

function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	switch(obj.name) {
	case "btn_dlv_track" :
		//우체국 : "http://service.epost.go.kr/trace.RetrieveRegiPrclDeliv.postal?sid1=" + itemno
		//CJ대한통운 : "https://www.doortodoor.co.kr/parcel/doortodoor.do?fsp_action=PARC_ACT_002&fsp_cmd=retrieveInvNoACT&invc_no=" + itemno
		if (cbo_dlv_no.index <= 0) return;
		
		var sDlvNo = ds_dlvList.getColumn(ds_dlvList.rowposition, "dlv_no");
		var sDlvTypeCd = ds_dlvList.getColumn(ds_dlvList.rowposition, "dlv_type_cd");
		
		if (gfn_isNull(sDlvNo)) return;
		var url = "";
		switch(sDlvTypeCd) {
		case "10":
			url = "http://service.epost.go.kr/trace.RetrieveRegiPrclDeliv.postal?sid1="+sDlvNo;
			break;
			
		case "20":
			url = "https://www.doortodoor.co.kr/parcel/doortodoor.do?fsp_action=PARC_ACT_002&fsp_cmd=retrieveInvNoACT&invc_no="+sDlvNo;
			break;
		}
		
		if (sUrl != "") {
			system.execBrowser(url);
		}
		break;
	case "btn_dlv_detail" :
		var sPrmr = {dlv_stat_cd : ""}
		gfn_diaLog("물류 수정", -1, -1, 673,655, "DL450000::PS450402.xfdl", false, sPrmr, false, false, true, null, null, true);
		break;
	}
	
}
