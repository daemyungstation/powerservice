//조회 결과
function fn_postSearch()
{
    if(ds_list_trgtList.rowcount < 1) {
        fn_copyRow();
        sta_totalCnt.text = "조회 결과가 없습니다.";
        fn_changeComponent("R");
        //서브타겟리스트 초기화
        ds_list_subTrgtList.clearData();
        fn_copyRow_sub();
        sta_totalCnt_sub.text = "조회 결과가 없습니다.";
        fn_changeComponent_sub("R");
    } else {
        fn_copyRow();
        fn_changeComponent("R");
        fn_commSearch_sub(gfn_getCommTypeInit()); // 조회
    }
    return;
}

//서브타겟리스트 조회결과
function fn_postSearch_sub()
{
    if(ds_list_subTrgtList.rowcount < 1){
        fn_copyRow_sub();
        sta_totalCnt_sub.text = "조회 결과가 없습니다.";
        fn_changeComponent_sub("R");
    }else{
        fn_copyRow_sub();
        fn_changeComponent_sub("R");
    }
}

//리스트 데이터셋 뷰 데이터셋에 카피
function fn_copyRow(){
    if(ds_list_trgtList.rowcount < 1){
        ds_trgtList.clearData();
        ds_trgtList.addRow();
    }else{
        ds_trgtList.clearData();
        ds_trgtList.addRow();
        ds_trgtList.copyRow(ds_trgtList.rowposition,ds_list_trgtList,ds_list_trgtList.rowposition);

        //등록 수정일시 데이터타입에 맞게
        var rgsnDttmStr = edt_rgsn_dttm.value;
        edt_rgsn_dttm.value = gfn_strDttm(rgsnDttmStr);

        var amntDttmStr = edt_amnt_dttm.value;
        edt_amnt_dttm.value = gfn_strDttm(amntDttmStr);
    }
}

//리스트 데이터셋 뷰 데이터셋에 카피
function fn_copyRow_sub(){
    if(ds_list_subTrgtList.rowcount < 1){
        ds_subTrgtList.clearData();
        ds_subTrgtList.addRow();
    }else{
        ds_subTrgtList.clearData();
        ds_subTrgtList.addRow();
        ds_subTrgtList.copyRow(ds_subTrgtList.rowposition,ds_list_subTrgtList,ds_list_subTrgtList.rowposition);

        //등록 수정일시 데이터타입에 맞게
        var rgsnDttmStr = edt_rgsn_dttm_sub.value;
        edt_rgsn_dttm_sub.value = gfn_strDttm(rgsnDttmStr);

        var amntDttmStr = edt_amnt_dttm_sub.value;
        edt_amnt_dttm_sub.value = gfn_strDttm(amntDttmStr);
    }
}


//버튼 AND 컴포넌트 변경함수
function fn_changeComponent(rMode){
    switch(rMode){
        case "R" : //읽기 모드
        if(ds_list_trgtList.rowcount < 1){ //조회결과 없을때
            btn_create.enable = true;
            btn_update.enable = false;
            btn_delete.enable = false;

            grd_sub_list.formatid ="";
            //캠페인유형
            edt_cmpg_typ_cd.value = null;

            //수행팀
            edt_trgt_list_team_nm.value = null;

            //목록건수
            edt_cust_data_extc_cnt.visible = false;

            //추출유형
            edt_alct_typ.value = null;
        }else{ //조회결과 있을때
            btn_create.enable = true;
            btn_update.enable = true;
            var sTrgtId = ds_trgtList.getColumn(ds_trgtList.rowposition , 'trgt_list_id');
			//소스변경 
			//변경자 : 정훈
			//변경이유 웹DB 추가에 따른 소스길이 간소화
            if((ds_trgtList.getColumn(ds_trgtList.rowposition , 'cmpg_typ_cd')== '4100' && ds_trgtList.getColumn(ds_trgtList.rowposition , 'cmpg_prfm_team_cd') =='T20300' && (sTrgtId.substr(0,15)=='TAR999999999999'))){
                grd_sub_list.formatid ="TMB2Q";

            }else{
                grd_sub_list.formatid ="";
            }
            if(ds_trgtList.getColumn(ds_trgtList.rowposition, 'alct_yn') == 'Y'){
                btn_delete.enable = false;
            }else{
				//소스변경 
				//변경자 : 정훈
				//변경이유 웹DB 추가에 따른 소스길이 간소화
                if((ds_trgtList.getColumn(ds_trgtList.rowposition , 'cmpg_typ_cd')== '4100' && ds_trgtList.getColumn(ds_trgtList.rowposition , 'cmpg_prfm_team_cd') =='T20300' && (sTrgtId.substr(0,15)=='TAR999999999999'))){
                    btn_delete.enable = false;
                    grd_sub_list.formatid ="TMB2Q";
                }else{
                    btn_delete.enable = true;
                }
            }

            //목록건수
            edt_cust_data_extc_cnt.visible = true;

            //캠페인유형
            edt_cmpg_typ_cd.value = ds_trgtList.getColumn(ds_trgtList.rowposition, 'hgrn_hrch_nm') + ' > '+
                                ds_trgtList.getColumn(ds_trgtList.rowposition, 'cmpg_typ_nm');

            if(ds_trgtList.getColumn(ds_trgtList.rowposition, 'cmpg_prfm_team_nm') == null){
                edt_trgt_list_team_nm.value= null;
            }else{
                //수행부서
                edt_trgt_list_team_nm.value = ds_trgtList.getColumn(ds_trgtList.rowposition, 'hgrn_team_nm') + ' > '+
                                    ds_trgtList.getColumn(ds_trgtList.rowposition, 'cmpg_prfm_team_nm');
            }

            //추출유형
            gfn_getHrchCodeLoad(cbo_cmpg_typ_cd.value,"SELECT",ds_cmpgTypCdList2,null);
            var nRowIdx = 0;
            nRowIdx = ds_cmpgTypCdList2.findRow('hrch_cd',ds_trgtList.getColumn(ds_trgtList.rowposition, 'cmpg_typ_cd'));
            var sTrgtId = ds_trgtList.getColumn(ds_trgtList.rowposition , 'trgt_list_id');
            //소스변경 
			//변경자 : 정훈
			//변경이유 웹DB 추가에 따른 소스길이 간소화
            if(cbo_cmpg_prfm_team_cd2_trgt_list.value == 'T20300' &&cbo_cmpg_typ_cd2.value == '4100'  && (sTrgtId.substr(0,15)=='TAR999999999999')){
                edt_alct_typ.value = 'DB'
            }else if(cbo_cmpg_prfm_team_cd2_trgt_list.value == 'T20300' &&cbo_cmpg_typ_cd2.value == '4100'  && sTrgtId =='TAR202207135923241'){
                edt_alct_typ.value = 'DB'
            }else if(cbo_cmpg_prfm_team_cd2_trgt_list.value == 'T20100' &&cbo_cmpg_typ_cd2.value == '4100'  && sTrgtId =='TAR202102268150909'){
                edt_alct_typ.value = 'DB'
            }else if(cbo_cmpg_prfm_team_cd2_trgt_list.value == 'T20600' &&cbo_cmpg_typ_cd2.value == '4100'  && sTrgtId =='TAR202105119262669'){
                edt_alct_typ.value = 'DB'
            }else{
                edt_alct_typ.value = ds_cmpgTypCdList2.getColumn(nRowIdx,'hrch_adtl_cd_nm');
            }
        }

        btn_create.visible = true;
        btn_update.visible = true;
        btn_delete.visible = true;
        btn_cancel.visible = false;
        btn_save.visible = false;

        //대상목록명
        edt_trgt_list_nm.readonly = true;

        //실행채널
        cbo_cmpg_pran_chnl_cd.readonly = true;
        cbo_cmpg_pran_chnl_cd.enable = true;

        //조사방법
        cbo_cmpg_invt_mthd_cd.readonly = true;
        cbo_cmpg_invt_mthd_cd.enable = true;

        //캠페인유형
        cbo_cmpg_typ_cd.visible = false;
        cbo_cmpg_typ_cd2.visible = false;
        edt_cmpg_typ_cd.visible = true;
        edt_cmpg_typ_cd.move(stc_cmpg_typ_cd.position.right+1,stc_cmpg_typ_cd.position.top+3);
        edt_cmpg_typ_cd.readonly = true;

        //수행부서
        edt_trgt_list_team_nm.readonly = true;
        edt_trgt_list_team_nm.visible = true;
        edt_trgt_list_team_nm.move(stc_team_trgt_list.position.right+1,stc_team_trgt_list.position.top+3);
        cbo_cmpg_prfm_team_cd_trget_list.visible = false;
        cbo_cmpg_prfm_team_cd2_trgt_list.visible = false;

        //설명
        tea_trgt_list_expi.readonly = true;

        //할당여부
        edt_alct_yn.readonly = true;
        if(edt_alct_yn.value == 'Y'){
            edt_alct_yn.value = '할당';
        }else if(edt_alct_yn.value == 'N'){
            edt_alct_yn.value = '미할당';
        }

        //목록건수
        edt_cust_data_extc_cnt.readonly = true;
        edt_cust_data_extc_cnt.value = gfn_commaStr(ds_trgtList.getColumn(ds_trgtList.rowposition, 'cust_data_extc_cnt'))+" 건"; //목록건수
        break;

        case "C" : //신규모드
        btn_create.visible = false;
        btn_update.visible = false;
        btn_delete.visible = false;

        btn_save.move(btn_update.position.left , btn_update.position.top);
        btn_cancel.move(btn_delete.position.left , btn_delete.position.top);

        btn_save.visible = true;
        btn_cancel.visible = true;

        //sub trgt list btn
        btn_create_sub.enable = false;
        
        btn_oneadd.enable = false; // 단일건 추가
        btn_resnadd.enable = false; // 해약 미처리건 추가
        
        btn_update_sub.enable = false;
        btn_delete_sub.enable = false;
        btn_dfctOpen.enable = false;//불량건수

        //대상목록명
        edt_trgt_list_nm.readonly = false;

        //실행채널
        //cbo_cmpg_pran_chnl_cd.readonly = false;
        //cbo_cmpg_pran_chnl_cd.index = 0;
        cbo_cmpg_pran_chnl_cd.index = 1;


        //조사방법
        //cbo_cmpg_invt_mthd_cd.readonly = false;
        //cbo_cmpg_invt_mthd_cd.enable = false;
        //cbo_cmpg_invt_mthd_cd.index = 0;
        cbo_cmpg_invt_mthd_cd.index = 1;

        //캠페인유형
        cbo_cmpg_typ_cd.visible = true;
        cbo_cmpg_typ_cd2.visible = true;
        edt_cmpg_typ_cd.visible = false;
        cbo_cmpg_typ_cd.readonly = false;
        cbo_cmpg_typ_cd.index = 0;
        cbo_cmpg_typ_cd.enable = true;
        cbo_cmpg_typ_cd2.readonly = false;
        cbo_cmpg_typ_cd2.index= 0;
        cbo_cmpg_typ_cd2.enable = false;

        //설명
        tea_trgt_list_expi.readonly = false;

        //수행그룹
        cbo_cmpg_prfm_team_cd_trget_list.readonly = false;
        cbo_cmpg_prfm_team_cd_trget_list.index = 0;
        cbo_cmpg_prfm_team_cd_trget_list.enable = true;
        cbo_cmpg_prfm_team_cd2_trgt_list.readonly = false;
        cbo_cmpg_prfm_team_cd2_trgt_list.index = 0;
        cbo_cmpg_prfm_team_cd2_trgt_list.enable = false;

        edt_trgt_list_team_nm.visible = false;

        cbo_cmpg_prfm_team_cd_trget_list.visible = true;
        cbo_cmpg_prfm_team_cd2_trgt_list.visible = true;

        //추출유형
        edt_alct_typ.value = null;
        break;

        case "U" : //수정모드
        btn_create.visible = false;
        btn_update.visible = false;
        btn_delete.visible = false;

        btn_save.move(btn_update.position.left , btn_update.position.top);
        btn_cancel.move(btn_delete.position.left , btn_delete.position.top);

        btn_save.visible = true;
        btn_cancel.visible = true;

        //sub trgt list btn
        btn_create_sub.enable = false;
        btn_oneadd.enable = false; // 단일건 추가
        btn_resnadd.enable = false; // 해약 미처리건 추가
        btn_update_sub.enable = false;
        btn_delete_sub.enable = false;
        btn_dfctOpen.enable = false;//불량건수

        //대상목록명
        edt_trgt_list_nm.readonly = false;

        //실행채널
        cbo_cmpg_pran_chnl_cd.readonly = false;
        cbo_cmpg_pran_chnl_cd.enable = false;

        //조사방법
        cbo_cmpg_invt_mthd_cd.readonly = false;
        cbo_cmpg_invt_mthd_cd.enable = false;


        //캠페인유형
        cbo_cmpg_typ_cd.visible = true;
        cbo_cmpg_typ_cd2.visible = true;
        edt_cmpg_typ_cd.visible = false;
        cbo_cmpg_typ_cd.readonly = false;
        cbo_cmpg_typ_cd2.readonly = false;
        cbo_cmpg_typ_cd.enable = false;
        cbo_cmpg_typ_cd2.enable = false;

        gfn_getHrchCodeLoad(cbo_cmpg_typ_cd.value,"SELECT",ds_cmpgTypCdList2,null);

        //설명
