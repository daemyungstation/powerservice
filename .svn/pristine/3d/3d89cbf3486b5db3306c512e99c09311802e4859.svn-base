}



/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) {
    if (errcd != 0) {

    } else {
        switch (svcid) {
            case "srchCdSet": //코드셋 코드콜백
                gfn_getCode("CAM060", "ALL", ds_srch_cmpgInvtMthdCd,ds_cdSet);  //조상방법 srch
                gfn_getCode("CAM120", "ALL", ds_srch_cmpgPranChnlCd,ds_cdSet);  //실행채널 srch
                gfn_getCode("CAM060", "SELECT", ds_cmpgInvtMthdCd,ds_cdSet);    //조사방법
                gfn_getCode("CAM120", "SELECT", ds_cmpgPranChnlCd,ds_cdSet);    //실행채널
                gfn_getCode("CAM070", "SELECT", ds_trgtListExtcTypCd,ds_cdSet);    //추출유형

                cbo_srch_cmpg_pran_chnl_cd.index=0;
                cbo_srch_cmpg_invt_mthd_cd.index =0;
            break;

            case "selectTeamCode": //팀코드 조회
                gfn_getTeamCodeLoad("team_cd == 'T20000' || team_cd == 'T30000'|| team_cd == 'T10000'", "SELECT", ds_teamCd01, ds_teamCd, "1","Y");
                gfn_getTeamCodeLoad("team_cd == 'T20000' || team_cd == 'T30000'|| team_cd == 'T10000'", "SELECT", ds_teamCd01_trgtList, ds_teamCd, "1","Y");
            break;

            case "srchHrchCdSet": // 캠페인 유형
                gfn_getHrchCodeLoad("","SELECT",ds_cmpgTypCdList,null,1);
                gfn_getHrchCodeLoad("","ALL",ds_srchCmpgTypCdList,null,1);
            break;

            case "srchTrgtList": //대상고객 목록 조회
                gfn_appendSearchDs(ds_list_trgtList, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
                gfn_setTotalRowCount(ds_list_trgtList, sta_totalCnt, total_count); // 페이지건수 표출
                fn_postSearch();
            break;

            case "deleteTrgtList"://대상고객 목록 삭제 콜백
                fn_commSearch(gfn_getCommTypeInit()); // 조회
            break;

            case "saveTrgtList" : //대상고객 목록 저장 콜백
                fn_commSearch(gfn_getCommTypeInit()); // 조회
            break;

            case "srchSubTrgtList" ://서브대상고객 목록 조회
                gfn_appendSearchDs(ds_list_subTrgtList, ds_list_tmp_sub); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
                gfn_setTotalRowCount(ds_list_subTrgtList, sta_totalCnt_sub, total_count_sub); // 페이지건수 표출
                fn_postSearch_sub();
            break;

            case "deleteSubTrgtList" :
                ds_list_trgtList.setColumn(ds_list_trgtList.rowposition, 'cust_data_extc_cnt', total_cust_data_extc_cnt);
                edt_cust_data_extc_cnt.value = gfn_commaStr(ds_list_trgtList.getColumn(ds_list_trgtList.rowposition, 'cust_data_extc_cnt'))+" 건"; //목록건수
                fn_commSearch_sub(gfn_getCommTypeInit()); // 조회
            break;

            case "saveSubTrgtList" : //서브대상고객 저장 콜백
                ds_list_trgtList.setColumn(ds_list_trgtList.rowposition, 'cust_data_extc_cnt', total_cust_data_extc_cnt);
                edt_cust_data_extc_cnt.value = gfn_commaStr(ds_list_trgtList.getColumn(ds_list_trgtList.rowposition, 'cust_data_extc_cnt'))+" 건"; //목록건수
                edt_file_or_db.value = null;
                fn_commSearch_sub(gfn_getCommTypeInit()); // 조회
            break;

            case "insertTmTrgtCust": //
                ds_list_trgtList.setColumn(ds_list_trgtList.rowposition, 'cust_data_extc_cnt', total_cust_data_extc_cnt);
                edt_cust_data_extc_cnt.value = gfn_commaStr(ds_list_trgtList.getColumn(ds_list_trgtList.rowposition, 'cust_data_extc_cnt'))+" 건"; //목록건수
                edt_file_or_db.value = null;
                fn_commSearch_sub(gfn_getCommTypeInit()); // 조회
            break;
            
            case "mst":
				alert("작업 완료");
            break;
            
            case "mst1":
				alert("작업 완료");
            break;
            
            case "mst2":
				alert("작업 완료");
            break;
            
		    case "mst3":
				alert("작업 완료");
            break;
            
             case "mst4":
				alert("작업 완료");
            break;
            
            case "mst5":
				alert("작업 완료");
            break;
        }
    }
}
