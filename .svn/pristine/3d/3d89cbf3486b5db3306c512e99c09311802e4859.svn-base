//대상고객목록 조회전 처리 조회조건셋팅
function fn_preSearch(sType)
{
    if (sType == gfn_getCommTypeInit())
    {
        // 조회조건 및 default 정렬
        ds_list_trgtList.keystring = ""; // 정렬컬럼 초기화

        ds_inqCndt.clearData();
        ds_inqCndt.addRow();

        //정훈 소스변경 업체간 검색조건 변경
        var sUserAthrCd =gds_UserInfo.getColumn(gds_UserInfo.rowposition,'athr_cd');
        if(sUserAthrCd.substr(0,2)=='TM'){
            ds_inqCndt.setColumn(0, "cmpg_prfm_team_cd", gds_UserInfo.getColumn(gds_UserInfo.rowposition,'team_cd'));
        }
        ds_inqCndt.setColumn(0, "cmpg_pran_chnl_cd", cbo_srch_cmpg_pran_chnl_cd.value);
        ds_inqCndt.setColumn(0, "cmpg_invt_mthd_cd", cbo_srch_cmpg_invt_mthd_cd.value);
        ds_inqCndt.setColumn(0, "trgt_list_nm", edt_srch_trgt_list_nm.value);
        ds_inqCndt.setColumn(0, "alct_yn", cbo_srch_alct_yn.value);
        ds_inqCndt.setColumn(0, "orderBy", "rgsn_dttm");
        ds_inqCndt.setColumn(0, "orderDirection", "desc");
        ds_inqCndt.setColumn(0, "hgrn_hrch_cd", cbo_srch_cmpg_type.value);
        
    }

    return true;
}

//대상고객목록 저장전 처리
function fn_preSaveData(){
    if(gfn_isNull(edt_trgt_list_nm.value)){ //대상목록
        alert("대상목록명을 입력하세요.");
        return false;
    }

    if(gfn_isNull(cbo_cmpg_pran_chnl_cd.value)){ //실행채널
        alert("실행채널을 선택하세요.");
        return false;
    }

    if(gfn_isNull(cbo_cmpg_invt_mthd_cd.value)){ //조사방법
        alert("조사방법을 선택하세요.");
        return false;
    }

    if(gfn_isNull(cbo_cmpg_typ_cd.value)){ //캠페인유형 대
        alert("캠페인유형을 선택하세요.");
        return false;
    }

    if(gfn_isNull(cbo_cmpg_typ_cd2.value)){ //캠페인유형 소
        alert("캠페인유형을 선택하세요.");
        return false;
    }

    if(gfn_isNull(cbo_cmpg_prfm_team_cd_trget_list.value)){ //수행부서 대
        alert("수행부서를 선택하세요.");
