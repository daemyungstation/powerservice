//콤보박스 전환 이벤트
function cbo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
    switch(obj){
        case cbo_srch_cmpg_pran_chnl_cd : // 조회부 실행채널 변경시
            if(obj.index == 0){ //전체
                cbo_srch_cmpg_invt_mthd_cd.enable = false;
            }else if(obj.index == 1){ //전화
                ds_srch_cmpgInvtMthdCd.filter("");
                ds_srch_cmpgInvtMthdCd.filter("cd <30");
                cbo_srch_cmpg_invt_mthd_cd.enable = true;
            }else if(obj.index == 2){//문자
                ds_srch_cmpgInvtMthdCd.filter("");
                ds_srch_cmpgInvtMthdCd.filter("cd==''||cd==30");
                cbo_srch_cmpg_invt_mthd_cd.enable = true;
            }else if(obj.index == 3){//외부
                ds_srch_cmpgInvtMthdCd.filter("");
                cbo_srch_cmpg_invt_mthd_cd.enable = true;
            }
            cbo_srch_cmpg_invt_mthd_cd.index = 0;
        break;

        case cbo_cmpg_pran_chnl_cd : // 상세부 실행채널 변경시
            if(obj.index == 0){ //전체
                cbo_cmpg_invt_mthd_cd.enable = false;
            }else if(obj.index == 1){ //전화
                ds_cmpgInvtMthdCd.filter("");
                ds_cmpgInvtMthdCd.filter("cd <30");
                cbo_cmpg_invt_mthd_cd.enable = true;
            }else if(obj.index == 2){//문자
                ds_cmpgInvtMthdCd.filter("");
                ds_cmpgInvtMthdCd.filter("cd==''||cd==30");
                cbo_cmpg_invt_mthd_cd.enable = true;
            }else if(obj.index == 3){//외부
                ds_cmpgInvtMthdCd.filter("");
                cbo_cmpg_invt_mthd_cd.enable = true;
            }
            cbo_cmpg_invt_mthd_cd.index = 0;
        break;

        case cbo_cmpg_typ_cd : //캠페인 유형(대) 변경시
            if(cbo_cmpg_typ_cd.value == '3000'){
                gfn_getHrchCodeLoad("hrch_cd=='3100' ||hrch_cd=='3200' ||hrch_cd=='3500'","SELECT",ds_cmpgTypCdList2,null,null,'Y');
            }else{
                gfn_getHrchCodeLoad(cbo_cmpg_typ_cd.value,"SELECT",ds_cmpgTypCdList2,null);
            }
            cbo_cmpg_typ_cd2.index = 0;
            if(obj.value != "" && obj.value != null){
                cbo_cmpg_typ_cd2.enable = true;
            }else{
                cbo_cmpg_typ_cd2.enable = false;
                cbo_cmpg_typ_cd2.value = "";
            }
        break;

        case cbo_cmpg_prfm_team_cd_trget_list: //대상고객 목록 상세 수행그룹 대 변경시
            gfn_getTeamCodeLoad(cbo_cmpg_prfm_team_cd_trget_list.value, "SELECT", ds_teamCd02_trgtList, ds_teamCd);
            cbo_cmpg_prfm_team_cd2_trgt_list.index = 0;
            if(obj.value != "" && obj.value != null){
                cbo_cmpg_prfm_team_cd2_trgt_list.enable = true;
            }else{
                cbo_cmpg_prfm_team_cd2_trgt_list.enable = false;
                cbo_cmpg_prfm_team_cd2_trgt_list.value = "";
            }
        break;

        case cbo_cmpg_prfm_team_cd: //서브대상목록 상세 수행그룹 대 변경시
            gfn_getTeamCodeLoad(cbo_cmpg_prfm_team_cd.value, "SELECT", ds_teamCd02, ds_teamCd);
            cbo_cmpg_prfm_team_cd2.index = 0;
            if(obj.value != "" && obj.value != null){
                cbo_cmpg_prfm_team_cd2.enable = true;
            }else{
                cbo_cmpg_prfm_team_cd2.enable = false;
                cbo_cmpg_prfm_team_cd2.value = "";
            }
        break;
        
        case cbo_trgt_list_extc_typ_cd : //추출유형 변경시
         
			if(!gfn_isNull(edt_file_or_db.value)){
				obj.value= e.prevalue;
			}
			if(obj.value == '10'){// file일때
				 stc_fileOrDb.text = "File데이터";
			}else if(obj.value  == '20'){
				 stc_fileOrDb.text = "DB데이터";
			}
        break;
    }
}




