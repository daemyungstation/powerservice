    return true;
}

//서브타겟리스트 조회전 처리 조회조건셋팅
function fn_preSearch_sub(sType)
{
    if (sType == gfn_getCommTypeInit())
    {
        // 조회조건 및 default 정렬
        ds_list_subTrgtList.keystring = ""; // 정렬컬럼 초기화

