//온로드
function PS220400_onload(obj:Form, e:LoadEventInfo)
{
    gfn_athrPgm(gfn_gdsCd_admin());//권한조회

    //조사방법 : 코드셋(CAM060) 실행채널 : 코드셋(CAM120) 캠페인유형 : 코드셋(CAM040) 추출유형 : 코드셋(CAM070)
    gfn_getList("cd","CAM060,CAM120,CAM040,CAM070","fn_callBack",null,"srchCdSet","ds_cdSet");   // 코드셋
    gfn_getList("hrchCd","CAM990","fn_callBack",null,"srchHrchCdSet");   //캠페인유형(계층유형코드)
    gfn_getList("teamCd","", "fn_callBack", null, null, "ds_teamCd"); //팀코드조회
	fn_init();
    gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
    fn_commSearch(gfn_getCommTypeInit()); // 조회
}

//초기화
function fn_init()
{
    cbo_srch_cmpg_pran_chnl_cd.index = 0; // 실행채널
    cbo_srch_cmpg_invt_mthd_cd.index = 0; // 조사방법
