//서브대상고객목록 저장전 처리
function fn_preSaveData_sub(){
    if(gfn_isNull(cbo_cmpg_prfm_team_cd.value)){ //수행그룹
        alert("수행그룹을 선택하세요.");
        return false;
    }
    if(gfn_isNull(cbo_cmpg_prfm_team_cd.value)){ //수행그룹
        alert("수행그룹을 선택하세요.");
        return false;
    }

    if(gfn_isNull(edt_sub_trgt_list_nm.value)){ //서브목록명
        alert("서브목록명을 입력하세요.");
        return false;
    }

    if(gfn_isNull(edt_invt_cnts_id.value)){ //조사컨텐츠
        alert("조사컨텐츠를 선택하세요.");
        return false;
    }

    if(gfn_isNull(cbo_trgt_list_extc_typ_cd.value)){ //추출유형
        alert("추출유형을 선택하세요.");
        return false;
    }

    if(gfn_isNull(cbo_trgt_list_extc_typ_cd.value)){ //추출유형
        alert("추출유형을 선택하세요.");
        return false;
    }

    if(ds_subTrgtList.getColumn(ds_subTrgtList.rowposition, 'sub_trgt_list_id') == null ){
        if(cbo_trgt_list_extc_typ_cd.index == 1){//File
            if(gfn_isNull(edt_file_or_db.value)){
                alert("File 데이터를 선택하세요.");
                return false;
            }
        }else if(cbo_trgt_list_extc_typ_cd.index == 2){//DB
            if(gfn_isNull(edt_file_or_db.value)){
                alert("DB 데이터를 선택하세요.");
                return false;
            }
        }
        ds_subTrgtList.setColumn(ds_subTrgtList.rowposition, 'trgt_list_id', ds_list_trgtList.getColumn(ds_list_trgtList.rowposition,'trgt_list_id'));
    }

    return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//대상목록조회
function fn_commSearch(sType)
{
    if (!fn_preSearch(sType)) return false;

