function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {		
	
		    // 공통코드
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("0170", "ALL", ds_0170, gds_DmlCd); // 코드조회결과 필터			
			fn_init(); // 검색부 초기화
			break;
			
		case "search":
			fn_postSearch();
			break;

		case "save":
			fn_postSave();
			break;
	}
}


