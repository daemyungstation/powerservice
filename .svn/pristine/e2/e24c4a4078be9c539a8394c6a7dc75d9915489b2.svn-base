function fnRoleSelect(row) {
	ds_rltn_list.clearData();
	if (ds_list.rowcount < 1) { return false; }
	if (ds_list.getColumn(row, "roleId") == null) { 
		ds_input.setColumn( 0, "roleIdRltn", "");
		div_body.btn_roleAdd.enable = false;
		div_body.btn_roleDel.enable = false;
	} else {
		if (ds_role_scrn_rltn_list.rowcount > 0 ) {
			ds_role_scrn_rltn_list.setColumn(0, "roleId", ds_list.getColumn(row, "roleId"));
		}
		ds_input.setColumn( 0, "roleIdRltn", ds_list.getColumn(row, "roleId"));
		div_body.btn_roleAdd.enable = ( "Y" == gfn_getReDefnBtnYn("SYS0000201200", "btn_roleAdd"));
		div_body.btn_roleDel.enable = ( "Y" == gfn_getReDefnBtnYn("SYS0000201200", "btn_roleDel"));
		fn_rltnSearch();
	}
	
}

