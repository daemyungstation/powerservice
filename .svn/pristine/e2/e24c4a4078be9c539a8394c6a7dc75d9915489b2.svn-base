		alert("먼저 등록할 권한ID를 선택하세요.");
		ds_rltn_list.clearData();
		ds_rltn_list.enableevent = false;
		return false;
	}
	var roleId = ds_role_scrn_rltn_list.getColumn(0, "roleId");
	
	if (gfn_isNull(ds_role_scrn_rltn_list.getColumn(0, "scrnNo")) ) {
 		var hgrnStrcNo = ds_role_scrn_rltn_list.getColumn(0, "menuStrcNo");
 		var sttRow = ds_list_Tree.findRow("menuStrcNo", hgrnStrcNo);
 		var len = hgrnStrcNo.length;
 		
 
		for (var i= 0 ; i < ds_list_Tree.rowcount ; i++ ) {
			var menuStrcNo = ds_list_Tree.getColumn(i,"menuStrcNo");
			var scrnNo = ds_list_Tree.getColumn(i, "scrnNo");
			//@ 상위메뉴가 다른 경우와, 화면ID가 없을 경우에는 등록하지 않는다.
			if(hgrnStrcNo != menuStrcNo.substr(0,len) || gfn_isNull(scrnNo) ){ continue; }
			
			var menuCd = ds_list_Tree.getColumn(i, "menuCd");
			
			
