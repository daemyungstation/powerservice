 ***************************************************************************/
function fn_preSave(sType)
{
	
	if((ds_list.rowcount == 0 || !gfn_isUpdateDs(ds_list))
	    && (ds_rltn_list.rowcount == 0 || !gfn_isUpdateDs(ds_rltn_list))) {
		alert("저장할 데이타가 없습니다.");
		return false;
	}
	
	if(!confirm("저장하시겠습니까?")) return false;
	
	//TO-DO:필수 입력 체크 등
	// 직책인 경우 필수 입력
	for (var i = 0; i < ds_list.rowcount; i ++ ) {
		if (ds_list.getColumn(i, "xRowStatusType") == "C" ||ds_list.getColumn(i, "xRowStatusType") == "U" ) {
			if (ds_list.getColumn(i, "roleTypCd") == "DTR" && ds_list.getColumn(i, "dtrsCd") == "") {
				alert("'역할유형'이 '직책'일 때, '직책'은 필수 입력입니다.");
				return false;
			}
		}
	}
	return true;
}

