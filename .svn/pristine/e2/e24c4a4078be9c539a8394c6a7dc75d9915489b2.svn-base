			ds_rltn_list.setColumn(nRow, "menuLnpSqnc", ds_role_scrn_rltn_list.getColumn(0, "menuLnpSqnc"));
			ds_rltn_list.setColumn(nRow, "scrnShrdCd", ds_role_scrn_rltn_list.getColumn(0, "scrnShrdCd"));	
			ds_rltn_list.setColumn(nRow, "systIdntId", "KRP");
		} else {
			alert("이미 등록된 데이타가 있습니다.\n다시 확인하시기 바랍니다.");
