/***************************************************************************
 * 기      능		: 하위에 변경된 데이타 체크
 ***************************************************************************/
function ds_list_canrowposchange(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	if ( gfn_isUpdateDs(ds_rltn_list) ) {
		if(!confirm("현재 수정중인 데이타가 있습니다. 현재 수정 중인 데이타는 전체 초기화됩니다. \n\n초기화 하시겠습니까?")) {
			return false;
		}
	}
	
	if(ds_list.getColumn(e.newrow, "roleId") == null){
		ds_input.setColumn( 0, "roleIdRltn", ds_list.getColumn(e.newrow, "roleId"));
		div_body.btn_roleAdd.enable = false;
		div_body.btn_roleDel.enable = false;
	}else{
		div_body.btn_roleAdd.enable = ( "Y" == gfn_getReDefnBtnYn("SYS0000201200", "btn_roleAdd"));
		div_body.btn_roleDel.enable = ( "Y" == gfn_getReDefnBtnYn("SYS0000201200", "btn_roleDel"));
	}
	ds_rltn_list.clearData();
	return true;
}

