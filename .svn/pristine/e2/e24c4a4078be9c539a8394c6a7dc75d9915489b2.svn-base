 ***************************************************************************/
function fn_commSave_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	} 
	// 저장 후 처리
	ds_list.clearData();
	ds_rltn_list.clearData();
	fn_postSave(strSvcID);
}

