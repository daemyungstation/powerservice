	fn_init(); // 초기화
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {		
			case "getOnlnConsList":
				gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_list); // 페이지건수 표출
				
				if(!gfn_isNull(xRow)) {
					ds_list.rowposition = ds_list.findRow("prsn_cnsl_seq", xRow);
					xRow = "";
				}
				
				//trace(ds_list.saveXML());
				if(ds_list.rowcount > 0){
					xID = ds_list.getColumn(ds_list.rowposition,"id");					
					//fn_commSearch(gfn_getCommTypeAppend(), "getOnlnMemberDtlList");
					fn_component(ds_list.getColumn(ds_list.rowposition, "prcs_cd"));
					if(ds_list.getColumn(ds_list.rowposition, "prcs_cd") == "01") {
						cbo_prcs_cd.enable = false;
						btn_check_user.enable = true;
						txt_answ.enable = false;
						btn_save.enable = false;
						ds_prcs_cd.filter("");
						btn_sms.enable = false;
					} else if(ds_list.getColumn(ds_list.rowposition, "prcs_cd") == "02") {
						cbo_prcs_cd.enable = true;
						btn_check_user.enable = false;
						txt_answ.enable = true;
						btn_save.enable = true;
						ds_prcs_cd.filter("cd != '01'");
						btn_sms.enable = false;
					} else if(ds_list.getColumn(ds_list.rowposition, "prcs_cd") == "03") {
						cbo_prcs_cd.enable = false;
						btn_check_user.enable = false;
						txt_answ.enable = false;
						btn_save.enable = false;
						ds_prcs_cd.filter("");
						btn_sms.enable = true;
					}
				} else {
					alert("조회 내역이 없습니다.");
					//ds_list_dtl.clearData();					
				}
			break;
			case "getOnlnMemberList": // 홈페이지 회원조회
				gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_list); // 페이지건수 표출
				
				if (cbo_search_gbn.value == "04"){
					// 탈퇴회원 조회 후 
					div_resn.visible = true;					
				} else {
					div_resn.visible = false;
					// 조회결과 존재시 하단 정보창 DS COPY
					if(ds_list.rowcount > 0){
						xID = ds_list.getColumn(ds_list.rowposition,"id");					
						fn_commSearch(gfn_getCommTypeAppend(), "getOnlnMemberDtlList");
					} else {
						alert("조회 내역이 없습니다.");
						ds_list_dtl.clearData();					
					}				
				}
				
			break;
			
			
			
			case "updateOnlnConschk":
				SetComponent("chk");
			break;
			
			case "updateOnlnConssave":
				SetComponent("save");
			break;
			
			case "getOnlnMemberDtlList": // 홈페이지 회원 상세
				SetComponent(); //버트 및 컴퍼넌트 설정
			break;
								
			case "updateMemberInfo": // 저장처리
				alert("저장 완료");
				fn_commSearch(gfn_getCommTypeAppend(), "getOnlnMemberDtlList");
			break;
			
			case "deleteExtSpecial": // 삭제처리
				alert("삭제 완료");
				fn_commSearch(gfn_getCommTypeInit(), "getOnlnMemberList"); // 조회
			break;
			
			case "getOnlnMemberListExcel": // 엑셀
				gfn_exportGridToExcel(grd_list_excel, "특수고객청구관리_"+gfn_getTodayFull()+"");
			break;				
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{
	if(gfn_isNull(edt_srch_id.value)) {
		if(gfn_isNull(cal_srchSttDt.value) || gfn_isNull(cal_srchEndDt.value)) {
			alert("검색기간은 필수항목 입니다.");
			return false;
		}
// 		if(gfn_getDayDiff(cal_srchSttDt.value, cal_srchEndDt.value) > 7) {
// 			alert("날짜 범위는 7일을 초과 할 수 없습니다.");
// 			return false;
// 		}
	}	
	if(gfn_getDayDiff(cal_srchSttDt.value, cal_srchEndDt.value) < 0) {
		alert("시작일은 종료일을 초과 할 수 없습니다.");
		return false;
	}
	// 조회조건 및 default 정렬
	ds_inqCndt.clearData();
	ds_list.keystring = ""; // 정렬컬럼 초기화

	ds_inqCndt.addRow();
	ds_inqCndt.setColumn(0, "stt_dt", cal_srchSttDt.value);
	ds_inqCndt.setColumn(0, "end_dt", cal_srchEndDt.value);
	ds_inqCndt.setColumn(0, "id", edt_srch_id.value);
	ds_inqCndt.setColumn(0, "search_gbn", cbo_search_gbn.value);
	ds_inqCndt.setColumn(0, "prcs_cd", cbo_srch_prcs_cd.value);
	
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 조회
function fn_commSearch(sType, searchInfo)
{
    switch(searchInfo) {
    
		// 조회
        case "getOnlnConsList":
			if (!fn_preSearch(sType)) return false;
			
			// 호출 건수 설정 및 조회여부 설정
			if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 100, tc_list)) return;
		
			var szSvcid       = "getOnlnConsList";
			var szController  = "/dlw/onlnMng/getOnlnConsList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
		break;
		
		// 조회
        case "getOnlnMemberDtlList":					
        if (!fn_preSearch(sType)) return false;
        
			var szSvcid       = "getOnlnMemberDtlList";
			var szController  = "/dlw/onlnMng/getOnlnMemberDtlList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_dtl=ds_output ds_pay_list=ds_output2";
			var szParam       = "sID=" + xID;
			var szRetCallback = "fn_callBack";
		break;		
		
		// 엑셀 조회
        case "getOnlnMemberListExcel":
			var szSvcid       = "getOnlnMemberListExcel";
			var szController  = "/dlw/onlnMng/getOnlnMemberList";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_excel=ds_output";
			var szParam       = "excel_yn=Y";
			var szRetCallback = "fn_callBack";
		break;
	}

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function fn_save(g_mode){
	
	if(g_mode == "save") {
		if(!confirm("저장하시겠습니까?")) return false;
	} else if (g_mode == "chk") {
		cbo_prcs_cd.value = "02";
	}
	ds_list_dtl.clearData();
	ds_list_dtl.addRow();
	ds_list_dtl.copyRow(0, ds_list, ds_list.rowposition);
	
	// 유효성체크
	//if(!fn_preSave(g_mode)) return false;
		
	var szSvcid       = "updateOnlnCons"+g_mode;
    var szController  = "/dlw/onlnMng/OnlnConsUpdate/"+g_mode;
    var szInDs        = "ds_input=ds_list_dtl";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

