var dltnYn; //삭제권한
var pritYn; //출력권한
var xlsYn; //엑셀권한

// 팝업창 호출 파라미터
var fv_accnt_no 		= "";

// 회원조회 팝업 관련 파라미터
var tc_custAcnt = 0; // 회원조회 - 입력창 엔터키 입력시 조회 건수
var fv_accnt_search_button = undefined; // 회원조회 버튼 이름

var fv_lgnId 		= "";


/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function frm_onload(obj:Form, e:LoadEventInfo)
{
	fv_lgnId = gfn_getUserInfo("lgn_id");
	gfn_initForm(obj);	
	fn_init();
	
	if ( !gfn_isEmpty(p_accnt_no) ) {
		fv_accnt_no = p_accnt_no;
	}
	
	if ( !gfn_isEmpty(fv_accnt_no) ) {
		// fn_searchMain(fv_accnt_no);
		fn_search(fv_accnt_no);
	} else {
		ds_list.addRow();
	}
}

// 초기화 함수
function fn_init() 
{
	ds_list.clearData();
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	var i, 
		nPos,
		arrCol,
		sItemNm,
		sItemVal,
		dummy;
	
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {		
			case "search":
				fn_postSearch();
				break;
				
			case "searchMain":
				fn_postSearchMain();
				break;
				
			case "save":
				fn_postSave();
				break;
		}
	}
}

//회원별 수당 관리 조회 전 처리
function fn_preSearch(accnt_no)
{
	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "accnt_no", accnt_no);
	return true;
}

//회원별 수당 관리 조회
function fn_search(accnt_no)
{	
	// 유효성 체크
	if ( !fn_preSearch(accnt_no) ) {
		return;
	}
	
	var szSvcid       = "search";    
    var szController  = "/dlw/alow/selectAlowMngForAccnt";
    
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_list=ds_output2 ds_list2=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 회원별 수당 관리 후 처리
function fn_postSearch() {
	//
}

// 메인정보조회 전 처리
function fn_preSearchMain(accnt_no)
{
	ds_cond.clearData();
	ds_cond.addRow();
	
	ds_cond.setColumn(0, "tab_gubun"	, "tpg_mem");
	ds_cond.setColumn(0, "excel_yn"		, "Y");
	ds_cond.setColumn(0, "accnt_no"		, accnt_no);
	ds_cond.setColumn(0, "start_dt"		, "200101");
	ds_cond.setColumn(0, "end_dt"		, "999912");
	return true;
}

//메인정보조회
function fn_searchMain(accnt_no)
{	
	// 유효성 체크
	if ( !fn_preSearchMain(accnt_no) ) {
		return;
	}
	
	var szSvcid       = "searchMain";
    var szController  = "/dlw/alow/selectAlowListForAccnt";
    
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_list=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 메인정보조회 후 처리
function fn_postSearchMain() {
	
	trace(ds_list.saveXML());
	
	if (ds_list.rowcount < 1) {
		alert("조회된 데이터가 없습니다.");
	}
}

function fn_close(obj:Button,  e:ClickEventInfo)
{
	close();
}

function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var nPos = ds_list.rowposition;
	var mRst = {dept_cd:ds_list.getColumn(nPos, "dept_cd"),
				dept_nm:ds_list.getColumn(nPos, "dept_nm"),
				high_dept_cd:ds_list.getColumn(nPos, "high_dept_cd"),
				lvl:ds_list.getColumn(nPos, "lvl"),
				use_yn:ds_list.getColumn(nPos, "use_yn")};	
	close(mRst);
}

function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_list2.rowcount > 0) {
		gfn_exportGridToExcel(grd_list, "회원별 수당 관리_"+gfn_getTodayFull()+"");
	} else {
		alert("엑셀다운로드 할 항목이 없습니다.");
	}
}


//조회 전 처리
function fn_preSave()
{
	var nPos = -1;
	var t21 		= medt_t21.value;
	var t32 		= medt_t32.value;
	var t33 		= medt_t33.value;
	var f7 			= medt_f7.value;
	var f8 			= medt_f8.value;
	var f10 		= medt_f10.value;
	var f11 		= medt_f11.value;
	var f12 		= medt_f12.value;
	var note 		= edt_note.value;
	
	var emple_no	= edt_emple_no.value;
	var alow_dt 	= div_monthCal.getDate();
	var accnt_no 	= edt_accnt_no.value;
	
	if (gfn_isEmpty(accnt_no)) {
		edt_accnt_no.setFocus();
		alert("회원번호를 입력하세요.");
		return false;
	}
	else if(gfn_isEmpty(alow_dt)) {
		div_monthCal.setFocus();
		alert("발생월을 입력하세요.");
		return false;
	}	
	else if(gfn_isEmpty(emple_no)) {
		edt_emple_no.setFocus();
		alert("사원번호를 입력하세요.");
		return false;
	}
	
	if( !div_monthCal.isValid() ) {
		div_monthCal.setFocus();
		alert("발생월이 잘못되었습니다.");
		return false;
	}
		
	if (gfn_isEmpty(t21) && gfn_isEmpty(t32) && gfn_isEmpty(f7) && gfn_isEmpty(f11) && gfn_isEmpty(f12)) 
	{
		if(gfn_isEmpty(note)){
			alert("수당을 입력해주세요.");
			return false;
		}else{
			t21 = 999999;
		}
	}
	
	if (0==t21 && 0==t32 && 0==f7 && 0==f11 && 0==f12) 
	{
		alert("수당을 입력해주세요.");
		return false;
	}
	
	ds_list3.clearData();
	nPos = ds_list3.addRow();
	ds_list3.setColumn(nPos, "alow_cd"	, "T21"	);
	ds_list3.setColumn(nPos, "amt"		, t21	);
	
	nPos = ds_list3.addRow();
	ds_list3.setColumn(nPos, "alow_cd"	, "T32"	);
	ds_list3.setColumn(nPos, "amt"		, t32	);	
	
// 	nPos = ds_list3.addRow();
// 	ds_list3.setColumn(nPos, "alow_cd"	, "T33"	);
// 	ds_list3.setColumn(nPos, "amt"		, t33	);	
	
	nPos = ds_list3.addRow();
	ds_list3.setColumn(nPos, "alow_cd"	, "F7"	);
	ds_list3.setColumn(nPos, "amt"		, f7	);	
	
// 	nPos = ds_list3.addRow();
// 	ds_list3.setColumn(nPos, "alow_cd"	, "F8"	);
// 	ds_list3.setColumn(nPos, "amt"		, f8	);	
// 	
// 	nPos = ds_list3.addRow();
// 	ds_list3.setColumn(nPos, "alow_cd"	, "F10"	);
// 	ds_list3.setColumn(nPos, "amt"		, f10	);	
	
	nPos = ds_list3.addRow();
	ds_list3.setColumn(nPos, "alow_cd"	, "F11"	);
	ds_list3.setColumn(nPos, "amt"		, f11	);	
	
	nPos = ds_list3.addRow();
	ds_list3.setColumn(nPos, "alow_cd"	, "F12"	);
	ds_list3.setColumn(nPos, "amt"		, f12	);
	
	for (var i=0; i<ds_list3.rowcount; ++i) {
		ds_list3.setColumn(i, "accnt_no"	, accnt_no	);
		ds_list3.setColumn(i, "alow_dt"		, alow_dt	);
		ds_list3.setColumn(i, "emple_no"	, emple_no	);
		ds_list3.setColumn(i, "note"		, note		);
	}
	
	fn_magamSrch(); // 수당마감여부 조회
	var cnt = ds_magam.getColumn(0, "cnt");
	if(parseInt(cnt) > 0){
		alert("해당월은 수당마감 처리되었습니다.");
		return false;
	}	
	
	return true;
}

// 저장
function fn_save(obj, e)
{
	// 유효성 체크
	if ( !fn_preSave() ) {
		return;
	}

	var szSvcid       = "save";
    var szController  = "/dlw/alow/insertAlowDaDtl";
    var szInDs        = "ds_input=ds_list3:U";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 저장 후 처리
function fn_postSave() {	

	div_monthCal.setDate("");
	medt_t21.value = "";
	medt_t32.value = "";
	medt_f7.value = "";
	medt_f11.value = "";
	medt_f12.value = "";
	medt_t33.value = "";
	medt_f8.value = "";
	medt_f10.value = "";
	edt_note.value = "";

	fn_search(edt_accnt_no.value);
	alert("저장되었습니다.");
}

// 수당마감여부 조회 전 처리
function fn_premsrch()
{
	var alow_dt = div_monthCal.getDate();
	
	ds_inqcnt.clearData();
	ds_inqcnt.addRow();
	ds_inqcnt.setColumn(0, "alow_dt", alow_dt);
	ds_inqcnt.setColumn(0, "sbit", '002'); // 수당마감조회(001:수수료 / 002:수당)
	
	return true;
}

// 해당월 수당마감여부 조회
function fn_magamSrch(){
	// 유효성 체크
	if ( !fn_premsrch() ) {
		return;
	}
	
	var szSvcid       = "searchMagam";
    var szController  = "/dlw/alow/selectMagamCnt";
    var szInDs        = "ds_input=ds_inqcnt";
