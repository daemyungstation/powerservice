}

// accnt_no로 회원검색
function btn_accnt_no_onclick(obj:Button,  e:ClickEventInfo)
{
	fv_accnt_search_button = obj.name;
	fn_openAccntPopup("accnt_no", edt_accnt_no.value);
}

// 회원 조회 버튼 클릭
function fn_openAccntPopup (sSrchTyep, sSrchVal) {
    
    var option = "";
    var gubun_mode = "";

    var sPrmr = {p_srch_typ:sSrchTyep, p_srch_vl:sSrchVal, p_gubun_mode:gubun_mode, p_option:option};
    gfn_diaLog("회원 조회", -1, -1, 884, 601, "PS990000::PS992400.xfdl", false, sPrmr, true, false, true, null, null, true);
}


// 회원 조회 콜백 함수
function fn_setAccntNo(){
	
	if (ds_accnt_info.rowcount < 1) {
		if ("btn_accnt_no" == fv_accnt_search_button) {
			edt_accnt_no.value = "";
		}
	} 
	else {
		if ("btn_accnt_no" == fv_accnt_search_button) {
			var accnt_no = ds_accnt_info.getColumn(0, "accnt_no");
			edt_accnt_no.value = accnt_no;
			
			// fn_searchMain(accnt_no);
			fn_search(accnt_no);
		}
	}
}

// 수수료 & 해약환수 & 연체환수 등록 및 수정 팝업 오픈
function btn_fee_hwpop(obj:Button,  e:ClickEventInfo)
{
	var emple_no = edt_emple_no.value;
	var alow_dt  = div_monthCal.getDate();
	var accnt_no = edt_accnt_no.value;
	
	if (gfn_isEmpty(accnt_no)) {
		edt_accnt_no.setFocus();
		alert("회원번호를 입력하세요.");
		return false;
	}
	else if(gfn_isEmpty(alow_dt)) {
		div_monthCal.setFocus();
		alert("발생월을 입력하세요.");
		return false;
	}	
	else if(gfn_isEmpty(emple_no)) {
		edt_emple_no.setFocus();
		alert("사원번호를 입력하세요.");
		return false;
	}
		
	var alow_cd = "";
	if(obj.name == "btn_fee_hwpop1"){ // 수수료
		alow_cd = "T33";
	}else if(obj.name == "btn_fee_hwpop2"){ // 해약환수
		alow_cd = "F8";
	}else if(obj.name == "btn_fee_hwpop3"){ // 연체환수
		alow_cd = "F10";
	}
	
    var sPrmr = {p_emple_no:emple_no, p_alow_dt:alow_dt, p_accnt_no:accnt_no, p_alow_cd:alow_cd};
    gfn_diaLog("수수료/해약환수/연체환수 관리", -1, -1, 884, 601, "DL470000::DL470900P03.xfdl", false, sPrmr, true, false, true, null, null, true);	
}
