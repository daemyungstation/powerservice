var tc_cust_memo = 0; // 전체건수

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function PS240600_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정
	fn_init();
	fn_commSearch(gfn_getCommTypeInit());
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "getExamResultList": 
				//trace(ds_list_tmp.saveXML());
				gfn_appendSearchDs(ds_list_examResult, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list_examResult, sta_totalCnt, tc_cust_memo); // 페이지건수 표출
				//alert(ds_list_examResult.rowcount);
				if(ds_list_examResult.rowcount > 0){
					fn_commSearch2();
					//fn_commSearch3();
				}else{
					ds_list_examTrpr.clearData(); 
					ds_list_examPrbmFieldResult.clearData(); 
				}
				
 			break;
 			
 			case "getExamTrprList":
				gfn_appendSearchDs(ds_list_examTrpr, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				fn_commSearch3();
				//alert(ds_list_examTrpr.rowcount);
				if(ds_list_examTrpr.rowcount > 0){
					fn_commSearch4();
				}else{
					ds_list_examPrbmFieldResult2.clearData(); 
				}
				
 			break;
 			
 			case "getExamPrbmFieldResultList"://직무평가별
				gfn_appendSearchDs(ds_list_examPrbmFieldResult, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				//trace(ds_list_examPrbmFieldResult.saveXML());
 			break;
 			
 			case "getExamPrbmFieldResultList2"://상담원별
				//trace(ds_list_tmp.saveXML());
				gfn_appendSearchDs(ds_list_examPrbmFieldResult2, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
 			break;
		}	
 	}		
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 체크
function fn_preSearch(sType)
{
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_list_tmp.clearData();
		ds_list_examResult.clearData();
		ds_inqCndt.clearData();
		ds_list_examResult.keystring = ""; // 정렬컬럼 초기화
		
		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "exam_nm", edt_exam_nm.value);
		ds_inqCndt.setColumn(0, "dateType", Combo_dateType.value);
		ds_inqCndt.setColumn(0, "startd", cal_srch_stt_dt.value);
		ds_inqCndt.setColumn(0, "endd", cal_srch_end_dt.value);
		ds_inqCndt.setColumn(0, "orderBy", "AMNT_DTTM");
		ds_inqCndt.setColumn(0, "orderDirection", "DESC");
	}
	
	return true;
}	
		
/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//상단조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list_examResult, sta_totalCnt, 25, tc_cust_memo)) return;
	
	var szSvcid       = "getExamResultList";
    var szController  = "/exam/exam-basi/result-list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}	
	
//하단그리드조회
function fn_commSearch2()
{	
	ds_list_tmp.clearData();
