    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var nTotalListCnt = 0;

var xSaveType; //입력방법(I:INSERT, U:UPDATE)
var bPastYn = "";	//과거조회

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL562001_onload(obj:Form, e:LoadEventInfo)
{
	//////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2018.01.11 정승철
	gvMenuId = GV_LOGMENUID;
	//////////////////////////////////////////////////////////////////////////////////////////
	
	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정;
	
	//fn_setAthrPgmBtn();
	
	fn_init();		
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
			case "selectNonPayTitleList":				
			break;
			case "selectNonPayList":
			break;
	    }
		return false;
	} else {
		switch (svcid) {										
			case "selectNonPayTitleList":
				if(ds_listNonPayTit.rowcount < 1) {
					alert("대상목록이 없습니다.");
					fn_setCRUDComponentType("INIT");
					return;					
				}					
				fn_commSearch(gfn_getCommTypeInit(), "selectNonPayList");
				fn_setCRUDComponentType("READ");	
			break;
			
			case "insertNonPayMentMst":
				alert("저장이 완료 되었습니다.");
				fn_setCRUDComponentType("READ");	
				fn_commSearch(gfn_getCommTypeInit(), "selectNonPayTitleList");
			break;			
			
			case "selectNonPayList": 			
				gfn_appendSearchDs(ds_listNonPay, ds_listNonPayTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_listNonPay, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출
				if(ds_listNonPay.rowcount < 1) {
					ds_listNonPay.clearData();
				}
							
				var sToday = gfn_getToday();
				var toDay = gfn_SubStr(sToday,0,6);
				
				if(div_monthCal.getDate() < toDay){
					bPastYn = "Y";	
					fn_setCRUDComponentType("PAST");
				}	
			break;			
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(searchInfo)
{
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();
		
	if ( gfn_isEmpty(div_monthCal.getDate()) ) {
		alert("미납생성월은 필수 입력입니다.");
		return false;
	}
	
	switch(searchInfo){
		case "selectNonPayTitleList" :
			ds_inqCndt.setColumn(0, "ext_dt"		, div_monthCal.getDate());
			ds_inqCndt.setColumn(0, "np_gbn"		, cbo_nonPayGubun.value);
			ds_inqCndt.setColumn(0, "del_fg"		, cbo_nonPayUseYn.value);		
		break;
		
		case "selectNonPayList" :
			trace(edt_seq.value);
			if(gfn_IsNull(edt_seq.value)){
				return false;
			}
		
			ds_inqCndt.setColumn(0, "ext_dt"		, div_monthCal.getDate());
			ds_inqCndt.setColumn(0, "np_gbn"		, cbo_NpTiltleGbn.value);
			ds_inqCndt.setColumn(0, "mst_seq"		, edt_seq.value);				
		break;
	}
	

		
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회
function fn_commSearch(sType, searchInfo) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    if ( !fn_preSearch(searchInfo) ) {
		return;
	}
    
    switch(searchInfo) {
        //미납생성월 기준 미납TITLE 조회
        case "selectNonPayTitleList": 
	    szSvcid       = "selectNonPayTitleList";
		szController  = "/dlw/nonPay/titList";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_listNonPayTit=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
        
        case "selectNonPayList": 
        
        if(!gfn_pageSearch(sType, ds_listNonPay, sta_TotalCnt, 50, nTotalListCnt)) return;		
        
	    szSvcid       = "selectNonPayList";
		szController  = "/dlw/nonPay/NpDtlList";
		szInDs        = "ds_input=ds_inqCndt";
		szOutDs       = "ds_listNonPayTemp=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;
    }
    
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{
	ds_inqCndt.clearData();
	ds_listNonPayTit.clearData();
	ds_listNonPay.clearData();
	ds_listNonPayTemp.clearData();
	
	//  검색기간 설정
	var sToday = gfn_getToday();
	div_monthCal.setDate(sToday);
	
	fn_setCRUDComponentType("INIT");	
	div_monthCal.setDateCallback("fn_divMonthCalValueChanged");
}

/* 저장전 유효성 체크 */
function fn_preSave(sType, svcid) 
{
	switch (svcid) 
	{	
		case "insertNonPayMentMst" :
			if(gfn_IsNull(div_monthCal.getDate()) == true)
			{
				alert("미납생성월을 정확하게 입력하여 주십시오.");
				return false;
			}						
			var sSeq = edt_seq.value;
						
			ds_listNonPayTit_Send.clearData();
			var nRow = ds_listNonPayTit_Send.addRow();
			ds_listNonPayTit_Send.setColumn(nRow, "mst_seq", sSeq);
			ds_listNonPayTit_Send.setColumn(nRow, "ext_dt", div_monthCal.getDate());
			ds_listNonPayTit_Send.setColumn(nRow, "np_gbn", cbo_NpTiltleGbn.value);
			ds_listNonPayTit_Send.setColumn(nRow, "np_title", edt_NpTitle.value);
			ds_listNonPayTit_Send.setColumn(nRow, "np_etc", edt_NpTitleMemo.value);
			ds_listNonPayTit_Send.setColumn(nRow, "del_fg", cbo_NpUseYn.value);
			ds_listNonPayTit_Send.setColumn(nRow, "sched_yn", cbo_NpScheYn.value);
			ds_listNonPayTit_Send.setColumn(nRow, "sched_dt", cal_NpScheDt.value);
			ds_listNonPayTit_Send.setColumn(nRow, "sched_hh", cbo_NpScheHH.value);
			ds_listNonPayTit_Send.setColumn(nRow, "sched_mm", cbo_NpScheMM.value);
			
		break;
	}
	return true;
}

