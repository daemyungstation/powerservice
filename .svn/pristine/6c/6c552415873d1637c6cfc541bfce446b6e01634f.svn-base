function fn_setCRUDComponentType(sType){
	var gbnVal = cbo_NpTiltleGbn.value;	
	var schedVal = cbo_NpScheYn.value;	
	
	trace(sType);
	
	if(sType == "UPDATE"){
		xSaveType = "U";
		edt_NpTitle.enable = true;
		edt_NpTitleMemo.enable = true;
		
		btn_insert.visible = false;
		btn_update.visible = false;
		btn_save.visible = true;
		btn_cancel.visible = true;
		btn_save.position = "absolute 841 63 913 83";
		btn_cancel.position = "absolute 915 63 987 83";
		cbo_NpUseYn.enable = true;
					
		if(gbnVal == "001"){
			cbo_NpScheYn.enable = true;				
			if (schedVal == "Y"){
				cal_NpScheDt.enable = true;
				cbo_NpScheHH.enable = true;
				cbo_NpScheMM.enable = true;
			} else 
				cal_NpScheDt.enable = false;
				cbo_NpScheHH.enable = false;
				cbo_NpScheMM.enable = false;
		}else {
			cbo_NpScheYn.enable = false;	
			cal_NpScheDt.enable = false;
			cbo_NpScheHH.enable = false;
			cbo_NpScheMM.enable = false;			
		}			
	} else if(sType == "READ"){
		btn_insert.visible = true;
		btn_update.visible = true;
		btn_nonPayCalcuration.enable = true;
		cbo_NpTiltleGbn.enable = false;
		edt_NpTitle.enable = false;
		edt_NpTitleMemo.enable = false;
		btn_save.visible = false;
		btn_cancel.visible = false;
		cbo_NpUseYn.enable = false;
		cbo_NpScheYn.enable = false;	
		cal_NpScheDt.enable = false;
		cbo_NpScheHH.enable = false;
		cbo_NpScheMM.enable = false;		
	}  else if(sType == "INSERT") {
		
		xSaveType = "I";	    
		
	    ds_listNonPayTit.addRow();
	    ds_listNonPay.clearData();
		cbo_NpTiltleGbn.enable = true;
		edt_NpTitle.enable = true;
		edt_NpTitleMemo.enable = true;
		btn_insert.visible = false;
		btn_update.visible = false;
		btn_save.visible = true;
		btn_cancel.visible = true;
		cbo_NpUseYn.enable = true;
		cbo_NpUseYn.value = "N";
		btn_save.position = "absolute 841 63 913 83";
		btn_cancel.position = "absolute 915 63 987 83";		
	} else if(sType == "SCHE_Y"){
		cbo_NpScheYn.enable = true;		
		cbo_NpScheYn.value = "Y";
		cal_NpScheDt.enable = true;
		cbo_NpScheHH.enable = true;
		cbo_NpScheMM.enable = true;
	} else if(sType == "SCHE_N"){
		cbo_NpScheYn.enable = false;	
		cal_NpScheDt.enable = false;
		cbo_NpScheHH.enable = false;
		cbo_NpScheMM.enable = false;
		cbo_NpScheYn.value = "N";	
		cal_NpScheDt.value = "";
		cbo_NpScheHH.value = "";
		cbo_NpScheMM.value = "";				
	} else if(sType == "SCHE_DT"){		
		cal_NpScheDt.enable = false;
		cbo_NpScheHH.enable = false;
		cbo_NpScheMM.enable = false;
		cal_NpScheDt.value = "";
		cbo_NpScheHH.value = "";
		cbo_NpScheMM.value = "";			
	} else if(sType == "INIT"){
		btn_insert.visible = true;
		btn_update.visible = false;		
		btn_save.visible = false;
		btn_cancel.visible = false;
		cbo_nonPayGubun.index = 0;	
		bPastYn = "N";
		
		ds_listNonPay.clearData();
		
	} else if(sType == "PAST"){
		btn_insert.visible = false;
		btn_update.visible = false;		
		btn_save.visible = false;
		btn_cancel.visible = false;
		btn_nonPayCalcuration.enable = false;
		cbo_nonPayGubun.index = 0;	
		bPastYn = "N";
	}
} 

function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_setCRUDComponentType("READ");
}

