function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
		// 유효성 체크
	if(!fn_preSave(gfn_getCommTypeInit(), "insertNonPayMentMst")) {
		return false;
	}
	
	if(!confirm("저장하시겠습니까?")){
		return false;
	}
	
	var szSvcid       = "insertNonPayMentMst";
    var szController  = "/dlw/nonPay/insertNonPayMentMst";
    var szInDs        = "ds_input=ds_listNonPayTit_Send";
    var szOutDs       = "";
    var szParam       = "sType=" + xSaveType;
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

function cbo_NpTiltleGbn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	trace(obj.value);
	if(obj.value == "001"){		
		fn_setCRUDComponentType("SCHE_Y");
	} else {
		fn_setCRUDComponentType("SCHE_N");
	}
	
}

function cbo_NpScheYn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(obj.value == "Y"){		
		fn_setCRUDComponentType("SCHE_Y");
	} else {
		fn_setCRUDComponentType("SCHE_DT");
	}
}

function cbo_NpUseYn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	var objVal = obj.value;
	var gbnVal = cbo_NpTiltleGbn.value;
	
	if(objVal == "N"){
		if(gbnVal == "001"){
			fn_setCRUDComponentType("SCHE_Y");
		}else {
			fn_setCRUDComponentType("SCHE_N");
		}
	} else {
		fn_setCRUDComponentType("SCHE_N");
	}
}

