//청구중회원 체크  
function fn_req_Check(){
	var boolChek  = false;
	var sDelFg = ds_accnt_info.getColumn(0,"req_del");	
	if(sDelFg == "N")	{
		alert("현재 금액 산출이 되어있는  회원입니다. 산출 삭제후 등록 가능합니다.");
		boolChek = false;
	}
	else if(sDelFg == "R")	{
		if (confirm("현재 금액 청구가 진행중인 회원입니다. 그래도 저장하시겠습니까?")) {
			boolChek =true;
		}
	} else {
		boolChek = true;
	}	
	return boolChek;
}

