//**************************************************************************
// [ PART 1 ] Form에서 Include 할 라이브러리(xjs)파일 정의
//**************************************************************************
include "LIB::lib_All.xjs";
include "LIB::lib_BOP.xjs";

//**************************************************************************
// [ PART 2 ] Form에서 사용될 전역변수를 선언
//**************************************************************************
var fv_bizHdqrCd;    //사업본부코드 

var maxSelCnt = 0; //조직 최대 선택 갯수
//**************************************************************************
// [ PART 3 ] Form Event 정의
//**************************************************************************
/***************************************************************************
 * 기      능		: 폼 onload이벤트
 ***************************************************************************/
function FrmDev_onload(obj:Form, e:LoadEventInfo)
{	
	// 시스템 공통함수 호출
	gfn_commPopInit(obj);
	
	//공통코드 조회
	fn_getCommCode();		
	
	// 폼 공통함수 호출
	fn_init();
	
	// 그리드 공통버튼 초기화설정
	gfn_initCommGridBtn(obj, div_gridCommBtn, grd_list);
	
	// 그리드 공통버튼 상태변경
	gfn_setCommGridBtn_Status(div_gridCommBtn, false, false, false, false, false, true, false, true, true);			
}

/***************************************************************************
 * 함  수  명		: fn_init()
 * 기      능		: 폼 초기화 공통함수
   prmrSelType      : S:단건, M:다건 
 ***************************************************************************/
function fn_init()
{
	//1. param Data check! 
	if(gfn_isNull(pvSelType)) pvSelType = "S";
	
	//2.화면구성 
	fn_initScreen();
	
	//3.조회조건 구성
	fn_initCndt();
	
	//4.after 
	fn_initAfter();		
	
	try {
		maxSelCnt = pvMaxSelCnt;
	}
	catch(e) {
		maxSelCnt = 5;
	}
		
	/*
	if(!gfn_isNull(pMaxSelCnt)) {
		try {
			maxSelCnt = pMaxSelCnt;
		}
		catch(e) {
			trace(e);
		}
	}
	else {
		maxSelCnt = 5;
	}
	*/
}

/***************************************************************************
 * 함  수  명		: fn_initScreen()
 * 기      능		: 화면 구성 
 ***************************************************************************/
