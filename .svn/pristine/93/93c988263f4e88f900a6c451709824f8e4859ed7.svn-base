 ***************************************************************************/
function cbo_inqDiv_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(obj.value == "orgzCd") {
		edt_orgzCd.enable     = true;
		btn_searchOrgz.enable = true; 
		//cbo_lrrnBlng.enable   = true; 
		//comp_orgzLvl.enable   = true; 
		edt_inqVl.enable      = false; 
		edt_inqVl.value = "";

		if(!gfn_isNull(e) && e.postvalue == "orgzCd") {
			cbo_lrrnBlng.value = "N";
		}
	}
	else {
		edt_orgzCd.enable     = false;
		btn_searchOrgz.enable = false; 
		//cbo_lrrnBlng.enable   = false; 
		//comp_orgzLvl.enable   = false; 
		edt_inqVl.enable      = true; 
		edt_orgzCd.value   = "";
		edt_orgzNm.value   = "";
		cbo_lrrnBlng.value = "";
		comp_orgzLvl.setInit();		
	}
}

