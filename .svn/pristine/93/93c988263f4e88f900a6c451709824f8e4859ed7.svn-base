/***************************************************************************
 * 함  수  명		: fn_commSearch()
 * 기      능		: (공통버튼)조회 프로세스

 ***************************************************************************/
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 페이지 처리 기본설정
	if(!gfn_pageSearch(gfn_getTranTypeMCI(), sType, ds_orgzInfoList, sta_TotalCnt)) return;	
	
	if(sType == gfn_getCommTypeInit()) {		
		ds_orgzInfoList.clearData();	
	}
	ds_orgzInfoListTmpr.clearData();	

	ds_orgzInfoInqCndt.setColumn(0,"scrnDiv"  ,pvScrnDiv);
	ds_orgzInfoInqCndt.setColumn(0,"bizHdqrCd",fv_bizHdqrCd);
	ds_orgzInfoInqCndt.setColumn(0,"inqDiv"   ,cbo_inqDiv.value);

	if(cbo_inqDiv.value =="orgzCd") {
		var strOrgzLvlCd = comp_orgzLvl.getValue();
		var strOrgzLvlNm = comp_orgzLvl.getValue();
		
		if(!gfn_isNull(strOrgzLvlCd)) {
			var arrOrgzLvl = strOrgzLvlCd.split(",");			
			strOrgzLvlCd = "";
			for(var i=0; i<arrOrgzLvl.length; i++){
				if(i==0) strOrgzLvlCd = "'"+arrOrgzLvl[i]+"'";
				else strOrgzLvlCd += ","+ "'"+arrOrgzLvl[i]+"'";
			}
		}
 
		ds_orgzInfoInqCndt.setColumn(0,"inqVl"    ,"");
		ds_orgzInfoInqCndt.setColumn(0,"lrrnBlng" ,cbo_lrrnBlng.value);
		ds_orgzInfoInqCndt.setColumn(0,"orgzLvlCd",strOrgzLvlCd);		
		ds_orgzInfoInqCndt.setColumn(0,"orgzLvlNm",strOrgzLvlNm);	
        ds_orgzInfoInqCndt.setColumn(0,"orgzCd"   ,edt_orgzCd.value);
        ds_orgzInfoInqCndt.setColumn(0,"orgzNm"   ,edt_orgzNm.value);
	}else {
	
		ds_orgzInfoInqCndt.setColumn(0,"inqVl"    ,edt_inqVl.value);
		ds_orgzInfoInqCndt.setColumn(0,"lrrnBlng" ,"");
		ds_orgzInfoInqCndt.setColumn(0,"orgzLvlCd","");		
		ds_orgzInfoInqCndt.setColumn(0,"orgzLvlNm","");	
        ds_orgzInfoInqCndt.setColumn(0,"orgzCd"   ,"");
        ds_orgzInfoInqCndt.setColumn(0,"orgzNm"   ,"");	
	}	
	
	// KWData 설정
	gfn_kwDataInit();
	gfn_setKwData_trscCode("BOP00001R01");				// MCI
	gfn_setKwData_trscType(gfn_getTranTypeSearch());
	
	gfn_tranMCI(sType 
				, "ds_orgzInfoInqCndt=ds_orgzInfoInqCndt"
				, "ds_orgzInfoListTmpr=ds_orgzInfoList"
				, "fn_commSearch_callBack");	
}

/***************************************************************************
 * 함  수  명		: fn_commSearch_callBack()
 * 기      능		: [CallBack함수] Transaction Callback함수
 ***************************************************************************/
function fn_commSearch_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		return;
	}
	
	// 조회된 결과를 실제보여줄 데이타셋으로 Append처리
	gfn_appendSearchDs(ds_orgzInfoList, ds_orgzInfoListTmpr);
	
	//조직정렬순서
//	ds_orgzInfoList.keystring = 'S+';
	
//	alert(ds_orgzInfoList.rowcount);
	
	// 페이지건수 조회 처리
//	if(strSvcID != gfn_getCommTypeAppend()) {
//		alert('응? '+ds_orgzInfoList.rowcount);
		gfn_setTotalRowCount(ds_orgzInfoList, sta_TotalCnt);
//	}
	
	// 조회 후 처리
	fn_postSearch(strSvcID, nErrorCode);	
}

/***************************************************************************
 * 함  수  명		: fn_postSearch()
 * 기      능		: 조회 후 처리
 ***************************************************************************/
function fn_postSearch(sType, nErrorCode)
{
	if(nErrorCode != 0) {
		return;
	}
	
	if(ds_orgzInfoList.rowcount < 1){
		alert("조회 결과가 없습니다");
	} 
	
	if(pvSelType == "S") {
		if(!gfn_isNull(pvOrgzCd)) {
			var arrCd = pvOrgzCd.split(",");
			ds_orgzInfoList.rowposition = ds_orgzInfoList.findRow("orgzCd", arrCd[0]);			
		}
	}
}

/***************************************************************************
 * 함  수  명		: fn_Conf() 
 * 기      능		: 결과 데이터셋 리턴
***************************************************************************/
function fn_Conf(){
	var obj = new Object;
	var code ='';
	var name ='';
	var codein ='';
	
	if(pvSelType == "S") {
		code = ds_orgzInfoList.getColumn(ds_orgzInfoList.rowposition, "orgzCd");
		name = ds_orgzInfoList.getColumn(ds_orgzInfoList.rowposition, "orgzNm");	
		codein = "'"+ds_orgzInfoList.getColumn(ds_orgzInfoList.rowposition, "orgzCd")+"'";
		
		ds_rtnOrgzInfo.clearData();
		ds_rtnOrgzInfo.addRow();
		ds_rtnOrgzInfo.copyRow(0, ds_orgzInfoList, ds_orgzInfoList.rowposition);
	}else{
		code = gfn_gridColumeToComma(ds_rtnOrgzInfo,'orgzCd');
		name = gfn_gridColumeToComma(ds_rtnOrgzInfo,'orgzNm');	
		codein = gfn_gridColumeToCommaQuotation(ds_rtnOrgzInfo,'orgzCd');
	}

	obj["code"] = code;
	obj["name"] = name;	
	obj["codein"] = codein;	
	if(pvSelType == "S") {
		if(ds_orgzInfoList.rowposition >= 0) {
			obj["inqDs"] = ds_orgzInfoInqCndt.saveXML();
		}
	}else {
		if(ds_rtnOrgzInfo.rowcount >= 1) {
			obj["inqDs"] = ds_orgzInfoInqCndt.saveXML();			
		}	
	}
	
	var ds_rtnDs = new Dataset();
	ds_rtnDs.assign(ds_rtnOrgzInfo);	
	
	obj["rtnDs"] = ds_rtnDs;
	
	//trace("rtnDs=====>"+ds_rtnDs.saveXML());
	close(obj);
}

