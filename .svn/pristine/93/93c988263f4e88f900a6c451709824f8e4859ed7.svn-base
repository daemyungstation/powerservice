function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(div_body.visible == true) {
		//선택조직 그리드에 data 세팅
		fn_selItem();
	}else {	
		//결과 데이터셋 리턴
		fn_Conf();
	}
}

/***************************************************************************
 * 함  수  명		: div_body_grd_selInfo_oncelldblclick()
 * 기      능		: 선택조직 그리드 더블클릭 시 
 ***************************************************************************/
function div_body_grd_selInfo_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	ds_rtnOrgzInfo.deleteRow(ds_rtnOrgzInfo.rowposition);
}

/***************************************************************************
 * 함  수  명		: fn_selItem()
 * 기      능		: 선택조직 그리드에 data 세팅
 ***************************************************************************/
function fn_selItem(){
    
	var tmprRtnDs = ds_rtnOrgzInfo;
	var tmprListDs = ds_orgzInfoList;
	
	if(tmprListDs.rowcount == 0) {
		return;
	}
	
	if( tmprRtnDs.findRow("orgzCd", tmprListDs.getColumn(tmprListDs.rowposition, "orgzCd")) >= 0 ) {
		var tmprSelOrgz = "("+tmprListDs.getColumn(tmprListDs.rowposition, "orgzNm");
		tmprSelOrgz += "["+tmprListDs.getColumn(tmprListDs.rowposition, "orgzCd")+"])";
		alert("이미 선택되었습니다."+tmprSelOrgz);		
		return false;
	}	
	
    var iRowCnt = tmprRtnDs.rowcount;     
	
	if(iRowCnt >= maxSelCnt) {
		alert(maxSelCnt+"개 이상은 선택할 수 없습니다.","warning");
		return;
	}
	
	if(tmprListDs.rowcount > 0) {
		tmprRtnDs.addRow();
		tmprRtnDs.copyRow(tmprRtnDs.rowposition, tmprListDs, tmprListDs.rowposition);	
	}
}

/***************************************************************************
 * 함  수  명		: btn_searchOrgz_onclick()
 * 기      능		: 조직찾기 버튼 클릭시 
 ***************************************************************************/
function btn_searchOrgz_onclick(obj:Button,  e:ClickEventInfo)
{
	var strParam = {  pvScrnDiv:pvScrnDiv
					, pvOrgzCd:edt_orgzCd.value
				   };

	var reVal = gfn_diaLog("BOP0000110200", -1, -1, 385, 455, "CMM::BOP0000110200.xfdl", false, strParam);

	if(reVal instanceof Dataset) {		
		var iRowCnt = reVal.rowcount;

		edt_orgzCd.value = reVal.getColumn(0,"orgzcd");
		edt_orgzNm.value = reVal.getColumn(0,"orgznm");
	}
}

/***************************************************************************
 * 함  수  명		: cbo_lrrnBlng_onitemchanged()
 * 기      능		: [하위소속] 변경시
 ***************************************************************************/
function cbo_lrrnBlng_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{	
	if(obj.value == "Y") {
		comp_orgzLvl.enable = true;
	}else {
		comp_orgzLvl.enable = false;
		comp_orgzLvl.setValue("");
	}
}

/***************************************************************************
 * 함  수  명		: edt_orgzCd_ontextchanged()
 * 기      능		: [조직코드] 변경시
 ***************************************************************************/
function edt_orgzCd_ontextchanged(obj:Edit,  e:TextChangedEventInfo)
{
	edt_orgzNm.value = "";
}

/***************************************************************************
 * 함  수  명		: fn_searchAreaClear()
 * 기      능		: 조회영역 clear
 ***************************************************************************/
function fn_searchAreaClear()
{
	//component init
	cbo_inqDiv.value 	= "orgzNm";
	edt_inqVl.value 	= "";
	edt_orgzCd.value 	= "";
	edt_orgzNm.value 	= "";
	cbo_lrrnBlng.value 	= "";
	comp_orgzLvl.setInit();
	
	//dataset clear
	ds_orgzInfoInqCndt.clearData();
	ds_orgzInfoList.clearData();
	ds_orgzInfoListTmpr.clearData();
	ds_rtnOrgzInfo.clearData();
	
	//조회조건 dataset addrow
	ds_orgzInfoInqCndt.addRow();
	
	//조회건수 static init
	gfn_setTotalRowCount(ds_orgzInfoList, sta_TotalCnt, 0);
}]]></Script>
