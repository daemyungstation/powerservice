/***************************************************************************
 * 함  수  명		: fn_initCndt()
 * 기      능		: 조회조건 구성 
 ***************************************************************************/
 function fn_initCndt()
{
	//조회영역 clear
	fn_searchAreaClear();
	
	//조회조건 세팅 
	if(!gfn_isNull(pvInqDs)) {
		ds_orgzInfoInqCndt.loadXML(pvInqDs, true);	
			
		var strInqDiv = ds_orgzInfoInqCndt.getColumn(0, "inqDiv");
		if(strInqDiv == "") strInqDiv = "orgzNm";
		cbo_inqDiv.value = strInqDiv;
		if(strInqDiv == "orgzCd") {
			//조직 코드
			edt_orgzCd.value = ds_orgzInfoInqCndt.getColumn(0, "orgzCd");
			//조직 명
			edt_orgzNm.value = ds_orgzInfoInqCndt.getColumn(0, "orgzNm");
			//하위소속
			if(ds_orgzInfoInqCndt.getColumn(0, "lrrnBlng") == "N") 
				cbo_lrrnBlng.value = "N";
			else cbo_lrrnBlng.value = "Y";

			if(!gfn_isNull(pvInqDs)) {	
				if(gfn_getLength(ds_orgzInfoInqCndt.getColumn(0, "orgzLvlNm")) > 0) {	
					comp_orgzLvl.setValue(ds_orgzInfoInqCndt.getColumn(0, "orgzLvlNm"));
				}
			}			

		}
		else {
			edt_inqVl.value = ds_orgzInfoInqCndt.getColumn(0, "inqVl");
		}		
	}
	else{
		cbo_inqDiv.value="orgzNm";
	}	
}

/***************************************************************************
 * 함  수  명		: fn_initAfter()
 * 기      능		: 초기화 후처리  
 ***************************************************************************/
 function fn_initAfter()
{
	//부모창의 선택조직 구성 
	if(!gfn_isNull(pvOrgzCd)&&!gfn_isNull(pvOrgzNm)) {

		var arrCd = pvOrgzCd.split(",");	
		var arrNm = pvOrgzNm.split(",");	
		
		if(pvSelType == "S") {
			ds_orgzInfoList.rowposition = ds_orgzInfoList.findRow("orgzCd="+arrCd[0]);
		}
		else {
			for(var nRow=0; nRow<arrCd.length; nRow++){
				
				var tempDs  = gfn_getBopOrgzNm(pvScrnDiv, arrCd[nRow]);
				var tempRow = ds_rtnOrgzInfo.addRow();
				
				ds_rtnOrgzInfo.copyRow(tempRow,tempDs,0 );	
							
// 				ds_rtnOrgzInfo.addRow();
// 				ds_rtnOrgzInfo.setColumn(nRow,'orgzCd',arrCd[nRow]);
// 				ds_rtnOrgzInfo.setColumn(nRow,'orgzNm',arrNm[nRow]);
			}		
		}
	}		
	
	//조회조건 enable/disable
	cbo_inqDiv_onitemchanged(cbo_inqDiv);	
	
	if(gfn_getLength(edt_inqVl.value) > 0 || gfn_getLength(edt_orgzCd.value) > 0) {	
		//조회 시작 
		fn_commSearch(gfn_getCommTypeInit());	
	}
}

