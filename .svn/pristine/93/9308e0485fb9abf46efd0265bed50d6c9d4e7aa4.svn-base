    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var tc_prod = 0;	// 전체건수

var xlsYn; 		// 엑셀권한
var athrPpupYn; // 팝업오픈권한

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL420801_onload(obj:Form, e:LoadEventInfo)
{
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin()); // 권한
	
	fn_init(); // 검색부 초기화
	//fn_commSearch(gfn_getCommTypeInit()); // 상품 조회
}

/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			
			case "srchSlipCardList": // 상품 조회
			gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
			//gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_prod); // 페이지건수 표출
			
			if(ds_list.getRowCount() <= 0)
			{
				gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, 0);
			}
			else
			{
				ds_list.filter("cha > 0 || dae > 0");
				gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, ds_list.getRowCount());
			}
			
			break;
			
 			case "updateCardSlipClose": // 엑셀다운
			gfn_exportGridToExcel(grd_listA,"카드전표_"+gfn_getTodayFull());
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_preSearch(sType)
{	
	if (sType == gfn_getCommTypeInit())
	{
		// 조회조건 및 default 정렬
		ds_inqCndt.clearData();
		ds_list.keystring = ""; // 정렬컬럼 초기화

		ds_inqCndt.addRow();
		ds_inqCndt.setColumn(0, "card_proc_day", cal_card_proc_day.value);
		
		if (gfn_isEmpty(cal_card_proc_day.value)) 
		{
			alert("작성일을 입력해주십시오.");
			return false;
		}
		
		ds_inqCndt.setColumn(0, "orderBy", "");
		ds_inqCndt.setColumn(0, "orderDirection", "desc");
		ds_list.clearData();
	}
	
	return true;
}

// 엑셀다운로드 전
function fn_preSrchExcelDownList()
{	
	// 조회조건 및 default 정렬
	ds_inqCndt_excel.clearData();

	ds_inqCndt_excel.addRow();
	ds_inqCndt_excel.setColumn(0, "card_proc_day", cal_card_proc_day.value);
	
	ds_inqCndt_excel.setColumn(0, "excel_yn", "Y");
		
	ds_inqCndt_excel.setColumn(0, "orderBy", "");
	ds_inqCndt_excel.setColumn(0, "orderDirection", "desc");
	
	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 상품조회
function fn_commSearch(sType)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	//if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 50, tc_prod)) return;
	
	var szSvcid       = "srchSlipCardList";
    var szController  = "/dlw/slip/CardList";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 엑셀다운로드
function fn_srchExcelDownList()
{
	//gfn_exportGridToExcel(grd_listA, "카드전표" + gfn_getTodayFull() + "");
 	if (!fn_preSrchExcelDownList()) return false;
 	
	var szSvcid       = "updateCardSlipClose";
    var szController  = "/dlw/slip/updateCardSlipClose";
    var szInDs        = "ds_input=ds_inqCndt_excel";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
	
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/********************************************************************************************/
/**   6. 사용자 함수                                                                   	*/
/********************************************************************************************/
function fn_init()
{
	cal_card_proc_day.value = gfn_getToday();
}

/********************************************************************************************/
/**   7. 이벤트(Event) Function                                                          	*/
/********************************************************************************************/
// 그리드 헤더 사이즈 초기화
function btn_gdInit_onclick(obj:Button,  e:ClickEventInfo)
{
	gfn_resetColsize(grd_list);
}

// 그리드 헤더 클릭(정렬)
function grd_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	ds_inqCndt.setColumn(0, "sController", "/dlw/slip/CardList"); // 재정렬 위한 DB조회
	ds_inqCndt.setColumn(0, "dsName", "ds_output");	// 결과를 받을 데이터셋명
	
	//if(!gfn_pageSearch(gfn_getCommTypeOrderBy(), ds_list, sta_TotalCnt, 50, tc_prod)) return;
	
	gfn_grdHeadClick(obj, e, ds_inqCndt);
	//gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_prod); // 페이지건수 표출
	if(ds_list.getRowCount() <= 0)
	{
		gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, 0);
	}
	else
	{
		gfn_setTotalRowCountOfNotPaging(sta_TotalCnt, ds_list.getRowCount());
	}
}

// 스크롤 페이징
function grd_list_onvscrolllastover(obj:Grid, e:ScrollEventInfo)
{
	fn_commSearch(gfn_getCommTypeAppend());
}

// button click
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_init") {				// 초기화 버튼
		fn_init();
	} else if (obj.name == "btn_search") {		// 검색 버튼
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_excel") {		// 엑셀다운로드 버튼
		fn_srchExcelDownList();
	} else if (obj.name == "btn_openPop") {
		fn_openPopup();
	}
}

function fn_openPopup()
{
	var sPrmr = {p_card_proc_day:cal_card_proc_day.value};
	var reVal = gfn_diaLog("카드전표내역보기", -1, -1, -1, -1, "DL420000::DL422302P01.xfdl", false, sPrmr, true, false, true, null, null, true);
}
