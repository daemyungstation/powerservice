/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
			alert("코드 적재를 실패하였습니다.");
			break;
	    
			case "srchRltmCardLog": 
			alert("NicePay 실시간결제 로그조회를 실패하였습니다.\n" + errmsg);
			break;
			
			case "pymnCancNicePay":
			alert("NicePay 결제승인 취소를 실패하였습니다.\n" + errmsg);
			break;
	    }
		return false;
	} else {
		switch (svcid) {
		    // 공통
			case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("0159", "SELECT", ds_ackd_typ, gds_DmlCd); // 코드조회결과 필터
			gfn_getCode("0160", "SELECT", ds_pymn_typ, gds_DmlCd); // 코드조회결과 필터
			cbo_PymnMenu.index = 0;
			cbo_ackdStat.index = 0;
			fn_init(); // 검색부 초기화
			break;
			
			case "srchRltmCardLog": 
		    gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		    gfn_setTotalRowCount(ds_list, sta_TotalCnt, tc_log, null, true, null); // 페이지건수 표출
		    fn_postSearch(svcid);
		
		    
			break;
			
			case "srchRltmCardLogTotal":
				//gfn_exportGridToExcel(grd_list00, "realCardLogList_"+gfn_getTodayFull()+"");
				var dataAthrQury = ds_list_excel.getColumn(0, "dataAthrQury");
				ds_inqCndt.setColumn(0, "dataAthrQury" , dataAthrQury);
				
				var colDS = gfn_excelPrepareExcelDS("grd_list");
				gfn_excelDown("카드결제로그", "XLS_DLCC", "DlwCardMap.getDlwRltmCardLogList", "DlwCardMap.getDlwRltmCardLogCount", ds_inqCndt, true, colDS, true);
			break;
			
			case "pymnCancNicePay":
			if ("03" == rslt_cd) {
				alert("결제승인 취소 및 입금취소 처리가 완료되었습니다");
			} else {
			//result_msg
				if(!gfn_isNull(rslt_cd)) {
					alert("["+rslt_cd+"]"+rslt_nm+"\n결제승인 취소에 실패하였습니다. \n3회 이상 실패시 전산담당자에게 연락바랍니다.");
				} else {
					if(!gfn_isNull(rslt_msg )) {
						alert(rslt_msg);
					} else{
						alert("결제승인 취소에 실패하였습니다. \n3회 이상 실패시 전산담당자에게 연락바랍니다.");
					}
				}
			}
			fn_commSearch(gfn_getCommTypeInit(), "srchRltmCardLog"); // 조회
			break;
			case "selectCustaccntList":
			if(tc_custAcnt == 1) {
				fn_setAccntNo();
			} else if (tc_custAcnt > 1) {
				btn_accnt_onclick(null, "Y");
			} else {
				edt_accnt_no.value = "";
			}			
			break;
		}
	}
}

