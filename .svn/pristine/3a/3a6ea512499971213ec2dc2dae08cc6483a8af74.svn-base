//나이스페이 결제취소
function fn_pymnCancNicePay(num)
{
	var rtPayCnt = 0;
	var paycnt = ds_list.getColumn(num, "pay_cnt"); //납입횟수
	var payno = ds_list.getColumn(num, "pay_no");
	var pmcnt = ds_list.getColumn(num, "pm_cnt");
	var payday = ds_list.getColumn(num, "pay_day");
	
	rtPayCnt = gfn_transNullToNumber(paycnt) + gfn_transNullToNumber(payno) - 1
	payday = payday.substring(0,10).replace(/-/g,"/");
	
	var cancle_str = "취소하시겠습니까?";
	
	var pass = false;
	
	if (rtPayCnt == gfn_transNullToNumber(pmcnt)) {
		if (payday == gfn_getToday()) {
			pass = confirm(cancle_str);
		} else {
			pass = confirm("[주의]매입 후 취소입니다.\n" + cancle_str);
		}
	} else {
		pass = confirm("[주의]실시간 결제 후 입금이 발생한 회원입니다." + cancle_str);
	}
	
	if (pass) {
		ds_rgsnCard.clearData();
		ds_rgsnCard.addRow();
		ds_rgsnCard.copyRow(0, ds_list, num);
		
		fn_commSave("pymnCancNicePay");
	}
}

