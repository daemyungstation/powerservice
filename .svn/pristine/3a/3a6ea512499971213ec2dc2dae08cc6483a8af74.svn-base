// 취소버튼 클릭시
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	
	if (e.cell == "23") { //취소 버튼
	    // resultcode '00'일경우에만 작동하도록
		var resultcode = ds_list.getColumn(ds_list.rowposition, "result_cd");
		if ("00" != resultcode) {
			alert("결제성공건만 취소가 가능합니다.");
			return false;
		}
		fn_pymnCancNicePay(grd_list.selectendrow);
	}
}


