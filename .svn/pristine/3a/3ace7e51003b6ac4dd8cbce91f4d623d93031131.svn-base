	fn_init();
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			gfn_getCode("02", "ALL", ds_evt_typ, gds_DmlCd); // 코드조회결과 필터
			cbo_event.index = 0;
			fn_init(); // 검색부 초기화
		break;
		
		case "searchBenefit":
			fn_postSearch();
		break;

	}
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
//조회전 처리 조회조건셋팅
function fn_preSearch()
{
	ds_inqCndt.clearData();
	ds_inqCndt.addRow();	
	
	if ( gfn_isEmpty(cal_stt_dt.value) ) {
		alert("시작일자를 입력해주십시오.");
		return false;
	}	
	
	if ( gfn_isEmpty(cal_end_dt.value) ) {
		alert("종료일자를 입력해주십시오.");
		return false;
	}		
	
	var tab_id = tab_main.tabindex;

	ds_inqCndt.setColumn(0, "stt_dt", cal_stt_dt.value);
	ds_inqCndt.setColumn(0, "end_dt", cal_end_dt.value);
	ds_inqCndt.setColumn(0, "j_stt_dt", j_stt_dt.value);
	ds_inqCndt.setColumn(0, "j_end_dt", j_end_dt.value);	
	ds_inqCndt.setColumn(0, "prod_cd", edt_prod_cd.value);
	ds_inqCndt.setColumn(0, "evt_gubun", cbo_event.value);
	ds_inqCndt.setColumn(0, "iogbn", rdo_gubun.value);
	ds_inqCndt.setColumn(0, "mem_nm", edt_mem_nm.value);
	ds_inqCndt.setColumn(0, "mngr_nm", edt_mngr_nm.value);
	ds_inqCndt.setColumn(0, "custmr_nm", edt_custmr_nm.value);
	ds_inqCndt.setColumn(0, "tab_id", tab_id);
	
	return true;
}

//조회
function fn_search(obj, e)
{	
	var sSearchType = gfn_getCommTypeAppend();
	
	if(!fn_preSearch()) return false;
	
	var szSvcid       = "searchBenefit";
    var szController  = "/dlw/evnt/getBenefit-list";
    var szInDs        = "ds_input=ds_inqCndt";
    if(tab_main.tabindex == 0){
		var szOutDs       = "ds_tab1_list=ds_output"; // 행사
	}else if(tab_main.tabindex == 1){
		var szOutDs       = "ds_tab2_list=ds_output"; // 월
	}else if(tab_main.tabindex == 2){
		var szOutDs       = "ds_tab3_list=ds_output"; // 지부
	}else if(tab_main.tabindex == 3){
		var szOutDs       = "ds_tab4_list=ds_output"; // CP
	}else if(tab_main.tabindex == 4){
		var szOutDs       = "ds_tab5_list=ds_output"; // 외주업체
	}
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}

//조회 결과 
function fn_postSearch()
{
    if(tab_main.tabindex == 0){       // 행사
		if(ds_tab1_list.rowcount < 1) {
			alert("조회 결과가 없습니다.");
		}
	}else if(tab_main.tabindex == 1){ // 월
		if(ds_tab2_list.rowcount < 1) {
			alert("조회 결과가 없습니다.");
		}
	}else if(tab_main.tabindex == 2){ // 지부
		if(ds_tab3_list.rowcount < 1) {
			alert("조회 결과가 없습니다.");
		}
	}else if(tab_main.tabindex == 3){ // CP
		if(ds_tab4_list.rowcount < 1) {
			alert("조회 결과가 없습니다.");
		}
	}else if(tab_main.tabindex == 4){ // 외주업체
		if(ds_tab5_list.rowcount < 1) {
			alert("조회 결과가 없습니다.");
		}
	}
	
	return;
}


