//초기화
function fn_init()
{
	var objDate = new Date();
	strDate = objDate.toFormatString("%Y%m%d");
	endDate = objDate.toFormatString("%Y%m%d");	
	
	cal_stt_dt.value = strDate;
	cal_end_dt.value = endDate;	
		
	fn_chng_rdo();
	cbo_event.index = 0;
	rdo_gubun.value = "0000";
	
	edt_prod_cd.value = "";
	edt_prod_nm.value = "";
	edt_mem_nm.value = "";
	edt_mngr_nm.value = "";
	
	tab_main.tabindex = 0;
	fn_tab_chng();
}

// 외주 체크시에만 외주 탭 페이지 OPEN
function fn_chng_rdo()
{
	var gubun = rdo_gubun.value;

	if(gubun == "0002"){
		tab_main.tabpage5.enable = true;
		stc_04.visible = true;
		edt_custmr_nm.visible = true;
		stc_06.visible = true;
		sta_Dash01.visible = true;
		j_stt_dt.visible = true;
		j_end_dt.visible = true;
	}else if(gubun == "0001" || gubun == "0003" ){
		tab_main.tabpage5.enable = false;
		stc_04.visible = false;
		edt_custmr_nm.visible = false;	
		stc_06.visible = false;
		sta_Dash01.visible = false;
		j_stt_dt.visible = false;
		j_end_dt.visible = false;			
	}else{
		tab_main.tabpage5.enable = false;
		stc_04.visible = true;
		edt_custmr_nm.visible = true;
		stc_06.visible = false;
		sta_Dash01.visible = false;
		j_stt_dt.visible = false;
		j_end_dt.visible = false;		
	}
}

// 탭페이지에 따른 조건검색 Visible
function fn_tab_chng()
{
	var gubun = rdo_gubun.value;
	
	if(tab_main.tabindex == 0){
		stc_02.visible = true;
		stc_03.visible = true;
		stc_04.visible = true;
		edt_mem_nm.visible = true;
		edt_mngr_nm.visible = true;
		edt_custmr_nm.visible = true;
		if(gubun == "0002"){
			stc_06.visible = true;
			sta_Dash01.visible = true;
			j_stt_dt.visible = true;
			j_end_dt.visible = true;	
		}
	}else if(tab_main.tabindex == 3){
		stc_03.visible = true;
		edt_mngr_nm.visible = true;
		stc_02.visible = false;
		edt_mem_nm.visible = false;
		edt_mem_nm.value = "";
	}else{
		stc_02.visible = false;
		stc_03.visible = false;
		stc_04.visible = false;
		edt_mem_nm.visible = false;
		edt_mngr_nm.visible = false;
		edt_custmr_nm.visible = false;
		edt_mem_nm.value = "";
		edt_mngr_nm.value = "";	
		stc_06.visible = false;
		sta_Dash01.visible = false;
		j_stt_dt.visible = false;
		j_end_dt.visible = false;			
	}
}
