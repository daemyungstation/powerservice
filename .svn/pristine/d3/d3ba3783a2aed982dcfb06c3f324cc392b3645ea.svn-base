// 조회 전
function fn_pre_execPayDlProc()
{	

//	trace("accnt_no==>"+ds_list.getColumn(ds_list.rowposition,"accnt_no"));
	var sWork_mnth = div_srch_work_mnth.getDate();

	if (gfn_isNull(sWork_mnth) ) {
		alert("기준월 입력하세요.");
		return false;
	}

	if (gfn_isNull(cbo_dl_type.value) ) {
		alert("마감구분을 선택하세요.");
