/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
// 조회 전
function fn_pre_Search(sType)
{	
	var searchCnt = 0;	
	if (sType == gfn_getCommTypeInit())	{

/*
		if (gfn_isNull(div_srch_work_mnth.value)) {
			alert("작업월을 입력하세요.");
			return false;
		}
*/	
				
		var sWork_mnth = div_srch_work_mnth.getDate();
		
		if (!(gfn_isNull(sWork_mnth))) {
			searchCnt++;			
		}
		if (!(gfn_isNull(cal_srch_stt_dt.value))) {
			searchCnt++;			
		}
		if (!(gfn_isNull(cal_srch_end_dt.value))) {
			searchCnt++;			
		}
		if (!(gfn_isNull(edt_srch_accnt_no.value))) {
			searchCnt++;			
		}	
		if (!(gfn_isNull(edt_srch_mem_nm.value))) {
			searchCnt++;			
		}				
		if (!(gfn_isNull(edt_srch_prod_cd.value))) {
			searchCnt++;			
		}				
		if (!(gfn_isNull(edt_srch_prod_nm.value))) {
			searchCnt++;			
		}		
		if (!(gfn_isNull(cbo_dl_type.value))) {
			searchCnt++;			
		}			
		if (!(gfn_isNull(cbo_pay_yn.value))) {
			searchCnt++;			
		}					
		if (cal_srch_stt_dt.value > cal_srch_end_dt.value) {
			alert("검색기간 시작일은 종료일보다 작거나 같아야 합니다.");
			return false;
		}
				
		if(searchCnt == 0){
			alert("검색조건을 하나 이상 입력해야 합니다.");
			return false;	
		}else{
			// 조회조건 및 default 정렬
			ds_cond.clearData();
			
			ds_cond.addRow();
			ds_cond.setColumn(0, "work_mnth", sWork_mnth);
			ds_cond.setColumn(0, "srch_typ", cbo_srch_typ.value);
			ds_cond.setColumn(0, "stt_dt", cal_srch_stt_dt.value);
			ds_cond.setColumn(0, "end_dt", cal_srch_end_dt.value);
			ds_cond.setColumn(0, "mem_nm", edt_srch_mem_nm.value);
			ds_cond.setColumn(0, "accnt_no", edt_srch_accnt_no.value);
			ds_cond.setColumn(0, "prod_cd", edt_srch_prod_cd.value);
			ds_cond.setColumn(0, "prod_nm", edt_srch_prod_nm.value);
			ds_cond.setColumn(0, "dl_type", cbo_dl_type.value);
			ds_cond.setColumn(0, "pay_yn", cbo_pay_yn.value);

			ds_cond.setColumn(0, "orderBy", "accnt_no");
			ds_cond.setColumn(0, "orderDirection", "desc");
		}			

	}
	
	return true;	
	
	
}
/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 상품별 상세목록 리스트 조회
function fn_commSearch(sType)
{
	if (!fn_pre_Search(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_list, sta_TotalCnt, 500, total_cnt)) return;
	
	var szSvcid       = "selectPayCloseList";
    var szController  = "/dlw/payClose/selectPayCloseList";
    var szInDs        = "ds_input=ds_cond";
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
}



