    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var total_cnt = 0;	// 전체건수
var dupChk = 0;

var xlsYn; 		// 엑셀권한
var athrPpupYn; // 팝업오픈권한

var gvMenuId;  // 프로그램 로그 추가 2018.01.25
/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL421004_onload(obj:Form, e:LoadEventInfo)
{
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가 2018.01.25 정승철
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////	
	
	gfn_getList("dlwCd","S010","fn_callBack",false);
	gfn_createPagingDataset(obj); 	// 페이징처리 위한 데이터셋 설정
	gfn_athrPgm(gfn_gdsCd_admin()); // 권한

	fn_init();
	
	if (xlsYn == "N") {
		btn_excel.visible = false;
	}
}
	
	
// button click
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_init") {				// 초기화 버튼
		fn_init();
	} else if (obj.name == "btn_srchProd"){     // 상품팝업오픈
		fn_openPopup();
	} else if (obj.name == "btn_search") {		// 검색 버튼
		fn_commSearch(gfn_getCommTypeInit());
	} else if (obj.name == "btn_excel") {		// 엑셀다운로드 버튼
		var excel = gfn_ExportMultSheet("입금마감관리_LIST","grd_list::LIST");
		if(!excel){return;} // 엑셀 취소시 리턴
	} else if (obj.name == "btn_accntNo") {		// 회원번호조회 버튼
		fn_srchAccntNoPop();
	} else if (obj.name == "btn_dl_tot") {		// 월 마감처리 버튼
		fn_callDlTotExec();
	} else if (obj.name == "btn_dl_cancel_tot") {		// 월 마감 해지처리 버튼
		fn_dlTotCancelExec();
		
	}
		
		
	
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) {
		trace(errcd)
		return false;
	} else {
		switch (svcid) {
			case "selectDlwCode": // 공통코드 조회 callBack Service ID	
				gfn_getCode("S010", "ALL", ds_dl_type, gds_DmlCd);	//ds_evt_typ 만 변경 하면 됨
				
				cbo_dl_type.index = 0;					
			break;
			
			case "selectPayCloseList": // 리스트 조회
				gfn_appendSearchDs(ds_list, ds_list_tmp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
				gfn_setTotalRowCount(ds_list, sta_TotalCnt, total_cnt); // 페이지건수 표출
			break;
			
			case "selectDupChk": // 중복 체크 후 마감처리
				if(dupChk == 1){
					if(!confirm("기존에 마감 처리된 데이타가 있습니다. \n 새로운 마감 처리을 하시겠습니까?")) return false;	
					dupChk = 0;
					fn_dlTotExec();
				}else{ //중복 데이타가 없는 경우
					fn_dlTotExec();
				}
			break;
			
			case "execPayDlTotProc": // 월마감 처리
				alert("마감 처리가 완료 되었습니다.");
			break;
			
			case "execPayDlTotCancelProc": // 월마감 처리
				alert("월 마감 해지 처리가 완료 되었습니다.");
			break;
						
 			case "srchProdExcelList": // 엑셀다운
				gfn_exportGridToExcel(grd_list_excel,"상품조회_"+gfn_getTodayFull());
			break;
			
		}
	}
}

