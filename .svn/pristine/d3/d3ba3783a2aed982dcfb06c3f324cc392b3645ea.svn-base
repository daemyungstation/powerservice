	ds_cond.clearData();

	ds_cond.addRow();
	ds_cond.setColumn(0, "work_mnth", sWork_mnth);
	ds_cond.setColumn(0, "dl_type", cbo_dl_type.value);
	ds_cond.setColumn(0, "accnt_no", ds_list.getColumn(ds_list.rowposition,"accnt_no"));

	return true;	
}


