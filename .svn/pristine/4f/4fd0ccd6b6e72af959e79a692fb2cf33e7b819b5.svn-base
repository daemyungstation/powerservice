/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
// 회원 조회
function fn_commSearch(sType, searchInfo)
{
	if (!fn_preSearch(sType)) return false;
	
	// 호출 건수 설정 및 조회여부 설정
	if(!gfn_pageSearch(sType, ds_mem_list, sta_TotalCnt, 500, tc_mem)) return;
	
	switch(searchInfo) {
        case "srchCustBasi": 
			var szSvcid       = "srchCustBasi";
			var szController  = "/cons/cust-basi/resnauth";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "";
			var szRetCallback = "fn_callBack";
        break;
        
        case "srchCustBasiTot":
			var szSvcid       = "srchCustBasiTot";
			var szController  = "/cons/cust-basi/resnauth";
			var szInDs        = "ds_input=ds_inqCndt";
			var szOutDs       = "ds_list_tmp=ds_output";
			var szParam       = "excel_fg=Y";
			var szRetCallback = "fn_callBack";
        break;
    }
    if(searchInfo == "srchCustBasi")
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","","S",gvMenuId);
	else
		gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


