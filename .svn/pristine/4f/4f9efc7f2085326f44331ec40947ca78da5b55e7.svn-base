    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var xInsertFailMsg = ""; // 입력유효성체크 실패메시지

/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL450700_01_onload(obj:Form, e:LoadEventInfo)
{
	gfn_initForm(obj);
	fn_init();
	
	if(!gfn_isNull(p_month))
	{
		cal_reqMonth.value = p_month;
	}
	
	fn_commSearch(gfn_getCommTypeInit(), "getCsvFileBatchDay"); // 조회
}
/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg)
{
	if (errcd != 0) 
	{
		trace(errcd)
		return false;
	} 
	else 
	{
		switch (svcid) 
		{
			case "getCsvFileBatchDay": 
				if(ds_listCsvFileBatchDay.getRowCount() > 0)
				{
					ds_listCsvFileBatchDayDetail.clearData();
					ds_listCsvFileBatchDayDetail.addRow();
					ds_listCsvFileBatchDayDetail.copyRow(0, ds_listCsvFileBatchDay, ds_listCsvFileBatchDay.rowposition);
					btn_delete.enable = true;
				} 
				else 
				{
					alert("조회 내역이 없습니다.");
					ds_listCsvFileBatchDayDetail.clearData();
				    fn_controlComponents();
				}
			break;
			
			// 저장
			case "insertCsvFileBatchDay":
				if(gfn_isNull(xInsertFailMsg)) 
				{
					alert("등록 완료");
					fn_controlComponents();
					fn_commSearch(gfn_getCommTypeInit(), "getCsvFileBatchDay"); // 재조회
				} 
				else 
				{
					alert(xInsertFailMsg);
					xInsertFailMsg = "";
				}
			break;
			
			// 삭제
			case "deleteCsvFileBatchDay":
				alert("삭제 완료");
				fn_commSearch(gfn_getCommTypeInit(), "getCsvFileBatchDay"); // 재조회
			break;
		}
	}
}

// 저장전 유효성 체크
function fn_preSave(sType) 
{
    var sToday = gfn_getToday();
    var sBatchProcessDt = cal_batchProcessDt.value;
    
	if (gfn_isNull(cal_batchProcessDt.value)) 
	{
		alert("배치처리일자를 입력해 주십시오.");
		cal_batchProcessDt.setFocus();
		return false;
	}
	
	if(gfn_isNull(rdo_batCalyn.value)) 
	{
		alert("배치처리구분을 입력해 주십시오.");
		rdo_batCalyn.setFocus();
		return false;
	}
	
	if(gfn_getDayDiff(sToday, sBatchProcessDt) <= 0)
	{
		alert("배치처리등록은 금일이후의 날짜만 등록가능합니다.");
		return false;
	}
	
	return true;
}

// 초기화
function fn_init()
{
	
}

// 닫기
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	close();
}

//조회 버튼 이벤트
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_commSearch(gfn_getCommTypeInit(), "getCsvFileBatchDay"); // 조회
}

//조회
function fn_commSearch(sType, searchInfo) 
{
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) 
    {
        // 산출마감체크 조회
		case "getCsvFileBatchDay":
			if (!fn_preSearch(sType, "getCsvFileBatchDay")) return false;
			szSvcid       = "getCsvFileBatchDay";
			szController  = "/dlw/pay/getCsvFileBatchDay";
			szInDs        = "ds_input=ds_inqCndt";
			szOutDs       = "ds_listCsvFileBatchDay=ds_output";
			szParam       = "";
			szRetCallback = "fn_callBack";
        break;
    }
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback,"","");
}

/* 조회전 처리*/
function fn_preSearch(sType, svcid) 
{
	switch(svcid) 
    {
		case "getCsvFileBatchDay":
			ds_inqCndt.clearData();
			ds_inqCndt.addRow();
			ds_inqCndt.setColumn(0, "ext_req_month",String(cal_reqMonth.value).substr(0,6)); // 청구월
		break;
	}
	
	return true;
}

/* 신규 입력 */
function btn_insert_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_listCsvFileBatchDayDetail.clearData();
	ds_listCsvFileBatchDayDetail.addRow();
	
	// 산출일 SET (** 현재 날짜 + 1 )
	ds_listCsvFileBatchDayDetail.setColumn(0, "bat_proc_dt", gfn_addDate(gfn_getToday(),1));
	
	// 컴포넌트 control 처리
	fn_controlComponents("Y");
}

/* 컴포넌트 control 처리 */
function fn_controlComponents(flag) {
    // 신규입력시
    if(flag == "Y") 
    {
		cal_batchProcessDt.enable = true;
		rdo_batCalyn.enable = true;
		btn_save.enable = true;
		btn_delete.enable = false;
	} 
	else 
	{
		cal_batchProcessDt.enable = false;
		rdo_batCalyn.enable = false;
		btn_save.enable = false;
		btn_delete.enable = false;
	}
}

/* 셀 선택시 */
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	ds_listCsvFileBatchDayDetail.clearData();
	ds_listCsvFileBatchDayDetail.addRow();
	ds_listCsvFileBatchDayDetail.copyRow(0, ds_listCsvFileBatchDay, ds_listCsvFileBatchDay.rowposition);	
	
	fn_controlComponents();
	btn_delete.enable = true;
}

/* 저장 */
function btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	// 유효성 체크
	if(!fn_preSave("insertCsvFileBatchDay")) return false;
	
	if(!confirm("저장하시겠습니까?")) return false;
	
	var szSvcid       = "insertCsvFileBatchDay";
    var szController  = "/dlw/pay/insertCsvFileBatchDay";
    var szInDs        = "ds_input=ds_listCsvFileBatchDayDetail";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

/* 삭제 */
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	if(!confirm("삭제하시겠습니까?")) return false;
		
	var szSvcid       = "deleteCsvFileBatchDay";
    var szController  = "/dlw/pay/deleteCsvFileBatchDay";
    var szInDs        = "ds_input=ds_listCsvFileBatchDayDetail";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "fn_callBack";
    
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}]]></Script>
