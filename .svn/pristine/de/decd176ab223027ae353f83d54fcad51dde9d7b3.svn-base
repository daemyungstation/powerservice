    {
		gfn_appendSearchDs(ds_listNonPay, ds_listNonPayTemp); // 조회된 결과를 실제보여줄 데이타셋으로 Append처리
		gfn_setTotalRowCount(ds_listNonPay, sta_TotalCnt, nTotalListCnt); // 페이지건수 표출
		fn_postSearch();	
    }
}

function fn_postSearch()
{
	if(ds_listNonPay.getRowCount() < 1) 
	{
		alert("조회 결과가 없습니다.");
	} else {
	}
	return;
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch()
{
	
// 		var sMasterNo = edt_masterNo.value;
// 		var sInfo = edt_info.value;
// 		
	ds_inqCndt.clearData();
	var nRow = ds_inqCndt.addRow();
	
	if ( gfn_isEmpty(div_monthCal.getDate()) ) {
		alert("미납생성월은 필수 입력입니다.");
		return false;
	}
	
	ds_inqCndt.setColumn(nRow, "ext_dt", div_monthCal.getDate());
	ds_inqCndt.setColumn(nRow, "diff_cnt_stt", edt_diff_cnt_stt.value);
	ds_inqCndt.setColumn(nRow, "diff_cnt_end", edt_diff_cnt_end.value);
	ds_inqCndt.setColumn(nRow, "sction_t_np", cbo_sction_t_np.value);
	ds_inqCndt.setColumn(nRow, "accnt_no", edt_accnt_no.value);

	return true;
}

/********************************************************************************************/
/**   5. 서비스 호출(gfn_Transaction)                                                   	*/
/********************************************************************************************/
function fn_commSearch(sType, searchInfo)
{	
    if (!fn_preSearch()) return false;	
    
    if(!gfn_pageSearch(sType, ds_listNonPay, sta_TotalCnt, 500, nTotalListCnt)) return;		
	
	var szSvcid       = "selectNonPayPopList";
	var szController  = "/dlw/nonPay/pop/list";
    var szInDs        = "ds_input=ds_inqCndt";
    var szOutDs       = "ds_listNonPayTemp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_srchCallBack";

    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

