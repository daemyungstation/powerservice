/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) 
	{
		trace(errcd)
		return false;
	} 
	else 
	{
		switch (svcid) 
		{
			case "getMemberBasicList" : 
			
				if (ds_list01.getColumn(0, "total_use_amt") == "" || ds_list01.getColumn(0, "total_use_amt") == undefined)
				{
					Div03.edt_takeAmt.value = gfn_setComma(toNumber(p_res_amt.replace(",","")));
					
				}
				else
				{
					Div03.edt_takeAmt.value = gfn_setComma(toNumber(p_res_amt.replace(",","")) - toNumber(ds_list01.getColumn(0, "total_use_amt")));
				}
				if(ds_list02.rowcount > 0) {
					ds_list04.copyData(ds_list02);
					ds_list05.copyData(ds_list02);
					
					ds_list04.filter("gubun == 'I'");
					ds_list05.filter("gubun == 'R'");
					
					var nUseAmt1 = 0;
					var nCanAmt1 = 0;
					var nUseAmt2 = 0;
					var nCanAmt2 = 0;
					var nUseAmt3 = 0;
					var nCanAmt3 = 0;
					
					for(i=0; i<ds_list02.rowcount; i++) {
						if(ds_list02.getColumn(i, "type") == "사용") {
							nUseAmt1 = nUseAmt1+toNumber(ds_list02.getColumn(i, "amt"));
						}
						if(ds_list02.getColumn(i, "type") == "취소") {
							nCanAmt1 = nCanAmt1+toNumber(ds_list02.getColumn(i, "amt"));
						}
					}
					Div01.edt_useAmt.value = gfn_NumFormat(nUseAmt1);
					Div01.edt_canAmt.value = gfn_NumFormat(nCanAmt1);
					
					for(i=0; i<ds_list04.rowcount; i++) {
						if(ds_list04.getColumn(i, "type") == "사용") {
							nUseAmt2 = nUseAmt2+toNumber(ds_list04.getColumn(i, "amt"));
						}
						if(ds_list04.getColumn(i, "type") == "취소") {
							nCanAmt2 = nCanAmt2+toNumber(ds_list04.getColumn(i, "amt"));
						}
					}
					Div05.edt_useAmt.value = gfn_NumFormat(nUseAmt2);
					Div05.edt_canAmt.value = gfn_NumFormat(nCanAmt2);
					
					for(i=0; i<ds_list05.rowcount; i++) {
						if(ds_list05.getColumn(i, "type") == "사용") {
							nUseAmt3 = nUseAmt3+toNumber(ds_list05.getColumn(i, "amt"));
						}
						if(ds_list05.getColumn(i, "type") == "취소") {
							nCanAmt3 = nCanAmt3+toNumber(ds_list05.getColumn(i, "amt"));
						}
					}
					Div06.edt_useAmt.value = gfn_NumFormat(nUseAmt3);
					Div06.edt_canAmt.value = gfn_NumFormat(nCanAmt3);
				}
				fn_preSendState();
			break;
			
			case "getResnMemberState" :
				Div00.edt_amt.value = gfn_setComma(ds_list01.getColumn(0, "amt"));
				Div00.edt_totalUseAmt.value = gfn_setComma(ds_list01.getColumn(0, "total_use_amt"));
				Div00.edt_totalAmt.value = gfn_setComma(ds_list01.getColumn(0, "total_amt"));
				
				if(Div03.edt_resAmt.value == 0 || gfn_isNull(Div03.edt_resAmt.value)) {
					fn_resAmtSearch();
				}
			break;
			
			case "srchResnAmt" :
				if(ds_list06.rowcount > 0) {
					Div03.edt_resAmt.value = gfn_setComma(ds_list06.getColumn(0, "resn_amt"));
				} else {
					Div03.edt_resAmt.value = "0";
				}
			break;

			case "insertMemberBasicAmt" : 
				alert("저장이 완료되었습니다.");
			break;
			
			case "updateResnMemberState" :
				alert("회원의 상태가 저장되었습니다.");
			break;
			
			case "insertMemberBasicNew" :
				alert("신규 회원 전송이 완료되었습니다.")
			break;
			
			case "insertMemberBasicAmt" : 
				alert("적립금 전송이 완료되었습니다.");
			break;
			
			case "srchResnGubn":
				trace("청약구분 값 : " + gv_resn_gubn + '  ( ** 구분 값 0 : 청약기간 아님 / 1 : 청약기간 )');
			break;
		}
	}
}



// button click
function btn_onclick(obj:Button,  e:ClickEventInfo)
{
	if (obj.name == "btn_search")      // 검색 버튼
	{
		fn_commSearch(gfn_getCommTypeInit());
	} 
	else if (obj.name == "btn_init")   // 초기화 버튼
	{
		fn_init();
	} 
	else if (obj.name == "btn_excel")  // 엑셀다운로드 버튼
	{
		var excel = gfn_ExportMultSheet("상품별 부가서비스_LIST","grd_list::LIST");
		if(!excel){return;} // 엑셀 취소시 리턴
	} 
	else if (obj.name == "btn_insert") // 신규등록
	{
		fn_insert();		
	} 
	else if (obj.name == "btn_save")   // 저장
	{
		fn_save();
	} 
	else if (obj.name == "btn_close")  // 듣기
	{
		close();
	}
	else if(obj.name == "btn_send")
	{
		fn_send();
	}
	else if(obj.name == "btn_sendState")
	{
		fn_sendState();
	}
}

