				  ,emple_nm
				  ,conf_no
				  ,grade
				  ,ent_day
				  ,chg_reson
				  ,posn_cd
				  ,jobduty_cd
				  ,bank_accnt_no
				  ,home_phon
				  ,cell
				  ,idn_no
				  ,zip
				  ,addr
				  ,addr2
				  ,alow_grade
				  ,renf_man_id
				  ,note
				  ,tmp_eid
				  ,img_nm
				  ,appr_auth
				  ,email
				  ,birth_mon_day
				  ,reg_dm
				  ,reg_man
				  ,upd_dm
				  ,upd_man
				  ,del_fg
				  ,depr
				  ,bank_cd
				  ,loc_cd
				  ,incom_cl
				  ,grp_emple_no
				 ,ocp_cluster
		  		 ,INTRA_PASSWD
		  		 ,NO_SALE_ACCNT_EMP
		  		 , DATA_AUTH
		  		 , GRP_DATA_AUTH
		  		 , CTI_ID
		  		 , CTI_PWD
		  		 , CTI_EXT
		  		 , CTI_FG
		  		 , HSF_UPLOAD_AUTH_YN
		  		 , REPORT_AUTH
		  		 , REPORT_ADMIN_AUTH
		  		 , WH_CD
				 , APPFORM_DAY          
				  )
		  VALUES (
		            #{emple_no}
		          , #{dept_cd}
		          , #{emple_nm}
		          , #{conf_no}
		          , #{grade}
		          , #{ent_day}
		          , #{chg_reson}
		          , #{posn_cd}
		          , #{jobduty_cd}
		          , fn_encrypt_temp(#{bank_accnt_no})
		          , #{home_phon}
		          , #{cell}
		          , fn_encrypt_temp(#{idn_no})
		          , #{zip}
		          , #{addr}
		          , #{addr2}
		          , #{alow_grade}
		          , #{renf_man_id}
		          , #{note}
		          , #{tmp_eid}
		          , #{img_nm}
		          , #{appr_auth}
		          , #{email}
		          , #{birth_mon_day}
		          , sysdate
		          , #{rgsr_id}
		          , sysdate
		          , #{rgsr_id}
		          ,'N'
		          , #{depr}
		          , #{bank_cd}
		          , #{loc_cd}
		          , #{incom_cl}
				  , #{grp_emp_no}
				 ,#{ocp_cluster}
