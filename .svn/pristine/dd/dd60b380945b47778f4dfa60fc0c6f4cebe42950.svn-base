	//var sPrmr = {p_mem_no:sMemNo, p_accnt_no:sAccntNo, p_consno_grop_no:"", p_consno_sqno:"", p_pageTyp:"PS030600"};
	var sPrmr = {p_man_day:div_monthCal.getDate()};
