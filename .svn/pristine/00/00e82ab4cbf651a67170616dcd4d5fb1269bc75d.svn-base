			edt_brdt.value = ds_accnt_info.getColumn(0,"brth_mon_day").substr(2,6);
									
			// CMS기존에 등록이 되었던 회원의 경우 (CMS업데이트 NEW_CANCLE 인서트)
			if(gv_card_yn == "U"){										
				edt_card_mon.value = "";		
				edt_card_year.value = "";
				edt_card_no.value = "";
				edt_reg_dm.value = "";
				edt_reg_man.value = "";
			}																										
		break;				
	}
	//생년월일 : 810505
