			} else {
				btn_free_realtime.visible = true;
				stc_10.visible = false;				
			}
																						
			//장기할부 결제 화면 열림
			this.getOwnerFrame().position.height = 300;
			this.stc_popup.position.height = 298;
		break;	
			
		case "ADD":
			//이전 데이터 저장
			edt_mem_nm.readonly = true;			
