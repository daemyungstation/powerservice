function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	obj.enableevent = false;
	if ("renf_man_nm" == e.columnid) {
		if (e.oldvalue != e.newvalue) {
			obj.setColumn(e.row, "renf_man_id", "");
		}
	}
	obj.enableevent = true;
}

