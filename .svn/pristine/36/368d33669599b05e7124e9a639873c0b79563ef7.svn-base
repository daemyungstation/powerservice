 	gfn_getList("dlwCd","51,06,10,63,96,51,52,0124,0161","fn_callBack",false); 	
 	
 	fv_login_emple_no = gfn_getUserInfo("empl_no");
	if ( fv_login_emple_no == "1234567890") {
		btn_pay.enable = false;
