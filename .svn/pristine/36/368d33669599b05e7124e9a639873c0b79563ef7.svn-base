	var del_fg = "";
	var isDel = true;
	
	if (ds_list_tmp.rowcount > 0) {
		fn_showDupEmp(true);		
		
		for (var i=0; i<ds_list_tmp.rowcount; ++i) {
			del_fg = ds_list_tmp.getColumn(i, "del_fg");
			if ("N" == del_fg) {
				isDel = false;
			}
		}
		/* asis 에 주석처리되어 있음
		if ( isDel ) {
			fn_setIdnNoEnable();
		}
		*/
		fn_setIdnNoEnable();		
	} 
	else {
		fn_showDupEmp(false);
		fn_setIdnNoEnable();
	}
}

function fn_setIdnNoEnable() {
	fv_isIdnNoCheck = true;
	medt_idn_no.readonly = true;
	btn_idn_no.visible = false;
	cal_ent_day.setFocus();
}

function fn_showDupEmp(visible) {
	var diff = 85;
	
	grd_list_tmp.visible = visible;
	
	var objParentFrame = this.getOwnerFrame();
	
	if (visible) {
		this.position.height = fv_orig_form_height + diff;
	} else {
		this.position.height = fv_orig_form_height;			
	}
	
	objParentFrame.position.height = this.position.height;
	stc_31.position.height = this.position.height;
}

