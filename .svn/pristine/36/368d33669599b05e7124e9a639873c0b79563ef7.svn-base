		cal_ent_day.setFocus();
		return false;
	}
	if ( gfn_isEmpty(grp_emp) ){
		alert( "그룹사번은 필수 입력항목입니다.");
		edt_grp_emp_no.setFocus();
		return false;
	}
	
	/* 주민등록번호 필수체크 추가 - 2017.12.27 정승철 */
