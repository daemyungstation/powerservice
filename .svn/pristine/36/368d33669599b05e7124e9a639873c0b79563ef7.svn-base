// 중복체크를 위한 사원조회
function fn_duplCheck(colId) { // asis : duplCheck
	var sVal = "";
	
	ds_cond.clearData();
	ds_cond.addRow();
	
	if ( !gfn_isEmpty(fv_emple_no) ) return;
	
	if ("emple_nm" == colId) 
	{		
		sVal = ds_list.getColumn(0, colId);
		if ( gfn_isEmpty(sVal) ) {
			alert("이름을 입력해 주세요.");
			edt_emple_nm.setFocus();
			return;
		}
		ds_cond.setColumn(0, "emple_nm", sVal);
		edt_conf_no.setFocus();
	}
	
	if ("home_phon" == colId) 
	{
		sVal = cbo_home_phon1.value + "-" + edt_home_phon2.value + "-" + edt_home_phon3.value;
		if ( gfn_isEmpty(cbo_home_phon1.value) || gfn_isEmpty(edt_home_phon2.value) || gfn_isEmpty(edt_home_phon3.value) ) {
			sVal = "";
		}		
		if ( gfn_isEmpty(sVal) ) {
			alert("전화번호를 입력해 주세요.");
			return;
		}
		ds_cond.setColumn(0, "home_phon", sVal);
		cbo_home_phon1.setFocus();
	}
	
	if ("cell" == colId) 
	{
		sVal = cbo_cell1.value + "-" + edt_cell2.value + "-" + edt_cell3.value;
		if ( gfn_isEmpty(cbo_cell1.value) || gfn_isEmpty(edt_cell2.value) || gfn_isEmpty(edt_cell3.value) ) {
			sVal = "";
		}		
		if ( gfn_isEmpty(sVal) ) {
			alert("휴대폰번호를 입력해 주세요.");
			return;
		}
		ds_cond.setColumn(0, "cell", sVal);
		cbo_cell1.setFocus();
	}
	
	ds_cond.setColumn(0, "full_match_yn", "Y");
	
	var szSvcid       = "getDupEmple";
    var szController  = "/dlw/emp/selectEmpleList";
    var szInDs        = "ds_input=ds_cond";    
    var szOutDs       = "ds_list_tmp=ds_output";
    var szParam       = "";
    var szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}


