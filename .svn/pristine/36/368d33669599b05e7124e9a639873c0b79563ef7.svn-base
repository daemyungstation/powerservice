	}
	
	if (!gfn_isEmpty(edt_cell2.value) || !gfn_isEmpty(edt_cell3.value)) {
		if (gfn_isEmpty(cbo_cell1.value)) {
			alert("휴대폰번호 앞자리를 선택해 주세요.");
			cbo_cell1.setFocus();
			return false;
		}
		cell = cbo_cell1.value + "-" + edt_cell2.value + "-" + edt_cell3.value;
