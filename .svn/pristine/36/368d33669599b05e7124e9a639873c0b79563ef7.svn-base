function frm_onload(obj:Form, e:LoadEventInfo)
{	
	if ( !gfn_isEmpty(p_emple_no) ) {
		fv_emple_no = p_emple_no;
		btn_resin_cancel.enable = true;
		
		btn_name.visible 		= false;
		btn_home_phon.visible 	= false;
		btn_cell.visible 		= false;
		btn_idn_no.visible		= false;
	} 
	else {
		btn_resin.enable = false;
		btn_posn.enable = false;		
		fv_isIdnNoCheck = false;
	}
	
	fn_getWhCdList(); // 창고코드
	
	fn_getDataAuthGrpList(); // 자료권한 그룹 조회	
	
	// 현황권한이 보여지는 부분 - 주석풀어야 함
