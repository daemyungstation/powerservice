/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
	        // 공통
  			case "selectDlwCode":
				alert("코드 적재를 실패하였습니다.");
			break;
	    }
		return false;
	} 
	
	switch (svcid) {
		// 공통
		case "selectDlwCode": // 공통코드 조회 callBack Service ID
			fn_postSelectDlwCode();			
			break;
		
		case "search":
			fn_postSearch();
			break;

		case "save":
			fn_postSave();
			break;
			
		case "delete":
			fn_postDelete();
			break;
			
		case "getWhCdList":
			fn_postGetWhCdList();
			break;
			
		case "selectDataAuthGrpList": // 자료권한그룹
			fn_postGetDataAuthGrpList();
			break;
			
		case "getDupEmple": // 중복사원 조회
			fn_postDuplCheck();
			break;
			
		case "getDupIdnNo": // 주민등록번호 중복사원 조회
			fn_postGetDupIdnNo();			
			break;
			
		case "selectReportAuthGrp":
			fn_postSelectReportAuthGrp();
			break;
			
		case "resinCancel":
			fn_postResinCancel();
			break;			
	}
}

