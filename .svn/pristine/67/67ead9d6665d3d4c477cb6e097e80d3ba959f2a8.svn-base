        break;
                
		// 임의삭제
        case "deleteWdrwTemp":
        ds_delWdrwAccnt.clearData();
		ds_delWdrwAccnt.addRow();
		ds_delWdrwAccnt.setColumn(0, "accnt_no", edt_accnt_no.value);
