        // 가상계좌 전송
        case "sendVtrTemp":
			var accnt_no = ds_accnt_base_info.getColumn(0,"accnt_no");			
			var mem_no = ds_accnt_base_info.getColumn(0,"mem_no");
			var mem_nm = ds_accnt_base_info.getColumn(0,"mem_nm");									
