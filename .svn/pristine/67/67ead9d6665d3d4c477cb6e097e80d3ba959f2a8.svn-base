function fn_dupAccntNo(accnt_no) {
	var dup_fg = true;
	if (opener.ds_list != null){
		for(var i=0;i<opener.ds_list.rowcount;i++)
		{
			var parent_accnt_no = opener.ds_list.getColumn(i, "accnt_no");
			if(accnt_no == parent_accnt_no){
				dup_fg = false;
			}
		}
	}
	return dup_fg
}


