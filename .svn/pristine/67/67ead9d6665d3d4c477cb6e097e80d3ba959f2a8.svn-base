			ds_send_info.setColumn(0,"mem_nm", mem_nm);             // 
			ds_send_info.setColumn(0,"end_day", end_day);           // 
			ds_send_info.setColumn(0,"ichae_cd", ichae_cd);         // 
			ds_send_info.setColumn(0,"pay_amt", pay_amt);           // 
