			gfn_getCode("93", "SELECT", ds_die_reason, gds_DmlCd);
			gfn_getCode("108", "SELECT", ds_cnl_reason, gds_DmlCd);
			gfn_getCode("0123", "SELECT", ds_sex, gds_DmlCd);
			gfn_getCode("0142", "SELECT", ds_brplot, gds_DmlCd);
			
			if (this.parent.parent.event_mode != "update") {
				ds_evnt_list.clearData();
				var i = ds_evnt_list.addRow();
				ds_evnt_list.setColumn(0, "sort", "");
				ds_evnt_list.setColumn(0, "event_man", "01");
				ds_evnt_list.setColumn(0, "rel", "");
				ds_evnt_list.setColumn(0, "die_reson", "");
				ds_evnt_list.setColumn(0, "cnl_reason_cd", "");
				ds_evnt_list.setColumn(0, "sex", "");
				ds_evnt_list.setColumn(0, "wedding_opt", "");
				ds_evnt_list.setColumn(0, "brplot", "");
			}
			
			fn_init();
			break;
			
			case "insertEvent":
			if (edt_yn == "N") {
				alert("마감된 데이터는 수정할 수 없습니다.");
				return false;
			}
			if (result_msg == "error") {
				alert("저장하지 못했습니다.\n이미 등록된 건인지 확인하세요.");
				return false;
			}
			
			fn_commSave("saveCons", "new");
			alert("저장되었습니다.");
			btn_save.enable = true;
			this.parent.parent.event_mode = "update";
			this.parent.parent.event_stat_fg = ds_evtSeq.getColumn(0, "event_stat_fg");
			this.parent.parent.ds_evnt_info.setColumn(0, "seq", ds_evtSeq.getColumn(0, "seq"));
			this.parent.parent.ds_evnt_info.setColumn(0, "accnt_no", ds_evtSeq.getColumn(0, "accnt_no"));
			this.parent.parent.fn_init();
			
			break;
			
			case "updateEvent":
			if (edt_yn == "N") {
				alert("마감된 데이터는 수정할 수 없습니다.");
				return false;
			}
			
			if (!del_fg) {
				alert("저장되었습니다.");
				//행사리스트 재조회 (행사조회 > 행사수정화면 경우..)
				openerReSearch();
			} else {
				fn_commSave("saveCnclReg");
				del_fg = false;;
			}
			break;
			
			case "saveCnclReg":
			fn_commSave("saveCons", "cancel");
			alert("삭제되었습니다.");
			//행사리스트 재조회 (행사조회 > 행사수정화면 경우..)
			openerReSearch("cancel");
			this.parent.parent.close();
			break;
			
			case "deleteEvent":
			if ("N" == newYn) {
				alert("공제 조합에 행사 신고된 건입니다.\n공제전송모듈에서 행사취소 전문 전송 후\n행사취소를 진행하시기 바랍니다.");
				return false;
			}
			if ("Y" == isEvtRptGen) {
				alert("행사보고서 작성된건은 취소할 수 없습니다.");
				return false;
			}
			if ("N" == edt_yn) {
				alert("마감된 데이터는 수정할 수 없습니다.");
				return false;
			}
			fn_commSave("saveEvent");
			break;
			
			case "checkB2bPay":
			// Y(CMS 해지가능), F(CMS 해지불가)
			var chkNum = 0;
				if(chkNum == 0){
					chkNum++;
					if(checkB2bPay == 'Y'){
						if(cmsInitFlag){ // cms 초기화 성공이면 부가서비스 해지
							//부가서비스 신청에 필요한 정보
						    fn_commSearch(gfn_getCommTypeInit(), "srchAccntInfo");
						}
					}else{
						alert("결합상품 비용이 남아있어 CMS해지 불가능합니다.");
						this.parent.parent.close();	
					}
				}
			break;
			
			case "srchAccntInfo":
			if (ds_cmsAccnt.rowcount > 0) {
				if (ds_cmsAccnt.getColumn(0,"del_fg") == "N") {
					//해지
					fn_commSave("saveCmsRgsnAnxtSrvc");
				} else {
					alert("저장이 실패했습니다. (이미 해지된 회원)");
					openerReSearch();
					this.parent.parent.close();
				}
			} else {
				alert("저장이 실패했습니다. (계좌조회 불가)");
				openerReSearch();
				this.parent.parent.close();
			}
			break;
			
			case "saveCmsRgsnAnxtSrvc":
			alert("저장이 완료되었습니다.");
			openerReSearch();
			this.parent.parent.close();
			break;
			
			case "getWdrwReqAccntEventConfirm" :
				trace(ds_listWdrwReq.saveXML());
			break;
			
			case "getPerfMainPayAmt" : 
				if(ds_listPerfMain.getRowCount() > 0)
				{
					edt_sum_pay_amt.value = gfn_commaStr(ds_listPerfMain.getColumn(ds_listPerfMain.rowposition, "perf_amt"));
					edt_perf_amt.value    = gfn_commaStr(ds_listPerfMain.getColumn(ds_listPerfMain.rowposition, "perf_amt"));
					edt_lst_remain_amt.value = gfn_commaStr(gfn_transNullToNumber(ds_evnt_list.getColumn(0, "remain_amt")) + 
															gfn_transNullToNumber(ds_evnt_list.getColumn(0, "remain_allt_amt")) + 
															gfn_transNullToNumber(ds_evnt_list.getColumn(0, "remain_add_amt")) + 
															gfn_transNullToNumber(ds_evnt_list.getColumn(0, "shop_use_amt")) + 
															gfn_transNullToNumber(ds_evnt_list.getColumn(0, "pay_after_event")) -
															gfn_transNullToNumber(ds_listPerfMain.getColumn(ds_listPerfMain.rowposition, "perf_amt")));
				}
				else
				{
					edt_sum_pay_amt.value = gfn_commaStr(gfn_transNullToNumber(ds_listPerfMain.getColumn(0, "sum_pay_amt")));
					edt_perf_amt.value    = 0;
				}
			break;
		}
	}
}

