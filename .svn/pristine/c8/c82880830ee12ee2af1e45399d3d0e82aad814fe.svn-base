function fn_setPayInfo(objDs) {
	//입금정보
	//상조금액/할부원금/추가부담금/합계
	if (objDs.name == "ds_evnt_list") {
		edt_tot_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "tot_month_pay_amt")));
	} else {
		edt_tot_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "tot_amt")));
	}
	edt_extr_allt_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "extr_allt_amt")));
	edt_extr_add_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "extr_add_amt")));
	if (objDs.name == "ds_evnt_list") {
		edt_sum_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "prod_mo")) + gfn_transNullToNumber(objDs.getColumn(0, "tot_allt_amt")));
	} else {
		edt_sum_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "tot_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "extr_allt_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "extr_add_amt")));
	}
	
	//총납입금/할부원금/추가부담
	edt_pay_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "pay_amt")));
	edt_allt_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "allt_amt")));
	edt_add_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "add_amt")));
	
	//총납입금합계
	edt_napip_tot_amt.value = gfn_transNullToNumber(objDs.getColumn(0, "pay_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "allt_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "add_amt"));
	
	//상조잔액/할부잔액/추가잔액
	edt_remain_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "remain_amt")));
	edt_remain_allt_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "remain_allt_amt")));
	edt_remain_add_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "remain_add_amt")));
	
	//총잔금합계
	//edt_tot_remain.value = gfn_transNullToNumber(objDs.getColumn(0, "remain_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "remain_allt_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "remain_add_amt"));
	// 레디캐시 추가 따른 총잔금합계 가산 처리 추가 - 2018.01.29 정승철 */
	edt_tot_remain.value = gfn_transNullToNumber(objDs.getColumn(0, "remain_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "remain_allt_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "remain_add_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "shop_use_amt"));
	
	// DC금/특별금
	// 금액이 존재할 경우 바탕색 변경 처리
	edt_dc_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "dc_amt")));
	edt_new_chan_money.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "new_chan_money")));
	
	/* 쇼핑몰사용금액추가 - 2018.01.29 정승철 */
	edt_shop_use_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "shop_use_amt")));
	
	if(gfn_transNullToNumber(objDs.getColumn(0, "dc_amt")) > 0){
		edt_dc_amt.style.color = "red";
	}
	
	if(gfn_transNullToNumber(objDs.getColumn(0, "new_chan_money")) > 0){
		edt_new_chan_money.style.color = "red";
	}
	
	/* 금액이 존재할 경우 바탕색 변경 처리 (** 레디캐시) - 2018.01.29 정승철 */
	if(gfn_transNullToNumber(objDs.getColumn(0, "shop_use_amt")) > 0){
		edt_shop_use_amt.style.color = "red";
	}
	
	// 할인금액합계
	edt_tot_disamt.value = gfn_transNullToNumber(objDs.getColumn(0, "dc_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "new_chan_money"));
	
	//실지급금액/행사후입금액
	edt_sum_pay_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "sum_pay_amt")));
	if (objDs.name == "ds_evnt_list") {
		edt_pay_after_event.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "pay_after_event")));
		// 최종 잔액 = 잔액(합계) + 행사후 입금액
		edt_lst_remain_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "remain_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "remain_allt_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "remain_add_amt")) + gfn_transNullToNumber(objDs.getColumn(0, "shop_use_amt"))
                                                + gfn_transNullToNumber(objDs.getColumn(0, "pay_after_event")));
	}
	
	
	if (objDs.name != "ds_evnt_list") {
		if (objDs.name == "ds_event_accnt"){
			// 추후 변경가능성! 콜센터 화면 회원조회 시 타지 않도록!
		}else {
			//결합상품일경우
			if("03" == objDs.getColumn(0, "prod_cl")) {
				ckb_cms_wdrw_req_yn.visible = true;
				if ("Y" == objDs.getColumn(0, "cms_wdrw_req_yn")) {
					ckb_cms_wdrw_req_yn.value = "Y";
				} else {
					ckb_cms_wdrw_req_yn.value = "N";
				}
			} else {
				ckb_cms_wdrw_req_yn.visible = false;
				ckb_cms_wdrw_req_yn.value = "N";
			}
		}
	}
	
	fn_getWdrwReqAccntConfirm();
	//fn_getPerfMainPayAmt(); 장례이행 보증 현황 반영시 이부분 반드시 풀고 반영하기
}


