function fn_cbo_event_sort_comb_canitemchange(obj:Combo, e:ItemChangeEventInfo)
{
	var objVal = "";
	if (e == null) {
		objVal = obj.value;
	} else {
		objVal = e.postvalue;
	}
	if (objVal == "01") { // 장의
		div_evt_cp.visible = true;
		div_funeral.visible = true;
		div_wedding.visible = false;
		div_etc_event.visible = false;
		stc_annv.text = "기일";
		div_funeral.move(div_etc_event.position.left,div_etc_event.position.top)
	} else if (objVal == "02") { //결혼
		div_evt_cp.visible = false;
		div_funeral.visible = false;
		div_wedding.visible = true;
		div_etc_event.visible = false;
		stc_annv.text = "기념일";
		div_wedding.move(div_etc_event.position.left,div_etc_event.position.top)
	} else { //기타
		div_evt_cp.visible = false;
		div_funeral.visible = false;
		div_wedding.visible = false;
		div_etc_event.visible = true;
		stc_annv.text = "기념일";
	}
}

//행사등록일 변경시
function cal_rgs_dt_onchanged(obj:Calendar, e:ChangeEventInfo)
{
	if(e.postvalue != "") {
		//기념일
		cal_annv_dt.value = e.postvalue;
		
		//행사시작일
		cal_proc_dt.value = gfn_addDate(e.postvalue, 0);
		
		//접수일시
		cal_accept_day.value = gfn_addDate(e.postvalue, 0);
		spn_accept_tm_mm.value = 000000000;
		
		//행사종료일
		cal_event_comp_day.value = gfn_addDate(e.postvalue, 2);
		spn_comp_tm_mm.value = 000000000;
		
		//입관일시
		div_funeral.cal_ent_day.value = gfn_addDate(e.postvalue, 1);
		div_funeral.spn_ent_tm_mm.value = 000000000; 
		
		//발인일시
		div_funeral.cal_cfn_day.value = gfn_addDate(e.postvalue, 2);
		div_funeral.spn_cfn_tm_mm.value = 000000000;
		
		//출동일시
		cal_move_day.value = gfn_addDate(e.postvalue, 0);
		spn_move_tm_mm.value = 000000000;
		
		//도착일시
		cal_arrive_day.value = gfn_addDate(e.postvalue, 0);
		spn_arrive_tm_mm.value = 000000000;
		
		//사망일시
		cal_d_day.value = gfn_addDate(e.postvalue, 0);
		
		if(ds_listPerfMain.getRowCount() > 0)
		{
			edt_sum_pay_amt.value = gfn_commaStr(ds_listPerfMain.getColumn(ds_listPerfMain.rowposition, "perf_amt"));
			edt_perf_amt.value    = gfn_commaStr(ds_listPerfMain.getColumn(ds_listPerfMain.rowposition, "perf_amt"));
			edt_lst_remain_amt.value = gfn_commaStr(gfn_transNullToNumber(ds_evnt_list.getColumn(0, "remain_amt")) + 
													gfn_transNullToNumber(ds_evnt_list.getColumn(0, "remain_allt_amt")) + 
													gfn_transNullToNumber(ds_evnt_list.getColumn(0, "remain_add_amt")) + 
													gfn_transNullToNumber(ds_evnt_list.getColumn(0, "shop_use_amt")) + 
													gfn_transNullToNumber(ds_evnt_list.getColumn(0, "pay_after_event")) -
													gfn_transNullToNumber(ds_listPerfMain.getColumn(ds_listPerfMain.rowposition, "perf_amt")));
		}
		else
		{
			edt_sum_pay_amt.value = gfn_commaStr(gfn_transNullToNumber(objDs.getColumn(0, "sum_pay_amt")));
			edt_perf_amt.value    = 0;
		}
	}
}

