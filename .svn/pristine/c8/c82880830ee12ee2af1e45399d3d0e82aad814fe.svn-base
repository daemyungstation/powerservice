			gfn_getCode("02", "SELECT", ds_evt_typ, gds_DmlCd);
			gfn_getCode("07", "SELECT", ds_rltn_typ, gds_DmlCd);
			gfn_getCode("61", "SELECT", ds_evt_man, gds_DmlCd);
