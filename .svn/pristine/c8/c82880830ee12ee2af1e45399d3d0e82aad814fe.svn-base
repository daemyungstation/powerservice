//조회
function fn_commSearch(sType, searchInfo) {
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(searchInfo) {
        case "checkB2bPay": 
		szSvcid       = "checkB2bPay";
		szController  = "/dlw/evnt/check-b2b-pay";
		szInDs        = "";
		szOutDs       = "";
		szParam       = "accnt_no="+ds_evnt_list.getColumn(0, "accnt_no");
		szRetCallback = "fn_callBack";
        break;
        
        case "srchAccntInfo":
		szSvcid       = "srchAccntInfo";
		szController  = "/dlw/cms/accnt-info";
		szInDs        = "";
		szOutDs       = "ds_cmsAccnt=ds_output";
		szParam       = "accnt_no="+ds_evnt_list.getColumn(0, "accnt_no");
		szRetCallback = "fn_callBack";
        break;
    }
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

//저장
function fn_commSave(svcid, gbn) {
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
    switch(svcid) {
        case "saveEvent": 
        if (!fn_preSave("saveEvent")) return false;
        if (this.parent.parent.event_mode == "update") {
			szSvcid       = "updateEvent";
			szController  = "/dlw/evnt/update";
        } else {
			szSvcid       = "insertEvent";
			szController  = "/dlw/evnt/insert";
        }
		szInDs        = "ds_input=ds_rgsnEvnt";
		szOutDs       = "ds_evtSeq=ds_output";
		szParam       = "";
		szRetCallback = "fn_callBack";
        break;      
        
        case "saveCnclReg":
		szSvcid       = "saveCnclReg";
		szController  = "/dlw/evnt/cncl-reg";
		szInDs        = "";
		szOutDs       = "";
		szParam       = "accnt_no="+ds_evnt_list.getColumn(0, "accnt_no") + " cl=02"; //취소내역구분 (01-해약 02-행사)
		szRetCallback = "fn_callBack";
        break;
        
        case "saveCons":
        if (!fn_preSave("saveCons",gbn)) return false;
		szSvcid       = "saveCons";
		szController  = "/dlw/cms/save-cons";
		szInDs        = "ds_input=ds_rgsnCons";
		szOutDs       = "";
		szParam       = "";
		szRetCallback = "fn_callBack";  
        break;
        
        case "deleteEvent":
        if (!fn_preSave("deleteEvent")) return false;
		szSvcid       = "deleteEvent";
		szController  = "/dlw/evnt/delete";
		szInDs        = "ds_input=ds_rgsnEvnt";
		szOutDs       = "";
		szParam       = "";
		szRetCallback = "fn_callBack";  
        break;
        
        case "saveCmsRgsnAnxtSrvc": //CMS 부가서비스 등록
		if (!fn_preSave("saveCmsRgsnAnxtSrvc")) return false;   
		//웹모듈 연동		
		var resl = fn_rltmWebModuleRequest();
		if (!resl) {
			return false;
		}
		szSvcid       = "saveCmsRgsnAnxtSrvc";
		szController  = "/dlw/cms/cms-anxt-srvc/save";
		szInDs        = "ds_input=ds_rgsnCms";
		szOutDs       = "";
		szParam       = "";
		szRetCallback = "fn_callBack";   
    }
    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback);
}

// 결제정보 내역 담기(SMS 현금영수증 정보)
function fn_smspayment(){
	
	var szSvcid       = "";
    var szController  = "";
    var szInDs        = "";
    var szOutDs       = "";
    var szParam       = "";
    var szRetCallback = "";
    
	szSvcid       = "smsPayMent";
	szController  = "/dlw/evnt/smspayment";
	szInDs        = "";
	szOutDs       = "ds_payment=ds_output";
	szParam       = "accnt_no="+ds_evnt_list.getColumn(0, "accnt_no") + " rpt_gubun=" + ds_evnt_list.getColumn(0, "rpt_gubun");
	szRetCallback = "fn_callBack";

    // 트랜젝션 공통함수 호출
    gfn_transaction(this, szSvcid, szController, szInDs, szOutDs, szParam, szRetCallback, false);
}

