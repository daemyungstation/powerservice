/* 결과등록 */
function fn_addFile()
{
	var bResult;
	fileDialog = objects["fileDialog"];
	
	// 업로드파일 선택
	fv_virtualFile = FileDialog_RESULT.open("결과파일(CSV) 추가", FileDialog_RESULT.LOAD);
	if (undefined == fv_virtualFile || null == fv_virtualFile) return;	//파일을 선택하지 않은 경우	
 	
 	// 주의 : HttpObject 객체는 동적으로 생성해야 한다.
 	// 동적으로 생성하지 않으면 이벤트 핸들러를 Property 장에서 연결시켜야 한다.
 	httpObjectInput = objects["httpObject"];

 	if (httpObjectInput == null) {
		httpObjectInput = new HttpObject();
		httpObjectInput.onstatus.setHandler(HttpObject_onstatus);
		httpObjectInput.onload.setHandler(fn_httpObject_onload);
	}
	
	// httpObjectInput.asyncflag = false; /* CY_TRUE : 동기, CY_FALSE : 비동기 */
	httpObjectInput.timeout = 600;
	httpObjectInput.retry  	= 1;

	trace("fv_svcPath  : " + fv_svcPath);
	//bResult = httpObjectInput.upload(fv_svcPath +"dlw/pay/insertResultInfo?fname="+fv_virtualFile.filename, FileDialog.LOAD, fv_virtualFile, true);
	// 기존에 파일이름(filename) 대신 청구일(req_day) 로 대체
	bResult = httpObjectInput.upload(fv_svcPath +"dlw/pay/insertResultInfo", FileDialog.LOAD, fv_virtualFile, true);
	
	//bResult = httpObjectInput.upload(fv_svcPath +"dlw/pay/insertResultInfo?req_day="cal_req_day.value, FileDialog.LOAD, fv_virtualFile, true);
	
	if(bResult == true) {
		alert("결과등록 완료");
		fn_commSearch(gfn_getCommTypeInit(), "srchReqResultCount"); // 결과 데이터 재조회
		btn_insert_result.enable = false;
	}
	
	return true;
}

