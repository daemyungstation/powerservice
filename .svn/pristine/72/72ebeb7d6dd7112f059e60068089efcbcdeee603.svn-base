/* 산출 전 청구일자 체크 */
function fn_chkReq(svcid) {
    var return_bool = false;
    var conOk;
    
    if(gReqPayYn == "N") {
		alert("청구시작을 누르고 진행하십시오.");
		return false;
    }
    
    fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit"); // 조회
        
    switch(svcid) {		
        case "saveimsiIchaeDt":		
			if (cms_yn =='Y' && card_yn =='Y'){
				alert("CMS/카드 청구가 이미 완료되었습니다.(산출불가)");
			}
			else if (cms_yn =='Y' && card_yn =='N'){
				conOk = confirm("카드만 산출 하시겠습니까?(CMS청구완료)");
				if (conOk == true) return_bool = true;
			}
			else if (cms_yn =='N' && card_yn =='Y'){
				conOk = confirm("CMS만 산출 하시겠습니까?(카드청구완료)");
				if (conOk == true) return_bool = true;
			} else {
				conOk = confirm("임시건 산출 하시겠습니까?");
				if (conOk == true) return_bool = true;
			}
        break;  
        
        // 특수 산출
        case "saveSpecialIchaeDt":		
			if (cms_yn =='Y' && card_yn =='Y'){
				alert("CMS/카드 청구가 이미 완료되었습니다.(산출불가)");
			} else {
				//conOk = confirm("특수 산출 하시겠습니까?");
				conOk = confirm("["+cbo_special_payMthd.text+"]" + " 특수 산출 하시겠습니까?");
				if (conOk == true) return_bool = true;
			}
        break; 
        
		case "createReqCardData":	
			var nSumCard = 0;
			if (isNaN(edt_sum_card_n_cnt.value) == false){
				nSumCard = edt_sum_card_n_cnt.value;
			}
			if (card_yn =='Y'){
				alert("카드 청구가 이미 완료되었습니다.(산출불가)");
			} else if (nSumCard <= 0){
				alert("산출할 카드 데이터가 없습니다.(산출불가)");			
			} else {
				conOk = confirm("카드 청구 하시겠습니까?");
				if (conOk == true) return_bool = true;
			}			
        break;          
		case "createReqCmsData":	
			
			var nSumCms = 0;
			if (isNaN(edt_sum_cms_n_cnt.value) == false){
				nSumCms = edt_sum_cms_n_cnt.value;
			}
			if (cms_yn =='Y'){
				alert("CMS 청구가 이미 완료되었습니다.(산출불가)");
			} else if (nSumCms <= 0){
				alert("산출할 CMS 데이터가 없습니다.(산출불가)");						
			} else {
				conOk = confirm("CMS 청구 하시겠습니까?");
				if (conOk == true) return_bool = true;
			}			
        break;
        case "createReqCardNData":	
			var nSumCardN = 0;
			if (isNaN(edt_sum_card_an_cnt.value) == false){
				nSumCardN = edt_sum_card_an_cnt.value;
			}
			if (card_nauth_yn == 'Y'){
				alert("카드(무승인) 청구가 이미 완료되었습니다.(산출불가)");
			} else if (nSumCardN <= 0){
				alert("산출할 카드(무승인) 데이터가 없습니다.(산출불가)");			
			} else {
				conOk = confirm("카드(무승인) 청구 하시겠습니까?");
				if (conOk == true) return_bool = true;
			}			
        break;
        
        case "savenauthCancel":		
			if (card_nauth_yn == 'Y'){
				alert("카드(무승인) 청구가 이미 완료되었습니다.(산출불가)");
			} else {
				conOk = confirm("무승인취소 산출 하시겠습니까?");
				if (conOk == true) return_bool = true;
			}
        break;  
        
        case "createReqiniCisCardData" :
            var nSumIniCard = 0;
            
            if (isNaN(edt_sum_card_ini_n_cnt.value) == false)
            {
				nSumIniCard = edt_sum_card_ini_n_cnt.value;
            }
            
			if (card_ini_yn == 'Y') 
			{
				alert("이니시스카드 청구가 이미 완료되었습니다.(산출불가)");
			}
			else if (nSumIniCard <= 0)
			{
				alert("산출할 이니시스카드 데이터가 없습니다.(산출불가)");
			}
			else
			{
				conOk = confirm("이니시스카드 산출 하시겠습니까?");
				if(conOk == true) return_bool = true;
			}
        break;
    }
	return return_bool;
}


