    <Script type="xscript4.0"><![CDATA[/********************************************************************************************/
/* 1. Include Library include, variables Declaration 영역                                  */
/********************************************************************************************/
include "LIB::lib_All.xjs";

var gvMenuId;  // 프로그램 로그 추가
var gWdrwGubun = "카드";  // 산출구분(카드 또는 CMS)

var holy_yn;  // 휴일 체크
var cms_yn;  // CMS청구 체크
var card_yn;  // 카드 청구 체크 
var card_nauth_yn; //카드 무승인 체크
var card_ini_yn; //이니시스 카드 체크


var fv_svcPath;
var fv_virtualFile = null;	// FileDialog 선택을 통한 File Array

// 카드일 경우 csv 파일
var fv_fileFilter = "audio files (*.csv;)|*.csv"; // csv 파일만 저장 가능

var sumCardExtCnt = 0;      // Card 산출건 총합
var sumCmsExtCnt = 0;		// CMS  산출건 총합
var sumCardReqCnt = 0;		// Card 청구건 총합
var sumCmsReqCnt = 0;		// CMS  청구건 총합
var sumCardResultCnt = 0;	// Card 결과건 총합
var sumCmsResultCnt = 0;    // CMS  결과건 총합
var sumCardPayCnt = 0;      // Card 입금건 총합
var sumCmsPayCnt = 0;       // CMS 입금건 총합

var sumCardNExtCnt = 0;      // Card 산출건 총합(무승인)
var sumCardNReqCnt = 0;		// Card 청구건 총합(무승인)
var sumCardNResultCnt = 0;	// Card 결과건 총합(무승인)
var sumCardNPayCnt = 0;      // Card 입금건 총합(무승인)

var sumIniCardExtCnt = 0; // 이니시스Card 산출건 총합
var sumIniCardReqCnt = 0; // 이니시스Card 청구건 총합
var sumIniCardPayCnt = 0; // 이니시스Card 입금건 총합
var sumIniCardResultCnt = 0; // 이니시스Card 결과건 총합

var gReqPayYn = ""; 


/********************************************************************************************/
/* 2. Form 관련(onload 및 Form 이벤트) 처리                                           	*/
/********************************************************************************************/
function DL520001_onload(obj:Form, e:LoadEventInfo)
{
		trace(ds_payMthd.saveXML());
	
	///////////////////////////////////////////////////////////////////////////////////////////
	// 프로그램 로그 추가
	gvMenuId = GV_LOGMENUID;
	///////////////////////////////////////////////////////////////////////////////////////////

	gfn_initForm(obj);
	gfn_createPagingDataset(obj); // 페이징처리 위한 데이터셋 설정;
	
	cbo_payMthd.index = 0;
	cbo_special_payMthd.index = 0;
	cal_req_day.value = gfn_addDate(gfn_getToday(),1);
	
	fn_init();
	
	fv_svcPath = gfn_getServiceUrl("fileUrl");
	
	gfn_getList("basVl", "req_pay_yn", null, null, "BasVl", "ds_basVlInfo");  // 공통코드조회
}


/********************************************************************************************/
/* 3. Callback(gfn_Transaction 호출 후 처리내용)                                   		*/
/********************************************************************************************/
function fn_callBack(svcid, errcd, errmsg) 
{
	if (errcd != 0) {
	    switch (svcid) {
			case "srchWdrwbyReqbit":
			break;
			case "convertFile":
			alert("파일 변환실패");
			break;
	    }
		return false;
	} else {
		switch (svcid) {
			case "BasVl":
				
				gReqPayYn = ds_basVlInfo.getColumn(ds_basVlInfo.findRow("bas_vl_nm", "req_pay_yn"), "bas_vl");
				if(gReqPayYn == "Y") {
					btn_add.enable = false;
					btn_cancel.enable = true;
				} else if(gReqPayYn == "N") {
					btn_add.enable = true;
					btn_cancel.enable = false;
				}
			break;
			case "srchWdrwbyReqbit":  // 1차산출 구분별 조회

				if(cal_req_day.value <= gfn_getToday()){
					btn_tempMmbrWdrw.enable = false;
					btn_nauthCancel.enable = false;
					btn_wdrwReq.enable = false;
				}else{
					btn_tempMmbrWdrw.enable = true;
					btn_nauthCancel.enable = true;
					btn_wdrwReq.enable = true;
				}
				
				//청구일에 청구 모두 끝났으면 버튼을 막는다 
				if (cms_yn =='Y' && card_yn == 'Y' && card_nauth_yn == 'Y' && card_ini_yn == 'Y'){
					btn_tempMmbrWdrw.enable = false;
					btn_nauthCancel.enable = false;
					btn_wdrwReq.enable = false;
				}
				
				//청구일이 휴무일이면 버튼을 막는다.
				if (holy_yn =='H'){
					alert("해당일은 휴무일입니다.");
					btn_tempMmbrWdrw.enable = false;
					btn_nauthCancel.enable = false;
					btn_wdrwReq.enable = false;
					btn_card_down.enable = false;
					return;
				}	
                //trace("holy_yn :" + holy_yn);
                //trace("cms_yn :" + cms_yn);
                //trace("card_yn :" + card_yn);
                
                // 특수산출 버튼권한 추가
                var sUserId = gds_UserInfo.getColumn(gds_UserInfo.rowposition,"user_id");
                if ( sUserId == '2012060331' // 임동진
				  || sUserId == '2016040005' // 박세라
				  || sUserId == '2017080003' // 정승철
				  || sUserId == '2017120011' // 우옥두
				) {
				    btn_specialWdrw.enable = true;
				    cbo_special_payMthd.enable = true;
				}
				
				// 1차산출 구분별 조회
				fn_postSearch("srchWdrwbyReqbit");
			break;
			
			case "saveimsiIchaeDt": // 임시건 산출
			alert("임시건 산출완료");
			fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit"); // 재조회
			break;
			
			case "saveSpecialIchaeDt": // 특수 산출
			alert("특수 산출완료");
			fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit"); // 재조회
			break;

			case "createReqCmsData":
				alert("[CMS]" + " 파일 작성 완료");
				ds_list.clearData();
				ds_card_list.clearData();
				ds_cms_list.clearData();	
				fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit");
			break;
			
			case "createReqCardData":
				alert("[CARD]" + " 파일 작성 완료");
				ds_list.clearData();
				ds_card_list.clearData();
				ds_cms_list.clearData();			
				fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit");
			break;			
			
			case "createReqData":  // 파일작성 및 결과 업데이트 후
				ds_list.clearData();
				ds_card_list.clearData();
				ds_cms_list.clearData();
				alert("[" + gWdrwGubun + "]" + " 파일 작성 완료");
				fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit");
			break;
			
			case "createReqCardNData":
				alert("[CARD(무)]" + " 파일 작성 완료");
				ds_list.clearData();
				ds_card_list.clearData();
				ds_cms_list.clearData();			
				fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit");
			break;
			
			// 결과 데이터
			case "srchReqResultCount":				
				fn_setComponentsUseYN();
			break;
			
			// 파일변환 (** 결과반영)
			case "convertFile":
				alert("파일 변환완료");
				fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit"); // 재조회
			break;
			
			case "savenauthCancel":
				alert("무승인취소 산출완료");
				fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit"); // 재조회
			break;
			
			case "createReqiniCisCardData" :
				alert("이니시스카드" + " 파일 작성 완료");
				ds_list.clearData();
				ds_card_list.clearData();
				ds_cms_list.clearData();			
				fn_commSearch(gfn_getCommTypeInit(), "srchWdrwbyReqbit");
			break;
			
			case "saveBasV" :
			break;
		}
	}
}

/********************************************************************************************/
/**   4. Validation 처리                                                                   	*/
/********************************************************************************************/
function fn_preSearch(sType, svcid) {
    var return_bool = false;
    switch(svcid) {
        case "srchWdrwbyReqbit":               
			// 초기화
			fn_init();
			return_bool = true;
        break;
        
        // 결과 데이터 조회전
        case "srchReqResultCount":
		//ds_result_list.clearData();
			return_bool = true;
        break;
    }
	
	ds_inqCndt.clearData();
    ds_inqCndt.addRow();
