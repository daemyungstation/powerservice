//공통 초기화 처리(Login화면에서 로그인성공 후 처리)
function fn_Comm_Init(sWsoRvYn, sBzdpDiv)
{
	// 파일 실제 존재여부용 칼럼 생성
	gds_Menu.addColumn("isExists", "int", 1);

	ds_findMenu.assign(gds_Menu);

	//trace(gds_Menu.saveXML());

	if(sWsoRvYn == "Y") {
		gds_Menu.filter("bzdpDivCd=='"+sBzdpDiv+"'");
	} else if(sBzdpDiv == "PMS" || sBzdpDiv == "MET") {
		gds_Menu.filter("bzdpDivCd=='"+sBzdpDiv+"'");
	}
	
	// TopFrame 초기화 처리
	ds_Menu.copyData(gds_Menu, true);
	
	ds_Menu.filter("menuLvlVl==0");
	ds_Menu_Popup.copyData(gds_Menu, true);
	
	fn_Init();
	
	// LeftFrame 초기화 처리
	ap_LeftFrame.form.ds_Menu.copyData(gds_Menu, true);
	ap_LeftFrame.form.fn_setTreeState(false);
	ap_LeftFrame.form.ds_Menu.filter("menuCd==M");
	ap_LeftFrame.form.fn_init();	// 환경설정정보 반영
}

function fn_Init()
{
	var nBtnSize_H	= 38;
	var nBtnSize_W;
	var nLeft 		= 0;
	var nGap  		= 0;

	var objFont = gfn_getObjFont(11,"Dotum");
	objFont.bold = true;

	for(var i=0;i<this.ds_Menu.rowcount;i++)
	{
		var sMenuText 	= ds_Menu.getColumn(i, "menuNm");
		var objTextSize = gfn_getTextSize(sMenuText, objFont);
		
		nBtnSize_W = objTextSize.cx+30;
		
		// 메뉴버튼 생성
		var objAddBtn = new Button("name", nLeft, 0, nLeft+nBtnSize_W, nBtnSize_H);
		var sBtnName = "btn_Add_" + ds_Menu.getColumn(i, "menuCd");
		objAddBtn.name = sBtnName;
		div_Menu.addChild(sBtnName, objAddBtn);
		objAddBtn.text = sMenuText;
		objAddBtn.class = "btn_TF_Menu";
		objAddBtn.show();
		
		objAddBtn.onclick.setHandler(fn_ShowMenu);
		objAddBtn.onmouseenter.setHandler(fn_MouseEnterMenu);
		objAddBtn.onmouseleave.setHandler(fn_MouseLeaveMenu);
		
		nLeft += nBtnSize_W + nGap;
	}

	div_Menu.resetScroll();
	
	fn_BtnEnable();
}

