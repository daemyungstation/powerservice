// 공통코드 초기화 처리
function fn_initCommCd(bInit)
{
	gfn_setWaitCursor(true);

	// 기준일자 설정
	_gfn_getStandardDate(ds_inputDate, gfn_getKrpFileNm_ComCode(), 	"KRP_CMM_COMN_CODE",	bInit);		// 공통코드 기준일자
	_gfn_getStandardDate(ds_inputDate, gfn_getKrpFileNm_ComMsg(), 	"KRP_CMM_COMN_MSG", 	bInit);		// 공통메세지 기준일자

	gfn_kwDataInit();
	gfn_setKwData_trscCode("SYS00002R11");	

	var inputDatas  = "ds_Login=ds_Login";
		inputDatas += " ds_inputDate=ds_inputDate";

	var outputDatas  = "ds_ComCode=ds_ComCode"
		outputDatas += " ds_ComMsg=ds_ComMsg";

	gfn_tranMultiDIS(gfn_getCommTypeAppend()
				, inputDatas
				, outputDatas
				, ""
				, false);	
				
	// 공통코드, 공통메세지 처리
	if(!gfn_getTranCommInfo(ds_ComCode, bInit, gds_ComCode, "02")) { 
		alert("공통코드 처리 중 오류가 발생했습니다.");
		//return;	// Comn Code	
	}
	
	if(!gfn_getTranCommInfo(ds_ComMsg, bInit, gds_ComMsg, 	"01")) {
		alert("공통메세지 처리 중 오류가 발생했습니다.");
		//return;	// Comn MSG
	}				
	
	// 기준일자 설정
	_gfn_getStandardDate(ds_inputDate, gfn_getKrpFileNm_OrdrTyp(), 		"KRP_CMM_COMN_ORDERTYP",	bInit);		// 주문유형코드 기준일자
	_gfn_getStandardDate(ds_inputDate, gfn_getKrpFileNm_LgstCntr(), 	"KRP_CMM_COMN_LGSTCNTR",	bInit);		// 물류센터및거점창고 기준일자
	_gfn_getStandardDate(ds_inputDate, gfn_getKrpFileNm_BswrDtrsCd(), 	"KRP_BOP_BSWRDTRSCD",		bInit);		// 업무별 직책코드(사업자영역) 기준일자
	_gfn_getStandardDate(ds_inputDate, gfn_getKrpFileNm_OrgzLvlCd(), 	"KRP_BOP_ORGZLVLCD",		bInit);		// 조직레벨코드(사업자영역) 기준일자

	gfn_kwDataInit();
	gfn_setKwData_trscCode("SYS00002R12");	

	var inputDatas = "ds_Login=ds_Login";
		inputDatas += " ds_inputDate=ds_inputDate";

	var outputDatas  = "ds_OrdrTyp=ds_OrdrTyp"
		outputDatas += " ds_LgstCntr=ds_LgstCntr";
		outputDatas += " ds_BswrDtrsCd=ds_BswrDtrsCd";
		outputDatas += " ds_OrgzLvlCd=ds_OrgzLvlCd";

	gfn_tranMultiDIS(gfn_getCommTypeAppend()
				, inputDatas
				, outputDatas
				, ""
				, false);	
					
	if(GV_BZDPDIVCD != "PMS" && GV_BZDPDIVCD != "MET") {
 		if(!gfn_getTranCommInfo(ds_OrdrTyp, bInit, gds_OrdrTyp, "03")) {
			alert("주문유형코드 처리 중 오류가 발생했습니다.");
			//return;	// 주문유형코드 처리
		}	
 		
 		if(!gfn_getTranCommInfo(ds_LgstCntr, bInit, gds_LgstCntr, "06")) {
			alert("물류 센터 및 거점 창고 코드 처리 중 오류가 발생했습니다.");
			//return;	// 물류 센터 및 거점 창고 코드
		}
			
 		if(!gfn_getTranCommInfo(ds_BswrDtrsCd, bInit, gds_BswrDtrsCd, "07")) {
			alert("업무별 직책 코드(사업자 영역) 처리 중 오류가 발생했습니다.");
			//return;	// 업무별 직책 코드(사업자 영역)
		}
			
 		if(!gfn_getTranCommInfo(ds_OrgzLvlCd, bInit, gds_OrgzLvlCd, "08")) {
			alert("조직 레벨 코드(사업자 영역) 처리 중 오류가 발생했습니다.");
			//return;	// 조직 레벨 코드(사업자 영역)
		}	
	}	
	
	gfn_setWaitCursor(false);
}


