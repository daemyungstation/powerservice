function btn_Left_onclick(obj:Button,  e:ClickEventInfo)
{
	var npos = div_Menu.hscrollbar.pos - div_Menu.position.width;
	
	var nrealpos = fn_MenuPositionChange("L", npos);
	
	div_Menu.hscrollbar.pos = nrealpos;
	
	fn_BtnEnable();
}

function btn_Right_onclick(obj:Button,  e:ClickEventInfo)
{
	var npos = div_Menu.hscrollbar.pos + div_Menu.position.width;
	
	var nrealpos = fn_MenuPositionChange("R", npos);
	
	div_Menu.hscrollbar.pos = nrealpos;
	
	fn_BtnEnable();
}

function fn_MenuPositionChange(sGbn, npos) 
{
	var objBtnMenu = div_Menu.components;
	
	if(sGbn == "L") {
		for(var i=objBtnMenu.length-1;i>=0;i--)
		{
			if(npos >= objBtnMenu[i].position.left) {
				return objBtnMenu[i].position.left;
			}
		}
	} else if(sGbn == "R") {
		for(var i=0;i<objBtnMenu.length;i++)
		{
			if(npos <= objBtnMenu[i].position.right) {
				return objBtnMenu[i].position.left;
			} else {
				if(npos >= div_Menu.hscrollbar.max)
					return div_Menu.hscrollbar.max;
			}
		}
	}	
}

function div_Menu_onsize(obj:Div, e:SizeEventInfo)
{
	fn_BtnEnable();
}

function fn_BtnEnable()
{
	var npos 	= div_Menu.hscrollbar.pos;
	var nmin 	= div_Menu.hscrollbar.min;
	var nmax 	= div_Menu.hscrollbar.max;
	var nDivW 	= div_Menu.position.width;
	
	if(npos == 0) {
		if(npos == nmin) {
			btn_Left.enable 	= false;
			btn_Right.enable 	= true;
		} else if(npos == nmax) {
			btn_Left.enable 	= true;
			btn_Right.enable 	= false;
		} else {
			btn_Left.enable 	= false;
			btn_Right.enable 	= false;
		}
	} else if(npos > 0) {
		if(npos == nmax) {
			btn_Left.enable 	= true;
			btn_Right.enable 	= false;
		} else {
			btn_Left.enable 	= true;
			btn_Right.enable 	= true;
		}
	} else if(npos < 0) {
		btn_Left.enable 		= false;
		btn_Right.enable 		= false;
	}
}

